A,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,EAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,cAAA;;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,KAAA;;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,uBAAA;AACA,SAJA,MAIA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,kBAAA;AACA;AACA,OAhBA;AAiBA;AApLA;AAvCA,CAAA","sourcesContent":["\r\n<template>\r\n  <div class=\"main-content\">\r\n    <main >\r\n   \r\n      <div class=\"header-bio\">\r\n           \r\n        <div class=\"container d-flex justify-content-between align-items-center px-4\">\r\n          <div class=\"d-flex\">\r\n            <img src=\"/images/left-arrow.png\" width=\"10\" @click=\"goBack()\">\r\n            <h4 class=\"font-weight-700 px-3\"> Document</h4>\r\n          </div>\r\n        </div>\r\n\r\n      </div>\r\n      <div class=\"main-body4\" id=\"main_body\">\r\n        \r\n        <div class=\"mt-4 flex-align px-3\" style=\"gap:10px;\">\r\n          <div  class=\"input-group input-group-lg mb-0\">\r\n            <input  type=\"text\" class=\"form-control\" autocomplete=\"off\" placeholder=\"Select Farmer\" :value=\"farmer.id?farmer.name+' ('+farmer.farmer_code+')':''\" readonly @click=\"$bvModal.show('select-farmer')\">\r\n          </div>\r\n        </div>\r\n\r\n        <b-modal id=\"select-farmer\" title=\"Select Farmer\" hide-footer>\r\n          <div>\r\n            <div class=\"\" v-show=\"input_col==0\">\r\n              <h4 class=\"text-light pr-4 py-2\"> ICS: </h4>\r\n              <div class=\"inspection-btn\">\r\n                <button type=\"button\" class=\"btn\" v-for=\"(ics,key) in all_ics\" :class=\"filter.ics==ics.id?'active-btn':''\" @click=\"filter.ics=ics.id;nextCol();changeICS()\">{{ics.name}}</button>\r\n                <input type=\"hidden\" v-model=\"filter.ics\" :class=\"'check_field_'+0\" required>\r\n              </div>\r\n            </div>\r\n            <div class=\"\" v-show=\"input_col==1\">\r\n              <h4 class=\"text-light pr-4 py-2\"> VFC: </h4>\r\n              <div class=\"inspection-btn\">\r\n                <button type=\"button\" class=\"btn\" v-for=\"(vfc,key) in all_vfc\" :class=\"filter.vfc==vfc.id?'active-btn':''\" @click=\"filter.vfc=vfc.id;nextCol();searchFarmers()\">{{vfc.name}}</button>\r\n                <input type=\"hidden\" v-model=\"filter.vfc\" :class=\"'check_field_'+1\" required>\r\n              </div>\r\n            </div>\r\n            <div class=\"\" v-show=\"input_col==2\">\r\n              <h4 class=\"text-light pr-4 py-2\"> Farmers: </h4>\r\n              <div class=\"inspection-btn\">\r\n                <button type=\"button\" class=\"btn\" v-for=\"(farmer2,key) in farmers\" :class=\"farmer.id==farmer2.id?'active-btn':''\" @click=\"farmer=farmer2;nextCol()\">{{farmer2.farmer_code}}-{{farmer2.name}} </button>\r\n                <input type=\"hidden\" v-model=\"farmer.id\" :class=\"'check_field_'+2\" required>\r\n              </div>\r\n            </div>\r\n            <hr class=\"my-4\">\r\n            <div class=\"filter-item flex-align float-right\">\r\n              <div class=\"\" style=\"display: flex;justify-content: flex-end;gap: 10px;\">\r\n                <button type=\"button\" class=\"btn btn-light\" @click=\"input_col>0?backCol():''\"> <i class=\"fa fa-angle-left\" style=\"\"></i> Back</button>\r\n                <button type=\"button\" class=\"btn btn-secondary\" @click=\"nextCol()\">{{input_col==2?'Select':'Next'}} <i class=\"fa fa-angle-right\" style=\"color:white;\"></i></button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n\r\n        </b-modal>\r\n\r\n        <div class=\"my-3\">\r\n          <div class=\"mx-3 my-2 px-3 py-2 card\" v-for=\"farmer_file in farmer_files\" @click=\"crnt_type=farmer_file.file_type;crnt_file=farmer_file\">\r\n            <div class=\"d-flex justify-content-between align-items-center  px-0 py-1\">\r\n              <h4 class=\"font-weight-500 text-18 pr-2\">{{farmer_file.file_type}}</h4>\r\n              <h4 class=\"badge\" :class=\"farmer_file.status=='Approved'?'badge-success':farmer_file.status=='Pending'?'badge-warning':farmer_file.status=='Diclined'?'badge-dager':'badge-primary'\">{{farmer_file.status}}</h4>\r\n            </div>\r\n            <div class=\"d-flex justify-content-between px-0 py-1 \">\r\n              <div>\r\n                <h5 class=\" font-weight-500 text-light\">{{dateFormate(farmer_file.created_on)}}</h5>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n          <div v-if=\"farmer.id\">\r\n            <div class=\" m-3\">\r\n              <h4 class=\"text-light pr-4 pt-2\">{{crnt_file.id?'Update':'New'}} File <span class=\"badge\" :class=\"crnt_file.status=='Approved'?'badge-success':crnt_file.status=='Pending'?'badge-warning':crnt_file.status=='Diclined'?'badge-dager':'badge-primary'\" v-if=\"crnt_file.id\">{{crnt_file.status}}</span></h4>\r\n              <b-dropdown id=\"dropdown-1\" block variant=\"primary\" :text=\"crnt_type\" class=\"dropdown-container\">\r\n                <b-dropdown-item v-for=\"file_type in file_types\" :active=\"crnt_type==file_type\" @click=\"crnt_type=file_type;clearFileinput()\">{{file_type}}</b-dropdown-item>\r\n              </b-dropdown>\r\n            </div> \r\n            <div class=\"m-3\" v-if=\"crnt_type!='Select file type'\">\r\n              <h4 class=\"text-light pr-4 py-2\">Upload Section</h4>\r\n              <input type=\"hidden\" name=\"file_type\" :value=\"crnt_type\">\r\n              <label class=\"w-100\">\r\n               <div class=\"attatch-photo\" >\r\n                  <input  type=\"file\" id=\"up_input\" name=\"file\" class=\"form-control text-center w-100\" @change=\"selectFile\" />\r\n                  <div class=\"attacth_icon\">\r\n                    <img src=\"/images/documents.png\" v-if=\"previewFile\" style=\"height:60px;margin-bottom:10px\">\r\n                    <img src=\"/images/upload.png\" v-else>\r\n                    <h5 class=\"text-light\">{{previewFile?previewFile:'Upload a photo or Take a picture'}}</h5>\r\n                  </div>\r\n                </div>\r\n              </label>\r\n              <button :type=\"is_upload?'button':'submit'\" class=\"btn btn-primary w-100\">{{is_upload?'Uploading...':'Upload'}}</button>\r\n            </div>\r\n            <b-modal id=\"confirmPopup\" title=\"Alert\"  hide-footer>\r\n              <div class=\"modal-body p-0 m-0\">\r\n                  <div class=\"row mx-0 mb-3\">\r\n                      <div class=\"col-md-12\">\r\n                        Are you sure want to this action?\r\n                      </div>\r\n                  </div>\r\n              </div>\r\n              <div class=\"modal-footer\">\r\n                  <button type=\"button\" class=\"btn btn-success px-4\" @click=\"dataForm('yes')\" v-if=\"is_upload==false\">Yes</button>\r\n                  <button type=\"button\" class=\"btn btn-outline-secondary px-4\" @click=\"$bvModal.hide('confirmPopup')\">No</button>\r\n              </div>\r\n            </b-modal>\r\n          </div>\r\n        </form>\r\n      </div>\r\n      <div class=\"filter-footer\">\r\n        <div class=\"footer-item2 d-flex align-items-center\" @click=\"$router.push('/')\">\r\n          <img src=\"/images/home.png\" class=\"\" width=\"25\">\r\n          <h5 class=\"m-0 pl-2 text-white\">Home</h5>\r\n        </div>\r\n      </div>\r\n\r\n\r\n    </main>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Home\"\r\n  },\r\n  data() {\r\n    return {\r\n      input_col:0,\r\n      crnt_type:'Select file type',\r\n      previewFile:'',\r\n      filter:{ics:null,panchayath:null,vfc:null,count:0},\r\n      farmer:{id:null,name:'', farmer_code:''},\r\n      farmers:[],\r\n      farmer_files:[],\r\n      crnt_file:{id:null},\r\n      is_upload:false,\r\n      not_start:true,\r\n      action:'create',\r\n      lock_btn:false,\r\n    };\r\n  },\r\n  mounted(){\r\n    this.action = this.$route.params.action\r\n    this.checkLogin()\r\n    this.getIcs()\r\n    if(this.action=='edit'){\r\n      this.crnt_file.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      if(this.$route.query.farmer){\r\n        this.farmer.id = this.$route.query.farmer\r\n        if(this.$route.query.file_type){\r\n          this.crnt_type = this.$route.query.file_type\r\n        }\r\n        this.getFarmer()\r\n      }else{\r\n        this.$bvModal.show('select-farmer')\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    nextCol(){\r\n      var self = this\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      setTimeout(function(){\r\n        self.lock_btn = false\r\n        var is_input = document.getElementsByClassName('check_field_'+self.input_col)\r\n        var next = true\r\n        for(var i=0;i<is_input.length;i++){\r\n          is_input[i].style.border = 'none'\r\n          if(is_input[i].value==''){\r\n            is_input[i].style.border = '1px red solid'\r\n            next = false\r\n          }\r\n        }\r\n        if(next){\r\n          if(self.input_col==2){\r\n            self.getFarmerFiles()\r\n            self.input_col--\r\n          }\r\n          self.input_col++\r\n        }else{\r\n          self.showAlert('Please select a farmer!')\r\n        }\r\n      },300)\r\n    },\r\n    backCol(){\r\n      this.input_col--\r\n    },\r\n    getForm(){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/employee/farmer_files/'+this.crnt_file.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.crnt_file = jsonData\r\n        this.crnt_type = jsonData.file_type\r\n        this.farmer = jsonData.farmer_detail\r\n      })\r\n    },\r\n    getFarmerFiles(){\r\n      this.$bvModal.hide('select-farmer')\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/employee/farmer_files/?farmer='+this.farmer.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmer_files = jsonData\r\n      })\r\n    },\r\n    searchFarmers(line=''){\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      formdata.append('vfc',this.filter.vfc)\r\n      formdata.append('search_type','sample_procurement')\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/search_farmers/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmers = jsonData\r\n      })\r\n    },\r\n    getFarmer(){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/employee/farmers/'+this.farmer.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmer = jsonData\r\n        this.getFarmerFiles()\r\n      })\r\n    },\r\n    clearFileinput() {\r\n      this.previewFile = ''\r\n      this.crnt_file={id:null}\r\n      if(document.getElementById('up_input')){\r\n        document.getElementById('up_input').value = \"\"\r\n      }\r\n    },\r\n    selectFile(e) {\r\n      var img_file = e.target.files[0];\r\n      if(img_file){\r\n        this.previewFile = ''\r\n        var self=this\r\n        setTimeout(function(){\r\n          self.previewFile = img_file.name\r\n          // self.previewFile = URL.createObjectURL(img_file);\r\n        },10)\r\n      }\r\n    },\r\n    dataForm(action='no') {\r\n      if(action=='no'){\r\n        this.$bvModal.show('confirmPopup')\r\n        return false\r\n      }\r\n      this.$bvModal.hide('confirmPopup')\r\n      this.is_upload=true\r\n      if(this.crnt_file.id){\r\n        this.updateFormdata()\r\n      }else{\r\n        this.createFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('farmer',this.farmer.id)\r\n      formdata.append('action','Create')\r\n      formdata.append('year',this.this_year)\r\n      formdata.append('aeo','')\r\n      formdata.append('status','Pending')\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmer_files/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.getFarmerFiles()\r\n          this.is_upload = false\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('farmer',this.farmer.id)\r\n      formdata.append('action','Update')\r\n      formdata.append('year',this.this_year)\r\n      formdata.append('aeo','')\r\n      formdata.append('status','Pending')\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmer_files/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.getFarmerFiles()\r\n          this.is_upload = false\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n@import './style/style.css';\r\n#select-farmer .modal-body{\r\n    overflow-y: auto;\r\n    height: 80vh;\r\n}\r\n#select-farmer .inspection-btn{\r\n    max-height: 50vh;\r\n    overflow-y: auto;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/app/pages"}]}