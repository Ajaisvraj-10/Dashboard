           </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link :to=\"'/app/unloading/view/'+id\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n          <h5 v-if=\"form_data.id\">Final Weight</h5>\r\n          <h5 v-else>Initial Weight</h5>\r\n        </b-col>\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mb-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">WareHouse  : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"warehouse\" v-model=\"form_data.warehouse\">\r\n                                  <option v-for=\"warehouse in warehouses\" :value=\"warehouse.id\">{{warehouse.name}}</option>\r\n                              </select>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Wb Slip : </span>\r\n                              <input placeholder=\" Wb Slip\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.Wb_slip\" name=\"Wb_slip\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\" v-show=\"!form_data.id\">\r\n                              <span style=\"width:120px;\"> Initial Wb (Kg): </span>\r\n                              <input placeholder=\"Initial Wb\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.initial_Wb\" name=\"initial_Wb\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\" v-if=\"form_data.id\">\r\n                              <span style=\"width:120px;\">Final Wb (Kg): </span>\r\n                              <input placeholder=\" Final Wb\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.final_Wb\" name=\"final_Wb\" required>\r\n                            </div>\r\n                            <div>\r\n                              <input type=\"hidden\" name=\"normal_procurement\" :value=\"id\">\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:[],\r\n      id:null,\r\n      action:'create',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'unloading'\r\n    this.id = this.$route.params.id\r\n    this.getForm()\r\n    this.getWarehouses()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/weighting_bridge/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.action = 'edit'\r\n          this.form_data = jsonData\r\n        }\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/weighting_bridge/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/weighting_bridge/'+this.form_data.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/unloading"}]}