       <span v-if=\"props.column.field == 'id'\">\r\n            <router-link :to=\"'/app/maintenance/machine-issue/edit/'+props.row.id\">\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"selectFarmer(props.row)\" v-if=\"props.row.status=='Maintenance'\">Fix now</b-button>\r\n              <b-button  variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"selectFarmer(props.row)\" v-else>Fixed</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'machine_detail'\">\r\n            {{props.row.machine_detail.name}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'department'\">\r\n            {{props.row.machine_detail.category}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'date'\">\r\n            {{dateFormate(props.row.start_date)}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'start_time'\">\r\n            {{timeFormate(props.row.start_time)}}\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Machine\",\r\n          field: \"machine_detail\"\r\n        },\r\n        {\r\n          label: \"Department\",\r\n          field: \"department\"\r\n        },\r\n        {\r\n          label: \"Date\",\r\n          field: \"date\"\r\n        },\r\n        {\r\n          label: \"Issude Time\",\r\n          field: \"start_time\"\r\n        },\r\n        {\r\n          label: \"Status\",\r\n          field: \"status\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n      ],\r\n      rows: [],\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'maintenance'\r\n    this.getIssuedMachines()\r\n  },\r\n  methods: {\r\n    getIssuedMachines() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/break_down/?break_type=machine_issues', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/maintenance"}]}