">\r\n        <span style=\"width:120px;\"> State: </span>\r\n        <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"state_id\" name=\"state\" @change=\"restLocation(2)\" required :disabled=\"readonly\">\r\n            <option selected disabled>Select a state</option>\r\n            <option v-for=\"state in states\" :value=\"state.id\" v-if=\"state.country==country_id\">{{state.name}}</option>\r\n        </select>\r\n    </div>\r\n    <div class=\"o_thread_search_input\" v-if=\"location_limit>3\">\r\n        <span style=\"width:120px;\"> District: </span>\r\n        <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"district_id\" name=\"district\" @change=\"restLocation(3)\" required :disabled=\"readonly\">\r\n            <option selected disabled>Select a district</option>\r\n            <option v-for=\"district in districts\" :value=\"district.id\" v-if=\"district.state==state_id\">{{district.name}}</option>\r\n        </select>\r\n    </div>\r\n    <div class=\"o_thread_search_input\" v-if=\"location_limit>4\">\r\n        <span style=\"width:120px;\"> Panchayath: </span>\r\n        <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"panchayath_id\" name=\"panchayath\" @change=\"restLocation(4)\" required :disabled=\"readonly\">\r\n            <option selected disabled>Select a panchayath</option>\r\n            <option v-for=\"panchayath in panchayaths\" :value=\"panchayath.id\" v-if=\"panchayath.district==district_id\">{{panchayath.name}}</option>\r\n        </select>\r\n    </div>\r\n    <div class=\"o_thread_search_input\" v-if=\"location_limit>5\">\r\n        <span style=\"width:120px;\"> Revenue Village: </span>\r\n        <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"revenue_village_id\" name=\"revenue_village\" @change=\"restLocation(5)\" required :disabled=\"readonly\">\r\n            <option selected disabled>Select a revenue village</option>\r\n            <option v-for=\"revenue_village in revenue_villages\" :value=\"revenue_village.id\" v-if=\"revenue_village.panchayath==panchayath_id\">{{revenue_village.name}}</option>\r\n        </select>\r\n    </div>\r\n    <div class=\"o_thread_search_input\" v-if=\"location_limit>6\">\r\n        <span style=\"width:120px;\"> Local Village: </span>\r\n        <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"local_village_id\" name=\"local_village\" @change=\"restLocation(6)\" required :disabled=\"readonly\">\r\n            <option selected disabled>Select a local village</option>\r\n            <option v-for=\"local_village in local_villages\" :value=\"local_village.id\" v-if=\"local_village.revenue_village==revenue_village_id\">{{local_village.name}}</option>\r\n        </select>\r\n    </div>\r\n    <div class=\"o_thread_search_input\" v-if=\"location_limit>7\">\r\n        <span style=\"width:120px;\"> Post Office: </span>\r\n        <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"post_office_id\" name=\"post_office\" @change=\"restLocation(7)\" required :disabled=\"readonly\">\r\n            <option selected disabled>Select a post office</option>\r\n            <option v-for=\"post_office in post_offices\" :value=\"post_office.id\" v-if=\"post_office.local_village==local_village_id\">{{post_office.name}}</option>\r\n        </select>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  props: ['id','location_limit','readonly'],\r\n  data() {\r\n    return {\r\n      countries:[],\r\n      states:[],\r\n      districts:[],\r\n      panchayaths:[],\r\n      revenue_villages:[],\r\n      local_villages:[],\r\n      post_offices:[],\r\n      country_id:null,\r\n      state_id:null,\r\n      district_id:null,\r\n      panchayath_id:null,\r\n      revenue_village_id:null,\r\n      local_village_id:null,\r\n      post_office_id:null,\r\n      loc_type:'',\r\n    };\r\n  },\r\n  mounted() {\r\n    if(this.location_limit==1){\r\n      this.loc_type = 'countries'\r\n    }else if(this.location_limit==2){\r\n      this.loc_type = 'state'\r\n    }else if(this.location_limit==3){\r\n      this.loc_type = 'district'\r\n    }else if(this.location_limit==4){\r\n      this.loc_type = 'panchayath'\r\n    }else if(this.location_limit==5){\r\n      this.loc_type = 'revenue_village'\r\n    }else if(this.location_limit==6){\r\n      this.loc_type = 'local_village'\r\n    }else if(this.location_limit>=7){\r\n      this.loc_type = 'post_office'\r\n    }\r\n    if(this.id){\r\n        this.getLocation()\r\n    }else{\r\n        this.getCountries()\r\n        this.getStates()\r\n        this.getDistricts()\r\n        this.getPanchayaths()\r\n        this.getRevenueVillages()\r\n        this.getLocalVillage()\r\n        this.getPostOffices()\r\n    }\r\n  },\r\n  methods: {\r\n    getLocation() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/'+this.loc_type+'/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var detail = jsonData;\r\n        if(this.location_limit>=7){\r\n          this.post_office_id = detail.id\r\n          detail = detail.local_village_detail\r\n        }\r\n        if(this.location_limit>=6){\r\n          this.local_village_id = detail.id\r\n          detail = detail.revenue_village_detail\r\n        }\r\n        if(this.location_limit>=5){\r\n          this.revenue_village_id = detail.id\r\n          detail = detail.panchayath_detail\r\n        }\r\n        if(this.location_limit>=4){\r\n          this.panchayath_id = detail.id\r\n          detail = detail.district_detail\r\n        }\r\n        if(this.location_limit>=3){\r\n          this.district_id = detail.id\r\n          detail = detail.state_detail\r\n        }\r\n        if(this.location_limit>=2){\r\n          this.state_id = detail.id\r\n          detail = detail.country_detail\r\n        }\r\n        if(this.location_limit>=1){\r\n          this.country_id = detail.id\r\n        }\r\n        this.getCountries()\r\n        this.getStates()\r\n        this.getDistricts()\r\n        this.getPanchayaths()\r\n        this.getRevenueVillages()\r\n        this.getLocalVillage()\r\n        this.getPostOffices()\r\n      })\r\n    },\r\n    getCountries() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/countries/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.countries = jsonData\r\n      })\r\n    },\r\n    getStates() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/state/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.states = jsonData\r\n      })\r\n    },\r\n    getDistricts() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/district/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.districts = jsonData\r\n      })\r\n    },\r\n    getPanchayaths() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/panchayath/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.panchayaths = jsonData\r\n      })\r\n    },\r\n    getRevenueVillages() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/revenue_village/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.revenue_villages = jsonData\r\n      })\r\n    },\r\n    getLocalVillage() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/local_village/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.local_villages = jsonData\r\n      })\r\n    },\r\n    getPostOffices() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/post_office/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.post_offices = jsonData\r\n      })\r\n    },\r\n    restLocation(location_limit){\r\n       if(location_limit<7){\r\n          this.post_office_id = null\r\n        }\r\n        if(location_limit<6){\r\n          this.local_village_id = null\r\n        }\r\n        if(location_limit<5){\r\n          this.$emit('panchayathId', this.panchayath_id)\r\n          this.revenue_village_id = null\r\n        }\r\n        if(location_limit<4){\r\n          this.panchayath_id = null\r\n        }\r\n        if(location_limit<3){\r\n          this.district_id = null\r\n        }\r\n        if(location_limit<2){\r\n          this.state_id = null\r\n        }\r\n        if(location_limit<1){\r\n          this.country_id = null\r\n        }\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/views/app/oodo/components"}]}