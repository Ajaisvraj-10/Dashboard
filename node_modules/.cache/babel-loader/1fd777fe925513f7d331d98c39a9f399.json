pan v-if=\"props.column.field == 'id'\">\r\n            <!-- <router-link :to=\"'/app/fpo/farmer/edit/'+props.row.id\">\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">Edit</b-button>\r\n            </router-link> -->\r\n            <router-link :to=\"'/app/fpo/farmer/view/'+props.row.id\">\r\n              <b-button  variant=\"primary\" class=\" px-3 py-1  d-sm-block mx-1\">View</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'phone'\">\r\n            {{props.row.phone}}\r\n          </span>\r\n        </template>\r\n        <div slot=\"emptystate\" class=\"text-center\">\r\n          <div class=\"table_loading\" v-if=\"load_first\"></div>\r\n          <div v-else>No records found</div>\r\n        </div>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"Farmers\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Name\",\r\n          field: \"name\"\r\n        },\r\n        {\r\n          label: \"Farmer Code\",\r\n          field: \"farmer_code\"\r\n        },\r\n        {\r\n          label: \"Phone\",\r\n          field: \"phone\"\r\n        },\r\n        {\r\n          label: \"Country\",\r\n          field: \"country\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: [\r\n       \r\n      ],\r\n      load_first:true,\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'fpo'\r\n    this.getEmployees()\r\n  },\r\n  methods: {\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmers/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.load_first = false\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n    onPageChange(rows){\r\n      console.log(rows)\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/fpo"}]}