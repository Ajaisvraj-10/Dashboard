   if (\n        this.getSideBarToggleProperties.isSideNavOpen &&\n        this.getSideBarToggleProperties.isSecondarySideNavOpen &&\n        isMobile\n      ) {\n        this.changeSidebarProperties();\n        this.changeSecondarySidebarProperties();\n      } else if (\n        this.getSideBarToggleProperties.isSideNavOpen &&\n        this.getSideBarToggleProperties.isSecondarySideNavOpen\n      ) {\n        this.changeSecondarySidebarProperties();\n      } else if (this.getSideBarToggleProperties.isSideNavOpen) {\n        this.changeSidebarProperties();\n      } else if (\n        !this.getSideBarToggleProperties.isSideNavOpen &&\n        !this.getSideBarToggleProperties.isSecondarySideNavOpen &&\n        !this.getSideBarToggleProperties.isActiveSecondarySideNav\n      ) {\n        this.changeSidebarProperties();\n      } else if (\n        !this.getSideBarToggleProperties.isSideNavOpen &&\n        !this.getSideBarToggleProperties.isSecondarySideNavOpen\n      ) {\n        // console.log(\"4\");\n\n        this.changeSidebarProperties();\n        this.changeSecondarySidebarProperties();\n        // console.log(\"4\");\n      }\n    }\n  }\n};\n</script>\n<style>\n.layout-sidebar-large .main-header .logo img {\n    width: 90px!important;\n    height: auto!important;\n    margin: -60px auto!important;\n  \n}\n.layout-sidebar-large .main-header .logo {\n    width: 110px!important;\n}\n\n.layout-sidebar-large .main-header .header-icon1 {\n    font-size: 19px;\n    cursor: pointer;\n    height: 36px;\n    width: 36px;\n    line-height: 10px;\n    display: inline-block;\n    text-align: center;\n    border-radius: 8px;\n    margin: 0 2px;\n}\n.main-header ul.dropdown-menu.show {\n    position: absolute;\n    transform: translate3d(0px, 56px, 0px);\n    top: 3px!important;\n    left: 16px!important;\n    will-change: transform;\n}\n.main-header .search-dropdown  .bv-no-focus-ring {\n    height: auto!important;\n    overflow: auto;\n    padding:15px 0;\n}\n.search-dropdo