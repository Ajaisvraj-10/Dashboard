,\r\n          {\r\n            path: \"inspection/:action/:id?\",\r\n            component: () => import(\"./views/app/pages/inspection-form\"),\r\n          },\r\n          {\r\n            path: \"inspection-land/:id\",\r\n            component: () => import(\"./views/app/pages/inspection-land\"),\r\n          },\r\n          {\r\n            path: \"inspection-plant/:id\",\r\n            component: () => import(\"./views/app/pages/inspection-plant\"),\r\n          },\r\n          {\r\n            path: \"inspection-questions/:id\",\r\n            component: () => import(\"./views/app/pages/inspection-questions\"),\r\n          },\r\n          {\r\n            path: \"inspection-digital-sign/:id\",\r\n            component: () => import(\"./views/app/pages/inspection-digital-sign\"),\r\n          },\r\n          {\r\n            path: \"sample-procurement\",\r\n            component: () => import(\"./views/app/pages/sample-procurement\"),\r\n          },\r\n          {\r\n            path: \"sample-procurement-detail/:id\",\r\n            component: () => import(\"./views/app/pages/sample-procurement-detail\"),\r\n          },\r\n          {\r\n            path: \"sample-procurement/:action/:id?\",\r\n            component: () => import(\"./views/app/pages/sample-procurement-form\"),\r\n          },\r\n          {\r\n            path: \"normal-procurement\",\r\n            component: () => import(\"./views/app/pages/normal-procurement\"),\r\n          },\r\n          {\r\n            path: \"normal-procurement-detail/:id\",\r\n            component: () => import(\"./views/app/pages/normal-procurement-detail\"),\r\n          },\r\n          {\r\n            path: \"normal-procurement/:action/:id?\",\r\n            component: () => import(\"./views/app/pages/normal-procurement-form\"),\r\n          },\r\n          {\r\n            path: \"documents\",\r\n            component: () => import(\"./views/app/pages/documents\"),\r\n          },\r\n          {\r\n        