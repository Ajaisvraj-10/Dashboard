ss=\"mb-0\">Clean Machines</h5>\r\n                \r\n          </slot>\r\n          <div class=\"d-flex\">\r\n            <b-button variant=\"success\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"dataForm()\">Save\r\n            </b-button>\r\n            <router-link :to=\"'/app/production/process/processing?department='+production_process.department\">\r\n              <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Discard\r\n              </b-button>\r\n            </router-link>\r\n          </div>\r\n           \r\n      </div>\r\n      <div class=\"my-3 border-top\"></div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll my-2\">\r\n            <h5>Machine List</h5>\r\n            <div class=\"o_thread_search_input my-2\">\r\n              <span style=\"width:120px\"> Cleaning Type: </span>\r\n              <select name=\"clean_type\" v-model=\"form_data.clean_type\">\r\n                <option v-for=\"clean_type in ['Pre Production','Post Production']\" :value=\"clean_type\"> {{clean_type}}</option>\r\n              </select>\r\n            </div>\r\n            <table class=\"table\">\r\n              <tr>\r\n                <th>No</th>\r\n                <th>Machine</th>\r\n                <th>Process</th>\r\n                <th>Qc Status</th>\r\n                <th>Action</th>\r\n              </tr>\r\n              <tr v-for=\"(machine,key) in machines\">\r\n                <td>\r\n                  {{key+1}}\r\n                </td>\r\n                <td>\r\n                  {{machine.name}}\r\n                </td>\r\n                <td>\r\n                  <span class=\"badge badge-danger mr-1\" v-for=\"pro_pros in machine.process_list\" v-if=\"pro_pros[0]==production_process.id\">\r\n                    {{pro_pros[1]}}\r\n                  </span>\r\n                </td>\r\n                <td>\r\n                  Waiting\r\n                </td>\r\n                <td>\r\n                  <label class=\"btn py-1\" :for=\"'machine'+machine.id\" :class=\"form_data.machines.includes(machine.id)?'btn-success':'btn-primary'\">\r\n                    {{form_data.machines.includes(machine.id)?'Cleaned':'Clean'}}\r\n                    <input type=\"checkbox\" name=\"machines\" :value=\"machine.id\" v-model=\"form_data.machines\"  :id=\"'machine'+machine.id\" v-show=\"false\" />\r\n                  </label>\r\n                </td>\r\n              </tr>\r\n            </table>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll my-2\">\r\n            <h5>Cleaning Labours</h5>\r\n            <div>\r\n              <WorkersForm :worker_data=\"form_data\" :work_type=\"'wages'\"></WorkersForm>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport WorkersForm from '/src/views/app/oodo/components/workers-form'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Machine\"\r\n  },\r\n  components:{\r\n    WorkersForm\r\n  },\r\n  data() {\r\n    return {\r\n      machines: [],\r\n      form_data:{\r\n        machines:[],\r\n        workers:[],\r\n        wage_workers:[],\r\n        work_type:'wages',\r\n        overtime_hourse_st1:2,\r\n      },\r\n      production_process:{id:null},\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'production'\r\n    this.production_process.id = this.$route.params.process_id\r\n    this.getProductionProcess()\r\n    this.getWarehouses()\r\n  },\r\n  methods: {\r\n    getProductionProcess() {\r\n      this.scroll_pos = {left:false,right:true,current_scroll:0}\r\n      this.scroll_click = false\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/production_process/'+this.production_process.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n        return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.production_process = jsonData\r\n        if(jsonData.machine_clean.id){\r\n          this.form_data = jsonData.machine_clean\r\n        }\r\n        this.getMachines()\r\n      })\r\n    },\r\n    getMachines() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/machines/?production_process='+this.production_process.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.machines = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('production_process', this.production_process.id)\r\n      formdata.append('supervisor', this.employeeId)\r\n      formdata.append('status', 'Cleaned')\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/machine_cleaning/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/qc"}]}