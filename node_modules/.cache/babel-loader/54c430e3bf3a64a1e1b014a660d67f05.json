ass=\"text-16 add-btn-text ml-2\" style=\"margin-right: 15px;\">Approve</h6>\r\n        </div>\r\n      </div>\r\n      <div class=\"\" v-if=\"not_start\">\r\n        <div class=\"loading\" style=\"z-index:999999999999999\"></div>\r\n      </div>\r\n      <div class=\"main-body4 px-3 py-3 create\" :style=\"not_start?'opacity:0.5':''\">\r\n        <h5 class=\"my-2\">Inspection:</h5>\r\n        <div class=\"card p-0\">\r\n          <div class=\"p-4\">\r\n           \r\n            \r\n            <div class=\"row\">\r\n              <h2 class=\"col-12\" style=\"font-size:18px!important;\" >\r\n                <span class=\"d-none\">Name:</span>\r\n                <i class=\"fa fa-user\" style=\"zoom: 0.9;\"></i>\r\n                <span class=\"pl-2\" >{{form_data.farmer_detail.name}}</span>\r\n              </h2>\r\n            </div>\r\n            <hr class=\"my-3\">\r\n            <div class=\"row details-row\"> \r\n              <div class=\"col-12 pb-1\" style=\"display:flex;align-items:center;gap:10px;\">\r\n                <h2 class=\"\">ICS:</h2>\r\n                <span class=\"badge badge-primary \" v-for=\"ics in icss\" v-if=\"form_data.ics==ics.id\">{{ics.name}}</span>\r\n              </div>\r\n              <div class=\"col-12 mb-2\" style=\"display:flex;align-items-center;gap:10px;\">\r\n                <h2 class=\"\">VFC:</h2>\r\n                <span class=\"badge badge-info  \" v-for=\"vfc in vfcs\" v-if=\"form_data.vfc==vfc.id\">{{vfc.name}}</span>\r\n              </div>\r\n              <div class=\"col-6\" >\r\n                <h2 class=\"\">Farmer Code:</h2>\r\n                <span class=\"badge badge-success \">{{form_data.farmer_detail.farmer_code}}</span>\r\n              </div>\r\n              <div class=\"col-6\" >\r\n                <h2 class=\"\">Farmer Status:</h2>\r\n                <span class=\"\">\r\n                  <span class=\"badge mr-1\" v-for=\"status in form_data.farmer_detail.statuses\" :class=\"status=='NEW'?'badge-warning':status=='ORG'?'badge-success':'badge-info'\">{{status}}</span>\r\n                </span>\r\n              </div>\r\n              <div class=\"col-12\" v-if=\"form_data.date_of_previous_inspection\">\r\n                <h2 class=\"   \">Date of previous inspection:</h2>\r\n                <h2 class=\"\">{{dateFormate(form_data.date_of_previous_inspection)}}</h2>\r\n              </div>\r\n              <div class=\"col-6\">\r\n                <h2 class=\"\">Date:</h2>\r\n                <h2 class=\"\">{{dateFormate(form_data.date_of_inspection)}}</h2>\r\n              </div>\r\n              <div class=\"col-6\">\r\n                <h2 class=\"\">Inspector:</h2>\r\n                <h2 class=\"\">{{form_data.inspector_detail.name}}</h2>\r\n              </div>\r\n              <div class=\"col-6 pb-0\">\r\n                <h2 class=\"\">Phone:</h2>\r\n                <h2 class=\"\">{{form_data.inspector_detail.phone}}</h2>\r\n              </div>\r\n              <div class=\"col-6 pb-0\">\r\n                <h2 class=\"\">Inspection:</h2>\r\n                <h2 class=\"\">\r\n                  <b>\r\n                    <span class=\"text-success\" v-if=\"form_data.is_approved\">Approved</span>\r\n                    <span class=\"text-danger\" v-else>Not Approved</span>\r\n                  </b>\r\n                </h2>\r\n              </div>\r\n            </div>\r\n\r\n          </div>\r\n        </div>\r\n\r\n        <div v-if=\"!not_start\">\r\n          <div class=\"row\">\r\n            <div class=\"col-8\">\r\n              <h5 class=\"my-4\">Land Details:</h5>\r\n            </div>\r\n            <div class=\"col-4 text-right\">\r\n              <router-link :to=\"'/inspection-land/'+id\" v-if=\"!form_data.is_approved && form_data.lands.id\">\r\n                <b-button variant=\"danger\" type=\"button\" class=\"d-sm-block mx-1 mt-3 mb-2\" style=\"min-height: 25px;border-radius: 5px\">\r\n                  Edit Land\r\n                </b-button>\r\n              </router-link>\r\n              <router-link :to=\"'/inspection-land/'+id\" v-else-if=\"!form_data.lands.id\">\r\n                <b-button variant=\"secondary\" type=\"button\" class=\"d-sm-block mx-1 mt-3 mb-2\" style=\"min-height: 25px;border-radius: 5px\">\r\n                  Add Land\r\n                </b-button>\r\n              </router-link>\r\n            </div>\r\n          </div>\r\n          <div class=\"card p-0\">\r\n            <div class=\"p-4\">\r\n              <div class=\"row details-row\">\r\n                <div class=\"col-6\">\r\n                  <h2 class=\"\">Survey Number:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.survey_number}}</h2>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                  <h2 class=\"   \">Year:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.year}}</h2>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                  <h2 class=\"   \">Total Organic Area:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.organic_area}}Ha</h2>\r\n                </div>\r\n                <div class=\"col-6\" v-if=\"form_data.lands.ic3_land_area>0\">\r\n                  <h2 class=\"   \">Total Area IC3:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.ic3_land_area}}Ha</h2>\r\n                </div>\r\n                <div class=\"col-6\" v-if=\"form_data.lands.ic2_land_area>0\">\r\n                  <h2 class=\"   \">Total Area IC2:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.ic2_land_area}}Ha</h2>\r\n                </div>\r\n                <div class=\"col-6\" v-if=\"form_data.lands.ic1_land_area>0\">\r\n                  <h2 class=\"   \">Total Area IC1:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.ic1_land_area}}Ha</h2>\r\n                </div>\r\n                <div class=\"col-6\" v-if=\"form_data.lands.new_land_area>0\">\r\n                  <h2 class=\"   \">Total Area NEW:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.new_land_area}}Ha</h2>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                  <h2 class=\"\">Conventional Land Area:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.conventional_land_area}}Ha</h2>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                  <h2 class=\" \">Wet Land:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.wet_land}}Ha</h2>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                  <h2 class=\" \">Other Land:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.other_land}}Ha</h2>\r\n                </div>\r\n                <div class=\"col-12 pb-0\">\r\n                  <h2 class=\" \">Total Farming Area:</h2>\r\n                  <h2 class=\"  \">{{form_data.lands.total_farming_area}}Ha</h2>\r\n                </div>\r\n                <!-- <div class=\"col-6 pt-2\">\r\n                  <h2 class=\" \">File:</h2>\r\n                  <div class=\"  \" v-if=\"form_data.lands.file\">\r\n                    <img :src=\"form_data.lands.file\" width=\"150\" height=\"40\" class=\"rounded\" style=\"object-fit:cover;\">\r\n                  </div>\r\n                </div> -->\r\n              </div>\r\n             \r\n\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"row\">\r\n            <div class=\"col-8\">\r\n              <h5 class=\"my-4\">Plant Details:</h5>\r\n            </div>\r\n            <div class=\"col-4 text-right\">\r\n              <router-link :to=\"'/inspection-plant/'+id\" v-if=\"!form_data.is_approved && form_data.plants.id\">\r\n                <b-button variant=\"danger\" type=\"button\" class=\"d-sm-block mx-1 mt-3 mb-2\" style=\"min-height: 25px;border-radius: 5px\">\r\n                  Edit Plant\r\n                </b-button>\r\n              </router-link>\r\n              <router-link :to=\"'/inspection-plant/'+id\" v-else-if=\"!form_data.plants.id\">\r\n                <b-button variant=\"secondary\" type=\"button\" class=\"d-sm-block mx-1 mt-3 mb-2\" style=\"min-height: 25px;border-radius: 5px\">\r\n                  Add Plant\r\n                </b-button>\r\n              </router-link>\r\n            </div>\r\n          </div>\r\n          <div class=\"mb-1\" v-for=\"category in categories\">\r\n            <div class=\"card mb-3\" v-for=\"(plant,key) in category.plants\" v-if=\"plant.number\">\r\n              <div class=\"p-1\">\r\n                <div class=\"row details-row\">\r\n                  <div class=\"col-12\">\r\n                    <h2 class=\"\"><b>{{plant.name}} {{plant.plant_unit}}:</b></h2>\r\n                    <h2 class=\"  \">{{plant.number}}<small>{{plant.plant_unit=='Area'?plant.land_unit:''}}</small></h2>\r\n                  </div>\r\n                  <div class=\"col-6 pb-0\">\r\n                    <h2 class=\"   \">Year:</h2>\r\n                    <h2 class=\"  text-info\">{{form_data.plants.year}}</h2>\r\n                  </div>\r\n                  <div class=\"col-6\">\r\n                    <h2 class=\"\">Product Status:</h2>\r\n                    <h2 class=\"\" :class=\"plant.organic_status=='Organic'?'text-success':'text-primary'\">{{plant.organic_status}}</h2>\r\n                  </div>\r\n                  <div class=\"col-6 pb-0\" v-for=\"(product,key2) in plant.products\">\r\n                    <h2 class=\"\">Estimate {{product.product_type_detail.name}}:</h2>\r\n                    <h2 class=\"  \">{{plant['estimate'+product.product_type+plant.id]?plant['estimate'+product.product_type+plant.id]:0}}KG</h2>\r\n                  </div>\r\n                </div>\r\n\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div v-for=\"(category,index) in q_categories\">\r\n            <div class=\"row\">\r\n              <div class=\"col-8\">\r\n                <h5 class=\"my-4\">{{category.name}}:</h5>\r\n              </div>\r\n              <div class=\"col-4 text-right\">\r\n                <router-link :to=\"'/inspection-questions/'+id+'?category='+category.id\" v-if=\"!form_data.is_approved && category.answerd_count>0\">\r\n                  <b-button variant=\"danger\" type=\"button\" class=\"d-sm-block mx-1 mt-3 mb-2\" style=\"min-height: 25px;border-radius: 5px\">\r\n                    Edit Q/N\r\n                  </b-button>\r\n                </router-link>\r\n                <router-link :to=\"'/inspection-questions/'+id+'?category='+category.id\" v-else-if=\"category.answerd_count<=0\">\r\n                  <b-button variant=\"secondary\" type=\"button\" class=\"d-sm-block mx-1 mt-3 mb-2\" style=\"min-height: 25px;border-radius: 5px\">\r\n                    Add Q/N\r\n                  </b-button>\r\n                </router-link>\r\n              </div>\r\n            </div>\r\n            <div class=\"card \">\r\n              <div class=\"pb-1\" v-for=\"question in category.questions\">\r\n                <div class=\" mx-0  py-2 my-1 border-bottom\">\r\n                  <h2 class=\"  pb-2 \"><b>Q)</b> {{question.name}}</h2>\r\n                  <span class=\" font-weight-500 badge\" :class=\"question.answers.answer=='NO'?'badge-danger':'badge-success'\" v-if=\"question.field=='Boolean'\">{{question.answers.answer}}</span>\r\n                  <span class=\" font-weight-500 badge\" v-else>{{question.answers.answer}}</span>\r\n                </div>\r\n                <div class=\"mx-0  py-2 my-1 border-bottom\" v-if=\"question.remark\">\r\n                  <h2 class=\" pb-2  \">{{question.remark}}</h2>\r\n                  <h2 class=\" font-weight-500 \">{{question.answers.remark}}</h2>\r\n                </div>\r\n                <div class=\"mx-0  py-2 my-1\" v-if=\"question.field=='File'\">\r\n                  <h2 class=\"   p-0 \">Photo</h2>\r\n                  <div class=\" font-weight-500 mt-3\">\r\n                    <img :src=\"question.answers.answer\" class=\"rounded\" style=\"object-fit:cover;\">\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"row\">\r\n            <div class=\"col-8\">\r\n              <h5 class=\"my-4\">Digital Sign:</h5>\r\n            </div>\r\n            <div class=\"col-4 text-right\">\r\n              <router-link :to=\"'/inspection-digital-sign/'+id\" v-if=\"!form_data.is_approved && form_data.photo\">\r\n                <b-button variant=\"danger\" type=\"button\" class=\"d-sm-block mx-1 mt-3 mb-2\" style=\"min-height: 25px;border-radius: 5px\">\r\n                  Edit Sign\r\n                </b-button>\r\n              </router-link>\r\n              <router-link :to=\"'/inspection-digital-sign/'+id\" v-else-if=\"!form_data.photo\">\r\n                <b-button variant=\"secondary\" type=\"button\" class=\"d-sm-block mx-1 mt-3 mb-2\" style=\"min-height: 25px;border-radius: 5px\">\r\n                  Add Sign\r\n                </b-button>\r\n              </router-link>\r\n            </div>\r\n          </div>\r\n          <div class=\"card p-0 mb-5\">\r\n            <div class=\"p-4\">\r\n              <div class=\"row details-row\">\r\n                <div class=\" col-6\">\r\n                  <h2 class=\"\">Name:</h2>\r\n                  <h2 class=\"\">{{form_data.verifyer_name}}</h2>\r\n                </div>\r\n                \r\n                <div class=\"col-6\">\r\n                  <h2 class=\"\">Photo:</h2>\r\n                  <div class=\"\" v-if=\"form_data.photo\">\r\n                    <img :src=\"setImgURL(form_data.photo)\" width=\"150\" height=\"auto\" class=\"rounded\" style=\"object-fit:cover;\" @click=\"$bvModal.show('view-photo')\">\r\n                    <b-modal id=\"view-photo\" hide-footer :no-close-on-backdrop=\"true\">\r\n                      <div style=\"max-height:80vh;overflow-y:auto\">\r\n                        <img :src=\"setImgURL(form_data.photo)\" style=\"width:100%;height:auto;object-fit:cover;\" class=\"rounded\">\r\n                      </div>\r\n                    </b-modal>\r\n                  </div>\r\n                </div>\r\n                <div class=\" col-6 pb-0\" v-if=\"form_data.verifyer=='Other'\">\r\n                  <h2 class=\"\">Relationship:</h2>\r\n                  <h2 class=\"\">{{form_data.relationship}}</h2>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"filter-footer\">\r\n        <div class=\"footer-item2 d-flex align-items-center\" @click=\"$router.push('/')\">\r\n          <img src=\"/images/home.png\" class=\"\" width=\"25\">\r\n          <h5 class=\"m-0 pl-2 text-white\">Home</h5>\r\n        </div>\r\n        \r\n      </div>\r\n    </main>\r\n    <div>\r\n      <b-modal id=\"confirmPopup2\" title=\"Approve\" hide-footer @close=\"confirm_text.show=false\">\r\n        <div class=\"popup_body\" style=\"max-height:78vh\">\r\n          <table class=\"table\" v-if=\"!confirm_text.show\">\r\n            <tr @click=\"form_data.lands.id?'':$router.push('/inspection-land/'+id)\">\r\n              <th class=\"border-top-0\">\r\n                Land Details\r\n              </th>\r\n              <td class=\"border-top-0\">\r\n                <span v-if=\"form_data.lands.id\">\r\n                  <i class=\"fas fa-check-circle text-success\"></i>\r\n                </span>\r\n                <span v-else>\r\n                  <i class=\"fas fa-times-circle text-danger\"></i>\r\n                </span>\r\n              </td>\r\n            </tr>\r\n            <tr @click=\"form_data.plants.id?'':$router.push('/inspection-plant/'+id)\">\r\n              <th>\r\n                Plant Details\r\n              </th>\r\n              <td>\r\n                <span v-if=\"form_data.plants.id\">\r\n                  <i class=\"fas fa-check-circle text-success\"></i>\r\n                </span>\r\n                <span v-else>\r\n                  <i class=\"fas fa-times-circle text-danger\"></i>\r\n                </span>\r\n              </td>\r\n            </tr>\r\n            <tr v-for=\"(category,index) in q_categories\" @click=\"category.answerd_count>0?'':$router.push('/inspection-questions/'+id+'?category='+category.id)\">\r\n              <th>\r\n                {{category.name}}\r\n              </th>\r\n              <td>\r\n                <span v-if=\"category.answerd_count>0\">\r\n                  <i class=\"fas fa-check-circle text-success\"></i>\r\n                </span>\r\n                <span v-else>\r\n                  <i class=\"fas fa-times-circle text-danger\"></i>\r\n                </span>\r\n              </td>\r\n            </tr>\r\n            <tr class=\"border-bottom\" @click=\"not_start=false;$bvModal.hide('allConfirmationPopup')\">\r\n              <th>\r\n                Digita Sign\r\n              </th>\r\n              <td class=\"border-bottom\">\r\n                <span v-if=\"form_data.photo\">\r\n                  <i class=\"fas fa-check-circle text-success\"></i>\r\n                </span>\r\n                <span v-else>\r\n                  <i class=\"fas fa-times-circle text-danger\"></i>\r\n                </span>\r\n              </td>\r\n            </tr>\r\n          </table>\r\n          <div v-if=\"confirm_text.show\">\r\n            <div class=\"mb-3 pb-2 border-bottom\">\r\n              <b>REMARKS FROM ICS</b>\r\n              <textarea placeholder=\"Write Here\" class=\"form-control\" autocomplete=\"off\" v-model=\"confirm_text.remark\" style=\"height:200px\"></textarea>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"w-100 text-center\" v-if=\"confirm_text.show\">\r\n          <button type=\"button\" class=\"btn btn-success px-4 mx-2\" @click=\"aprroveInspection()\" v-if=\"not_start==false\">Save & Approve</button>\r\n        </div>\r\n        <div class=\"w-100 text-center\" v-else>\r\n          <button type=\"button\" class=\"btn btn-success px-4 mx-2\" @click=\"confirm_text.show=true;\" v-if=\"not_start==false\">Approve</button>\r\n          <button type=\"button\" class=\"btn btn-danger px-4 mx-2\" @click=\"$bvModal.hide('confirmPopup2')\">Decline</button>\r\n        </div>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Home\"\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{\r\n        year:2022,\r\n        type:null,\r\n        ics:null,\r\n        vfc:null,\r\n        farmer:null,\r\n        farmer_detail:{lands:{created_on:null}},\r\n        internal_inspector:null,\r\n        inspector_detail:{phone:''},\r\n        date_of_inspection:'',\r\n        lands:{id:null},\r\n        plants:{id:null}\r\n      },\r\n      types:[],\r\n      icss:[],\r\n      vfcs:[],\r\n      farmers:[],\r\n      inspectors:[],\r\n      categories:[],\r\n      q_categories:[],\r\n      id:this.$route.params.id,\r\n      confirm_text:{show:false, remark:''},\r\n      background_start:true,\r\n      lastScrollPosition: 0,\r\n      not_start:true,\r\n    };\r\n  },\r\n  mounted(){\r\n    this.form_data.date_of_inspection = this.currentDate()\r\n    this.getTypes()\r\n    if(this.id){\r\n      this.getInspection()\r\n    }\r\n    var self = this\r\n    document.getElementsByClassName(\"main-body4\")[0].addEventListener(\"scroll\", () => {\r\n        self.handleScroll()\r\n    }); \r\n  },\r\n  methods: {\r\n    getTypes() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/employee/get_farmer/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.types = jsonData\r\n        if(this.id){\r\n          var self = this\r\n          setTimeout(function(){\r\n            self.change_select('type')\r\n            self.change_select('ics')\r\n            self.change_select('vfc')\r\n            self.change_select('farmer')\r\n            self.change_select('inspector')\r\n          },500)\r\n        }\r\n      })\r\n    },\r\n    getFarmer() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/fpo/farmers/'+this.form_data.farmer+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.farmer_detail = jsonData\r\n        this.form_data.type = jsonData.fpo\r\n        this.form_data.ics = jsonData.ics\r\n        this.form_data.vfc = jsonData.vfc\r\n        var self = this\r\n        setTimeout(function(){\r\n          self.change_select('type')\r\n          self.change_select('ics')\r\n          self.change_select('vfc')\r\n          self.change_select('farmer')\r\n        },500)\r\n      })\r\n    },\r\n    getInspection() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/employee/inspections/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.getPlants()\r\n        this.getQuestions()\r\n      })\r\n    },\r\n    aprroveInspection() {\r\n      this.not_start = true\r\n      var headers = new Headers();\r\n      var formdata = new FormData()\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      formdata.append('inspection',this.id)\r\n      formdata.append('remark',this.confirm_text.remark)\r\n      fetch(this.api_url+'/employee/approve_inspection/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.not_start = false\r\n        if(jsonData.success){\r\n          this.showAlert('Inspection Approved!')\r\n          this.$bvModal.hide('confirmPopup2')\r\n          this.getInspection()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getPlants() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/plant_categories/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var self = this\r\n        jsonData.forEach(function(category,key1){\r\n          category.plants.forEach(function(plant,key2){\r\n            jsonData[key1].plants[key2]['number'] = ''\r\n            plant.products.forEach(function(product,key3){\r\n              jsonData[key1].plants[key2]['estimate'+product.product_type+plant.id] = ''\r\n              jsonData[key1].plants[key2]['land_unit'] = 'Number'\r\n              if(plant.plant_unit=='Area'){\r\n                jsonData[key1].plants[key2]['land_unit'] = 'Hectare'\r\n              }\r\n              self.setEstimate(plant,product)\r\n            })\r\n          })\r\n        })\r\n        this.categories = jsonData\r\n      })\r\n    },\r\n    setEstimate(plant,product){\r\n      if(this.form_data.plants.id){\r\n        var plants = this.form_data.plants.plants\r\n        plants.forEach(function(val){\r\n          if(product.id == val.product && val.organic_status==plant.organic_status){\r\n            plant['number'] = val.number\r\n            plant['estimate'+product.product_type+plant.id] = val.estimate\r\n            plant['land_unit'] = val.land_unit\r\n          }\r\n        })\r\n      }\r\n    },\r\n    getQuestions() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/employee/add_answers/?inspection='+this.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.not_start = false\r\n        jsonData.forEach(function(category,key1){\r\n          category.questions.forEach(function(question,key2){\r\n            if(!question.answers){\r\n              jsonData[key1].questions[key2]['answers']['answer'] = ''\r\n              jsonData[key1].questions[key2]['answers']['remark'] = ''\r\n            }\r\n          })\r\n        })\r\n        this.q_categories = jsonData\r\n      })\r\n    },\r\n    change_select(type){\r\n      var value = ''\r\n      var self = this\r\n      if(type=='type'){\r\n        value = this.form_data.type\r\n        this.types.forEach(function(list){\r\n          if(list.id==value){\r\n            self.icss = list.ics\r\n          }\r\n        })\r\n      }else if(type=='ics'){\r\n        value = this.form_data.ics\r\n        this.icss.forEach(function(list){\r\n          if(list.id==value){\r\n            self.vfcs = list.vfc\r\n          }\r\n        })\r\n      }else if(type=='vfc'){\r\n        value = this.form_data.vfc\r\n      }else if(type=='inspector'){\r\n        value = this.form_data.internal_inspector\r\n        this.inspectors.forEach(function(list){\r\n          if(list.id==value){\r\n            self.form_data.inspector_detail = list\r\n          }\r\n        })\r\n      }\r\n    },\r\n    handleScroll () {\r\n      const currentScrollPosition = document.getElementsByClassName(\"main-body4\")[0]\r\n          .scrollTop\r\n      if (currentScrollPosition < 0) {\r\n        return 0\r\n      }\r\n      this.background_start = currentScrollPosition < this.lastScrollPosition\r\n      this.lastScrollPosition = currentScrollPosition\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n.input-group-append{\r\n  padding-top: 0;\r\n  padding-bottom: 0;\r\n}\r\n.inspection-details h2{\r\n  font-size:14.5px;\r\n  color:#3e3e3e;\r\n  line-height: 22px;\r\n}\r\n\r\n.inspection-details h5{\r\n  font-size:16px;\r\n  font-weight:600;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/app/pages"}]}