</div>\r\n    \r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: true,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: true,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows\"\r\n      >\r\n        <div slot=\"table-actions\" class=\"mb-3 filter-but\">\r\n          \r\n        </div>\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <router-link :to=\"'/app/fpo/farmer-crops/edit/'+farmer_id+'/'+props.row.id\">\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">Edit</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.label == 'Product'\">\r\n            {{props.row.product_detail.category_detail.name}}\r\n          </span>\r\n          <span v-else-if=\"props.column.label == 'Product Name'\">\r\n            {{props.row.product_detail.name}}\r\n          </span>\r\n          <span v-else-if=\"props.column.label == 'Product Type'\">\r\n            {{props.row.product_detail.product_type_detail.name}}\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Year\",\r\n          field: \"year\"\r\n        },\r\n        {\r\n          label: \"Product\",\r\n          field: \"product_detail\"\r\n        },\r\n        {\r\n          label: \"Product Name\",\r\n          field: \"product_detail\"\r\n        },\r\n        {\r\n          label: \"Product Type\",\r\n          field: \"product_detail\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: [],\r\n      farmer_id:null,\r\n      farmer:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'fpo'\r\n    this.farmer_id = this.$route.params.farmer_id\r\n    this.getFarmer()\r\n    this.getCrops()\r\n  },\r\n  methods: {\r\n    getFarmer() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmers/'+this.farmer_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmer = jsonData\r\n      })\r\n    },\r\n    getCrops() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/crops/?farmer='+this.farmer_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../../styles/style.css';\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/fpo/farmer"}]}