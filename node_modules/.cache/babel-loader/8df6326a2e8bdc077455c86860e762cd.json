event.target.src='/img/blank-img.png'\">\r\n            </div>\r\n            \r\n            <div class=\" p-2\">\r\n              <h6 class=\"font-weight-700 mb-0 \" style=\"min-height:20px;\">{{proces.name}}</h6>\r\n              \r\n              <button class=\"btn btn-success px-2 w-100 py-1 btn-sm  my-2 \" @click=\"getProcess(proces)\">Process</button>\r\n            </div>\r\n          </div>\r\n      \r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div>\r\n      <b-modal id=\"list-main-process\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n           <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">{{current_process.name}}</span></h5>\r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('list-main-process')\">Close</b-button>\r\n              </div>\r\n            </div>\r\n            <div class=\" my-3 border-top\"></div>\r\n        </div>\r\n        <b-row class=\"main-scroll\">\r\n          <b-col lg=\"12\" xl=\"12\">\r\n            <b-card class=\"mt-2\" v-for=\"production_process in current_process.production_processes\">\r\n              <div class=\"row\">\r\n                <div class=\"col-2 align-self-center\">\r\n                  <img src=\"/img/crm.png\" >\r\n                </div>\r\n                <div class=\"col-7 align-self-center\">\r\n                  <b>{{production_process.name}} </b>\r\n                  <p class=\"m-0\"> products</p>\r\n                </div>\r\n                <div class=\"col-3 align-self-center\">\r\n                  <div>\r\n                    <router-link :to=\"'/app/production/process/processing/list/'+production_process.id\">\r\n                      <button class=\"btn btn-primary py-1 btn-sm w-100\">\r\n                        View\r\n                      </button>\r\n                    <router-link :to=\"'/app/production/clean-machine/view/'+production_process.id\" v-if=\"production_process.machine_count>0\">\r\n                      <button class=\"btn btn-danger py-1 btn-sm w-100 mt-1\">\r\n                        Clean Machine\r\n                      </button>\r\n                    </router-link>\r\n                    </router-link>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n          </b-col>\r\n        </b-row>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{},\r\n      departments:[],\r\n      department_id:null,\r\n      current_process:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    if(this.$route.query.department){\r\n      this.department_id = this.$route.query.department\r\n    }\r\n    this.getProcesses()\r\n  },\r\n  methods: {\r\n    getProcesses() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/departments/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.departments = jsonData\r\n        if(jsonData.length>0 && !this.$route.query.department){\r\n          this.department_id = jsonData[0].id\r\n        }\r\n      })\r\n    },\r\n    getProcess(proces){\r\n      this.current_process = proces\r\n      this.$bvModal.show('list-main-process')\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../../styles/style.css';\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/production/process"}]}