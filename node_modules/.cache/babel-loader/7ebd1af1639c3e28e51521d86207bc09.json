lass=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.distance\" name=\"distance\">\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\"> Gender: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"gender\" v-model=\"form_data.gender\">\r\n                          <option v-for=\"gend in ['M','F']\" :value=\"gend\">{{gend}}</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span class=\"\" style=\"width:120px;\"> Blood Group: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"blood_group\" v-model=\"form_data.blood_group\">\r\n                          <option v-for=\"blood_group in ['','A+','A-','B+','B-','O+','O-','AB+','AB-']\" :value=\"blood_group\">{{blood_group}}</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">ESI Number: </span>\r\n                      <input placeholder=\"ESI Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.ESI_Number\" name=\"ESI_Number\">\r\n                    </div>\r\n                    <div class=\"o_thread_search_input mb-2\">\r\n                      <span style=\"width:120px;\">PF Number: </span>\r\n                      <input placeholder=\"PF Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.PF_Number\" name=\"PF_Number\">\r\n                    </div>\r\n                    <b class=\"px-2\">Bank Details</b>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Bank Name: </span>\r\n                      <input placeholder=\"Bank Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.bank_name\" name=\"bank_name\">\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Branch Name: </span>\r\n                      <input placeholder=\"Branch Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.bank_branch\" name=\"bank_branch\">\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Account Number: </span>\r\n                      <input placeholder=\"Bank Account Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.bank_account_number\" name=\"bank_account_number\">\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">IFS Code: </span>\r\n                      <input placeholder=\"IFS Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.bank_ifs_code\" name=\"bank_ifs_code\">\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <hr>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"4\" xl=\"4\" class=\"pl-0\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div class=\"ul-contact-detail__timeline\">\r\n              <div class=\"row\">\r\n                <div class=\" col-md-12\">\r\n                  <div class=\"user-info pt-2\">\r\n                     <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\"> Department: </span>\r\n                        <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"department\" v-model=\"form_data.department\" @change=\"setJobPositions()\">\r\n                            <option selected disabled>Select a department</option>\r\n                            <option v-for=\"department in departments\" :value=\"department.id\" v-if=\"department.id==5\">{{department.name}}</option>\r\n                        </select>\r\n                     </div>\r\n                     <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\"> Job Position: </span>\r\n                        <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"job_position\" v-model=\"form_data.job_position\">\r\n                            <option value=\"9\">Labour</option>\r\n                        </select>\r\n                     </div>\r\n                      \r\n                     <!-- <div>\r\n                        <div class=\"o_thread_search_input mt-2\">\r\n                        <span style=\"width:122px;\"> Job Type: </span>\r\n                          <b style=\"width:55px\"> Wage : </b>\r\n                          <b-form-checkbox v-model=\"form_data.is_wage\" name=\"is_wage\" switch>\r\n                            <b>{{ form_data.is_wage?'Yes':'No' }}</b>\r\n                          </b-form-checkbox>\r\n                          <span style=\"width:60px\"> </span>\r\n                          <b style=\"width:70px\"> Contract : </b>\r\n                          <b-form-checkbox v-model=\"form_data.is_contract\" name=\"is_contract\" switch>\r\n                            <b>{{ form_data.is_contract?'Yes':'No' }}</b>\r\n                          </b-form-checkbox>\r\n                        </div>\r\n                     </div> -->\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\"> Job Type: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"work_type\" v-model=\"form_data.work_type\">\r\n                          <option v-for=\"work_type in ['Wage','Contract']\" :value=\"work_type\">{{work_type}}</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\"> Medical Test Report Due Date: </span>\r\n                      <input type=\"date\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.medical_test_due_date\" name=\"medical_test_due_date\">\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\"> Job Title: </span>\r\n                      <input placeholder=\"Job Title\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.job_title\" name=\"job_title\" required>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Joined Year: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"joined_year\" v-model=\"form_data.joined_year\">\r\n                          <option v-for=\"year in this_year\" :value=\"year\" v-if=\"year>2000\">{{year}}</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"px-2 mt-2\">\r\n                      <span class=\"px-1\"> Information: </span>\r\n                      <textarea placeholder=\"Write something\" class=\"ui-autocomplete-input w-100 mx-1\" autocomplete=\"off\" name=\"information\" rows=\"5\" v-model=\"form_data.information\"></textarea>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <hr>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport LocationFields from '/src/views/app/oodo/components/location_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    LocationFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{gender:'M', job_positions:5, department:5, joined_year:this.this_year},\r\n      id:null,\r\n      action:'create',\r\n      departments:[],\r\n      job_positions:[],\r\n      load_locations:false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'employee'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_locations = true\r\n      this.getDepartments()\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.getDepartments()\r\n        this.load_locations = true\r\n      })\r\n    },\r\n    getDepartments() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/jobs/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.departments = jsonData\r\n        var self= this\r\n        this.departments.forEach(function(val,key){\r\n          if(val.id==self.form_data.department){\r\n            self.job_positions = val.job_positions\r\n          }\r\n        })\r\n      })\r\n    },\r\n    setJobPositions(){\r\n      var self= this\r\n      this.departments.forEach(function(val,key){\r\n        if(val.id==self.form_data.department){\r\n          self.job_positions = val.job_positions\r\n        }\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.form_data.name = ''\r\n          this.form_data.mobile = ''\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/employee"}]}