ntent-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Panchayath</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/locations/panchayath\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info \">\r\n                             <div class=\"o_thread_search_input\">\r\n                                  <span style=\"width:120px;\"> Panchayath Name: </span>\r\n                                  <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" :value=\"location.name\" name=\"name\" required>\r\n                             </div>\r\n                             <div class=\"o_thread_search_input\">\r\n                                  <span style=\"width:120px;\"> Panchayath Code: </span>\r\n                                  <input placeholder=\"Code\" type=\"text\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" :value=\"location.code\" name=\"code\" required>\r\n                             </div>\r\n                             \r\n                             <LocationFields :id=\"id\" location_limit=\"4\"></LocationFields>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport LocationFields from '/src/views/app/oodo/components/location_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Locations - State\"\r\n  },\r\n  components:{\r\n    LocationFields\r\n  },\r\n  data() {\r\n    return {\r\n      location:[],\r\n      id:this.$route.params.id?this.$route.params.id:null,\r\n      action:'create',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'fpo'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getLocation()\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getLocation() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/panchayath/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.location = jsonData\r\n      })\r\n    },\r\n    locationForm() {\r\n      if(this.action == 'create'){\r\n        this.createLocation()\r\n      }else{\r\n        this.updateLocation()\r\n      }\r\n    },\r\n    createLocation() {\r\n      var formdata = new FormData(this.$refs['locationForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/panchayath/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateLocation() {\r\n      var formdata = new FormData(this.$refs['locationForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/panchayath/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/locations"}]}