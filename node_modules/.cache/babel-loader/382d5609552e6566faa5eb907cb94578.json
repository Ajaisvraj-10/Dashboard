KA1GA;AA2GA,IAAA,cA3GA,4BA2GA;AAAA;;AACA,UAAA,QAAA,GAAA,IAAA,QAAA,CAAA,KAAA,KAAA,CAAA,UAAA,CAAA,CAAA;AACA,MAAA,QAAA,CAAA,MAAA,CAAA,QAAA,EAAA,KAAA,SAAA;AACA,UAAA,OAAA,GAAA,IAAA,OAAA,EAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,eAAA,EAAA,WAAA,KAAA,KAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,KAAA,OAAA,GAAA,cAAA,GAAA,KAAA,EAAA,GAAA,GAAA,EAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,IAAA,EAAA,QAFA;AAGA,QAAA,OAAA,EAAA;AAHA,OAAA,CAAA,CAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,EAAA;AACA,OAPA,EAQA,IARA,CAQA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,EAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,uBAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,kBAAA;AACA;AACA,OAdA;AAeA;AA/HA;AA9BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"main-content create-main\">\r\n    <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n      <div>\r\n         <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Plant</span> / <router-link :to=\"'/app/fpo/farmer/edit/'+farmer_id\">{{farmer.farmer_code}} - {{farmer.name}}</router-link> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link :to=\"'/app/fpo/farmer-plant/'+farmer_id\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\" mb-30\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Farmer: </span>\r\n                              <input type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"farmer.length!=0?farmer.farmer_code+' - '+farmer.name:'Loading...'\" readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Plant Year: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"year\">\r\n                                  <option v-for=\"year in this_year\" :value=\"year\" :selected=\"year==form_data.year\" v-if=\"year>2000\">{{year}}</option>\r\n                              </select>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"user-info pt-5\" v-for=\"category in categories\">\r\n                            <b class=\"text-bold\" v-if=\"category.plants.length>0\">{{category.name}}</b>\r\n                            <div class=\"row px-3\" v-for=\"(plant,key) in category.plants\">\r\n                              <div class=\"o_thread_search_input col-md-5\">\r\n                                <span style=\"width:150px;\">{{plant.name}} Number: </span>\r\n                                <input placeholder=\"Enter a number\" type=\"number\" step=\"0.0000000000000000000000000000000000000000001\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"plant.number\" :name=\"'number'+plant.id\" @input=\"calculateEstimation(plant)\"> \r\n                              </div>\r\n                              <div class=\"o_thread_search_input\" v-for=\"(product,key2) in plant.products\" :class=\"key2==0?'col-md-4':'col-md-3'\">\r\n                                <span style=\"width:90px;\" v-if=\"key2==0\">Estimate Qty: </span>\r\n                                <input :placeholder=\"plant['approximate'+product.product_type+plant.id]?'Approximate :'+plant['approximate'+product.product_type+plant.id]:''\" type=\"number\" step=\"0.000000000000000000000000001\" class=\"ui-autocomplete-input append_btn_input_right\" v-model=\"plant['estimate'+product.product_type+plant.id]\" autocomplete=\"off\" @keyup=\"checkMax(plant)\" :name=\"'estimate'+product.product_type+plant.id\">\r\n                                <button class=\"btn btn-white append_btn_right\" type=\"button\">{{product.product_type_detail.name}}</button>\r\n                              </div>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport LocationFields from '/src/views/app/oodo/components/location_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - LAND\"\r\n  },\r\n  components:{\r\n    LocationFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:[],\r\n      id:null,\r\n      farmer_id:null,\r\n      farmer:[],\r\n      categories:[],\r\n      action:'create',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'fpo'\r\n    this.action = this.$route.params.action\r\n    this.farmer_id = this.$route.params.farmer_id\r\n    this.getFarmer()\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.getPlants()\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getPlants() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/plant_categories/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var self = this\r\n        jsonData.forEach(function(category,key1){\r\n          category.plants.forEach(function(plant,key2){\r\n            jsonData[key1].plants[key2]['number'] = ''\r\n            plant.products.forEach(function(product,key3){\r\n              jsonData[key1].plants[key2]['approximate'+product.product_type+plant.id] = ''\r\n              jsonData[key1].plants[key2]['estimate'+product.product_type+plant.id] = ''\r\n              self.setEstimate(plant,product)\r\n            })\r\n          })\r\n        })\r\n        this.categories = jsonData\r\n      })\r\n    },\r\n    calculateEstimation(plant){\r\n      plant.products.forEach(function(product,key){\r\n        plant['approximate'+product.product_type+plant.id] = plant.number * product.constant\r\n      })\r\n      this.checkMax(plant)\r\n    },\r\n    checkMax(plant){\r\n      plant.products.forEach(function(product,key){\r\n        if(plant['estimate'+product.product_type+plant.id] > plant['approximate'+product.product_type+plant.id]){\r\n          plant['estimate'+product.product_type+plant.id] = ''\r\n        }\r\n      })\r\n    },\r\n    setEstimate(plant,product){\r\n      if(this.action == 'edit'){\r\n        var plants = this.form_data.plants\r\n        plants.forEach(function(val){\r\n          if(product.id == val.product){\r\n            plant['number'] = val.number\r\n            plant['estimate'+product.product_type+plant.id] = val.estimate\r\n          }\r\n        })\r\n      }\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/plants/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.getPlants()\r\n      })\r\n    },\r\n    getFarmer() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmers/'+this.farmer_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmer = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append(\"farmer\",this.farmer_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/plants/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append(\"farmer\",this.farmer_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/plants/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../../styles/style.css';\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/fpo/farmer"}]}