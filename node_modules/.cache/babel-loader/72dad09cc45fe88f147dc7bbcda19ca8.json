rmer Estimate</td>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  props: ['farmer_id'],\r\n  data() {\r\n    return {\r\n      panchayaths:[],\r\n      product_types:[],\r\n      products:[],\r\n      panchayath_id:null,\r\n    };\r\n  },\r\n  mounted() {\r\n    if(this.product_id){\r\n        this.getProduct()\r\n    }else{\r\n        this.getCategories()\r\n    }\r\n  },\r\n  methods: {\r\n    getProduct() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/products/'+this.product_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n          this.products = jsonData\r\n          this.category_id = jsonData.category_detail.id\r\n          this.product_type_id = jsonData.product_type_detail.id\r\n          this.getCategories()\r\n      })\r\n    },\r\n    getCategories() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/categories/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.categories = jsonData\r\n        this.getProductTypes()\r\n      })\r\n    },\r\n    getProductTypes() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/product_types/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_types = jsonData\r\n        this.getProducts()\r\n      })\r\n    },\r\n    getProducts() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/products/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.products = jsonData\r\n      })\r\n    },\r\n    restProducts(location_limit){\r\n      this.product_id = null\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/views/app/oodo/components"}]}