m-block mx-1\">Edit</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'no'\">\r\n            {{props.row.id}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'action'\">\r\n            <div class=\"badge p-1\" :class=\"props.row.action=='Update'?'badge-primary':props.row.action=='Create'?'badge-info':''\">{{(props.row.action)}}</div>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'file_type'\">\r\n            <b>{{props.row.file_type}}</b> \r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'farmer'\">\r\n            {{props.row.farmer_detail.name}} \r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'farmer_code'\">\r\n            <span class=\"badge badge-success\">{{props.row.farmer_detail.farmer_code}}</span>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'aeo'\">\r\n            {{props.row.aeo_detail.name}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'created_on'\">\r\n            {{dateFormate(props.row.created_on,true)}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'status'\">\r\n            \r\n            <div :class=\"props.row.status=='Approved'?'badge badge-success':props.row.status=='Declined'?'badge badge-danger':props.row.status=='Pending'?'badge badge-warning':''\">{{(props.row.status)}}</div>\r\n          </span>\r\n        </template>\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"ID\",\r\n          field: \"no\"\r\n        },\r\n        {\r\n          label: \"Method\",\r\n          field: \"action\"\r\n        },\r\n        {\r\n          label: \"File\",\r\n          field: \"file_type\"\r\n        },\r\n        {\r\n          label: \"Farmer\",\r\n          field: \"farmer\"\r\n        },\r\n        {\r\n          label: \"Farmer\",\r\n          field: \"farmer_code\"\r\n        },\r\n        {\r\n          label: \"Aeo\",\r\n          field: \"aeo\"\r\n        },\r\n        {\r\n          label: \"Status\",\r\n          field: \"status\"\r\n        },\r\n        {\r\n          label: \"Date & Time\",\r\n          field: \"created_on\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: [],\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'fpo'\r\n    this.getFarmerFileUpdation()\r\n  },\r\n  methods: {\r\n    getFarmerFileUpdation() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmer_files/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/fpo"}]}