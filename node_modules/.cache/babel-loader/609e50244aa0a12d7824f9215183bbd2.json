uter-link tag=\"a\" class to=\"/app/security-gate/movement-recuter-vehicles/create\"><b-button variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >+ Create\r\n              </b-button></router-link>\r\n              <router-link tag=\"a\" class to=\"/app/oodo/import\"><b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Import\r\n              </b-button></router-link>\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: true,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: true,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows\"\r\n      >\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <div v-if=\"props.row.gate_out\">\r\n              In ({{dateFormate(props.row.gate_in_time,true)}}) - Out ({{dateFormate(props.row.gate_out_time,true)}})\r\n            </div>\r\n            <b-button  variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"changeStatus(props.row)\" v-else-if=\"props.row.gate_in\">Out</b-button>\r\n            <b-button  variant=\"primary\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"changeStatus(props.row)\" v-else>In</b-button>\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Vehicle Number\",\r\n          field: \"vehicle_number\"\r\n        },\r\n        {\r\n          label: \"Name\",\r\n          field: \"name\"\r\n        },\r\n        {\r\n          label: \"Contact\",\r\n          field: \"contact\"\r\n        },\r\n        {\r\n          label: \"Vehicle Type\",\r\n          field: \"vehicle_type\"\r\n        },\r\n        {\r\n          label: \"Type\",\r\n          field: \"vehicle_for\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: [\r\n       \r\n      ]\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'security-gate'\r\n    this.getRows()\r\n  },\r\n  methods: {\r\n    getRows() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/security/security_book/?vehicle_for=not_raw_arraival', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n    changeStatus(row){\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      var d = new Date();\r\n      var current_time = this.currentDate()+' '+d.getHours()+':'+d.getMinutes();\r\n      formdata.append('name', row.name)\r\n      formdata.append('vehicle_for', row.vehicle_for)\r\n      if(!row.gate_in && !row.gate_out){\r\n        formdata.append('gate_in', 1)\r\n        formdata.append('gate_in_time', current_time)\r\n      }else if(row.gate_in && !row.gate_out){\r\n        formdata.append('gate_out', 1)\r\n        formdata.append('gate_out_time', current_time)\r\n        formdata.append('gate_in', row.gate_in)\r\n        formdata.append('gate_in_time', row.gate_in_time)\r\n      }else{\r\n        return false\r\n      }\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/security/security_book/'+row.id+'/', {\r\n          method : 'put',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.getRows()\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/security-gate"}]}