aceholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: false,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows\"\r\n      >\r\n        <div slot=\"table-actions\" class=\"mb-3 filter-but\">\r\n\r\n        </div>\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <router-link :to=\"'/app/employee/employee/edit/'+props.row.id\">\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1 mx-1\">Edit</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'status'\">\r\n            <span class=\"badge\" :class=\"props.row.status?'badge-success':'badge-danger'\">{{props.row.status?'Active':'Deactive'}}</span>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'user'\">\r\n            <div class=\"text-info h5 cursor-pointer\" v-if=\"props.row.user\" @click=\"addUser(props.row)\">\r\n              {{props.row.user_detail.username}}\r\n            </div>\r\n            <b-button  variant=\"warning\" class=\" px-3 py-1 mx-1\" @click=\"addUser(props.row)\" v-else>Add to User</b-button>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'job_position_detail'\">\r\n            {{props.row.job_position_detail?props.row.job_position_detail.name:''}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'department_detail'\">\r\n            {{props.row.department_detail?props.row.department_detail.name:''}}\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n    <div>\r\n      <b-modal id=\"add-user\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n          <div class=\"main-content create-main\">\r\n            <form ref=\"userFormdata\" @submit.prevent=\"userFormdata()\">\r\n              <div>\r\n                <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                    <slot name=\"header\">\r\n                       <h5 class=\"mb-0\"><span class=\"text-success\">Add to User</span> / {{current_user.id?'Edit':'Add'}}</h5>\r\n                          \r\n                    </slot>\r\n                    <div class=\"d-flex\">\r\n                      <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\">Save\r\n                      </b-button>\r\n                      <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('add-user')\"\r\n                        >Discard\r\n                      </b-button>\r\n                    </div>\r\n                   \r\n                </div>\r\n                <div class=\" my-3 border-top\"></div>\r\n              </div>\r\n\r\n              <b-row class=\"main-scroll\">\r\n                 <b-col lg=\"12\" xl=\"12\" class=\"my-3\">\r\n                  <div>\r\n                    <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:90px;\"> User Name: </span>\r\n                        <input placeholder=\"User Name\" type=\"text\" class=\"ui-autocomplete-input\" v-model=\"current_user.username\" autocomplete=\"new-password\" name=\"username\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:90px;\"> Password: </span>\r\n                        <input placeholder=\"Password\" type=\"password\" class=\"ui-autocomplete-input\" v-model=\"current_user.password1\" autocomplete=\"new-password\" name=\"password1\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:90px;\"> Confirm Password: </span>\r\n                        <input placeholder=\"Password\" type=\"password\" class=\"ui-autocomplete-input\" v-model=\"current_user.password\" name=\"password\" required>\r\n                      </div>\r\n                      <div v-if=\"current_user.job_position==2\">\r\n                         <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\"> FPO: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"fpo\" v-model=\"current_user.fpo\">\r\n                                <option v-for=\"type in types\" :value=\"type.id\">{{type.name}}</option>\r\n                            </select>\r\n                         </div>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\" v-show=\"false\">\r\n                        <input type=\"text\" class=\"ui-autocomplete-input\" v-model=\"current_user.first_name\" name=\"first_name\" required>\r\n                        <input type=\"text\" class=\"ui-autocomplete-input\" v-model=\"current_user.email\" name=\"email\" required>\r\n                        <input type=\"text\" class=\"ui-autocomplete-input\" v-model=\"current_user.phone\" name=\"phone\">\r\n                        <input type=\"text\" class=\"ui-autocomplete-input\" v-model=\"current_user.employee\" name=\"employee\" required>\r\n                        <input type=\"checkbox\" name=\"is_staff\" value=\"1\" checked>\r\n                        <div v-if=\"current_user.job_position==1\">\r\n                          <input type=\"checkbox\" name=\"is_admin\" value=\"1\" checked>\r\n                          <input type=\"text\" class=\"ui-autocomplete-input\" value=\"Document Manager\" name=\"last_name\" required>\r\n                        </div>\r\n                        <div v-if=\"current_user.job_position==2\">\r\n                          <input type=\"checkbox\" name=\"is_aeo\" value=\"1\" checked>\r\n                          <input type=\"text\" class=\"ui-autocomplete-input\" value=\"AEO\" name=\"last_name\" required>\r\n                        </div>\r\n                        <div v-if=\"current_user.job_position==3\">\r\n                          <input type=\"checkbox\" name=\"is_admin\" value=\"1\" checked>\r\n                          <input type=\"text\" class=\"ui-autocomplete-input\" value=\"Production Manager\" name=\"last_name\" required>\r\n                        </div>\r\n                        <div v-if=\"current_user.job_position==4\">\r\n                          <input type=\"checkbox\" name=\"is_supervisor\" value=\"1\" checked>\r\n                          <input type=\"text\" class=\"ui-autocomplete-input\" value=\"Supervisor\" name=\"last_name\" required>\r\n                        </div>\r\n                        <div v-if=\"current_user.job_position==5\">\r\n                          <input type=\"checkbox\" name=\"is_admin\" value=\"1\" checked>\r\n                          <input type=\"text\" class=\"ui-autocomplete-input\" value=\"Purchase Manager\" name=\"last_name\" required>\r\n                        </div>\r\n                        <div v-if=\"current_user.job_position==6\">\r\n                          <input type=\"checkbox\" name=\"is_security\" value=\"1\" checked>\r\n                          <input type=\"text\" class=\"ui-autocomplete-input\" value=\"Security Guard\" name=\"last_name\" required>\r\n                        </div>\r\n                        <div v-if=\"current_user.job_position==7\">\r\n                          <input type=\"checkbox\" name=\"is_qc\" value=\"1\" checked>\r\n                          <input type=\"text\" class=\"ui-autocomplete-input\" value=\"QC\" name=\"last_name\" required>\r\n                        </div>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input mt-2\">\r\n                        <b style=\"width:96px\"> Status: </b>\r\n                        <b-form-checkbox v-model=\"current_user.is_active\" name=\"is_active\" switch>\r\n                          <b>{{ current_user.is_active?'Active':'Deactive' }}</b>\r\n                        </b-form-checkbox>\r\n                      </div>\r\n                    </b-card>\r\n                  </div>\r\n                </b-col>\r\n              </b-row>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Name\",\r\n          field: \"name\"\r\n        },\r\n        {\r\n          label: \"Email\",\r\n          field: \"email\"\r\n        },\r\n        {\r\n          label: \"User Status\",\r\n          field: \"status\"\r\n        },\r\n        {\r\n          label: \"Job Position\",\r\n          field: \"job_position_detail\"\r\n        },\r\n        {\r\n          label: \"Department\",\r\n          field: \"department_detail\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n        {\r\n          label: \"User\",\r\n          field: \"user\"\r\n        },\r\n       \r\n      ],\r\n      rows: [],\r\n      current_user:{},\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'employee'\r\n    this.getEmployees()\r\n    this.getTypes()\r\n  },\r\n  methods: {\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n    getTypes() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/types/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.types = jsonData\r\n      })\r\n    },\r\n    addUser(employee){\r\n      if(employee.user){\r\n        this.current_user = employee.user_detail\r\n        this.current_user.job_position= null\r\n      }else{\r\n        this.current_user = employee\r\n        this.current_user.first_name= employee.name\r\n        this.current_user.phone= employee.contact\r\n        this.current_user.employee= employee.id\r\n        this.current_user.is_active= true\r\n        this.current_user.id= null\r\n      }\r\n      this.$bvModal.show('add-user')\r\n    },\r\n    userFormdata(method='POST') {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var param = ''\r\n      if(this.current_user.id){\r\n        method = 'PUT'\r\n        param = this.current_user.id+'/'\r\n      }\r\n      var formdata = new FormData(this.$refs['userFormdata'])\r\n      fetch(this.api_url+'/users/users/'+param, {\r\n          method : method,\r\n          body : formdata\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.current_user.id= jsonData.id\r\n          this.attachUser()\r\n        }else{\r\n          this.lock_btn = false\r\n          this.showAlert('User already exists!','red')\r\n        }\r\n      })\r\n    },\r\n    attachUser() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      var formdata = new FormData()\r\n      formdata.append('employee', this.current_user.employee)\r\n      formdata.append('user', this.current_user.id)\r\n      formdata.append('fpo', this.current_user.fpo)\r\n      fetch(this.api_url+'/employee/add_user/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.success){\r\n          this.showAlert('Saved Successfully!')\r\n          this.getEmployees()\r\n          this.$bvModal.hide('add-user')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/employee"}]}