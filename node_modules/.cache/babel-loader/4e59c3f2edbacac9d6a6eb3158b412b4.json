ate slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <router-link :to=\"'/app/procurement/sample-procurement/view/'+props.row.id\">\r\n              <b-button  variant=\"primary\" class=\" px-3 py-1  d-sm-block mx-1\">View</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'sample_id'\">\r\n            #{{props.row.id}}/Sample \r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'farmer'\">\r\n            {{props.row.farmer}} ({{props.row.farmer_code}}) \r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'product'\">\r\n            {{props.row.category}}> {{props.row.product}}\r\n            <sup>\r\n              <span class=\"badge badge-success\" v-if=\"props.row.farmer_status=='Organic'\">ORG</span>\r\n            </sup>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'date'\">\r\n            {{dateFormate(props.row.created_on,true)}}\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Id\",\r\n          field: \"sample_id\"\r\n        },\r\n        {\r\n          label: \"Farmer\",\r\n          field: \"farmer\"\r\n        },\r\n        {\r\n          label: \"Product\",\r\n          field: \"product\"\r\n        },\r\n        {\r\n          label: \"AEO\",\r\n          field: \"aeo\"\r\n        },\r\n        {\r\n          label: \"QC Status\",\r\n          field: \"qc_status\"\r\n        },\r\n        {\r\n          label: \"Date&Time\",\r\n          field: \"date\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: [\r\n       \r\n      ]\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'procurement'\r\n    this.getProcurements()\r\n  },\r\n  methods: {\r\n    getProcurements() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_procurement/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/procurement"}]}