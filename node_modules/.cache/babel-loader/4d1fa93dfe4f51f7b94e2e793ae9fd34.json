KAAA,CAAA,UAAA,CAAA,CAAA;AACA,UAAA,OAAA,GAAA,IAAA,OAAA,EAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,eAAA,EAAA,WAAA,KAAA,KAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,KAAA,OAAA,GAAA,gBAAA,GAAA,KAAA,EAAA,GAAA,GAAA,EAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,IAAA,EAAA,QAFA;AAGA,QAAA,OAAA,EAAA;AAHA,OAAA,CAAA,CAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,EAAA;AACA,OAPA,EAQA,IARA,CAQA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,EAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,uBAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,kBAAA;AACA;AACA,OAdA;AAeA;AAzFA;AAxBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"main-content create-main\">\r\n    <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n      <div>\r\n        <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n          <slot name=\"header\">\r\n             <h5 class=\"mb-0\"><span class=\"text-success\">AEO</span> / {{slugToWord(action)}}</h5>\r\n          </slot>\r\n          <div class=\"d-flex\">\r\n             <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n              >Save\r\n            </b-button>\r\n            <router-link to=\"/app/employee/aeo\">\r\n              <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Discard\r\n              </b-button>\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n        <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"4\" xl=\"4\" class=\" mb-30\">\r\n          <b-card no-body class=\"o-hidden card-scroll\">\r\n            <div class=\"card user-profile o-hidden mb-30 inputs\" style=\"box-shadow:none;\">\r\n              <div class=\"header-cover\" style=\"background: linear-gradient(51deg, #252d4b, #3c4f96); height: 120px;\"></div>\r\n              <div class=\"user-info\">\r\n                <label for=\"profile_photo\">\r\n                  <img class=\"bg-dark profile-picture avatar-lg p-2 mb-2 cursor-pointer\" src=\"/img/camera2.png\" alt=\"\">\r\n                </label>\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:90px;\"> Profile Photo </span>\r\n                  <input type=\"file\" name=\"photo\" id=\"profile_photo\" style=\"display:none\">\r\n                </div>\r\n              </div>\r\n              <hr>\r\n              <div class=\"oe_button_box\" v-if=\"id\">\r\n                <button type=\"button\" name=\"action_get_by_vfc\" class=\"btn oe_stat_button\">\r\n                  <i class=\"fa fa-fw o_button_icon fa-book\"></i>\r\n                  <div name=\"vfc_count\" class=\"o_field_widget o_stat_info o_readonly_modifier\">\r\n                      <span class=\"o_stat_value\">72</span>\r\n                      <span class=\"o_stat_text\">VFC</span>\r\n                  </div>\r\n                </button>\r\n                <button type=\"button\" name=\"action_get_by_farmer\" class=\"btn oe_stat_button\">\r\n                  <i class=\"fa fa-fw o_button_icon fa-book\"></i>\r\n                  <div name=\"farmer_count\" class=\"o_field_widget o_stat_info o_readonly_modifier\">\r\n                      <span class=\"o_stat_value\">1,112</span>\r\n                      <span class=\"o_stat_text\">Farmer</span>\r\n                  </div>\r\n                </button>\r\n                <button type=\"button\" name=\"action_get_by_ics\" class=\"btn oe_stat_button\">\r\n                  <i class=\"fa fa-fw o_button_icon fa-book\"></i>\r\n                  <div name=\"ics_count\" class=\"o_field_widget o_stat_info o_readonly_modifier\">\r\n                    <span class=\"o_stat_value\">5</span>\r\n                    <span class=\"o_stat_text\">ICS</span>\r\n                  </div>\r\n                </button>\r\n                <button type=\"button\" name=\"action_get_by_procurement\" class=\"btn oe_stat_button\">\r\n                  <i class=\"fa fa-fw o_button_icon fa-book\"></i>\r\n                  <div name=\"procurement_count\" class=\"o_field_widget o_stat_info o_readonly_modifier\">\r\n                    <span class=\"o_stat_value\">19</span>\r\n                    <span class=\"o_stat_text\">Procurement</span>\r\n                  </div>\r\n                </button>\r\n                <button type=\"button\" name=\"action_get_by_panchayath\" class=\"btn oe_stat_button\">\r\n                  <i class=\"fa fa-fw o_button_icon fa-book\"></i>\r\n                  <div name=\"panchayath_count\" class=\"o_field_widget o_stat_info o_readonly_modifier\">\r\n                    <span class=\"o_stat_value\">2</span>\r\n                    <span class=\"o_stat_text\">Panchayath</span>\r\n                  </div>\r\n                </button>\r\n                <button type=\"button\" name=\"action_get_by_panchayath\" class=\"btn oe_stat_button\">\r\n                  <i class=\"fa fa-fw o_button_icon fa-book\"></i>\r\n                  <div name=\"panchayath_count\" class=\"o_field_widget o_stat_info o_readonly_modifier\">\r\n                    \r\n                    <span class=\"o_stat_text\">Meeting</span>\r\n                  </div>\r\n                </button>\r\n              </div>\r\n                     \r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"8\" xl=\"8\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                             <div class=\"o_thread_search_input\">\r\n                                  <span style=\"width:120px;\"> Name: </span>\r\n                                  <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.employee\">\r\n                                    <option selected disabled>Select a name</option>\r\n                                    <option v-for=\"employee in employees\" :value=\"employee.id\" @click=\"form_data=employee\">{{employee.name}}</option>\r\n                                  </select>\r\n                                  <input type=\"hidden\" name=\"employee\" :value=\"form_data.name\">\r\n                             </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Email: </span>\r\n                              <input placeholder=\"Email\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.email\" name=\"email\" required readonly> \r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Mobile Number: </span>\r\n                              <input placeholder=\"Mobile\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.mobile\" name=\"mobile\" required readonly> \r\n                            </div>\r\n                             <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\"> FPO: </span>\r\n                                <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"type\" v-model=\"form_data.type\">\r\n                                    <option v-for=\"type in types\" :value=\"type.id\">{{type.name}}</option>\r\n                                </select>\r\n                             </div>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{employee_detail:{}},\r\n      id:null,\r\n      action:'create',\r\n      employees:[],\r\n      types:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'employee'\r\n    this.getEmployees()\r\n    this.getTypes()\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.employees = jsonData\r\n      })\r\n    },\r\n    getTypes() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/types/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.types = jsonData\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/aeo/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/aeo/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/aeo/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/employee"}]}