hecked=\"1\">\r\n      <div class=\"footer__row-count vgt-pull-left\" bis_skin_checked=\"1\">\r\n        <span class=\"footer__row-count__label\">Jump to page</span>\r\n        <select class=\"ml-2\" v-model=\"current_page\" @change=\"changePage(current_page)\">\r\n          <option v-for=\"p in max_page\" :value=\"p\">{{p}}</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"table_pagination_loading\" v-if=\"load_first\"></div>\r\n      <div class=\"footer__navigation vgt-pull-right\" v-else>\r\n        <a href=\"javascript:undefined\" tabindex=\"0\" class=\"footer__navigation__page-btn\" @click=\"rows.previous?changePage(current_page--):''\" :class=\"rows.previous?'':'disabled'\">\r\n          <span class=\"chevron left\"></span><span>Prev</span>\r\n        </a>\r\n        <div class=\"footer__navigation__info\" bis_skin_checked=\"1\">{{page}} - {{rows.results.length}} of {{rows.count}}</div>\r\n        <a href=\"javascript:undefined\" tabindex=\"0\" class=\"footer__navigation__page-btn\" :class=\"rows.next?'':'disabled'\" @click=\"rows.next?changePage(current_page++):''\">\r\n          <span>Next</span> <span class=\"chevron right\"></span>\r\n        </a>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: ['page','rows','load_first'],\r\n  data() {\r\n    return {\r\n      max_page:1,\r\n      current_page:this.page,\r\n      results:20,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.max_page = parseInt(this.rows.count/this.results)\r\n    if(this.rows.count%this.results!=0){\r\n      this.max_page++\r\n    }\r\n  },\r\n  methods: {\r\n    changePage(){\r\n      this.$emit('changePage', this.current_page)\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/views/app/oodo/components"}]}