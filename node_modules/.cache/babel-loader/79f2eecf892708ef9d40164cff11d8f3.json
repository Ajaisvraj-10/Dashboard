/production/processing/'+product+'/view/'+props.row.id\">\r\n              <b-button  variant=\"primary\" class=\" px-3 py-1 mx-1\">View Process</b-button>\r\n            </router-link>\r\n            <span v-if=\"props.row.status=='Completed'\">\r\n                <span class=\"badge badge-secondary px-3 py-2 mx-1 w-25\" style=\"font-size:13px\">Completed</span>\r\n            </span>\r\n            <span v-else>\r\n              <router-link :to=\"'/app/production/processing/'+product+'/start/'+props.row.id\">\r\n                <b-button  variant=\"success\" class=\" px-3 py-1 mx-1\">Start Process</b-button>\r\n              </router-link>\r\n              <router-link :to=\"'/app/production/processing/'+product+'/break/'+props.row.id\">\r\n                <b-button  variant=\"danger\" class=\" px-3 py-1 mx-1\">Break/Down</b-button>\r\n              </router-link>\r\n            </span>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'name'\">\r\n            Lot/{{props.row.id}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'product'\">\r\n            {{props.row.product_detail.name}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'date'\">\r\n            {{dateFormate(props.row.created_on,true)}}\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      product:'',\r\n      columns: [\r\n         {\r\n          label: \"Lot\",\r\n          field: \"name\"\r\n        },\r\n        {\r\n          label: \"Product\",\r\n          field: \"product\"\r\n        },\r\n        {\r\n          label: \"Date\",\r\n          field: \"date\"\r\n        },\r\n        {\r\n          label: \"Status\",\r\n          field: \"status\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: [\r\n       \r\n      ]\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'production'\r\n    this.product = this.$route.params.product\r\n    this.getProcurements()\r\n  },\r\n  methods: {\r\n    getProcurements() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/create_lots/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/process"}]}