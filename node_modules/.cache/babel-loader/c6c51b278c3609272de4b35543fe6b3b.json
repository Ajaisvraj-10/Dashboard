er  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Packing Material Purchase</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/inventory/packing-material-stock\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"4\" xl=\"4\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\" col-md-12\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">WareHouse  : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"warehouse\" v-model=\"form_data.warehouse\">\r\n                                  <option v-for=\"warehouse in warehouses\" :value=\"warehouse.id\">{{warehouse.name}}</option>\r\n                              </select>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Vendor Name : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"vendor\" v-model=\"form_data.vendor\" @change=\"createData('vendor')\">\r\n                                  <option v-for=\"vendor in vendors\" :value=\"vendor.id\" v-if=\"vendor.vendor_type=='Packing Material'\">{{vendor.name}}</option>\r\n                                  <option value=\"create\" style=\"color:green!important\">Create New Vendor</option>\r\n                              </select>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Buyer Name : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"buyer\" v-model=\"form_data.buyer\" @change=\"createData('buyer')\">\r\n                                  <option v-for=\"buyer in buyers\" :value=\"buyer.id\">{{buyer.name}}</option>\r\n                                  <option value=\"create\" style=\"color:green!important\">Create New Buyer</option>\r\n                              </select>\r\n                            </div>\r\n\r\n                            <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\"> Department: </span>\r\n                                <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.department\" name=\"department\">\r\n                                  <option v-for=\"department in departments\" :value=\"department.id\">{{department.name}}</option>\r\n                                </select>\r\n                            </div>\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Total Quantity (Kg): </span>\r\n                              <input placeholder=\"Total Quantity\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"total_quantity\" v-model=\"form_data.total_quantity\" readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Total Price Rs: </span>\r\n                              <input placeholder=\"Total Price\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"total_price\" v-model=\"form_data.total_price\" readonly>\r\n                            </div>\r\n                            <div>\r\n                              <input type=\"hidden\" name=\"status\" :value=\"form_data.status\">\r\n                            </div>\r\n                          </div>\r\n\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n        <b-col lg=\"8\" xl=\"8\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <h5>Packing Materials</h5>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-12\">\r\n                    <b-button variant=\"success\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"lineOperation('+')\">\r\n                      Add a material\r\n                    </b-button>\r\n                    <input type=\"hidden\" name=\"keys\" :value=\"form_data.values.length\">\r\n                  </div>\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\" style=\"overflow-x:auto\">\r\n                        <div class=\"col-md-6 px-2\" v-for=\"(line,key) in form_data.values\">\r\n                          <div class=\"card p-2 my-1\">\r\n                            <div class=\"o_thread_search_input px-0\">\r\n                              <select class=\"ui-autocomplete-input w-100 ml-0\" :name=\"'material'+key\" v-model=\"line.material\">\r\n                                <option v-for=\"material in materials\" :value=\"material.id\">{{material.name}}</option>\r\n                              </select>\r\n                              <div class=\"pb-3\">\r\n                                <b-button variant=\"danger\" type=\"button\" class=\"ml-2 px-3 py-1 d-sm-block\" @click=\"lineOperation(key)\">\r\n                                  Remove\r\n                                </b-button>\r\n                              </div>\r\n                            </div>\r\n                            <table class=\"table material_table mb-0\">\r\n                              <tbody>\r\n                                <tr v-for=\"(specification,key2) in line.specifications\" v-if=\"specification.material_ids.includes(line.material)\">\r\n                                  <th>{{specification.name}}</th>\r\n                                  <td>\r\n                                    <div>\r\n                                      <select class=\"ui-autocomplete-input w-100 mx-1\" :name=\"specification.id+'value'+key\" v-model=\"specification.value\" v-if=\"specification.values\">\r\n                                        <option v-for=\"value in specification.values\" :value=\"value.value\">{{value.value}}</option>\r\n                                      </select>\r\n                                      <div v-else><input type=\"checkbox\" :name=\"specification.id+'value'+key\" v-model=\"specification.value\" :id=\"'check'+key\"> <label :for=\"'check'+key\"> YES</label></div>\r\n                                    </div>\r\n                                  </td>\r\n                                </tr>\r\n                                <tr>\r\n                                  <th>Max Qty (Kg)</th>\r\n                                  <td>\r\n                                    <input placeholder=\"Max QTY\" type=\"text\" class=\"\" autocomplete=\"off\" v-model=\"line.max_quantity\" :name=\"'max_quantity'+key\" required>\r\n                                    <input type=\"hidden\" v-model=\"line.max_quantity\" name=\"'quantity_obtained'+key\">\r\n                                  </td>\r\n                                </tr>\r\n                                <tr>\r\n                                  <th>Price (1 piece) Rs.</th>\r\n                                  <td>\r\n                                    <input placeholder=\"Price\" type=\"text\" class=\"\" autocomplete=\"off\" v-model=\"line.price\" :name=\"'price'+key\" @input=\"calcultion()\" required>\r\n                                  </td>\r\n                                </tr>\r\n                                <tr>\r\n                                  <th>Quantity</th>\r\n                                  <td>\r\n                                    <input placeholder=\"QTY\" type=\"text\" class=\"\" autocomplete=\"off\" v-model=\"line.quantity\" :name=\"'quantity'+key\" @input=\"calcultion()\" required>\r\n                                    <input type=\"hidden\" v-model=\"line.quantity_obtained\" name=\"'quantity_obtained'+key\">\r\n                                  </td>\r\n                                </tr>\r\n                              </tbody>\r\n                            </table>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n    <div>\r\n      <b-modal id=\"create-vendor\" hide-header  hide-footer>\r\n        <form ref=\"createVendor\" @submit.prevent=\"createVendor()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Create Vendor</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('create-vendor')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2 pb-5\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <div style=\"width:50px;\">Name: </div>\r\n                            <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"name\" required>\r\n                          </div>\r\n                          <div>\r\n                            <input type=\"hidden\" name=\"vendor_type\" value=\"Packing Material\">\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n\r\n      <b-modal id=\"create-buyer\" hide-header  hide-footer>\r\n        <form ref=\"createBuyer\" @submit.prevent=\"createBuyer()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Create Buyer</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('create-buyer')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2 pb-5\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <div style=\"width:50px;\">Name: </div>\r\n                            <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"name\" required>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{warehouse:null,values:[],total_quantity:0,total_price:0,status:'Ordered'},\r\n      id:null,\r\n      action:'create',\r\n      materials:[],\r\n      specifications:[],\r\n      departments:[],\r\n      vendors:[],\r\n      buyers:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'inventory'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n    }\r\n    this.getMaterials()\r\n    this.getSpecifications()\r\n    this.getDepartments()\r\n    this.getVendors()\r\n    this.getBuyers()\r\n    this.getWarehouses()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n      })\r\n    },\r\n    getMaterials() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/materials/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.materials = jsonData\r\n      })\r\n    },\r\n    getSpecifications() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/specifications/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.specifications = jsonData\r\n      })\r\n    },\r\n    getDepartments() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/categories/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.departments = jsonData\r\n      })\r\n    },\r\n    getVendors() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/vendors/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.vendors = jsonData\r\n      })\r\n    },\r\n    createVendor() {\r\n      var formdata = new FormData(this.$refs['createVendor'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/vendors/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.getVendors()\r\n          this.form_data.vendor = jsonData.id\r\n          this.$bvModal.hide('create-vendor')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getBuyers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/buyers/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.buyers = jsonData\r\n      })\r\n    },\r\n    createBuyer() {\r\n      var formdata = new FormData(this.$refs['createBuyer'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/buyers/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.getBuyers()\r\n          this.form_data.buyer = jsonData.id\r\n          this.$bvModal.hide('create-buyer')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    createData(model){\r\n      if(model=='vendor'){\r\n        if(this.form_data.vendor=='create'){\r\n          this.form_data.vendor = ''\r\n          this.$bvModal.show('create-vendor')\r\n        }\r\n      }else if(model=='buyer'){\r\n        if(this.form_data.buyer=='create'){\r\n          this.form_data.buyer = ''\r\n          this.$bvModal.show('create-buyer')\r\n        }\r\n      }\r\n    },\r\n    lineOperation(operation){\r\n      if(operation=='+'){\r\n        var specification_values = [];\r\n        this.specifications.forEach(function(val,key){\r\n          if(val.specification_for=='Packing Material'){\r\n            specification_values.push({id:val.id, name:val.name ,value:'', is_values:val.is_values, values:val.values,material_ids:val.material_ids})\r\n          }\r\n        })\r\n        this.form_data.values.push({'material':this.materials[0].id,'quantity':0, price:0,max_quantity:0, specifications:specification_values})\r\n        this.total_keys++\r\n      }else{\r\n        this.form_data.values.splice(operation, 1)\r\n        this.total_keys--\r\n      }\r\n      this.calcultion()\r\n    },\r\n    calcultion(){\r\n      var quantity = 0\r\n      var price = 0\r\n      this.form_data.values.forEach(function(val,key){\r\n        quantity = quantity + parseFloat(val.quantity)\r\n        price = price + (parseFloat(val.price)*parseFloat(val.quantity))\r\n      })\r\n      this.form_data.total_quantity = quantity\r\n      this.form_data.total_price = price\r\n    },\r\n    createMaterialsFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('packing_material',this.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase_materials/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          // this.indentPackingDetails()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('warehouse', this.form_data.warehouse)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          // this.form_data = jsonData\r\n          this.id = jsonData.id\r\n          this.createMaterialsFormdata()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('warehouse', this.form_data.warehouse)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.createMaterialsFormdata()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n</style>\r\n<style>\r\n.table.material_table td, .table.material_table th{\r\n  padding: 3px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/inventory"}]}