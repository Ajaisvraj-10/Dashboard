lass=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: true,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: false,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows.results\"\r\n      >\r\n        <div slot=\"table-actions\" class=\"mb-3 filter-but\">\r\n        </div>\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <router-link :to=\"'/app/procurement/normal-procurement/edit/'+props.row.id\">\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">Edit</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'normal_id'\">\r\n            {{props.row.raw_number}} \r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'product'\">\r\n            {{props.row.category}}> {{props.row.product}}{{props.row.parameter?'> '+props.row.parameter:''}}\r\n            <sup>\r\n              <span class=\"badge badge-success\" v-if=\"props.row.farmer_status=='Organic'\">ORG</span>\r\n            </sup>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'total_farmers'\">\r\n            {{props.row.total_farmers}}Farmer{{checkMult(props.row.total_farmers)}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'total_procured_qty'\">\r\n            {{props.row.total_procured_qty}}KG\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'date'\">\r\n            {{dateFormate(props.row.created_on,true)}}\r\n          </span>\r\n        </template>\r\n      </vue-good-table>\r\n      <TablePagination :page=\"page\" :rows=\"rows\" :load_first=\"load_first\" @changePage=\"changePage\" v-if=\"rows.count>0\"></TablePagination>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TablePagination from '/src/views/app/oodo/components/table_pagination'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Procurement\"\r\n  },\r\n  components:{\r\n    TablePagination\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Dispatch No\",\r\n          field: \"normal_id\"\r\n        },\r\n        {\r\n          label: \"Product\",\r\n          field: \"product\"\r\n        },\r\n        {\r\n          label: \"Farmers\",\r\n          field: \"total_farmers\"\r\n        },\r\n        {\r\n          label: \"Quantity (KG)\",\r\n          field: \"total_procured_qty\"\r\n        },\r\n        {\r\n          label: \"Aeo\",\r\n          field: \"aeo\"\r\n        },\r\n        {\r\n          label: \"Status\",\r\n          field: \"status\"\r\n        },\r\n        {\r\n          label: \"Date\",\r\n          field: \"date\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n      ],\r\n      rows: {results:[],count:0},\r\n      page:0,\r\n      load_first:true,\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'procurement'\r\n    this.changePage(1)\r\n  },\r\n  methods: {\r\n    changePage(count){\r\n      this.page = count\r\n      this.load_first = true\r\n      this.getProcurements()\r\n    },\r\n    getProcurements() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/?page='+this.page+'&purchase_type=1', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.detail=='Invalid page.'){\r\n          this.rows = {results:[],count:0}\r\n        }else{\r\n          this.load_first = false\r\n          this.rows = jsonData\r\n        }\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/procurement"}]}