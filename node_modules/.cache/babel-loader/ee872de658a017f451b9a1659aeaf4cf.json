p>\r\n      </div>\r\n      <div class=\"col-md-12 text-center \" v-if=\"all_ics.length<=0 && !is_start\">\r\n        <p class=\"p-5 m-5\">\r\n          <div class=\"table_loading\"></div>\r\n        </p>\r\n      </div>\r\n      \r\n    </div>\r\n\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  data() {\r\n    return {\r\n      all_ics:[],\r\n      fpo:'ICS',\r\n      is_start:false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'fpo'\r\n    this.changeParam()\r\n  },\r\n  watch: {\r\n    '$route': 'changeParam'\r\n  },\r\n  methods: {\r\n    changeParam() {\r\n      if(this.$route.query.fpo){\r\n        this.fpo = this.$route.query.fpo\r\n        if(!this.fpo){\r\n          this.fpo = 'ICS'\r\n        }\r\n      }else{\r\n        this.fpo = 'ICS'\r\n      }\r\n    this.getICS()\r\n    },\r\n    getICS() {\r\n      this.is_start = false\r\n      var headers = new Headers();\r\n      var param = ''\r\n      if(this.fpo!='ICS'){\r\n        param = '?fpo='+this.fpo\r\n      }\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/ics/'+param, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.is_start = true\r\n        this.all_ics = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/fpo"}]}