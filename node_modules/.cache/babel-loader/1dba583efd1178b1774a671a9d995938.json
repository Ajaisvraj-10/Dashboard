r\n                 v-if=\"form_data.status=='Draft'\" @click=\"form_data.status='Starting'\">Start\r\n              </b-button>\r\n              <b-button variant=\"danger\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\" v-else-if=\"form_data.status=='Starting'\" @click=\"form_data.status='Stop';dataForm()\">Stop\r\n              </b-button>\r\n              <b-button variant=\"primary\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\" v-else-if=\"form_data.status=='Stop'\" @click=\"showOutput()\">Output\r\n              </b-button>\r\n\r\n              <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"goBack()\"\r\n                >Discard\r\n              </b-button>\r\n              <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                <b-dropdown-header id=\"dropdown-header-label\">\r\n                  <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                  <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n                </b-dropdown-header>\r\n              </b-dropdown>\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      \r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"7\" xl=\"7\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"user-info pt-2\">\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Product : </span>\r\n                    <input placeholder=\"Product\" type=\"text\" class=\"ui-autocomplete-input\" :value=\"product_name\" readonly >\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Shift: </span>\r\n                    <select class=\"ui-autocomplete-input w-100 ml-1\" name=\"shift\" v-model=\"form_data.shift\">\r\n                      <option v-for=\"shift in shifts\" :value=\"shift\"> {{shift}}</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Date : </span>\r\n                    <input placeholder=\"Date\" type=\"date\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.date\" name=\"date\" required>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Start Time: </span>\r\n                    <input placeholder=\"00:01\" type=\"time\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.start_time\" name=\"start_time\" style=\"margin-right:20px\">\r\n                    <span style=\"width:80px;\"> End Time: </span>\r\n                    <input placeholder=\"00:10\" type=\"time\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.end_time\" name=\"end_time\">\r\n                  </div>\r\n                  <div class=\"o_thread_search_input mt-5\">\r\n                    <table class=\"table\">\r\n                      <tr>\r\n                        <th>Total Workers</th>\r\n                        <th>:</th>\r\n                        <td>{{form_data.workers.length}}</td>\r\n                      </tr>\r\n                      <tr>\r\n                        <th>Workers (overtime)</th>\r\n                        <th>:</th>\r\n                        <td>{{form_data.overtime_workers.length}}</td>\r\n                      </tr>\r\n                    </table>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"5\" xl=\"5\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:70px\"> Workers: </span>\r\n                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"workers\" v-model=\"form_data.workers\" multiple>\r\n                    <option v-for=\"worker in workers\" :value=\"worker.id\" v-if=\"worker.job_position==5\"> #{{worker.id}}/{{worker.name}}</option>\r\n                  </select>\r\n                </div>\r\n                <div class=\"o_thread_search_input mt-2\">\r\n                  <span style=\"width:90px\"> Over Time? : </span>\r\n                  <b-form-checkbox v-model=\"form_data.is_over_time\" name=\"is_over_time\" switch>\r\n                    <b>{{ form_data.is_over_time?'Yes':'No' }}</b>\r\n                  </b-form-checkbox>\r\n                </div>\r\n\r\n                <div class=\"o_thread_search_input\" v-if=\"form_data.is_over_time\">\r\n                  <span style=\"width:70px;\"> Start: </span>\r\n                  <input placeholder=\"00:01\" type=\"time\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.overtime_start\" name=\"overtime_start\" required>\r\n                  <span style=\"width:35px;padding-left:5px\"> End: </span>\r\n                  <input placeholder=\"00:10\" type=\"time\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.overtime_end\" name=\"overtime_end\">\r\n                </div>\r\n                <div class=\"o_thread_search_input\" v-if=\"form_data.is_over_time\">\r\n                  <span style=\"width:70px\"> Workers (overtime): </span>\r\n                  <select class=\"ui-autocomplete-input w-100 ml-1\" name=\"overtime_workers\" v-model=\"form_data.overtime_workers\" multiple>\r\n                    <option v-for=\"worker in workers\" :value=\"worker.id\" v-if=\"worker.job_position==5\"> #{{worker.id}}/{{worker.name}}</option>\r\n                  </select>\r\n                </div>\r\n                <div class=\"o_thread_search_input my-5\">\r\n                  <input type=\"hidden\" name=\"product\" :value=\"form_data.start_process_detail.product\">\r\n                  <input type=\"hidden\" name=\"start_process\" :value=\"processing_id\">\r\n                  <select name=\"lots\" v-model=\"form_data.lots\" multiple v-show=\"false\">\r\n                    <option v-for=\"lot in form_data.lots\" :value=\"lot\">{{lot}}</option>\r\n                  </select>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{\r\n        date:null,\r\n        product:null,\r\n        lot:null,\r\n        status:'Draft',\r\n        shift:'DAY',\r\n        workers:[],\r\n        overtime_workers:[],\r\n        is_over_time:false,\r\n        start_process_detail:[],\r\n        lots:[],\r\n      },\r\n      process_detail:[],\r\n      id:null,\r\n      process_id:null,\r\n      processing_id:null,\r\n      product_name : '',\r\n      action:'create',\r\n      user_id:6,\r\n      workers:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.form_data.date = this.currentDate()\r\n    this.process_id = this.$route.params.process_id\r\n    this.processing_id = this.$route.params.processing_id\r\n    this.checkProcess()\r\n    this.getEmployees()\r\n  },\r\n  methods: {\r\n    checkProcess() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/check_process/'+this.process_id+'/'+this.processing_id+'/', {\r\n          method : 'post',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.form_data = jsonData.process\r\n          this.id = this.form_data.id\r\n          if(this.form_data.status == 'Stop'){\r\n            this.showOutput()\r\n          }\r\n          this.action = 'stop'\r\n        }else{\r\n          this.action = 'start'\r\n          this.getStartProcess()\r\n        }\r\n        this.getForm()\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/process_details/'+this.process_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.process_detail = jsonData\r\n        if(!this.process_detail.inputs){\r\n          this.product_name = this.form_data.start_process_detail.product_detail.name+'/'+this.form_data.start_process_detail.product_detail.product_type_detail.name\r\n        }else{\r\n          this.product_name = jsonData.input_material.name\r\n        }\r\n      })\r\n    },\r\n    getStartProcess() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/start_process/'+this.processing_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.start_process_detail = jsonData\r\n        var lots = []\r\n        jsonData.process_products_list.forEach(function(val){\r\n          lots.push(val.lot)\r\n        })\r\n        this.form_data.lots = lots\r\n      })\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameters/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.parameters = jsonData\r\n      })\r\n    },\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.workers = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'start'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('supervisor',this.user_id)\r\n      formdata.append('status',this.form_data.status)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/processing/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.action = 'stop'\r\n          this.id = jsonData.id\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('supervisor',this.user_id)\r\n      formdata.append('status',this.form_data.status)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/processing/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          if(jsonData.status=='Stop'){\r\n            this.showOutput()\r\n          }\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    showOutput(){\r\n      this.$router.push('/app/production/process/processing/view/'+this.id+'/output/'+this.processing_id)\r\n    },\r\n    updateBatchStatus(status) {\r\n      var formdata = new FormData()\r\n      formdata.append('status',status)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/update_lot_creation/'+this.lot_id+'/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body : formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../../styles/style.css';\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/production/process"}]}