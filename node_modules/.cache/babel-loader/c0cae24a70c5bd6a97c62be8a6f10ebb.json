    <div v-for=\"(farmer,key) in farmers\"> \r\n            <div class=\" borderless-card\">\r\n              <div class=\"d-flex justify-content-between align-items-center  px-0 py-1\">\r\n                <h4 class=\"font-weight-700\">\r\n                  #{{farmer.fields.farmer_code}}\r\n                  <h6 class=\" font-weight-500 text-light\">{{farmer.fields.name}}</h6>\r\n                </h4>\r\n                <h4 class=\"font-weight-700 px-2 py-1 text-green\" style=\"margin-right:-15px;\">{{dateFormate(farmer.created_on)}}</h4>\r\n\r\n              </div>\r\n             \r\n              <div class=\"d-flex justify-content-between px-0 py-1 \">\r\n                <div>\r\n                  <h6 class=\" font-weight-500 text-light\">{{farmer.form_type}} {{farmer.action}}</h6>\r\n                  \r\n                </div>\r\n                <h6 class=\"m-0 font-weight-700 pl-2\" :class=\"farmer.form_status=='Pending'?'text-warning':farmer.form_status=='Approved'?'text-green':'text-danger'\">{{farmer.form_status}}</h6>\r\n\r\n              </div>\r\n            </div>\r\n            <div class=\"border-bottom mx-5 my-2\"></div>\r\n          </div>  \r\n          <div class=\"\">\r\n            <h4 class=\"font-weight-700 my-3 pl-2 text-green text-center\" @click=\"getFarmers()\" v-if=\"load_more\">Load More</h4>\r\n          </div>\r\n        </div>   \r\n              \r\n          \r\n      </div>\r\n      <div class=\"filter-footer\">\r\n        <div class=\"footer-item2 d-flex align-items-center\" @click=\"$router.push('/')\">\r\n          <img src=\"/images/home.png\" class=\"\" width=\"25\">\r\n          <h5 class=\"m-0 pl-2 text-white\">Home</h5>\r\n        </div>\r\n        \r\n      </div>\r\n    </main>\r\n    \r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Home\"\r\n  },\r\n  data() {\r\n    return {\r\n      farmers:[],\r\n      temp_farmers:[],\r\n      searchQuery:'',\r\n      page:0,\r\n      load_more:false,\r\n    };\r\n  },\r\n  mounted(){\r\n    this.getFarmers()\r\n  },\r\n  methods: {\r\n    resultQuery(){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/fpo/aeo_updation/?search='+this.searchQuery, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmers = jsonData\r\n        this.load_more = false\r\n      })\r\n      if(this.searchQuery==''){\r\n        this.farmers = this.temp_farmers\r\n        this.getFarmers()\r\n      }\r\n    },\r\n    getFarmers() {\r\n      this.page++;\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/fpo/aeo_updation/?page='+this.page, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmers = jsonData\r\n        this.temp_farmers = this.farmers\r\n      })\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/pages"}]}