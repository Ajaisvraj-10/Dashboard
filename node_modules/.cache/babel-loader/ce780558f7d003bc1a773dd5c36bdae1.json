n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      \r\n\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n          <h5>Products</h5>\r\n        </b-col>\r\n\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mb-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-4\">\r\n                          <div class=\"user-info mb-5 pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Creation date : </span>\r\n                              <input placeholder=\"Lab Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.date\" name=\"date\" required>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                        <div class=\"col-md-12 px-2\">\r\n\r\n                          <table class=\"table\">\r\n                            <thead>\r\n                              <tr>\r\n                                <th>Department</th>\r\n                                <th>Category</th>\r\n                                <th>Product</th>\r\n                                <th>Material Of Construction</th>\r\n                                <th>Color</th>\r\n                                <th>Dimensions L X B X W (inches)</th>\r\n                                <th>Thikness</th>\r\n                                <th>Quantity</th>\r\n                                <th>Action</th>\r\n                              </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                              <tr v-for=\"line in lines\">\r\n                                <td>\r\n                                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"panchayath[]\">\r\n                                    <option v-for=\"panchayath in panchayaths\" :value=\"panchayath.id\" :selected=\"panchayath.id==line.panchayath\">{{panchayath.name}}</option>\r\n                                  </select>\r\n                                </td>\r\n                                <td>\r\n                                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"cluster_type\">\r\n                                    <option v-for=\"cluster_type in cluster_types\" :value=\"cluster_type\" :selected=\"cluster_type==line.cluster_type\">{{cluster_type}}</option>\r\n                                  </select>\r\n                                </td>\r\n                                <td>\r\n                                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"vfc[]\">\r\n                                    <option v-for=\"vfc in vfcs\" :value=\"vfc.id\" :selected=\"vfc.id==line.vfc\">{{vfc.name}}</option>\r\n                                  </select>\r\n                                </td>\r\n                                <td>\r\n                                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"farmer[]\">\r\n                                    <option v-for=\"farmer in farmers\" :value=\"farmer.id\" :selected=\"farmer.id==line.farmer\">{{farmer.name}}</option>\r\n                                  </select>\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"0\" type=\"text\" class=\"\" autocomplete=\"off\" :value=\"line.farmer_code\" readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"0\" type=\"text\" class=\"\" autocomplete=\"off\" :value=\"line.farmer_code\" readonly></td>\r\n                                <td>\r\n                                  <input placeholder=\"0\" type=\"text\" class=\"\" autocomplete=\"off\" :value=\"line.farmer_code\" required></td>\r\n                                <td>\r\n                                  <input placeholder=\"0\" type=\"text\" class=\"\" autocomplete=\"off\" :value=\"line.farmer_code\" required></td>\r\n                                <td>\r\n                                  <b-button variant=\"danger\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\"@click=\"lines--\">\r\n                                    Remove\r\n                                  </b-button></td>\r\n                              </tr>\r\n                              <tr>\r\n                                <td>\r\n                                  <b-button variant=\"success\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\"@click=\"lines++\">\r\n                                    Add a line\r\n                                  </b-button>\r\n                                </td>\r\n                                <td colspan=\"8\"></td>\r\n                              </tr>\r\n                            </tbody>\r\n                          </table>\r\n                        </div>\r\n                        <div class=\"col-md-12\">\r\n                          <div class=\"user-info mt-5 pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Notes : </span>\r\n                              <textarea placeholder=\"Lab Name\" type=\"text\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" :value=\"form_data.date\" name=\"date\" required></textarea>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:[],\r\n      id:null,\r\n      load_products:false,\r\n      lines:0,\r\n      product_id:null,\r\n      action:'create',\r\n      test_types:[],\r\n      labs:[],\r\n      billing_addresses:[],\r\n      parameters:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'purchase'\r\n    this.action = this.$route.params.action\r\n    this.getPanchayaths()\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/purchase/sample_procurement/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_id = jsonData.product\r\n        this.form_data = jsonData\r\n        this.load_products = true\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/purchase/sample_procurement/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/purchase/sample_procurement/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getPanchayaths() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/panchayath/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.panchayaths = jsonData\r\n        this.getVfc()\r\n      })\r\n    },\r\n    getVfc() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/vfc/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.vfcs = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/purchase-manager"}]}