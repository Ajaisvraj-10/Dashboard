              </b-button></router-link>-->\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: false,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: false,\r\n          mode: 'records',\r\n          perPage: 20,\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows.results\"\r\n      >\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <!-- <router-link :to=\"'/app/fpo/farmer/edit/'+props.row.id\">\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">Edit</b-button>\r\n            </router-link> -->\r\n            <router-link :to=\"'/app/fpo/farmer/view/'+props.row.id\">\r\n              <b-button  variant=\"primary\" class=\" px-3 py-1  d-sm-block mx-1\">View</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'image'\" >\r\n            <div class=\"text-center\">\r\n              <img src=\"/img/farmer2.jpg\" style=\"width:30px\"><br>\r\n              <small>{{props.row.id}}</small>\r\n            </div>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'phone'\" >\r\n            {{props.row.phone}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'farmer_code'\" class=\"badge badge-success\">\r\n            {{props.row.farmer_code}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'vfc_cluster'\">\r\n            {{props.row.vfc_cluster}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'status'\">\r\n            <span class=\"badge\" :class=\"props.row.status=='NEW'?'badge-warning':props.row.status=='ORG'?'badge-success':props.row.status=='IC1'?'badge-primary':props.row.status=='IC2'?'badge-info':props.row.status=='IC3'?'badge-secondary':''\">{{props.row.status}}</span>\r\n          </span>\r\n        </template>\r\n        <div slot=\"emptystate\" class=\"text-center\">\r\n          <div class=\"table_loading\" v-if=\"load_first\"></div>\r\n          <div v-else>No records found</div>\r\n        </div>\r\n      </vue-good-table>\r\n      <TablePagination :page=\"page\" :rows=\"rows\" :load_first=\"load_first\" @changePage=\"changePage\" v-if=\"rows.count>0\"></TablePagination>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TablePagination from '/src/views/app/oodo/components/table_pagination'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - Farmers\"\r\n  },\r\n  components:{\r\n    TablePagination\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Farmer\",\r\n          field: \"image\"\r\n        },\r\n        {\r\n          label: \"Name\",\r\n          field: \"name\"\r\n        },\r\n        {\r\n          label: \"FarmerCode\",\r\n          field: \"farmer_code\"\r\n        },\r\n        {\r\n          label: \"ICS\",\r\n          field: \"ics_name\"\r\n        },\r\n        {\r\n          label: \"VFC\",\r\n          field: \"vfc_name\"\r\n        },\r\n        {\r\n          label: \"Cluster\",\r\n          field: \"vfc_cluster\"\r\n        },\r\n        {\r\n          label: \"Status\",\r\n          field: \"status\"\r\n        },\r\n        {\r\n          label: \"Phone\",\r\n          field: \"phone\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: {results:[],count:0},\r\n      page:0,\r\n      load_first:false,\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'fpo'\r\n    this.changePage(1)\r\n  },\r\n  methods: {\r\n    changePage(count){\r\n      this.page = count\r\n      this.getFarmers()\r\n    },\r\n    getFarmers() {\r\n      if(this.load_first){\r\n        return false\r\n      }\r\n      this.load_first = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmers/?page='+this.page, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.detail=='Invalid page.'){\r\n          this.rows = {results:[],count:0}\r\n        }else{\r\n          this.load_first = false\r\n          this.rows = jsonData\r\n        }\r\n      })\r\n    },\r\n    onPageChange(pagination){\r\n      console.log(pagination)\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/fpo"}]}