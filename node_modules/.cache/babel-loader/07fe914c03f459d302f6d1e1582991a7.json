                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n        \r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n          <h5>Create Task</h5>\r\n        </b-col>\r\n\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mb-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1\"></div>\r\n                        <div class=\"col-md-10\">\r\n\r\n                          <div class=\"o_thread_search_input pt-5\">\r\n                            <span style=\"width:120px;\"> Assign to: </span>\r\n                             <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"supervisor\" v-model=\"form_data.supervisor\">\r\n                                <option v-for=\"employee in employees\" v-if=\"employee.job_position==4\" :value=\"employee.id\"> {{employee.name}}</option>\r\n                              </select>\r\n                          </div>\r\n                          <div class=\"row mx-0 my-4\">\r\n                            <div class=\"o_thread_search_input col-md-6\">\r\n                              <span style=\"width:120px;\"> Date: </span>\r\n                              <input type=\"date\" name=\"date\" v-model=\"form_data.date\" class=\"ui-autocomplete-input w-100 mx-1\">\r\n                              \r\n                            </div>\r\n                            <div class=\"o_thread_search_input col-md-6 pt-3\">\r\n                              <span style=\"width:50px;\"> Shift: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"shift\" v-model=\"form_data.shift\">\r\n                                <option v-for=\"shift in shifts\" :value=\"shift\"> {{shift}}</option>\r\n                              </select>\r\n                            </div>\r\n                            <div class=\"col-md-12\" style=\"padding-left:150px\" v-if=\"form_data.shift=='NIGHT'\">\r\n                              <small>Date : {{form_data.date}} - {{form_data.date}}</small>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\"> Task: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"process_type\" v-model=\"form_data.process_type\">\r\n                              <option v-for=\"process_type in process_types\" :value=\"process_type\"> {{process_type}}</option>\r\n                            </select>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"PRODUCTION - PLANNING\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:[],\r\n      id:null,\r\n      action:'create',\r\n      employees:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.action = this.$route.params.action\r\n    this.getEmployees()\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/planning/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('manager',5)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/planning/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('manager',5)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/planning/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.employees = jsonData\r\n      })\r\n    },\r\n    nextDate(date){\r\n      var day = 60 * 60 * 24 * 1000;\r\n      var endDate = new Date(date.getTime() + day);\r\n      return endDate\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/production"}]}