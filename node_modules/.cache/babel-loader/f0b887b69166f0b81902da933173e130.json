umbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: true,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: true,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows\"\r\n      >\r\n        <div slot=\"table-actions\" class=\"mb-3 filter-but\">\r\n          \r\n        </div>\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <router-link :to=\"'/app/logistics-department/indent/edit/'+props.row.id\">\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">Edit</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'indent'\">\r\n            #{{props.row.id}}Indent\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'products'\">\r\n            <div v-for=\"(indent,key) in props.row.products\">\r\n              {{indent.product_detail.name}}> {{indent.parameter_detail.name}} <b>({{indent.quantity}}KG)</b>\r\n            </div>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'dispatch_indent'\">\r\n            <span class=\"badge badge-success p-1\" v-if=\"props.row.dispatch_indent.id\">Yes</span>\r\n            <span class=\"badge badge-danger p-1\" v-else>No</span>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'expected_date'\">\r\n            {{monthFormate(props.row.expected_date)}}\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Indent\",\r\n          field: \"indent\"\r\n        },\r\n        {\r\n          label: \"Products\",\r\n          field: \"products\"\r\n        },\r\n        {\r\n          label: \"Dispatch\",\r\n          field: \"dispatch_indent\"\r\n        },\r\n        {\r\n          label: \"Expected Date\",\r\n          field: \"expected_date\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: [\r\n       \r\n      ]\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'logistics-department'\r\n    this.getProcurements()\r\n  },\r\n  methods: {\r\n    getProcurements() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/main_indent/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n    monthFormate(date){\r\n      var mydate = new Date(date);\r\n      var month = [\"January\", \"February\", \"March\", \"April\", \"May\", \"June\",\r\n      \"July\", \"August\", \"September\", \"October\", \"November\", \"December\"][mydate.getMonth()];\r\n      var time = '';\r\n      var str = month + ' ' +mydate.getFullYear()+' '+time;\r\n\r\n      return str\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/logistics-department"}]}