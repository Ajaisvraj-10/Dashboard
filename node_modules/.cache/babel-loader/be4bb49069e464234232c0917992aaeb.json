     #Lot{{props.row.packing_material_purchase}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'material_detail'\">\r\n            {{props.row.material_detail.name}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'specifications'\">\r\n            <small v-for=\"specification in props.row.specifications\" v-if=\"specification.material_ids.includes(props.row.material)\">\r\n              <b class=\"text-primary\"> {{specification.name}}</b>:{{specification.value}}, \r\n            </small>\r\n          </span>\r\n        </template>\r\n      </vue-good-table>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Lot\",\r\n          field: \"no\"\r\n        },\r\n        {\r\n          label: \"Material\",\r\n          field: \"material_detail\"\r\n        },\r\n        {\r\n          label: \"Specification\",\r\n          field: \"specifications\"\r\n        },\r\n        {\r\n          label: \"Quantity\",\r\n          field: \"quantity\"\r\n        },\r\n        {\r\n          label: \"Status\",\r\n          field: \"status\"\r\n        },\r\n      ],\r\n      rows: [\r\n       \r\n      ]\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'inventory'\r\n    this.getProcurements()\r\n  },\r\n  methods: {\r\n    getProcurements() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase_materials/?list_for=stock', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/inventory"}]}