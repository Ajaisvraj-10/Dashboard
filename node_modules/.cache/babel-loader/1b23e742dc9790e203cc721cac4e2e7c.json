/i>\r\n                    </template>\r\n                   \r\n                  </b-input-group>\r\n                  \r\n\r\n                </div>\r\n              </form>\r\n              <p class=\"text-right text-12\">Login with OTP</p>\r\n            </div>\r\n            \r\n        \r\n    </div>\r\n\r\n\r\n  \r\n    <footer class=\"footer-login\">\r\n     <button class=\"btn btn-secondary btn-lg btn-block font-weight-700\" @click=\"loginUser()\">LOGIN</button>\r\n       \r\n    </footer>\r\n\r\n\r\n  </main>\r\n\r\n\r\n\r\n\r\n\r\n\r\n  </div>\r\n</template>\r\n<script>\r\n\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Home\"\r\n  },\r\n  data() {\r\n    return {\r\n      current_code:'+91',\r\n    };\r\n  },\r\n  mounted(){\r\n\r\n  },\r\n  methods : {\r\n    loginUser() {\r\n      var formdata = new FormData(this.$refs['login_form'])\r\n      fetch(this.api_url+'/driver/login/', {\r\n          method : 'post',\r\n          body : formdata\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.status){\r\n            localStorage.setItem(\"userInfo\", JSON.stringify(jsonData));\r\n            this.checkLogin()\r\n            if(this.$root.token==''){\r\n                localStorage.removeItem(\"userInfo\");\r\n            }\r\n            else{\r\n                var self = this\r\n                setTimeout(function(){\r\n                    window.location.href = '/';\r\n                },500);\r\n            }\r\n        }else{\r\n            localStorage.removeItem(\"userInfo\");\r\n        }\r\n      })\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/pages"}]}