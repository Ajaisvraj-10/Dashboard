AAA,MAAA,GAAA,IAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,MAAA;AACA;AA9EA;AAxBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"main-content main-content2\">\r\n    <div>\r\n       <div class=\"d-flex mb-2 align-items-center justify-content-between\">\r\n            <slot name=\"header\">\r\n               <h5 class=\"mb-0\"><span class=\"text-success\">All</span>/ VFC</h5>\r\n            </slot>\r\n            <div class=\"d-flex\">\r\n              <h5 class=\"mb-0 mx-2 font-weight-600 cursor-pointer\" @click=\"fpo_id=null;getVFC()\" :class=\"!fpo_id?'text-15 text-info':'text-14'\">All ICS</h5>\r\n              <h5 class=\"mb-0 mx-2 font-weight-600 cursor-pointer\" v-for=\"type in types\" @click=\"fpo_id=type.id;getVFC()\" :class=\"fpo_id==type.id?'text-15 text-info':'text-14'\">{{type.name}}</h5>\r\n            </div>\r\n            <div class=\"d-flex filter_drpdwn\">\r\n              <b-dropdown id=\"dropdown-right\" ref=\"filter_drpdwn\" right variant=\"default\" class=\"p-0\">\r\n                <template #button-content>\r\n                  Filter <sup v-if=\"filter_count>0\"><span class=\"badge badge-danger pt-1\">{{filter_count}}</span></sup>\r\n                </template>\r\n                <b-dropdown-header id=\"dropdown-header-label\">\r\n                  <h6 class=\"cursor-pointer my-2\" v-b-toggle.collapse-custom>FPO</h6>\r\n                  <b-collapse id=\"collapse-custom\" class=\"mt-2\">\r\n                    <div class=\"d-flex flex-column\">\r\n                      <b-form > \r\n                          <b-form-group      \r\n                            id=\"input-group-1\"\r\n                            label=\"\"\r\n                            label-for=\"input-1\"\r\n                            class=\"\">\r\n                            <b-form-select v-model=\"fpo_id\" name=\"fpo\">\r\n                              <option v-for=\"type in types\" :value=\"type.id\">{{type.name}}</option>\r\n                            </b-form-select>\r\n                          </b-form-group>\r\n                      </b-form>\r\n                    </div>\r\n                  </b-collapse>\r\n                  <h6 class=\"cursor-pointer my-2\" v-b-toggle.collapse-custom2>ICS</h6>\r\n                  <b-collapse id=\"collapse-custom2\" class=\"mt-2\">\r\n                    <div class=\"d-flex flex-column\">\r\n                      <b-form > \r\n                          <b-form-group      \r\n                            id=\"input-group-2\"\r\n                            label=\"\"\r\n                            label-for=\"input-2\"\r\n                            class=\"\">\r\n                            <b-form-select name=\"ics\" v-model=\"ics_id\" @change=\"changeICS()\">\r\n                              <option v-for=\"ics in all_ics\" :value=\"ics.id\" v-if=\"ics.type==fpo_id\">{{ics.name}}</option>\r\n                            </b-form-select>\r\n                          </b-form-group>\r\n                      </b-form>\r\n                    </div>\r\n                  </b-collapse>\r\n                  <h6 class=\"cursor-pointer my-2\" v-b-toggle.collapse-custom3>Panchayath</h6>\r\n                  <b-collapse id=\"collapse-custom3\" class=\"mt-2\">\r\n                    <div class=\"d-flex flex-column\">\r\n                      <b-form > \r\n                          <b-form-group      \r\n                            id=\"input-group-3\"\r\n                            label=\"\"\r\n                            label-for=\"input-3\"\r\n                            class=\"\">\r\n                            <b-form-select v-model=\"panchayath_id\" name=\"panchayath\">\r\n                              <option v-for=\"panchayath in all_panchayaths\" :value=\"panchayath.id\">{{panchayath.name}}</option>\r\n                            </b-form-select>\r\n                          </b-form-group>\r\n                      </b-form>\r\n                    </div>\r\n                  </b-collapse>\r\n                </b-dropdown-header>\r\n                <div class=\"my-2 border-top\"></div>\r\n                <div class=\"d-flex\">\r\n                  <b-button class=\"m-2 mt-0 btn-sm px-2 py-1\" variant=\"info\" @click=\"getVFC()\">\r\n                    Apply\r\n                  </b-button>\r\n                  <b-button class=\"my-2 mt-0 btn-sm px-2 py-1\" variant=\"ligth\" @click=\"clearFilter()\">\r\n                    Cancel\r\n                  </b-button>\r\n                </div>\r\n              </b-dropdown>\r\n              <router-link tag=\"a\" class to=\"/app/fpo/vfc/create\">\r\n                <b-button variant=\"success\" class=\"py-1 px-3 d-sm-block mx-1\"\r\n                >+ Create\r\n              </b-button></router-link>\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n\r\n    <div class=\"row main-scroll\">\r\n      <div class=\"col-md-2 p-4\" v-for=\"vfc in all_vfc\">\r\n        <router-link :to=\"'/app/fpo/vfc/edit/'+vfc.id\">\r\n          <div class=\"card p-1 row\">\r\n            <div class=\"col-md-12 pt-3 text-center\">\r\n              <img src=\"/img/vfc.png\" style=\"width:60%\">\r\n            </div>\r\n            <div class=\"col-md-12 py-3 align-self-center text-center\">\r\n              <b>{{vfc.name}}</b>\r\n              <p class=\"m-0\">Farmer Count: {{vfc.farmers_count}}</p>\r\n            </div>\r\n          </div>\r\n        </router-link>\r\n      </div>\r\n      <div class=\"col-md-12 text-center \" v-if=\"all_vfc.length<=0 && is_start\">\r\n        <p>No data found!</p>\r\n      </div>\r\n      <div class=\"col-md-12 text-center \" v-if=\"all_vfc.length<=0 && !is_start\">\r\n        <p class=\"p-5 m-5\">\r\n          <div class=\"table_loading\"></div>\r\n        </p>\r\n      </div>\r\n      \r\n    </div>\r\n\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - VFC\"\r\n  },\r\n  data() {\r\n    return {\r\n      all_vfc:[],\r\n      vfc_id:null,\r\n      all_ics:[],\r\n      ics_id:null,\r\n      types:[],\r\n      fpo_id:null,\r\n      all_panchayaths:[],\r\n      panchayath_id:null,\r\n      filter_count:0,\r\n      is_start:false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'fpo'\r\n    this.getVFC()\r\n    this.getTypes()\r\n    this.getIcs()\r\n  },\r\n  methods: {\r\n    getVFC() {\r\n      this.all_vfc = []\r\n      this.is_start = false\r\n      this.filter_count=0\r\n      var headers = new Headers();\r\n      var param = '?list_vfc=true'\r\n      if(this.fpo_id){\r\n        param += '&fpo='+this.fpo_id\r\n        this.filter_count++\r\n      }if(this.ics_id){\r\n        param += '&ics='+this.ics_id\r\n        this.filter_count++\r\n      }if(this.panchayath_id){\r\n        param += '&panchayath='+this.panchayath_id\r\n        this.filter_count++\r\n      }\r\n      this.$refs.filter_drpdwn.hide()\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/vfc/'+param, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.is_start = true\r\n        this.all_vfc = jsonData\r\n      })\r\n    },\r\n    getTypes() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/types/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.types = jsonData\r\n      })\r\n    },\r\n    getIcs() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/ics/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.all_ics = jsonData\r\n      })\r\n    },\r\n    changeICS() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/filter_farmers_map/?data_type=ics_by_vfc_and_panchayaths&ics='+this.ics_id,{\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.all_panchayaths = jsonData.panchayaths\r\n      })\r\n    },\r\n    clearFilter(){\r\n      this.fpo_id = null\r\n      this.vfc_id = null\r\n      this.panchayath_id = null\r\n      this.getVFC()\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n  .filter_drpdwn{\r\n    height:30px;\r\n  }\r\n  .filter_drpdwn button{\r\n    padding:0 15px;\r\n  }\r\n  .filter_drpdwn .dropdown-menu.dropdown-menu-right.show{\r\n    width:250px!important;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/fpo"}]}