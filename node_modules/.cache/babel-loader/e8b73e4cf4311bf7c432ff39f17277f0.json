ss to=\"/app/oodo/import\"><b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Import\r\n              </b-button></router-link> -->\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: false,\r\n          placeholder: 'Search'\r\n        }\"\r\n        @on-page-change=\"onPageChange\"\r\n        :pagination-options=\"{\r\n          enabled: false,\r\n          mode: 'records',\r\n          perPage: 20,\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows.results\"\r\n      >\r\n      \r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <router-link :to=\"'/app/fpo/inspection/view/'+props.row.id\">\r\n              <b-button  variant=\"primary\" class=\" px-3 py-1  d-sm-block mx-1\">View</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'image'\" >\r\n            <div class=\"text-center\">\r\n              <img src=\"/img/inspections.png\" style=\"width:30px\"><br>\r\n              <small>{{props.row.id}}</small>\r\n            </div>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'name'\">\r\n            {{props.row.farmer_detail.name}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'farmer_code'\" class=\"badge badge-success\">\r\n            {{props.row.farmer_detail.farmer_code}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'land'\">\r\n            <span class=\"badge badge-success p-1\" v-if=\"props.row.lands.id\">Yes</span>\r\n            <span class=\"badge badge-danger p-1\" v-else>No</span>\r\n          </span>\r\n          <span class=\"pl-3\" v-else-if=\"props.column.field == 'plant'\">\r\n            <span class=\"badge badge-success p-1\" v-if=\"props.row.plants.id\">Yes</span>\r\n            <span class=\"badge badge-danger p-1\" v-else>No</span>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'qna'\">\r\n            <span class=\"badge badge-success p-1\" v-if=\"props.row.q_and_answers>0\">Yes</span>\r\n            <span class=\"badge badge-danger p-1\" v-else>No</span>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'digital_sign'\">\r\n            <span class=\"badge badge-success p-1\" v-if=\"props.row.photo\">Yes</span>\r\n            <span class=\"badge badge-danger p-1\" v-else>No</span>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'aeo'\">\r\n            {{props.row.aeo_detail.name}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'year'\">\r\n            <b>{{props.row.year}}</b>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'created_on'\">\r\n            {{dateFormate(props.row.created_on)}}\r\n          </span>\r\n        </template>\r\n        <div slot=\"emptystate\" class=\"text-center\">\r\n          <div class=\"table_loading\" v-if=\"load_first\"></div>\r\n          <div v-else>No records found</div>\r\n        </div>\r\n\r\n        \r\n      </vue-good-table>\r\n      <TablePagination :page=\"page\" :rows=\"rows\" :load_first=\"load_first\" @changePage=\"changePage\" v-if=\"rows.count>0\"></TablePagination>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TablePagination from '/src/views/app/oodo/components/table_pagination'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"Inspections\"\r\n  },\r\n  components:{\r\n    TablePagination\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"InsID\",\r\n          field: \"image\"\r\n        },\r\n        {\r\n          label: \"Name\",\r\n          field: \"name\"\r\n        },\r\n        {\r\n          label: \"Farmer Code\",\r\n          field: \"farmer_code\"\r\n        },\r\n        {\r\n          label: \"AEO\",\r\n          field: \"aeo\"\r\n        },\r\n        {\r\n          label: \"Land\",\r\n          field: \"land\"\r\n        },\r\n        {\r\n          label: \"Plant&Crops\",\r\n          field: \"plant\"\r\n        },\r\n        {\r\n          label: \"Q/A\",\r\n          field: \"qna\"\r\n        },\r\n        {\r\n          label: \"Digital Sign\",\r\n          field: \"digital_sign\"\r\n        },\r\n        {\r\n          label: \"Year\",\r\n          field: \"year\"\r\n        },\r\n        {\r\n          label: \"Date\",\r\n          field: \"created_on\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: {results:[],count:0},\r\n      page:0,\r\n      load_first:true,\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'fpo'\r\n    this.changePage(1)\r\n  },\r\n  methods: {\r\n    changePage(count){\r\n      this.page = count\r\n      this.load_first = true\r\n      this.getInspections()\r\n    },\r\n    getInspections() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/inspection/?page='+this.page, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.detail=='Invalid page.'){\r\n          this.rows = {results:[],count:0}\r\n        }else{\r\n          this.load_first = false\r\n          this.rows = jsonData\r\n        }\r\n      })\r\n    },\r\n    onPageChange(rows){\r\n      console.log(rows)\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/fpo"}]}