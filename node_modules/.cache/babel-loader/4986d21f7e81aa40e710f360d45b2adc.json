,MAAA,QAAA,CAAA,MAAA,CAAA,aAAA,EAAA,MAAA;AACA,UAAA,OAAA,GAAA,IAAA,OAAA,EAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,eAAA,EAAA,WAAA,KAAA,KAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,KAAA,OAAA,GAAA,oBAAA,GAAA,KAAA,EAAA,GAAA,GAAA,EAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,IAAA,EAAA,QAFA;AAGA,QAAA,OAAA,EAAA;AAHA,OAAA,CAAA,CAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,EAAA;AACA,OAPA,EAQA,IARA,CAQA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,EAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,4BAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,kBAAA;AACA;AACA,OAfA;AAgBA;AAxFA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"main-content create-main\">\r\n    <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n      <div>\r\n         <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Aeo Updations</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\" v-if=\"form_data.form_status=='Pending'\"\r\n                  >Approved\r\n                </b-button>\r\n                <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  @click=\"updateFormdata('Declined')\" v-if=\"form_data.form_status=='Pending'\">Declined\r\n                </b-button>\r\n                <router-link to=\"/app/fpo/aeo-updations\">\r\n                  <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Back\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n          <h5></h5>\r\n        </b-col>\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mb-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-4\">\r\n                        <div class=\" col-md-6\" v-for=\"field in total_fields\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">{{slugToWord(Object.keys(form_data.fields)[field])}} : </span>\r\n                              <input :name=\"Object.keys(form_data.fields)[field]\" type=\"text\" class=\"ui-autocomplete-input\" :value=\"form_data.fields[Object.keys(form_data.fields)[field]]\" readonly>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                        <div class=\"col-md-12\">\r\n                          <input type=\"hidden\" name=\"form_type\" :value=\"form_data.form_type\">\r\n                          <input type=\"hidden\" name=\"action\" :value=\"form_data.action\">\r\n                          <input type=\"hidden\" name=\"aeo\" :value=\"form_data.aeo\">\r\n                        </div>\r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{form_status:'Pending'},\r\n      id:null,\r\n      action:'create',\r\n      type:'farmer',\r\n      total_fields:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'fpo'\r\n    this.action = this.$route.params.action\r\n    this.type = this.$route.params.type\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/aeo_updation/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        var total = Object.keys(jsonData.fields).length\r\n        for(var i=0;i<total;i=i+1){\r\n          this.total_fields.push(i)\r\n        }\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.form_data.action == 'Create'){\r\n        this.createFarmer()\r\n      }else{\r\n        this.updateFarmer()\r\n      }\r\n    },\r\n    createFarmer() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmers/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.updateFormdata('Approved')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFarmer() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      var farmer_id = this.form_data.fields.farmer_id\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmers/'+farmer_id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.updateFormdata('Approved')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata(status) {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('form_status',status)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/aeo_updation/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert(status)\r\n          this.$router.push('/app/fpo/aeo-updations/'+this.type)\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/fpo"}]}