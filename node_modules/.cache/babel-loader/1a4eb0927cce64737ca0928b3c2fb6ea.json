scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <router-link :to=\"'/app/product/product-price/edit/'+props.row.id\">\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">Edit</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'category_detail'\">\r\n            {{props.row.category_detail.name}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'product_type_detail'\">\r\n            {{props.row.product_type_detail.name}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'qc_auto_approval'\">\r\n            <span class=\"badge badge-success\" v-if=\"props.row.qc_auto_approval\">YES</span>\r\n            <span class=\"badge badge-danger\" v-else>NO</span>\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Product\",\r\n          field: \"name\"\r\n        },\r\n        {\r\n          label: \"Department\",\r\n          field: \"category_detail\"\r\n        },\r\n        {\r\n          label: \"Type\",\r\n          field: \"product_type_detail\"\r\n        },\r\n        {\r\n          label: \"Price\",\r\n          field: \"price\"\r\n        },\r\n        {\r\n          label: \"Constant\",\r\n          field: \"constant\"\r\n        },\r\n        {\r\n          label: \"QC Auto Approval\",\r\n          field: \"qc_auto_approval\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: [\r\n       \r\n      ]\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'product'\r\n    this.getProcurements()\r\n  },\r\n  methods: {\r\n    getProcurements() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/products/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/product"}]}