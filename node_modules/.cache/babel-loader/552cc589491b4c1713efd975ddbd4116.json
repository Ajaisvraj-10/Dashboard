content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Break</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <b-button variant=\"danger\" type=\"button\" @click=\"closeBreakPopup()\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Discard\r\n                </b-button>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n         <b-col lg=\"12\" xl=\"12\" class=\"my-2\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"o_thread_search_input mt-3\">\r\n                  <span style=\"width:90px;\"> Break Type: </span>\r\n                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"break_type\" v-model=\"form_data.break_type\">\r\n                    <option v-for=\"break_type in break_types\" :value=\"break_type\"> {{break_type}}</option>\r\n                  </select>\r\n                </div>\r\n                <div v-if=\"form_data.break_type=='Break Down'\">\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Machine: </span>\r\n                    <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"machine\" v-model=\"form_data.machine\">\r\n                      <option v-for=\"machine in machines\" :value=\"machine.id\"> {{machine.name}}</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Motors: </span>\r\n                    <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"machine_mototrs\" v-model=\"form_data.machine_mototrs\" multiple>\r\n                      <option v-for=\"machine_mototr in machine_mototrs\" :value=\"machine_mototr.id\" v-if=\"machine_mototr.machine==form_data.machine\"> {{machine_mototr.name}}</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Break Time: </span>\r\n                    <button class=\"btn btn-white append_btn_left\" type=\"button\">Date</button>\r\n                    <input placeholder=\"00:01\" type=\"date\" class=\"ui-autocomplete-input append_btn_input_left\" autocomplete=\"off\" v-model=\"form_data.start_date\" name=\"start_date\" style=\"margin-right:10px\">\r\n                    <button class=\"btn btn-white append_btn_left\" type=\"button\">Time</button>\r\n                    <input placeholder=\"00:10\" type=\"time\" class=\"ui-autocomplete-input append_btn_input_left\" autocomplete=\"off\" v-model=\"form_data.start_time\" name=\"start_time\">\r\n                  </div>\r\n                </div>\r\n                <div v-else>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Break : </span>\r\n                    <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"break_time\" v-model=\"form_data.break_time\">\r\n                      <option v-for=\"break_time in break_times\" :value=\"break_time\"> {{break_time}}</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"user-info pt-2\">\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:90px;\"> Date : </span>\r\n                      <input placeholder=\"Date\" type=\"date\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.start_date\" name=\"start_date\" @input=\"form_data.end_date=form_data.start_date\" readonly>\r\n                      <input placeholder=\"Date\" type=\"date\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.end_date\" name=\"end_date\" readonly>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Break Time: </span>\r\n                    <button class=\"btn btn-white append_btn_left\" type=\"button\">Start</button>\r\n                    <input placeholder=\"00:01\" type=\"time\" class=\"ui-autocomplete-input append_btn_input_left\" autocomplete=\"off\" v-model=\"form_data.start_time\" name=\"start_time\" style=\"margin-right:10px\">\r\n                    <button class=\"btn btn-white append_btn_left\" type=\"button\">End</button>\r\n                    <input placeholder=\"00:10\" type=\"time\" class=\"ui-autocomplete-input append_btn_input_left\" autocomplete=\"off\" v-model=\"form_data.end_time\" name=\"end_time\">\r\n                  </div>\r\n                </div>\r\n                <div class=\"o_thread_search_input mt-3\">\r\n                  <span style=\"width:90px;\"> Reason: </span>\r\n                  <textarea class=\"ui-autocomplete-input w-100 mx-1\" name=\"reason\" v-model=\"form_data.reason\" placeholder=\"Write here\" rows=\"4\"></textarea>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  props: ['process_detail_id','processing_id'],\r\n  components:{\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{start_date:null,batch:null,process:null,break_type:'Break Down',machine_mototrs:[]},\r\n      process:{\r\n        id:null,\r\n        date:null,\r\n        product:null,\r\n        status:'Draft',\r\n        shift:'DAY',\r\n        workers:[],\r\n        work_type:'wages',\r\n        is_over_time:false,\r\n        machines:[],\r\n        start_process_detail:{process_products_list:[],main_process_detail:{}},\r\n        other_lots:[],\r\n        lot:null,\r\n        process_outputs:[],\r\n        barrel_detail:{},\r\n        next_process:null,\r\n      },\r\n      process_detail:{outputs:[],consumable_items_list:[],inputs:[]},\r\n      id:null,\r\n      action:'create',\r\n      machines:[],\r\n      machine_mototrs:[],\r\n      break_types:['Break Down','Break Time'],\r\n      break_times:['Lunch Break','Morning Break','Tea Break','Meeting','Others'],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.form_data.start_date = this.currentDate()\r\n    this.form_data.end_date = this.currentDate()\r\n    this.getProcess()\r\n    // this.getMachines()\r\n    this.getMotors()\r\n    // if(this.$route.query.process){\r\n    //   this.id = this.$route.query.process\r\n    //   this.action = 'edit'\r\n    // }\r\n  },\r\n  methods: {\r\n    getProcess() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/processing/'+this.processing_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.process = jsonData\r\n        this.getProcessDetails()\r\n      })\r\n    },\r\n    getProcessDetails() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/process_details/'+this.process_detail_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.process_detail = jsonData\r\n        this.machines = jsonData.machines\r\n      })\r\n    },\r\n    getMachines() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/machines/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.machines = jsonData\r\n      })\r\n    },\r\n    getMotors() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/machine_mototrs/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.machine_mototrs = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      if(this.$root.user_type!='supervisor'){\r\n        this.showAlert('You cannot add!')\r\n        return false\r\n      }\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('supervisor',this.employeeId)\r\n      formdata.append('process',this.processing_id)\r\n      if(this.form_data.break_type=='Break Down'){\r\n        formdata.append('status','Maintenance')\r\n      }else{\r\n        formdata.append('status','Break')\r\n      }\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/break_down/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n 