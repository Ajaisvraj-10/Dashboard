;AAFA,OAAA,CAAA,CAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,EAAA;AACA,OANA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,QAAA;AACA,OATA;AAUA,KA3LA;AA4LA,IAAA,UA5LA,wBA4LA;AAAA;;AACA,UAAA,OAAA,GAAA,IAAA,OAAA,EAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,eAAA,EAAA,WAAA,KAAA,KAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,KAAA,OAAA,GAAA,oBAAA,EAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAAA,CAAA,CAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,EAAA;AACA,OANA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,QAAA;AACA,OATA;AAUA;AAzMA;AA7BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"main-content main-content2\">\r\n    <div>\r\n      <div class=\"d-flex mb-2 align-items-center justify-content-between\">\r\n        <slot name=\"header\">\r\n           <h5 class=\"mb-0\">Farm <span class=\"text-success\">/{{farm_data.name}} </span></h5>\r\n              \r\n        </slot>\r\n        <div class=\"d-flex\">\r\n          <router-link :to=\"'/app/procurement/farm-expenses/view/'+farm_data.id\">\r\n            <b-button variant=\"primary\" class=\" px-3 py-1  d-sm-block mx-1\">\r\n              View Expenses\r\n            </b-button>\r\n          </router-link>\r\n          <b-button variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"createPlant()\">\r\n            + Add Plant\r\n          </b-button>\r\n          <router-link to=\"/app/procurement/farms\">\r\n            <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">\r\n              Discard\r\n            </b-button>\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n      <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <div class=\"\">\r\n      <div class=\"row mx-0\">\r\n        <div class=\"col-md-9 p-1\">\r\n          <div class=\"row mx-0\" style=\"height:70vh;overflow-y:auto\">\r\n            <div class=\"col-md-3 p-1\" v-for=\"plant in farm_data.plants\">\r\n              <div class=\"card\" style=\"min-height:140px\">\r\n                <div class=\"mb-1\">\r\n                  <div class=\"pt-3 p-2 text-center\">\r\n                    <div>\r\n                      <h5>{{plant.plant_details.name}}</h5>\r\n                    </div>\r\n                    <span>{{plant.land_unit=='Number'?'Number of Plants':'Plant Area'}}</span>\r\n                    <h4 class=\"\">{{plant.number}}<small>{{plant.land_unit!='Number'?plant.land_unit:''}}</small></h4>\r\n                    <div>\r\n                      <b-button type=\"button\" variant=\"danger\" class=\" px-1 py-0\" style=\"font-size:12px\" @click=\"addExpense(plant.id)\">Add Expense</b-button>\r\n                    </div>\r\n                    <!-- <tr v-for=\"product in plant.products\">\r\n                      <th >{{product.product_type}} Quantity</th>\r\n                      <th>:</th>\r\n                      <th class=\"text-success\">{{product.quantity}}<small>KG</small></th>\r\n                    </tr> -->\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-md-3 p-1\">\r\n          <div class=\"card\">\r\n            <div class=\"card p-2 m-0 flex-row border\">\r\n              <div class=\"card col-md-2 align-self-center p-1\">\r\n                <img src=\"/img/website.png\" style=\"vertical-align:top\">\r\n              </div>\r\n              <div class=\"col-md-5 align-self-center pr-0\">\r\n                <b>Expenses</b>\r\n                <div>\r\n                  <b-button type=\"button\" variant=\"danger\" class=\" px-1 py-0\" style=\"font-size:12px\" @click=\"addExpense()\">Add Expense</b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-md-5 text-center px-0\">\r\n                Total Expenses\r\n                <div>\r\n                   <b class=\"h5 text-danger\">Rs{{totalExpenses()}}</b>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"process_kanban\">\r\n              <div class=\"m-1\">\r\n                <span class=\"text-info\">Today</span>\r\n                <div class=\"card px-3 py-2 mb-1\" v-for=\"(expense,key) in expenses\">\r\n                  <table class=\"w-100\">\r\n                    <tr>\r\n                      <th class=\"text-success\"><img src=\"/img/financial.png\" style=\"width:15px\"> {{expense.title}}</th>\r\n                    </tr>\r\n                    <tr v-if=\"expense.plant_name\">\r\n                      <th >Plant: <b class=\"text-info\">{{expense.plant_name}}</b></th>\r\n                    </tr>\r\n                    <tr>\r\n                      <th >Amount: <b class=\"text-danger\">Rs{{expense.price}}</b></th>\r\n                    </tr>\r\n                    <tr>\r\n                      <th><b>Remark</b></th>\r\n                    </tr>\r\n                    <tr>\r\n                      <td>\r\n                        <div style=\"font-size:14px\" v-html=\"expense.remark\"></div>\r\n                      </td>\r\n                    </tr>\r\n                  </table>\r\n                </div>\r\n                <hr class=\"my-2\">\r\n              </div>\r\n              <div class=\"text-center\"> </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div>\r\n      <b-modal id=\"create-plant\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n       <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n          <slot name=\"header\">\r\n            <h5 class=\"mb-0\"><span class=\"text-success\">Plant</span> /{{plant_data.id?'Edit':'Add'}}</h5>\r\n          </slot>\r\n          <div class=\"d-flex\">\r\n            <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"dataForm()\">Save</b-button>\r\n            <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('create-plant')\">Close</b-button>\r\n          </div>\r\n        </div>\r\n        <div class=\" my-3 border-top\"></div>\r\n        <form class=\"start-product-body\" ref=\"dataForm\" @submit.prevent=\"dataForm()\" >\r\n          <fieldset style=\"min-height:80vh;overflow-y:auto\">\r\n            <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-12\">\r\n                  <div class=\"user-info pt-2 mb-5\">\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:150px;\"> Select Plant: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 ml-1\" v-model=\"product_plant\" @change=\"plant_data.product_plant=product_plant.id\">\r\n                        <optgroup v-for=\"category in plant_categories\" :label=\"category.name\" v-if=\"category.plants.length\">\r\n                          <option v-for=\"plant in category.plants\" :value=\"plant\"> {{plant.name}} {{plant.plant_unit}}</option>\r\n                        </optgroup>\r\n                      </select>\r\n                      <input type=\"hidden\" name=\"product_plant\" v-model=\"plant_data.product_plant\">\r\n                    </div>\r\n                    <div class=\"\" v-for=\"category in plant_categories\" v-if=\"product_plant.category==category.id\">\r\n                      <div class=\"px-3\" v-for=\"(plant,key) in category.plants\" v-if=\"product_plant.id==plant.id\">\r\n                        <div class=\"o_thread_search_input\">\r\n                          <div class=\"\" v-if=\"plant.plant_unit=='Area'\" style=\"width:250px;\">\r\n                            <span>{{plant.name}} {{plant.plant_unit}}: </span>\r\n                            <div class=\"pl-0\" style=\"zoom:0.7\">\r\n                              <b-form-group class=\"yes_or_no_btn\">\r\n                                <b-form-radio-group\r\n                                  v-model=\"plant.land_unit\"\r\n                                  :options=\"['Hectare','Acre']\"\r\n                                  button-variant=\"outline-secondary\"\r\n                                  :name=\"'land_unit'\"\r\n                                  buttons\r\n                                ></b-form-radio-group>\r\n                              </b-form-group>\r\n                            </div>\r\n                          </div>\r\n                          <div v-else class=\"\" style=\"width:250px;\">\r\n                            <span>{{plant.name}} {{plant.plant_unit}}: </span>\r\n                            <input type=\"hidden\" :name=\"'land_unit'\" value=\"Number\">\r\n                          </div>\r\n                          <input :placeholder=\"plant.plant_unit=='Area'?'Enter area':'Enter a number'\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"plant.number\" :name=\"'number'\"> \r\n                        </div>\r\n                        <div class=\"o_thread_search_input\" v-for=\"(product,key2) in plant.products\" v-show=\"false\">\r\n                          <span style=\"width:190px;\">{{plant.name}} {{product.product_type_detail.name}} Quantity: </span>\r\n                          <input placeholder=\"Enter Quantity\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input append_btn_input_right\" v-model=\"plant['quantity'+product.product_type]\" autocomplete=\"off\" :name=\"'quantity'+product.product_type\">\r\n                          <button class=\"btn btn-white append_btn_right\" type=\"button\">KG</button>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <input type=\"hidden\" name=\"farm\" v-model=\"farm_data.id\">\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n          </fieldset>\r\n        </form>\r\n      </b-modal>\r\n      <b-modal id=\"add-expense\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n       <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n          <slot name=\"header\">\r\n            <h5 class=\"mb-0\"><span class=\"text-success\">Expense</span> /{{plant_data.id?'Edit':'Add'}}</h5>\r\n          </slot>\r\n          <div class=\"d-flex\">\r\n            <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"expenseFormData()\">Save</b-button>\r\n            <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('add-expense')\">Close</b-button>\r\n          </div>\r\n        </div>\r\n        <div class=\" my-3 border-top\"></div>\r\n        <form class=\"start-product-body\" ref=\"expenseFormData\" @submit.prevent=\"expenseFormData()\" >\r\n          <fieldset style=\"min-height:80vh;overflow-y:auto\">\r\n            <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n              <div class=\"row\">\r\n                <div :class=\"view_expense_title_form?'col-md-7':'col-md-10'\">\r\n                  <div class=\"user-info pt-2 mb-5\">\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\"> Select Expense Title: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 ml-1\" name=\"farm_expense_title\" v-model=\"expense_data.farm_expense_title\" @change=\"expense_data.farm_expense_title==0?view_expense_title_form=true:view_expense_title_form=false\">\r\n                        <option v-for=\"expense_title in expense_titles\" :value=\"expense_title.id\"> {{expense_title.name}}</option>\r\n                        <option value=\"0\" style=\"color:green\">Create New Title</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\" v-if=\"expense_data.farm_plant\">\r\n                      <span style=\"width:120px;\"> Select Plant: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 ml-1\" name=\"farm_plant\" v-model=\"expense_data.farm_plant\">\r\n                        <option v-for=\"plant in farm_data.plants\" :value=\"plant.id\"> {{plant.plant_details.name}}</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Amount: </span>\r\n                      <input placeholder=\"Enter Price\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input append_btn_input_right\" v-model=\"expense_data.price\" autocomplete=\"off\" :name=\"'price'\">\r\n                      <button class=\"btn btn-white append_btn_right\" type=\"button\">RS</button>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\"> Remark: </span>\r\n                      <textarea placeholder=\"Remark\" class=\"ui-autocomplete-input w-100 ml-1\" name=\"remark\" v-model=\"expense_data.remark\" style=\"height:200px\"></textarea>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <input type=\"hidden\" name=\"farm\" v-model=\"farm_data.id\">\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-md-5 border-left\" v-if=\"view_expense_title_form\">\r\n                  <div class=\"user-info pt-2 mb-5\">\r\n                    <b>Create New Title</b>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:90px;\"> Expense Title: </span>\r\n                      <input placeholder=\"Enter Title\" class=\"ui-autocomplete-input w-100\" name=\"name\" v-model=\"new_expense_title.name\" />\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:90px;\"> Select Type: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 ml-1\" name=\"title_type\" v-model=\"new_expense_title.title_type\">\r\n                        <option v-for=\"title_type in ['Normal']\" :value=\"title_type\"> {{title_type}}</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"text-right px-2 mt-2\">\r\n                      <input type=\"hidden\" name=\"operation\" value=\"x\">\r\n                      <b-button variant=\"success\" class=\"px-2 py-1 w-25\" :disabled=\"!new_expense_title.name || !new_expense_title.title_type\" @click=\"createTitle()\">Create</b-button>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n          </fieldset>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      farm_data:{id:null, plants:[]},\r\n      product_plant:{},\r\n      plant_data:{id:null},\r\n      expense_data:{id:null, plant:null},\r\n      expense_titles:[],\r\n      new_expense_title:{name:'',title_type:'Normal'},\r\n      view_expense_title_form:false,\r\n      action:'create',\r\n      workers:[],\r\n      plant_categories:[],\r\n      expenses:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'procurement'\r\n    this.farm_data.id = this.$route.params.id\r\n    this.getForm()\r\n    this.getWorkers()\r\n    this.getPlants()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/farms/'+this.farm_data.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farm_data = jsonData\r\n        this.getExpenses()\r\n      })\r\n    },\r\n    createPlant(plant={id:null,product_plant:null}){\r\n      this.plant_data = plant\r\n      this.$bvModal.show('create-plant')\r\n    },\r\n    dataForm() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('user',this.userId)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/farm_plants/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.plant_data = jsonData\r\n          this.addPlantProducs()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    addPlantProducs() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('farm_plant',this.plant_data.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/farm_plant_producs/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.plant_data = jsonData\r\n          this.showAlert('Saved Successfully!')\r\n          this.$bvModal.hide('create-plant')\r\n          this.getForm()\r\n        }else{\r\n          this.showAlert('Something Wrong!','red')\r\n        }\r\n      })\r\n    },\r\n    getPlants() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/plant_categories/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var self = this\r\n        jsonData.forEach(function(category,key1){\r\n          category.plants.forEach(function(plant,key2){\r\n            jsonData[key1].plants[key2]['number'] = ''\r\n            plant.products.forEach(function(product,key3){\r\n              jsonData[key1].plants[key2]['quantity'+product.product_type] = 0\r\n              jsonData[key1].plants[key2]['land_unit'] = 'Number'\r\n              if(plant.plant_unit=='Area'){\r\n                jsonData[key1].plants[key2]['land_unit'] = 'Hectare'\r\n              }\r\n            })\r\n          })\r\n        })\r\n        this.plant_categories = jsonData\r\n      })\r\n    },\r\n    totalExpenses(){\r\n      var total=0\r\n      this.expenses.forEach(function(ex){\r\n        total+=ex.price\r\n      })\r\n      return total\r\n    },\r\n    addExpense(plant_id=null, expense={id:null}){\r\n      this.expense_data = expense\r\n      if(!expense.id && plant_id){\r\n        this.expense_data.farm_plant = plant_id\r\n      }\r\n      this.getTitles()\r\n      this.$bvModal.show('add-expense')\r\n    },\r\n    expenseFormData() {\r\n      var formdata = new FormData(this.$refs['expenseFormData'])\r\n      formdata.append('user',this.userId)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/farm_expenses/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.expense_data = jsonData\r\n          this.showAlert('Saved Successfully!')\r\n          this.$bvModal.hide('add-expense')\r\n          this.getExpenses()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    createTitle() {\r\n      if(!this.new_expense_title.name || !this.new_expense_title.title_type){\r\n        this.showAlert('Please fill form data!','red')\r\n        return false\r\n      }\r\n      var formdata = new FormData()\r\n      formdata.append('name',this.new_expense_title.name)\r\n      formdata.append('title_type',this.new_expense_title.title_type)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/farm_expense_titles/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.getTitles()\r\n          this.expense_data.farm_expense_title = jsonData.id\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!','red')\r\n        }\r\n      })\r\n    },\r\n    getExpenses() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/farm_expenses/?today=yes&farm='+this.farm_data.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.expenses = jsonData\r\n      })\r\n    },\r\n    getTitles() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/farm_expense_titles/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.expense_titles = jsonData\r\n      })\r\n    },\r\n    getWorkers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.workers = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/procurement"}]}