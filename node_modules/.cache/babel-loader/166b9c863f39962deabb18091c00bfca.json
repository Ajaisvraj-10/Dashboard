,UAAA,OAAA,GAAA,IAAA,OAAA,EAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,eAAA,EAAA,WAAA,KAAA,KAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,KAAA,OAAA,GAAA,eAAA,EAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,IAAA,EAAA,QAFA;AAGA,QAAA,OAAA,EAAA;AAHA,OAAA,CAAA,CAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,EAAA;AACA,OAPA,EAQA,IARA,CAQA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA;;AACA,YAAA,QAAA,CAAA,EAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,EAAA,GAAA,QAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,QAAA;;AACA,UAAA,MAAA,CAAA,cAAA;AACA,SAJA,MAIA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,kBAAA;AACA;AACA,OAjBA;AAkBA,KAnIA;AAoIA,IAAA,cApIA,4BAoIA;AAAA;;AACA,UAAA,QAAA,GAAA,IAAA,QAAA,CAAA,KAAA,KAAA,CAAA,UAAA,CAAA,CAAA;AACA,UAAA,OAAA,GAAA,IAAA,OAAA,EAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,eAAA,EAAA,WAAA,KAAA,KAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,KAAA,OAAA,GAAA,eAAA,GAAA,KAAA,EAAA,GAAA,GAAA,EAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,IAAA,EAAA,QAFA;AAGA,QAAA,OAAA,EAAA;AAHA,OAAA,CAAA,CAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,EAAA;AACA,OAPA,EAQA,IARA,CAQA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,EAAA,GAAA,CAAA,EAAA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,uBAAA;AACA,SAFA,MAEA;AACA,UAAA,MAAA,CAAA,SAAA,CAAA,kBAAA;AACA;AACA,OAdA;AAeA,KAvJA;AAwJA,IAAA,OAxJA,qBAwJA;AAAA;;AACA,UAAA,OAAA,GAAA,IAAA,OAAA,EAAA;AACA,MAAA,OAAA,CAAA,MAAA,CAAA,eAAA,EAAA,WAAA,KAAA,KAAA,CAAA,KAAA;AACA,MAAA,KAAA,CAAA,KAAA,OAAA,GAAA,UAAA,EAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAAA,CAAA,CAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,eAAA,QAAA,CAAA,IAAA,EAAA;AACA,OANA,EAOA,IAPA,CAOA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,QAAA;AACA,OATA;AAUA;AArKA;AApCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"main-content create-main\">\r\n    <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n      <div>\r\n         <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n            <slot name=\"header\">\r\n               <h5 class=\"mb-0\"><span class=\"text-success\">Lab Test</span> / {{slugToWord(action)}}</h5>\r\n                  \r\n            </slot>\r\n            <div class=\"d-flex\">\r\n               <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Save\r\n              </b-button>\r\n              <b-button button=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"goBack()\">Discard\r\n              </b-button>\r\n            </div>\r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n\r\n      <b-row class=\"main-scroll pt-2\">\r\n        <b-col lg=\"5\" xl=\"5\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <h5>Lab Details</h5>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\" col-md-12\">\r\n\r\n                    <div class=\"user-info pt-2\">\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\"> Date: </span>\r\n                        <input placeholder=\"Date\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"dateFormate(form_data.created_on)\" readonly>\r\n                        <input type=\"hidden\" name=\"test_type\" v-model=\"form_data.test_type\">\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\" v-if=\"form_data.test_type=='sample'\">\r\n                        <span style=\"width:120px;\"> Sample Id: </span>\r\n                        <input placeholder=\"Sample\" type=\"text\" class=\"ui-autocomplete-input\" :value=\"'#'+form_data.sample_material_detail.sample_procurement_detail.id+'/Sample'\" readonly>\r\n                        <input type=\"hidden\" name=\"sample_material\" v-model=\"form_data.sample_material\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\" v-if=\"form_data.test_type=='lot'\">\r\n                        <span style=\"width:120px;\"> Lot Id: </span>\r\n                        <input placeholder=\"Lot\" type=\"text\" class=\"ui-autocomplete-input\" :value=\"'#'+form_data.lot_detail.id+'/Lot'\" readonly>\r\n                        <input type=\"hidden\" name=\"lot\" v-model=\"form_data.lot\" required>\r\n                      </div>\r\n\r\n                      <ProductFields :product_id=\"product_id\" :readonly=\"true\" v-if=\"load_products\"></ProductFields>\r\n\r\n                      <div class=\"o_thread_search_input mt-2\">\r\n                        <span style=\"width:130px;\"> Organic Status: </span>\r\n                        <div>\r\n                          <input type=\"text\" class=\"ui-autocomplete-input w-50\" autocomplete=\"off\" v-model=\"form_data.status\" name=\"status\" readonly>\r\n                          <button type=\"button\" class=\"btn ml-2 py-1\" :class=\"form_data.status=='Approved'?'btn-success':'btn-outline-success'\" @click=\"form_data.status='Approved'\">Pass</button>\r\n                          <button type=\"button\" class=\"btn ml-2 py-1\" :class=\"form_data.status=='Rejected'?'btn-danger':'btn-outline-danger'\" @click=\"form_data.status='Rejected'\">Fail</button>\r\n                        </div>\r\n                      </div>\r\n\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\">Lab Name: </span>\r\n                        <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.lab\" name=\"lab\">\r\n                            <option v-for=\"lab in labs\" :value=\"lab.id\">{{lab.name}}</option>\r\n                        </select>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\"> Invoice No: </span>\r\n                        <input placeholder=\"Invoice No\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.invoice_no\" name=\"invoice_no\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\"> Testing Charge(RS): </span>\r\n                        <input placeholder=\"Testing Charge\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.testing_charge\" name=\"testing_charge\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\">Billing Address: </span>\r\n                        <textarea class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.billing_address\" name=\"billing_address\">\r\n                        </textarea>\r\n                      </div>\r\n                      \r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        \r\n        <b-col lg=\"7\" xl=\"7\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll mb-3\">\r\n            <div>\r\n              <h5>Parameters</h5>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-12 px-2\">\r\n                    <table class=\"table\">\r\n                      <thead>\r\n                        <tr>\r\n                          <th>Parameter</th>\r\n                          <th>Certificate</th>\r\n                          <th>Invoice</th>\r\n                        </tr>\r\n                      </thead>\r\n                      <tbody v-if=\"action == 'edit'\">\r\n                        <tr v-for=\"(physical_parameter,key) in physical_parameters\" v-if=\"physical_parameter.is_certificate\">\r\n                          <td class=\"o_thread_search_input\">\r\n                            <b>{{physical_parameter.name}}</b>\r\n                            <input type=\"hidden\" :name=\"'lab_test_parameter_id'+physical_parameter.physical_parameter\" v-model=\"physical_parameter.physical_parameter\">\r\n                          </td>\r\n                          <td class=\"\">\r\n                            <b-form-file :name=\"'certificate'+physical_parameter.physical_parameter\" class=\"ui-autocomplete-input w-100\"></b-form-file>\r\n                          </td>\r\n                          <td class=\"\">\r\n                            <b-form-file :name=\"'invoice'+physical_parameter.physical_parameter\" class=\"ui-autocomplete-input w-100\"></b-form-file>\r\n                          </td>\r\n                        </tr>\r\n                      </tbody>\r\n                    </table>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n          <!-- <b-card  header-bg-variant=\"transparent\" class=\"card-scroll mb-3\">\r\n            <div>\r\n              <h5>References</h5>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-12 px-2\">\r\n                    <table class=\"table\">\r\n                      <thead>\r\n                        <tr>\r\n                          <th>#</th>\r\n                          <th>Reference Number</th>\r\n                          <th>Action</th>\r\n                        </tr>\r\n                      </thead>\r\n                      <tbody v-if=\"action == 'edit'\">\r\n                        <tr v-for=\"(line,key) in lines\">\r\n                          <td>\r\n                            {{line.id}}\r\n                          </td>\r\n                          <td>\r\n                            <input placeholder=\"\" type=\"text\" class=\"\" autocomplete=\"off\" v-model=\"line.reference_number\" :name=\"'reference_number'+key\" required>\r\n                          </td>\r\n                          <td>\r\n                            <b-button variant=\"danger\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\"@click=\"lineOperation(key)\">\r\n                              Remove\r\n                            </b-button></td>\r\n                        </tr>\r\n                        <tr>\r\n                          <td>\r\n                            <input type=\"hidden\" name=\"lineskeys\" v-model=\"lines.length\">\r\n                            <b-button variant=\"success\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\"@click=\"lineOperation('+')\">\r\n                              Add a line\r\n                            </b-button>\r\n                          </td>\r\n                          <td colspan=\"2\"></td>\r\n                        </tr>\r\n                      </tbody>\r\n                    </table>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card> -->\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{created_on:new Date(),sample_material:null,test_type:'sample',status:'Checking',sample_material_detail:{farmer_status:'Waiting', sample_procurement_detail:{}},\r\n        lot_detail:{id:null},\r\n        lot:null,\r\n      },\r\n      id:null,\r\n      load_products:false,\r\n      lines:[],\r\n      product_id:null,\r\n      action:'create',\r\n      labs:[],\r\n      physical_parameters:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.form_data.created_on = this.currentDate()\r\n    this.$root.current_section = 'qc'\r\n    this.form_data.test_type = this.$route.params.test_type\r\n    this.action = this.$route.params.action\r\n    this.getLabs()\r\n    if(this.form_data.test_type=='sample'){\r\n      this.form_data.sample_material = this.$route.params.id\r\n      this.checkSampleLabTest()\r\n    }else if(this.form_data.test_type=='lot'){\r\n      this.form_data.lot = this.$route.params.id\r\n      this.checkLotLabTest()\r\n    }\r\n  },\r\n  methods: {\r\n    checkSampleLabTest() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/check_sample_test/'+this.form_data.sample_material+'/', {\r\n          method : 'POST',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.id = jsonData.id\r\n          this.getForm()\r\n        }else{\r\n          this.selectSample()\r\n        }\r\n      })\r\n    },\r\n    checkLotLabTest() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/create_lots/'+this.form_data.lot+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.lot_detail = jsonData\r\n        if(jsonData.lab_test){\r\n          this.id = jsonData.lab_test\r\n          this.getForm()\r\n        }else{\r\n          this.product_id = this.form_data.lot_detail.product\r\n          this.load_products = true\r\n          this.getPhysicalParameters()\r\n        }\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/lab_test/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.product_id = jsonData.sample_material_detail.sample_procurement_detail.product\r\n        this.load_products = true\r\n        this.physical_parameters = jsonData.lab_test_parameters_detail\r\n      })\r\n    },\r\n    selectSample() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_materials_test/'+this.form_data.sample_material+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.sample_material_detail = jsonData\r\n        this.product_id = jsonData.sample_procurement_detail.product\r\n        this.load_products = true\r\n        this.getPhysicalParameters()\r\n      })\r\n    },\r\n    getPhysicalParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/physical_parameters/?parameter_for=lab&product='+this.product_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.physical_parameters = jsonData\r\n      })\r\n    },\r\n    lineOperation(operation){\r\n      if(operation=='+'){\r\n        var id = this.lines.length+1\r\n        this.lines.push({'id':id})\r\n      }else{\r\n        this.lines.splice(operation, 1)\r\n      }\r\n    },\r\n    dataForm() {\r\n      if(this.form_data.id){\r\n        this.updateFormdata()\r\n      }else{\r\n        this.createFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/lab_test/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.id = jsonData.id\r\n          this.form_data = jsonData\r\n          this.updateFormdata()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/lab_test/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getLabs() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/lab/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.labs = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/qc"}]}