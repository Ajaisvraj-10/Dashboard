\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.is_start = true\r\n        this.site_header = jsonData\r\n      })\r\n    },\r\n  }\r\n\r\n};\r\n</script>\r\n<style scoped>\r\n  @import '/vid_theme/style/style.css';\r\n  @import '/vid_theme/style/style-2.css';\r\n  @import '/vid_theme/style/load-style.css';\r\n\r\n\r\n\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/app/vid_theme"}]}