"dataForm\" @submit.prevent=\"dataForm()\">\r\n      <div>\r\n         <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Packing Material Arraival</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/production/packing-materials\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n        </b-col>\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mb-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-6\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Bill Number : </span>\r\n                              <input placeholder=\"Bill Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"bill_number\" v-model=\"form_data.bill_number\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">WB Slip Number : </span>\r\n                              <input placeholder=\"WB Slip Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"Wb_slip\" v-model=\"form_data.Wb_slip\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\" v-show=\"form_data.id\">\r\n                              <span style=\"width:120px;\"> Initial Wb (Kg): </span>\r\n                              <input placeholder=\"Initial Wb\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.initial_Wb\" name=\"initial_Wb\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\" v-show=\"form_data.id\">\r\n                              <span style=\"width:120px;\"> Net Weight Wb (Kg): </span>\r\n                              <input placeholder=\"Initial Wb\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.net_weight_WB\" name=\"net_weight_WB\" @input=\"form_data.final_Wb=form_data.initial_Wb-form_data.net_weight_WB\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\" v-if=\"form_data.id\">\r\n                              <span style=\"width:120px;\">Final Wb (Kg): </span>\r\n                              <input placeholder=\"Final Wb\" type=\"number\" min=\"0\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.final_Wb\" name=\"final_Wb\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Remark : </span>\r\n                              <textarea placeholder=\"Remark\" rows=\"5\" class=\"ui-autocomplete-input w-100 mx-1\" autocomplete=\"off\" v-model=\"form_data.remark\" name=\"remark\"></textarea>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <!-- <span style=\"width:120px;\">Status : </span> -->\r\n                              <input placeholder=\"Status\" type=\"hidden\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"status\" v-model=\"form_data.status\" readonly>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"col-md-5 border-left pt-1\" style=\"overflow-y:auto;height:72vh\">\r\n                          <b-card  header-bg-variant=\"transparent\" class=\"mb-2\" v-for=\"material in form_data.values\">\r\n                            <div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Material : </span>\r\n                                <input placeholder=\"Material\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"material.material_detail.name\" readonly>\r\n                              </div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px\">Specifications:</span>\r\n                                <table class=\"pt-2\">\r\n                                  <tr v-for=\"specification in material.specifications\" v-if=\"specification.material_ids.includes(material.material)\">\r\n                                    <th>{{specification.name}}</th>\r\n                                    <th>:</th>\r\n                                    <td>{{specification.value}}</td>\r\n                                  </tr>\r\n                                </table>\r\n                              </div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Requested Quantity : </span>\r\n                                <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"material.requested_quantity\" readonly>\r\n                              </div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Quantity obtained(Kg): </span>\r\n                                <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"material.quantity_obtained\" :name=\"'quantity_obtained'+material.id\" required>\r\n                                <input type=\"hidden\" :name=\"'material_id'+material.id\" :value=\"material.id\">\r\n                              </div>\r\n                            </div>\r\n                          </b-card>\r\n                          <input type=\"hidden\" name=\"keys\" :value=\"form_data.values.length\">\r\n                        </div>\r\n\r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{status:'Recieved', values:[]},\r\n      id:null,\r\n      action:'create',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n    }\r\n    this.getPurchaseMaterials()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.form_data.status = 'Recieved'\r\n      })\r\n    },\r\n    updateMaterialsFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('purchase_material',this.form_data.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_purchase_materials_quantity/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          // this.indentPackingDetails()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    dataForm() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('buyer',this.form_data.buyer)\r\n      formdata.append('department',this.form_data.department)\r\n      formdata.append('vendor',this.form_data.vendor)\r\n      formdata.append('warehouse',this.form_data.warehouse)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.updateMaterialsFormdata()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n<style>\r\n.despatch label{\r\n  font-weight: 700;\r\n  white-space: nowrap;\r\n    align-self: center;\r\n    margin-right: 10px;\r\n    min-width:140px;\r\n}\r\n.unloading .btn-secondary{\r\n  width:100%;\r\n  \r\n    border: 2px solid #efefef;\r\n    background-color: #f8f9fa!important;\r\n    border-radius: 2px;\r\n    text-align: left;\r\n    height: calc(1.9695rem + 2px);\r\n    padding: 5px;\r\n}\r\n\r\n   .table-3 th {\r\n    border: 1px solid #dcdfe6;\r\n    background-color: #f3f4f7;\r\n}\r\n   .table-3 th, .table-3 td {\r\n    border: 1px solid #dcdfe6;\r\n}\r\n  .table-3 th {\r\n    padding:0.75em 1.5em 0.75em 0.75em;\r\n  }\r\n.table-3 .tr-bottom td:nth-child(2){\r\n  border: 2px solid #dcdfe6;\r\n background-color: #f3f4f7;\r\n  \r\n}\r\n.table-3 .tr-bottom td:nth-child(1){\r\nborder: none;\r\n  \r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/app/oodo/production"}]}