e","sourceRoot":"src/views/app/oodo/inventory","sourcesContent":["<template>\r\n  <div class=\"main-content create-main\">\r\n    <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n      <div>\r\n         <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Indent</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/logistics-department/indent\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n          <h5></h5>\r\n        </b-col>\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mb-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Indent Number : </span>\r\n                              <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"indent_number\" v-model=\"form_data.indent_number\" readonly>\r\n                            </div>\r\n\r\n                            <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\"></ProductFields>\r\n\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Quantity (Kg) : </span>\r\n                              <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.quantity\" name=\"quantity\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">PO Number : </span>\r\n                              <input placeholder=\"PO Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.PO_number\" name=\"PO_number\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Expected Date: </span>\r\n                              <input placeholder=\"Expected Date\" type=\"month\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.expected_date\" name=\"expected_date\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Spec : </span>\r\n                              <input placeholder=\"Spec\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.spec\" name=\"spec\" required>\r\n                            </div>\r\n\r\n                            <div class=\"o_thread_search_input table-3\">\r\n                              <span style=\"width:120px;\"> Packing Details: </span>\r\n                              <table class=\"my-3 ml-1\" style=\"width:100%\" >\r\n                                <tr>\r\n                                  <th style=\"width:25%\">Packing Method</th>\r\n                                  <th style=\"width:23%\">Quantity (Kg)</th>\r\n                                  <th style=\"width:20%\">Number</th>\r\n                                  <th style=\"width:20%\">Total</th>\r\n                                  <th style=\"width:12%\">Action</th>\r\n                                </tr>\r\n                                <tr v-for=\"(line,key) in form_data.lines\">\r\n                                  <td>\r\n                                    <div class=\"d-flex w-100 o_thread_search_input\">\r\n                                        <select class=\"ui-autocomplete-input w-100\" :name=\"'packing_method'+key\" v-model=\"line.packing_method\">\r\n                                          <option v-for=\"packing_method in packing_methods\" :value=\"packing_method\"> {{packing_method}}</option>\r\n                                        </select>\r\n                                    </div>\r\n                                  </td>\r\n                                  <td>\r\n                                    <div class=\"o_thread_search_input\">\r\n                                      <input placeholder=\"Qty\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"line.quantity\" @input=\"calcultion()\" :name=\"'quantity'+key\" required>\r\n                                    </div>\r\n                                  </td>\r\n                                  <td>\r\n                                    <div class=\"o_thread_search_input\">\r\n                                      <input placeholder=\"Num\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :name=\"'number'+key\" v-model=\"line.number\" @input=\"calcultion()\" required>\r\n                                    </div>\r\n                                  </td>\r\n                                  <td class=\"pl-3\">\r\n                                    {{line.quantity*line.number}}\r\n                                  </td>\r\n                                  <td class=\"pl-3\">\r\n                                    <b-button variant=\"danger\" type=\"button\" class=\" px-2 py-0\" @click=\"lineOperation(key)\">\r\n                                      Remove\r\n                                    </b-button>\r\n                                  </td>\r\n                                </tr>\r\n                                <tr>\r\n                                  <td colspan=\"3\">\r\n                                     <b-button variant=\"primary\" type=\"button\" class=\" px-3 py-1  d-sm-block ml-3 my-2\" @click=\"lineOperation('+')\">\r\n                                      Add Row\r\n                                    </b-button>\r\n                                    <input type=\"hidden\" name=\"keys\" :value=\"form_data.lines.length\">\r\n                                  </td>\r\n                                  <td class=\"pl-3 py-2\" colspan=\"2\">{{form_data.total}}</td>\r\n                                </tr>\r\n                               \r\n                              </table>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Remark : </span>\r\n                              <textarea placeholder=\"Remark\" rows=\"5\" class=\"ui-autocomplete-input w-100 mx-1\" autocomplete=\"off\" v-model=\"form_data.remark\" name=\"remark\"></textarea>\r\n                              <input type=\"hidden\" name=\"product\" :value=\"product_id\" v-if=\"id\">\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n              <div class=\" float-right pr-5 mr-5\" v-if=\"id\">\r\n                <div class=\"d-flex\">\r\n                  <b-button variant=\"primary\" class=\" p-2  d-sm-block mx-1\" @click=\"$bvModal.show('print-indent')\">\r\n                    Print Indent\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" p-2  d-sm-block mx-2\" @click=\"$bvModal.show('dispatch')\">\r\n                     Add to Dispatch\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n\r\n    <div>\r\n      <b-modal id=\"print-indent\" hide-header  hide-footer>\r\n        <form ref=\"printIndent\" @submit.prevent=\"printIndent()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Print Indent</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Print\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('print-indent')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Indent Number: </span>\r\n                            <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.indent_number\" name=\"indent_number\" readonly>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Slides : </span>\r\n                            <input type=\"radio\" name=\"slide\" class=\"ui-autocomplete-input mx-1\" value=\"1\" checked> 1\r\n                            <span style=\"width:20px;\"> </span>\r\n                            <input type=\"radio\" name=\"slide\" class=\"ui-autocomplete-input mx-1\" value=\"2\"> 2\r\n                            <span style=\"width:50px;\"> </span>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">ICO Number: </span>\r\n                            <input placeholder=\"ICO Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"ICO_number\" required>\r\n                          </div>\r\n\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n\r\n      <b-modal id=\"dispatch\" hide-header  hide-footer>\r\n        <form ref=\"dispatch\" @submit.prevent=\"dispatch()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Dispatch</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('dispatch')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Date: </span>\r\n                            <input placeholder=\"Purchase Type\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.date\" name=\"date\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:220px;\">Expected Vehicle Arraival Time : </span>\r\n                            <input placeholder=\"Time\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.expected_vehicle_arraival_time\" name=\"expected_vehicle_arraival_time\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Customer Code: </span>\r\n                            <input placeholder=\"Customer Code\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.customer_code\" name=\"customer_code\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Customer Name: </span>\r\n                            <input placeholder=\"Customer Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.customer_name\" name=\"customer_name\" required>\r\n                          </div>\r\n\r\n                          <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\"></ProductFields>\r\n\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Packing Material Details : </span>\r\n                            <input placeholder=\"packing_material_details\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.packing_material_details\" name=\"packing_material_details\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mt-4\">\r\n                            <span style=\"width:120px;\">ICO Number : </span>\r\n                            <input placeholder=\"ICO Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.ICO_number\" name=\"ICO_number\" readonly>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Indent Number : </span>\r\n                            <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.indent_number\" name=\"indent_number\" required>\r\n                            <input type=\"hidden\" name=\"product\" :value=\"product_id\" v-if=\"id\">\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Quantity (Kg) : </span>\r\n                            <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.quantity\" name=\"quantity\" required>\r\n                          </div>\r\n                          \r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{expected_date:'2022-03',total:0, lines:[], indent_number:''},\r\n      id:null,\r\n      action:'create',\r\n      product_id:null,\r\n      load_products:false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.form_data.expected_date = this.currentMonth()\r\n    this.form_data.indent_number = this.autoIndentNumber()\r\n    this.$root.current_section = 'logistics-department'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n      this.lineOperation('+')\r\n    }\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.product_id = jsonData.product\r\n        this.load_products = true\r\n      })\r\n    },\r\n    lineOperation(operation){\r\n      if(operation=='+'){\r\n        this.form_data.lines.push({'quantity':0,'number':0,'packing_method':'Bag'})\r\n      }else{\r\n        this.form_data.lines.splice(operation, 1)\r\n      }\r\n      this.calcultion()\r\n    },\r\n    calcultion(){\r\n      var total = 0\r\n      this.form_data.lines.forEach(function(val,key){\r\n        total = total + (val.quantity*val.number)\r\n      })\r\n      this.form_data.total = total\r\n\r\n    },\r\n    autoIndentNumber(){\r\n      var dateTime = new Date()\r\n      return dateTime.getTime()\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.form_data = jsonData\r\n          // this.indentPackingDetails()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    indentPackingDetails() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent_packing_details/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n<style>\r\n.despatch label{\r\n  font-weight: 700;\r\n  white-space: nowrap;\r\n    align-self: center;\r\n    margin-right: 10px;\r\n    min-width:140px;\r\n}\r\n.unloading .btn-secondary{\r\n  width:100%;\r\n  \r\n    border: 2px solid #efefef;\r\n    background-color: #f8f9fa!important;\r\n    border-radius: 2px;\r\n    text-align: left;\r\n    height: calc(1.9695rem + 2px);\r\n    padding: 5px;\r\n}\r\n\r\n   .table-3 th {\r\n    border: 1px solid #dcdfe6;\r\n    background-color: #f3f4f7;\r\n}\r\n   .table-3 th, .table-3 td {\r\n    border: 1px solid #dcdfe6;\r\n}\r\n  .table-3 th {\r\n    padding:0.75em 1.5em 0.75em 0.75em;\r\n  }\r\n.table-3 .tr-bottom td:nth-child(2){\r\n  border: 2px solid #dcdfe6;\r\n background-color: #f3f4f7;\r\n  \r\n}\r\n.table-3 .tr-bottom td:nth-child(1){\r\nborder: none;\r\n  \r\n}\r\n</style>\r\n"]}]}