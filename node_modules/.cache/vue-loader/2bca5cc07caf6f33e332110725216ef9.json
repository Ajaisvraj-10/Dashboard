CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"all-tasks.vue","sourceRoot":"src/views/app/oodo/task","sourcesContent":["<template>\r\n  <div class=\"main-content main-content2\">\r\n    <div>\r\n      <div class=\"d-flex mb-2 align-items-center justify-content-between\">\r\n        <slot name=\"header\">\r\n          <div style=\"width:100px;\">\r\n           <h5 class=\"mb-0\">Tasks Report</h5>\r\n          </div>\r\n        </slot>\r\n        <div class=\"d-flex\">\r\n          <h5 class=\"mb-0 mx-1 font-weight-600 cursor-pointer tab-breadcamp\" v-for=\"task_category in task_categories\" @click=\"selectTaskCat(task_category)\" :class=\"current_task_category==task_category?'tab-breadcamp-active task-blue':'tab-breadcamp'\">{{slugToWord(task_category)}}</h5>\r\n        </div>\r\n        <div class=\"d-flex\">\r\n          <b-dropdown id=\"dropdown-right\" ref=\"filter_drpdwn\" right variant=\"default\" class=\"p-0\">\r\n            <template #button-content>\r\n              Filter task  <sup v-if=\"filter_count\"><span class=\"badge badge-danger pt-1\">{{filter_count}}</span></sup>\r\n            </template>\r\n            <div class=\"d-flex flex-column p-2\">\r\n              <h6>Select Departmnet</h6>\r\n              <b-form > \r\n                <b-form-group      \r\n                  id=\"input-group-1\"\r\n                  label=\"\"\r\n                  label-for=\"input-1\"\r\n                  class=\"\">\r\n                  <b-form-select v-model=\"department_id\" name=\"\">\r\n                    <option v-for=\"department in departments\" :value=\"department.id\">{{department.name}}</option>\r\n                  </b-form-select>\r\n                </b-form-group>\r\n              </b-form>\r\n              <h6>Select Employee</h6>\r\n              <b-form > \r\n                <b-form-group      \r\n                  id=\"input-group-1\"\r\n                  label=\"\"\r\n                  label-for=\"input-1\"\r\n                  class=\"\">\r\n                  <b-form-select v-model=\"user_id\" name=\"\">\r\n                    <option v-for=\"user in users\" v-if=\"user.user && (!department_id || department_id==user.department)\" :value=\"user.user\">@{{user.user_detail.username}}</option>\r\n                  </b-form-select>\r\n                </b-form-group>\r\n              </b-form>\r\n              <h6>Start Date</h6>\r\n              <b-form > \r\n                <input type=\"date\" class=\"w-100\" v-model=\"start_date\">\r\n              </b-form>\r\n              <h6 class=\"mt-3\">End Date</h6>\r\n              <b-form > \r\n                <input type=\"date\" class=\"w-100\" v-model=\"end_date\">\r\n              </b-form>\r\n            </div>\r\n            <div class=\"my-2 border-top\"></div>\r\n            <div class=\"d-flex\">\r\n              <b-button class=\"m-2 mt-0 btn-sm px-2 py-1\" variant=\"info\" @click=\"getTasks();$refs.filter_drpdwn.hide()\">\r\n                Apply\r\n              </b-button>\r\n              <b-button class=\"my-2 mt-0 btn-sm px-2 py-1\" variant=\"ligth\" @click=\"cancelFilter();$refs.filter_drpdwn.hide()\">\r\n                Cancel Filter\r\n              </b-button>\r\n            </div>\r\n          </b-dropdown>\r\n          <div class=\"text-right cursor-pointer\" style=\"width:40px;\">\r\n            <img src=\"/img/refresh.png\" @click=\"reloadTasks()\" width=\"24\">\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"separator-breadcrumb border-top\"></div>\r\n    </div>\r\n\r\n    <div class=\"task-sec\">\r\n      <!-- contents -->\r\n      <div class=\"ul-todo-list-content\" style=\"margin-top:-20px\">\r\n        <!-- tasks -->\r\n        <div class=\"todo-left p-1\" style=\"height:80vh;overflow-y:auto;width:100%\">\r\n          <div class=\"mb-2\">\r\n            <b-card body-class=\"p-0\" class=\"h-100 mb-2\" v-for=\"task in tasks\"> \r\n              <div class=\"\" @click=\"current_main_task=task\">\r\n                <div class=\"d-flex justify-content-between task-card border-bottom\" @click=\"selectTask(task)\" :class=\"task.id==current_task.id?'active-task':''\" :style=\"task.status=='Completed'?'background-color:72f54280':''\">\r\n                  <b class=\" p-3 d-flex align-items-center cursor-pointer w-100\" style=\"gap:10px;\">\r\n                    <div class=\"w-100\" v-b-toggle=\"'sub_task_main_list'+task.id\">\r\n                      <h6 class=\"text-14 font-weight-700 mb-0\" :class=\"task.status=='Completed' && task.is_completed?'finish-task':''\">{{task.task}}</h6>\r\n                      <div class=\"task-filters pt-1\">\r\n                        <p class=\"text-12 text-darklight\">\r\n                          <span class=\"badge badge-light\">task created by @{{task.username}}</span>\r\n                        </p>\r\n                        <p class=\"text-12  badge badge-outline-info\"><i class=\"fa fa-calendar-o pr-1\"></i><span v-if=\"task.due_date\">Due Date - {{dateFormate(task.due_date)}}</span></p>\r\n                        <p class=\"text-12  badge\" v-if=\"task.priority!='Normal'\"\r\n                          :class=\"task.priority=='Urgent'?'badge-danger':task.priority=='Medium'?'badge-success':task.priority=='Normal'?'badge-primary':'badge-light'\"\r\n                        ><i class=\"fa fa-level-up\"></i> {{task.priority!='Normal'?task.priority:''}}</p>\r\n                        <p class=\"text-10 mb-0 text-darklight\" v-if=\"task.tag.id && !task.is_completed\">\r\n                          <span class=\"badge badge-light text-10\" :style=\"'background-color:'+task.tag.color+'!important;color:'+bgToTextColor(task.tag.color)\">\r\n                            <i class=\"fa fa-tags pr-1\"></i>{{task.tag.name}}\r\n                          </span>\r\n                        </p>\r\n                        <p class=\"text-12 text-darklight\">\r\n                          task assigned to\r\n                          <span class=\"badge badge-light\" style=\"\" v-for=\"(assign_user,aukey) in task.assign_users\">\r\n                            @{{assign_user.username}}\r\n                          </span>\r\n                        </p>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"pt-2\" v-b-toggle=\"'sub_task_main_list'+task.id\">\r\n                      <div class=\"bg-light p-0\" style=\"position:relative;width:150px;border-radius:5px;box-shadow:rgb(0 0 0 / 14%) -1px 7px 9px -5px\">\r\n                        <div class=\"badge-success p-0\" style=\"border-radius:5px 3px 3px 5px\" :style=\"'width:'+task.task_completion+'%'\">\r\n                          <small class=\"px-1\" style=\"white-space:nowrap;\">Completed {{parseInt(task.task_completion)}}%</small>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"pr-2\" style=\"width:48%\">\r\n                      <div class=\"text-right\">\r\n                        <i class=\"fa fa-tags\" style=\"font-size:18px;color:#434e75;\" @click=\"tagStatus(task)\"></i>\r\n                        <sup><span class=\"text-success\" v-if=\"task.activity_count>0\">{{task.activity_count}}</span></sup>\r\n                        <span class=\"mx-1 pl-1\"></span>\r\n                        <i class=\"fa fa-comments\" :class=\"task.notifications.activities>0?'msg_shake':''\" style=\"font-size:18px;color:#434e75;\" @click=\"getActivities(task)\"></i>\r\n                        <sup><span class=\"text-success\" v-if=\"task.message_count>0\">{{task.message_count}}</span></sup>\r\n                      </div>\r\n                      <div class=\"text-right\">\r\n                        <small>created on </small>{{dateFormate(task.created_on,true)}}, \r\n                        <small>{{timeAgo(task.created_on)}}</small>\r\n                      </div>\r\n                    </div>\r\n                  </b>\r\n                </div>\r\n                <div class=\"px-2\">\r\n                  <b-collapse :id=\"'sub_task_main_list'+task.id\">\r\n                    <div class=\"todo-collapse-container py-2\" style=\"flex-direction:column;align-items:start;gap:4px;\">\r\n                      <div class=\"w-100 text-center p-3\" v-if=\"task.sub_tasks<=0 && task.sub_tasks_count>0\">\r\n                        <div class=\"table_loading\"></div>\r\n                      </div>\r\n                      <div class=\"task-card todo-collapse2 d-flex align-items-center px-4 w-100 py-2\" v-for=\"sub_task in task.sub_tasks\" @click=\"selectTask(sub_task)\" :class=\"sub_task.id==current_task.id?'active-task':''\" :style=\"sub_task.status=='Completed'?'background-color:#72f54240!important':''\">\r\n                        <div class=\"w-100 cursor-pointer sub-task ml-3\">\r\n                          <h6 class=\"text-14 font-weight-500 mb-0\" :class=\"sub_task.status=='Completed' && sub_task.is_completed?'finish-task':''\">{{sub_task.task}}</h6>\r\n                          <div class=\"task-filters pt-1\">\r\n                            <p class=\"text-10 mb-0 text-darklight\">\r\n                              <span class=\"badge badge-light text-10\" style=\"margin-left:1px\">\r\n                                <span>Task created by @{{sub_task.username}}</span>\r\n                              </span>\r\n                            </p>\r\n                            <p class=\"text-10 mb-0 badge badge-outline-info\"><i class=\"fa fa-calendar-o pr-1\"></i><span v-if=\"sub_task.due_date\">Due Date - {{dateFormate(sub_task.due_date)}}</span></p>\r\n                            <p class=\"text-10 mb-0 badge\" v-if=\"sub_task.priority!='Normal'\"\r\n                              :class=\"sub_task.priority=='Urgent'?'badge-danger':sub_task.priority=='Medium'?'badge-success':sub_task.priority=='Normal'?'badge-primary':'badge-light'\"\r\n                            ><i class=\"fa fa-level-up\"></i> {{sub_task.priority!='Normal'?sub_task.priority:''}}</p>\r\n                            <p class=\"text-10 mb-0 text-darklight\" v-if=\"sub_task.tag.id && !sub_task.is_completed\">\r\n                              <span class=\"badge badge-light text-10\" :style=\"'background-color:'+sub_task.tag.color+'!important;color:'+bgToTextColor(sub_task.tag.color)\">\r\n                                <i class=\"fa fa-tags\"></i>{{sub_task.tag.name}}\r\n                              </span>\r\n                            </p>\r\n                            <p class=\"text-10 mb-0 text-darklight\">\r\n                              task assigned to\r\n                              <span class=\"badge badge-light text-10\" style=\"margin-left:1px\" v-for=\"(assign_sub_user,asukey) in sub_task.assign_users\">\r\n                                <span>@{{assign_sub_user.username}}</span>\r\n                              </span>\r\n                            </p>\r\n                          </div>\r\n                        </div>\r\n\r\n                        <div class=\"pr-3\" style=\"width:48%\">\r\n                          <div class=\"text-right cursor-pointer\">\r\n                            <i class=\"fa fa-tags\" style=\"font-size:18px;color:#434e75;\" @click=\"tagStatus(sub_task)\"></i>\r\n                            <sup><span class=\"text-success\" v-if=\"sub_task.activity_count>0\">{{sub_task.activity_count}}</span></sup>\r\n                            <span class=\"mx-1 pl-1\"></span>\r\n                            <i class=\"fa fa-comments\" style=\"font-size:18px;color:#434e75;\" @click=\"getActivities(sub_task)\"></i>\r\n                            <sup><span class=\"text-success\" v-if=\"sub_task.message_count>0\">{{sub_task.message_count}}</span></sup>\r\n                          </div>\r\n                          <div class=\"text-right\">\r\n                            <small>created on </small>{{dateFormate(sub_task.created_on,true)}}, \r\n                            <small>{{timeAgo(sub_task.created_on)}}</small>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </b-collapse>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n            <div class=\"text-center \" v-if=\"tasks.length<=0 && is_start\">\r\n              <p class=\"py-5\">No tasks found!</p>\r\n            </div>\r\n            <div class=\"text-center \" v-if=\"tasks.length<=0 && !is_start\">\r\n              <p class=\"p-3\">\r\n                <div class=\"table_loading\"></div>\r\n              </p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <b-modal id=\"activity\" title=\"Messages\" >\r\n      <div class=\"activity-container\" style=\"height: 68vh;overflow-y: auto;\">\r\n        <div class=\"activity-header mb-2\">\r\n          <div class=\"profile-dp\">\r\n            {{slugToWord($root.userInfo.username.length>0?$root.userInfo.username[0]:'X')}}\r\n          </div>\r\n          <div class=\"activity-collapse\">\r\n            <form ref=\"messageForm\" @submit.prevent=\"messageForm()\">\r\n              <div class=\"px-5 py-2\" v-if=\"new_activity.preview_image\">\r\n                <img :src=\"new_activity.preview_image\" class=\"w-100\">\r\n              </div>\r\n              <input placeholder=\"Write a Comment\" v-model=\"new_activity.note\" name=\"note\" v-b-toggle.collapse-action>\r\n              <!-- <textarea placeholder=\"Write a Comment\" v-b-toggle.collapse-action></textarea> -->\r\n              <b-collapse  class=\"activity-body\" id=\"collapse-action\">\r\n                <button type=\"submit\" class=\"btn btn-info\">Save</button>\r\n                <div class=\"activity-body-right\">\r\n                  \r\n                  <b-dropdown  no-caret>\r\n                    <template #button-content>\r\n                      <button class=\"btn-icons\"><i class=\"fa fa-paperclip\"></i></button>\r\n                    </template>\r\n                    \r\n                    <h6 class=\"text-center text-dark mb-0 p-3 pb-0\">Attatch</h6>\r\n                    <b-dropdown-divider></b-dropdown-divider>\r\n                    <b-dropdown-text>\r\n                      <label>\r\n                        <h6 class=\"mb-0\">Image</h6>\r\n                        <input type=\"file\" :name=\"new_activity.preview_image?'image':'no_img'\" style=\"opacity:0;z-index:99;position:absolute;top:0;left:0;width:100%;height:100%;cursor:pointer\" @change=\"selectFile\">\r\n                      </label>\r\n                    </b-dropdown-text>\r\n                  </b-dropdown>\r\n                 \r\n                </div>\r\n              </b-collapse>\r\n            </form>\r\n          </div>\r\n        </div>\r\n        <hr class=\"my-2\">\r\n        <div>\r\n          <div class=\"activity-header mb-1\" v-for=\"task_activity in task_activities\">\r\n            <div class=\"profile-dp\" :style=\"task_activity.user==userId?'':'background-color:#666fb3'\">\r\n              {{slugToWord(task_activity.username.length>0?task_activity.username[0]:'X')}}\r\n            </div>\r\n            <div class=\"mx-1 w-100\">\r\n              <b>@{{task_activity.username}}</b>\r\n              <div class=\"px-2 cursor-pointer\" v-if=\"task_activity.image\">\r\n                <img :src=\"task_activity.image\" style=\"max-height:100px;max-width:100%\">\r\n              </div>\r\n              <p class=\"w-100\">{{task_activity.note}} <small class=\"text-info pt-1\" style=\"float:right\">{{timeAgo(task_activity.created_on)}}</small></p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </b-modal>\r\n    <b-modal id=\"task-tags\" title=\"Activities\" >\r\n      <div class=\"activity-container p-2\" style=\"height: 68vh;overflow-y: auto;\">\r\n        <div class=\"row mx-0\">\r\n          <div class=\"col-md-3\">\r\n            <div class=\"card p-2\">\r\n              <span class=\"text-left\">\r\n                <button class=\"btn btn-success p-1\">\r\n                  <i class=\"fa fa-tasks\"></i> Task Created\r\n                </button>\r\n                <span class=\"text-12 d-block mt-1\">created by @{{current_task.username}}</span>\r\n              </span>\r\n              <span style=\"float:right\">\r\n                <small>{{timeAgo(current_task.created_on)}}</small>\r\n              </span>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-md-3\" v-for=\"tag_sta in tag_status\">\r\n            <div class=\"card p-2\">\r\n              <span class=\"text-left\">\r\n                <button class=\"btn btn-light p-1\" :style=\"'background-color:'+tag_sta.color+'!important;color:'+bgToTextColor(tag_sta.color)\">\r\n                  <i class=\"fa fa-tags\"></i>{{tag_sta.name}}\r\n                </button>\r\n                <span class=\"text-12 d-block mt-1\">updated by @{{tag_sta.username}}</span>\r\n              </span>\r\n              <span style=\"float:right\">\r\n                <small>{{timeAgo(tag_sta.created_on)}}</small>\r\n              </span>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-md-3\" v-if=\"current_task.is_completed\">\r\n            <div class=\"card p-2\">\r\n              <span class=\"text-left\">\r\n                <button class=\"btn btn-danger p-1\">\r\n                  <i class=\"fa fa-tasks\"></i> Task Completed\r\n                </button>\r\n                <span class=\"text-12 d-block mt-1\">{{dateFormate(current_task.updated_on)}}</span>\r\n              </span>\r\n              <span style=\"float:right\">\r\n                <small>{{timeAgo(current_task.updated_on)}}</small>\r\n              </span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </b-modal>\r\n  </div>  \r\n</template>\r\n<script>export default {\r\n  metaInfo: {\r\n   \r\n  },\r\n  data() {\r\n    return {\r\n      tasks:[],\r\n      users:[],\r\n      departments:[],\r\n      department_id:null,\r\n      user_id:null,\r\n      start_date:null,\r\n      end_date:null,\r\n      filter_count:0,\r\n      current_task:{is_completed:false, not_deleted:1, sub_tasks_count:0},\r\n      current_main_task:{is_completed:false, not_deleted:1},\r\n      task_categories:['all_tasks','completed_tasks'],\r\n      tags:[],\r\n      new_activity:{note:'', preview_image:''},\r\n      task_activities:[],\r\n      tag_status:[],\r\n      current_task_category:'all_tasks',\r\n      toggle:false,\r\n      is_start:false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'task'\r\n    this.getTasks()\r\n    this.getUsers()\r\n    this.getDepartments()\r\n  },\r\n  methods: {\r\n    reloadTasks(){\r\n      this.tasks = []\r\n      this.getTasks()\r\n    },\r\n    selectTaskCat(task_category){\r\n      this.current_task_category=task_category\r\n      this.tasks = []\r\n      this.current_task = {}\r\n      this.getTasks()\r\n    },\r\n    getDateIs(type='Today', action=''){\r\n      var today = new Date\r\n      var date = ''\r\n      if(type=='Today'){\r\n        date = today.setDate(today.getDate())\r\n      }\r\n      if(type=='Tomorrow'){\r\n        date=today.setDate(today.getDate() + 1);\r\n      }\r\n      if(type=='Next Week'){\r\n        date=today.setDate(today.getDate() + 7);\r\n      }\r\n      if(action=='click'){\r\n        return this.currentDate(date)\r\n      }\r\n      return this.dateFormate(date)\r\n    },\r\n    getTasks() {\r\n      this.is_start = false\r\n      this.tasks = []\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      var param = '?task_type='+this.current_task_category\r\n      this.filter_count = 0\r\n      if(this.department_id){\r\n        this.filter_count++\r\n        param+=param+'&department_id='+this.department_id\r\n      }\r\n      if(this.user_id){\r\n        this.filter_count++\r\n        param+=param+'&user_id='+this.user_id\r\n      }\r\n      if(this.start_date){\r\n        this.filter_count++\r\n        param+=param+'&start_date='+this.start_date\r\n      }\r\n      if(this.end_date){\r\n        this.filter_count++\r\n        param+=param+'&end_date='+this.end_date\r\n      }\r\n      fetch(this.api_url+'/employee/all_tasks/'+param, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.is_start = true\r\n        this.tasks = jsonData\r\n      })\r\n    },\r\n    cancelFilter(){\r\n      this.filter_count = 0\r\n      this.department_id = null\r\n      this.user_id = null\r\n      this.start_date = null\r\n      this.end_date = null\r\n      this.getTasks()\r\n    },\r\n    selectTask(task){\r\n      var self = this\r\n      this.current_task = {}\r\n      if(task.main_task){\r\n        setTimeout(function(){\r\n          self.current_task = task\r\n        },0)\r\n      }else{\r\n        this.getSubTasks(task)\r\n      }\r\n    },\r\n    getSubTasks(task){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/all_tasks/?task_type='+this.current_task_category+'&sub_task='+task.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(!this.current_task.id){\r\n          this.current_task = task\r\n        }\r\n        task.sub_tasks = jsonData\r\n      })\r\n    },\r\n    getUsers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.users = jsonData\r\n      })\r\n    },\r\n    getDepartments() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/department/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.departments = jsonData\r\n      })\r\n    },\r\n    selectFile(e) {\r\n      var img_file = e.target.files[0];\r\n      if(img_file){\r\n        this.new_activity.preview_image = ''\r\n        var self=this\r\n        setTimeout(function(){\r\n          self.new_activity.preview_image = URL.createObjectURL(img_file);\r\n        },10)\r\n      }\r\n    },\r\n    getActivities(task) {\r\n      this.current_task = task\r\n      this.task_activities = []\r\n      this.$bvModal.show('activity')\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/task_activities/?task='+this.current_task.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.task_activities = jsonData\r\n      })\r\n    },\r\n    tagStatus(task) {\r\n      this.current_task = task\r\n      this.tag_status = []\r\n      this.$bvModal.show('task-tags')\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/tag_status/?task='+this.current_task.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.tag_status = jsonData\r\n      })\r\n    },\r\n    messageForm(tag){\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      var formdata = new FormData(this.$refs['messageForm'])\r\n      formdata.append('task', this.current_task.id)\r\n      formdata.append('user', this.userId)\r\n      this.lock_btn = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/task_activities/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.new_activity = {note:'', preview_image:''}\r\n          this.getActivities(this.current_task)\r\n          this.showAlert('Message saved!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n</style>"]}]}