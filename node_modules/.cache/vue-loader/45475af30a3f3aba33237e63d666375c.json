"off\" :value=\"farmer.length!=0?farmer.farmer_code+' - '+farmer.name:'Loading...'\" readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Plant Year: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"year\">\r\n                                  <option v-for=\"year in this_year\" :value=\"year\" :selected=\"year==form_data.year\" v-if=\"year>2000\">{{year}}</option>\r\n                              </select>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"user-info pt-5\" v-for=\"category in categories\">\r\n                            <b class=\"text-bold\" v-if=\"category.plants.length>0\">{{category.name}}</b>\r\n                            <div class=\"row px-3\" v-for=\"(product,key) in category.plants\" v-if=\"category.id==1\">\r\n                              <div class=\"col-md-7\" v-if=\"key==0\">\r\n                                 \r\n                              </div>\r\n                              <div class=\"col-md-3 pl-5\" v-if=\"key==0\">\r\n                                <b>Dry</b>\r\n                              </div>\r\n                              <div class=\"col-md-2 pl-4\" v-if=\"key==0\">\r\n                                <b>Fresh</b>\r\n                              </div>\r\n                              <div class=\"o_thread_search_input col-md-6\">\r\n                                <span style=\"width:200px;\">{{product.name}} Number: </span>\r\n                                <input placeholder=\"Enter a number\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input\" :data=\"product.plants?calculateEstimation(product):setEstimate(product)\" autocomplete=\"off\" v-model=\"product.plants\" @input=\"calculateEstimation(product)\" :name=\"'number'+product.id\">\r\n                              </div>\r\n                              <div class=\"o_thread_search_input col-md-4\">\r\n                                <span style=\"width:120px;\">Estimate Qty: </span>\r\n                                <input :placeholder=\"product.approximate?'Approximate :'+product.approximate:''\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input\" v-model=\"product.estimate\" autocomplete=\"off\" @keyup=\"checkMax(product)\" :name=\"'estimate'+product.id\">\r\n                              </div>\r\n                              <div class=\"o_thread_search_input col-md-2\">\r\n                                <input :placeholder=\"product.approximate?'Approximate :'+product.approximate_fresh:''\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input\" v-model=\"product.estimate_fresh\" autocomplete=\"off\" @keyup=\"checkMax(product)\" :name=\"'estimate_fresh'+product.id\">\r\n                              </div>\r\n                            </div>\r\n                            <div class=\"row px-3\" v-for=\"(product,key) in category.plants\" v-if=\"category.id!=1 && category.plants.length!=0\">\r\n                              <div class=\"o_thread_search_input col-md-6\">\r\n                                <span style=\"width:200px;\">{{product.name}} Number: </span>\r\n                                <input placeholder=\"Enter a number\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input\" :data=\"product.plants?calculateEstimation(product):setEstimate(product)\" autocomplete=\"off\" v-model=\"product.plants\" @input=\"calculateEstimation(product)\" :name=\"'number'+product.id\">\r\n                              </div>\r\n                              <div class=\"o_thread_search_input col-md-6\">\r\n                                <span style=\"width:120px;\">Estimate Qty: </span>\r\n                                <input :placeholder=\"product.approximate?'Approximate :'+product.approximate:''\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input\" v-model=\"product.estimate\" autocomplete=\"off\" @keyup=\"checkMax(product)\" :name=\"'estimate'+product.id\">\r\n                              </div>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport LocationFields from '/src/views/app/oodo/components/location_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - LAND\"\r\n  },\r\n  components:{\r\n    LocationFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:[],\r\n      id:null,\r\n      farmer_id:null,\r\n      farmer:[],\r\n      categories:[],\r\n      action:'create',\r\n      product_price:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'fpo'\r\n    this.action = this.$route.params.action\r\n    this.farmer_id = this.$route.params.farmer_id\r\n    this.getFarmer()\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.getPlants()\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getPlants() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/categories/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.categories = jsonData\r\n      })\r\n    },\r\n    calculateEstimation(product){\r\n      if(product.category==1){\r\n        product.approximate = product.plants * product.constant\r\n        product.approximate_fresh = product.approximate * product.fresh\r\n      }else{\r\n        product.approximate = product.plants * product.constant\r\n      }\r\n      this.checkMax(product)\r\n      return ''\r\n    },\r\n    setEstimate(product){\r\n      if(this.action == 'edit'){\r\n        var plants = this.form_data.plants\r\n        plants.forEach(function(val){\r\n          if(product.product.includes(val.product)){\r\n            product.plants = val.number\r\n            product.estimate = val.estimate\r\n            var product_detail = val.product_detail\r\n            if(product_detail.category==1 && product_detail.product_type==1){\r\n              product.estimate_fresh = val.estimate\r\n            }\r\n          }\r\n        })\r\n      }\r\n    },\r\n    checkMax(product){\r\n      if(product.category==1){\r\n        if(product.approximate < product.estimate){\r\n          product.estimate = ''\r\n        }\r\n        if(product.approximate_fresh < product.estimate_fresh){\r\n          product.estimate_fresh = ''\r\n        }\r\n      }else{\r\n        if(product.approximate < product.estimate){\r\n          product.estimate = ''\r\n        }\r\n      }\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/plants/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.getPlants()\r\n      })\r\n    },\r\n    getFarmer() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmers/'+this.farmer_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmer = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append(\"farmer\",this.farmer_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/plants/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append(\"farmer\",this.farmer_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/plants/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}