ead_search_input\">\r\n                              <span style=\"width:120px;\">Name : </span>\r\n                              <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.name\" name=\"name\" @input=\"seachBook()\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Select : </span>\r\n                              <div class=\"inspection-btn mx-1\" >\r\n                                <button type=\"button\" class=\"btn btn-light btn-sm ml-1\" v-for=\"security_book in security_books\" :class=\"form_data.a_id==security_book.id?'active-btn':''\" @click=\"form_data.a_id=security_book.id;form_data.name=security_book.name;form_data.contact=security_book.contact;form_data.vehicle_type=security_book.vehicle_type;form_data.vehicle_number=security_book.vehicle_number\">{{security_book.name}} </button>\r\n                                <input type=\"hidden\" v-model=\"form_data.a_id\" name=\"employee\" v-if=\"form_data.vehicle_for=='Employee Vehicle'\">\r\n                                <input type=\"hidden\" v-model=\"form_data.a_id\" name=\"worker\" v-if=\"form_data.vehicle_for=='Workers Vehicle'\">\r\n\r\n                              </div>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Contact : </span>\r\n                              <input placeholder=\"Contact\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.contact\" name=\"contact\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Vehicle Type : </span>\r\n                              <input placeholder=\"Vehicle Type\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.vehicle_type\" name=\"vehicle_type\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Vehicle Number : </span>\r\n                              <input placeholder=\"Vehicle Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.vehicle_number\" name=\"vehicle_number\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Origin of the Vehicle : </span>\r\n                              <input placeholder=\"Origin of the Vehicle\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.origin_of_the_vehicle\" name=\"origin_of_the_vehicle\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\" v-if=\"form_data.vehicle_for=='Government Vehicle' || form_data.vehicle_for=='Customer/Client' || form_data.vehicle_for=='Guest' || form_data.vehicle_for=='Diocese'\">\r\n                              <span style=\"width:120px;\">Who came to see? : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"see_for\" v-model=\"form_data.see_for\">\r\n                                  <option value=\"\" selected disabled>Select </option>\r\n                                  <option v-for=\"employee in employees\" :value=\"employee.id\">{{employee.name}} ({{employee.job_position_detail.name}})</option>\r\n                              </select>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{date:'',vehicle_for:'', a_id:null, name:'', contact:'', vehicle_type:'', vehicle_number:'', see_for:''},\r\n      id:null,\r\n      action:'create',\r\n      security_books:[],\r\n      vehicle_types:['Car','Bike','Auto Rickshaw','Pickup','Lorry','Goods Auto','Tipper Lorry','Truck','Other'],\r\n      employees:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.form_data.date = this.currentDate()\r\n    this.$root.current_section = 'security-gate'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n    this.getEmployees()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/security/security_book/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.lines = jsonData.lab_test_reference_detail\r\n        this.parameters = jsonData.lab_test_parameters_detail\r\n      })\r\n    },\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.employees = jsonData\r\n      })\r\n    },\r\n    seachBook() {\r\n      this.security_books = []\r\n      if(!this.form_data.name){\r\n        return false\r\n      }\r\n      var formdata = new FormData()\r\n      formdata.append('name',this.form_data.name)\r\n      formdata.append('vehicle_for',this.form_data.vehicle_for)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/security/search_security_book/', {\r\n          method : 'post',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.security_books = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var d = new Date();\r\n      var current_time = this.currentDate()+' '+d.getHours()+':'+d.getMinutes();\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('gate_in', 1)\r\n      formdata.append('gate_in_time', current_time)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/security/security_book/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var d = new Date();\r\n      var current_time = this.currentDate()+' '+d.getHours()+':'+d.getMinutes();\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('gate_in', 1)\r\n      formdata.append('gate_in_time', current_time)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/security/security_book/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n@import '../styles/style.css';\r\n</style>\r\n"]}]}