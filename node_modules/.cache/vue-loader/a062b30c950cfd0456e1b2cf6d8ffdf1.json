    </div>\r\n\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\"> Processing Type: </span>\r\n                             <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"process_type\">\r\n                                <option value=\"warehouse.id\"> Hulling</option>\r\n                                <option value=\"warehouse.id\"> Unloading</option>\r\n                              </select>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\"> Total Time: </span>\r\n                            <input placeholder=\"0\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_time\" readonly>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\"> Total Cost: </span>\r\n                            <input placeholder=\"0\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_cost\" readonly>\r\n                          </div>\r\n                        </div>\r\n                        <div class=\"col-md-12\">\r\n                          <div class=\"user-info mt-5 pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">  : </span>\r\n                              <b-button  variant=\"success\" class=\" px-3 py-1 mx-1\">Start</b-button>\r\n                              <b-button  variant=\"danger\" class=\" px-3 py-1  mx-1\">Stop</b-button>\r\n                              <input type=\"hidden\" name=\"product\" :value=\"form_data.product\">\r\n                              <input type=\"hidden\" name=\"batch\" :value=\"form_data.batch\">\r\n                            </div>\r\n                          </