form_data.farmer_detail.vfc_name}}</h2>\r\n                </div>\r\n                \r\n              </div>\r\n\r\n            </div>\r\n          </div>\r\n          <h5 class=\"mt-4 mb-2\">Collection Details:</h5>\r\n          <div class=\"card p-0\">\r\n               \r\n            <div class=\"p-4\">\r\n              <div class=\"row details-row\">\r\n                <div class=\"col-6\">\r\n                  <h2 class=\" \">Product Category:</h2>\r\n                  <h2 class=\"   \">{{form_data.product_detail.category_detail.name}}</h2>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                  <h2 class=\" \">Product Types :</h2>\r\n                  <h2 class=\"   \">{{form_data.product_detail.product_type_detail.name}}</h2>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                  <h2 class=\" \">Product :</h2>\r\n                  <h2 class=\"   \">{{form_data.product_detail.name}}</h2>\r\n                </div>\r\n                \r\n                <div class=\"col-6\">\r\n                  <h2 class=\" \">Date</h2>\r\n                  <h2 class=\"   \">{{dateFormate(form_data.created_on)}}</h2>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                  <h2 class=\"  \">Estimate QTY:</h2>\r\n                  <h2 class=\"   \">{{form_data.estimate_qty}}KG</h2>\r\n                </div>\r\n                <div class=\"col-6\">\r\n                  <h2 class=\" p-0 \">Farmer Estimate QTY:</h2>\r\n                  <h2 class=\"   \">{{form_data.farmer_estimate}}KG</h2>\r\n                </div>\r\n                <div class=\"col-6 pb-0\">\r\n                  <h2 class=\" \">Available QTY:</h2>\r\n                  <h2 class=\"   \">{{form_data.available_qty}}KG</h2>\r\n                </div>\r\n                <div class=\"col-6 pb-0\">\r\n                  <h2 class=\" \">QC Status:</h2>\r\n                  <h2 class=\"  \"><span class=\" p-1 badge\" :class=\"form_data.qc_status=='Waiting'?'badge-warning':form_data.qc_status=='Approved'?'badge-success':form_data.qc_status=='Rejected'?'badge-danger':'badge-primary'\">{{form_data.qc_status}}</span></h2>\r\n                </div>\r\n              </div>\r\n\r\n            </div>\r\n          </div>\r\n\r\n      </div>\r\n      <div class=\"filter-footer\">\r\n        <div class=\"footer-item2 d-flex align-items-center\" @click=\"$router.push('/')\">\r\n          <img src=\"/images/home.png\" class=\"\" width=\"25\">\r\n          <h5 class=\"m-0 pl-2 text-white\">Home</h5>\r\n        </div>\r\n        \r\n      </div>\r\n    </main>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{purchase_type:1,date:new Date(),'estimate_qty':0,'farmer_estimate':0,'farmer_detail':{'name':'', 'farmer_code':''},'farmer':null, 'available_qty':0,'qc_status':'Waiting'},\r\n      id:this.$route.params.id,\r\n      load_products:false,\r\n      product_id:null,\r\n      action:'create',\r\n      farmers:[],\r\n      total_keys:0,\r\n      not_start:true,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.form_data.date = this.currentDate()\r\n    this.getPurchaseTypes()\r\n    if(this.id){\r\n      this.getForm()\r\n    }\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_procurement/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_id = jsonData.product\r\n        this.form_data = jsonData\r\n        this.load_products = true\r\n        this.not_start = false\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n.input-group