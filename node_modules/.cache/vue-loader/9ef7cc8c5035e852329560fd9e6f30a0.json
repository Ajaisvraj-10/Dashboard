  </div>\r\n              \r\n                <div class=\"card p-0 mb-2\" >\r\n                    <div class=\"p-4\">\r\n                      <div class=\"row\">\r\n                        <h2 class=\" col-12 \" style=\"font-size:18px!important;\">\r\n                          <span class=\"d-none\">Name:</span>\r\n                          <i class=\"fa fa-user\"></i>\r\n                          <span class=\"pl-3\">{{farmer.name}}</span>\r\n                        </h2>\r\n                      </div>\r\n                      <hr class=\"my-3\">\r\n                      \r\n                      <div class=\"row details-row\">\r\n                        <div class=\"col-6\" >\r\n                          <h2 class=\"\">Email:</h2>\r\n                          <h2 class=\"\">{{farmer.email}}</h2>\r\n                        </div>\r\n                        <div class=\"col-6\" >\r\n                          <h2 class=\"\">Phone:</h2>\r\n                          <h2 class=\"\">{{farmer.phone}}</h2>\r\n                        </div>\r\n                        <div class=\"col-6\" >\r\n                          <h2 class=\"\">Religion:</h2>\r\n                          <h2 class=\"\">{{farmer.religion}}</h2>\r\n                        </div>\r\n                        <div class=\"col-6\" >\r\n                          <h2 class=\"\">Category:</h2>\r\n                          <h2 class=\"\">{{farmer.category}}</h2>\r\n                        </div>\r\n                        <div class=\"col-6\" >\r\n                          <h2 class=\"\">DOB:</h2>\r\n                          <h2 class=\"\">{{dateFormate(farmer.dob)}}</h2>\r\n                        </div>\r\n                        \r\n                        <div class=\"col-6\">\r\n                          <h2 class=\"\">Gender:</h2>\r\n                          <h2 class=\"\">{{farmer.gender}}</h2>\r\n                        </div>\r\n                        <div class=\"col-12\" >\r\n                          <h2 class=\"\">Aadhaar Number:</h2>\r\n                          <h2 class=\"\">{{farmer.adhaar_number}}</h2>\r\n                        </div>\r\n                        <div class=\"col-12\">\r\n                          <h2 class=\"\">Address:</h2>\r\n                          <h2 class=\"\">{{farmer.house_name}}\r\n                          </h2>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                </div>\r\n              \r\n            </div>\r\n            <div class=\" col-12\">\r\n              <div >\r\n                <h3 class=\" my-3\">MEMBERSHIP INFORMATION </h3>\r\n              </div>\r\n              \r\n              <div class=\"card p-1 mb-2\">\r\n                <div class=\"p-3\">\r\n                  <div class=\"row details-row\">\r\n                    <div class=\"col-6\" >\r\n                      <h2 class=\"  \">FPO:</h2>\r\n                      <h3 class=\" text-blue \">{{farmer.fpo==1?'WOFFA':'KAFFA'}}</h3>\r\n                    </div>\r\n                    <div class=\"col-6\" >\r\n                      <h2 class=\"  \">ICS:</h2>\r\n                      <h2 class=\"\">{{farmer.ics_name}}</h2>\r\n                    </div>\r\n                    <div class=\"col-12\" >\r\n                      <h2 class=\"  \">VFC:</h2>\r\n                      <h2 class=\"\">{{farmer.vfc_name}}</h2>\r\n                    </div>\r\n                    <div class=\"col-6\" >\r\n                      <h2 class=\"  \">Register Date:</h2>\r\n                      <h2 class=\"\">{{dateFormate(farmer.date_register)}}</h2>\r\n                    </div>\r\n                    <div class=\"col-6\" >\r\n                      <h2 class=\"  \">Farmer Code:</h2>\r\n                      <h2 class=\" \">\r\n                        <span class=\"badge badge-success\">{{farmer.farmer_code}}</span>\r\n                      </h2>\r\n                    </div>\r\n                    <div class=\"col-6\" >\r\n                      <h2 class=\"  \">APEDA Code:</h2>\r\n                      <h2 class=\"\">{{farmer.apeda_code}}</h2>\r\n                    </div>\r\n                    <div class=\"col-6\" >\r\n                      <h2 class=\"  \">Fair Trade Code:</h2>\r\n                      <h2 class=\"\">{{farmer.fire_trade_code}}</h2>\r\n                    </div>\r\n                    <div class=\"col-12\" >\r\n                      <h2 class=\"  \">Status:</h2>\r\n                      <h2 class=\"\">\r\n                        <span class=\"badge mr-1\" v-for=\"status in farmer.statuses\" :class=\"status=='NEW'?'badge-warning':status=='ORG'?'badge-success':'badge-info'\">{{status}}</span>\r\n                      </h2>\r\n                    </div>\r\n\r\n                  </div>\r\n                </div>   \r\n              </div>\r\n             \r\n            </div>\r\n            <div class=\"col-12\">\r\n              <div class=\"d-flex align-items-center  justify-content-between\">\r\n                <h3 class=\" my-4\">FARMER LOCATION</h3>\r\n                <router-link :to=\"'/farmer-route-map/?farmer='+farmer.id\"> \r\n                  <b-button variant=\"secondary\" type=\"button\" class=\"d-sm-block mx-1\" style=\"min-height: 25px;border-radius: 5px\">\r\n                    View Map\r\n                  </b-button>\r\n                </router-link>\r\n              </div>\r\n              <div class=\"card p-1\">\r\n                <div class=\"p-3\">\r\n                  <div class=\"row details-row\">\r\n                    <div class=\"col-6\">\r\n                      <h2 class=\"  \">Country:</h2>\r\n                      <h2 class=\"  \">{{farmer.location.country}}</h2>\r\n                    </div>\r\n                    <div class=\"col-6\">\r\n                      <h2 class=\"  \">State:</h2>\r\n                      <h2 class=\"  \">{{farmer.location.state}}</h2>\r\n                    </div>\r\n                    <div class=\"col-6\">\r\n                      <h2 class=\"  \">District:</h2>\r\n                      <h2 class=\"  \">{{farmer.location.district}}</h2>\r\n                    </div>\r\n                    <div class=\"col-6\">\r\n                      <h2 class=\"  \">Panchayath:</h2>\r\n                      <h2 class=\"  \">{{farmer.location.panchayath}}</h2>\r\n                    </div>\r\n                    <div class=\"col-6\">\r\n                      <h2 class=\"  \">Revenue Village:</h2>\r\n                      <h2 class=\"  \">{{farmer.location.revenue_village}}</h2>\r\n                    </div>\r\n                    <div class=\"col-6\">\r\n                      <h2 class=\"  \">Local Village:</h2>\r\n                      <h2 class=\"  \">{{farmer.location.local_village}}</h2>\r\n                    </div>\r\n                    <div class=\"col-6\">\r\n                      <h2 class=\"  \">Post Office:</h2>\r\n                      <h2 class=\"  \">{{farmer.location.post_office}}</h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"col-12\" v-if=\"$root.user_type=='aeo'\">\r\n              <div> \r\n                <h2 class=\" my-3\">FARMER FILES</h2>\r\n              </div>\r\n              <div class=\"\" v-for=\"(farmer_file,key) in farmer_files\">\r\n                <div class=\"card px-3 mb-2\">\r\n                  <div class=\" row mx-0\" v-b-toggle=\"'accordion-file'+key\">\r\n                    <h2 class=\"  col-9 p-0 \">\r\n                      {{farmer_file.file_type}} \r\n                    </h2>\r\n                    <h2 class=\" col-2 text-right mx-0   font-weight-500 \">\r\n                      <span v-if=\"farmer_file.files.length>0\">\r\n                        <i class=\"fas fa-check-circle text-success\"></i></span>\r\n                      <span v-else><i class=\"fas fa-times-circle text-danger\"></i></span>\r\n                    </h2>\r\n                    <h2 class=\"text-15 col-1 mx-0 px-0 font-weight-500 \">\r\n                      <span class=\"\"><i class=\"fa fa-angle-down\"></i></span>\r\n                    </h2>\r\n                  </div>\r\n                  <b-collapse :id=\"'accordion-file'+key\" accordion=\"my-accordion\" role=\"tabpanel\" class=\"mt-3 pt-3 border-top\">\r\n                    <a class=\"card mx-1 px-3\" v-for=\"file in farmer_file.files\" :href=\"file.file\" target=\"_blank\">\r\n                      <div class=\" row mx-0\">\r\n                        <h2 class=\"  col-2 p-0 \">\r\n                          <img src=\"/images/documents.png\" style=\"height:30px\">\r\n                        </h2>\r\n                        <h2 class=\"  font-weight-500 pt-2\">\r\n                          {{farmer_file.file_type}}\r\n                        </h2>\r\n                        <h2 class=\"  font-weight-500 pt-2 \">\r\n                          {{dateFormate(file.created_on)}}\r\n                        </h2>\r\n                      </div>\r\n                    </a>\r\n                    <div class=\"text-center pb-2\" v-if=\"farmer_file.files.length<=0\">\r\n                      <router-link :to=\"'/document/create/?farmer='+farmer.id+'&file_type='+farmer_file.file_type\">\r\n                        <b-button type=\"button\">Upload {{farmer_file.file_type}}</b-button>\r\n                      </router-link>\r\n                    </div>\r\n                  </b-collapse>\r\n                </div>\r\n                \r\n              </div>\r\n            </div>\r\n            <div class=\"col-12\">\r\n              <h2 class=\" my-3\">LAND DETAILS <span class=\"text-success\">{{farmer.lands.year}}</span></h2>\r\n              <div class=\"row\">\r\n                <div class=\"col-6 pr-1\">\r\n                  <div class=\"card p-3 align-items-center\" style=\"gap:4px;\"> \r\n                    <h2 class=\" font-weight-500  \">Total Lands</h2>\r\n                    <div class=\"row \">\r\n                      <h2 class=\" col text-18 text-success  font-weight-800\">{{farmer.lands.total_farming_area?farmer.lands.total_farming_area.toFixed(2):'0.00'}}<small>Ha</small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-6 pl-1\">\r\n                  <div class=\"card p-3 align-items-center\" style=\"gap:4px;\"> \r\n                    <h2 class=\" font-weight-500   \">Organic</h2>\r\n                    <div class=\"row\">\r\n                      <h2 class=\" col text-18 text-success  font-weight-800\">{{farmer.lands.organic_area?farmer.lands.organic_area.toFixed(2):'0.00'}}<small>Ha</small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-6 mt-2 pr-1\">\r\n                  <div class=\"card p-3 align-items-center\" style=\"gap:4px;\">\r\n                    <h2 class=\" font-weight-500   \">Conventional</h2>\r\n                    <div class=\"row\">\r\n                      <h2 class=\" col text-18 text-success  font-weight-800\">{{farmer.lands.conventional_land_area?farmer.lands.conventional_land_area.toFixed(2):'0.00'}}<small>Ha</small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-6 mt-2 pl-1\">\r\n                  <div class=\"card p-3 align-items-center\" style=\"gap:4px;\"> \r\n                    <h2 class=\" font-weight-500   \">Wet Lands</h2>\r\n                    <div class=\"row\">\r\n                      <h2 class=\" col text-18 text-success  font-weight-800\">{{farmer.lands.wet_land?farmer.lands.wet_land.toFixed(2):'0.00'}}<small>Ha</small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-4 mt-2 pr-1\">\r\n                  <div class=\"card p-2 align-items-center\" style=\"gap:4px;\"> \r\n                    <h2 class=\" font-weight-500 text-center\">Total<br>Area IC3</h2>\r\n                    <div class=\"row\">\r\n                      <h2 class=\" col text-18 text-success  font-weight-800\">{{farmer.lands.ic3_land_area?farmer.lands.ic3_land_area.toFixed(2):'0.00'}}<small>Ha</small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-4 mt-2 px-0\">\r\n                  <div class=\"card p-2 align-items-center\" style=\"gap:4px;\"> \r\n                    <h2 class=\" font-weight-500 text-center\">Total<br>Area IC2</h2>\r\n                    <div class=\"row\">\r\n                      <h2 class=\" col text-18 text-success  font-weight-800\">{{farmer.lands.ic2_land_area?farmer.lands.ic2_land_area.toFixed(2):'0.00'}}<small>Ha</small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-4 mt-2 pl-1\">\r\n                  <div class=\"card p-2 align-items-center\" style=\"gap:4px;\"> \r\n                    <h2 class=\" font-weight-500 text-center\">Total<br>Area IC1</h2>\r\n                    <div class=\"row\">\r\n                      <h2 class=\" col text-18 text-success  font-weight-800\">{{farmer.lands.ic1_land_area?farmer.lands.ic1_land_area.toFixed(2):'0.00'}}<small>Ha</small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-6 mt-2 pr-1\">\r\n                  <div class=\"card p-2 align-items-center\" style=\"gap:4px;\"> \r\n                    <h2 class=\" font-weight-500 text-center\">Total Area NEW</h2>\r\n                    <div class=\"row\">\r\n                      <h2 class=\" col text-18 text-success  font-weight-800\">{{farmer.lands.new_land_area?farmer.lands.new_land_area.toFixed(2):'0.00'}}<small>Ha</small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-6 mt-2 pl-1\">\r\n                  <div class=\"card p-2 align-items-center\" style=\"gap:4px;\"> \r\n                    <h2 class=\" font-weight-500 text-center\">Other Lands</h2>\r\n                    <div class=\"row\">\r\n                      <h2 class=\" col text-18 text-success  font-weight-800\">{{farmer.lands.other_land?farmer.lands.other_land.toFixed(2):'0.00'}}<small>Ha</small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-12\">\r\n                  <div v-b-toggle.accordion-4 @click=\"getLands()\">\r\n                    <h2 class=\"text-16 font-weight-500 mt-3  \">LAND HISTORY <i class=\"fa fa-angle-down\"></i></h2>\r\n                  </div>\r\n                  <b-collapse id=\"accordion-4\" accordion=\"my-accordion\" role=\"tabpanel\" class=\"mt-2\">\r\n                    <div class=\"card p-3 mt-2\" v-for=\"(land,lkey) in lands\" v-if=\"lkey!=0\">\r\n                      <div class=\"row m-0\">\r\n                        <h3 class=\"text-16 font-weight-500  \">Year</h3>\r\n                        <h3 class=\"col text-16 text-darkgreen  font-weight-800\">\r\n                          {{land.year}}\r\n                        </h3>\r\n                      </div>\r\n                      <hr class=\"my-2\">\r\n                      <div class=\"row m-0 pb-2 pt-2\">\r\n                        <h4 class=\"text-15 font-weight-500  \">Total Lands</h4>\r\n                        <h4 class=\"col text-15 text-darkgreen  font-weight-800\">{{land.total_farming_area.toFixed(2)}}<small class=\"\">Ha</small></h4>\r\n                      </div>\r\n                      <div class=\"row m-0 pb-2\">\r\n                        <h4 class=\"text-15 font-weight-500  \">Organic Land</h4>\r\n                        <h4 class=\"col text-15 text-darkgreen  font-weight-800\">{{land.organic_area.toFixed(2)}}<small class=\"\">Ha</small></h4>\r\n                      </div>\r\n                      <div class=\"row m-0 pb-2\" v-if=\"land.ic3_land_area>0\">\r\n                        <h4 class=\"text-15 font-weight-500  \">Total Area IC3</h4>\r\n                        <h4 class=\"col text-15 text-darkgreen  font-weight-800\">{{land.ic3_land_area.toFixed(2)}}<small class=\"\">Ha</small></h4>\r\n                      </div>\r\n                      <div class=\"row m-0 pb-2\" v-if=\"land.ic2_land_area>0\">\r\n                        <h4 class=\"text-15 font-weight-500  \">Total Area IC2</h4>\r\n                        <h4 class=\"col text-15 text-darkgreen  font-weight-800\">{{land.ic2_land_area.toFixed(2)}}<small class=\"\">Ha</small></h4>\r\n                      </div>\r\n                      <div class=\"row m-0 pb-2\" v-if=\"land.ic1_land_area>0\">\r\n                        <h4 class=\"text-15 font-weight-500  \">Total Area IC1</h4>\r\n                        <h4 class=\"col text-15 text-darkgreen  font-weight-800\">{{land.ic1_land_area.toFixed(2)}}<small class=\"\">Ha</small></h4>\r\n                      </div>\r\n                      <div class=\"row m-0 pb-2\" v-if=\"land.new_land_area>0\">\r\n                        <h4 class=\"text-15 font-weight-500  \">Total Area NEW</h4>\r\n                        <h4 class=\"col text-15 text-darkgreen  font-weight-800\">{{land.new_land_area.toFixed(2)}}<small class=\"\">Ha</small></h4>\r\n                      </div>\r\n                      <div class=\"row m-0 pb-2\">\r\n                        <h4 class=\"text-15 font-weight-500  \">Conventional Land</h4>\r\n                        <h4 class=\"col text-15 text-darkgreen  font-weight-800\">{{land.conventional_land_area.toFixed(2)}}<small class=\"\">Ha</small></h4>\r\n                      </div>\r\n                      <div class=\"row m-0 \">\r\n                        <h4 class=\"text-15 font-weight-500  \">Other Lands</h4>\r\n                        <h4 class=\"col text-15 text-darkgreen  font-weight-800\">{{land.other_land.toFixed(2)}}<small class=\"\">Ha</small></h4>\r\n                      </div>\r\n                    </div>\r\n                  </b-collapse>\r\n                </div>\r\n              </div> \r\n            </div>\r\n\r\n            <div class=\"col-12\">\r\n              <h2 class=\" my-3\">PLANT DETAILS <span class=\"text-success\">{{farmer.plants.year}}</span></h2>\r\n              <div class=\"row\">\r\n                <div class=\"col-6 pr-1\">\r\n                  <div class=\"card p-3 align-items-center\" style=\"gap:4px;\">\r\n                    <h2 class=\" font-weight-500  \">Coffee Plants</h2>\r\n                    <div class=\"row \">\r\n                      <h2 class=\"col text-18 text-success  font-weight-800\">{{farmer.plants.coffee_plant_number?farmer.plants.coffee_plant_number.toFixed(2):'0.00'}}<small></small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-6 pl-1\">\r\n                  <div class=\"card p-3 align-items-center\" style=\"gap:4px;\">\r\n                    <h2 class=\" font-weight-500   \">Pepper Plants</h2>\r\n                    <div class=\"row\">\r\n                      <h2 class=\"col text-18 text-success  font-weight-800\">{{farmer.plants.pepper_plant_number?farmer.plants.pepper_plant_number.toFixed(2):'0.00'}}<small></small></h2>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-12\">\r\n                  <div v-b-toggle.accordion-5> \r\n                    <h2 class=\" mt-3 \">PLANT&CROP DETAILS <i class=\"fa fa-angle-down\"></i></h2>\r\n                  </div>\r\n                  <b-collapse id=\"accordion-5\" accordion=\"my-accordion\" role=\"tabpanel\" class=\"mt-2\">\r\n                    <div class=\"\" v-for=\"category in categories\">\r\n                      <div class=\"card p-1 mb-2\" v-for=\"(plant,key) in category.plants\" v-if=\"plant.number\">\r\n                        <div class=\"p-3\">\r\n                          <div class=\" row mx-0  pb-2\">\r\n                            <h2 class=\"   p-0 \"><b>{{plant.name}} {{plant.plant_unit}}:</b></h2>\r\n                            <h2 class=\"  text-primary px-2 font-weight-500 \">{{plant.number}}<small>{{plant.plant_unit=='Area'?plant.land_unit:''}}</small></h2>\r\n                          </div>\r\n                          <div class=\"row mx-0 pb-2\">\r\n                            <h2 class=\"  p-0  \">Year:</h2>\r\n                            <h2 class=\"pl-1 font-weight-500 text-info\">{{farmer.plants.year}}</h2>\r\n                          </div>\r\n                          <div class=\"row mx-0 pb-2\" v-for=\"(product,key2) in plant.products\">\r\n                            <h2 class=\"p-0 \">Estimate {{product.product_type_detail.name}}:</h2>\r\n                            <h2 class=\"px-1 font-weight-500 \">{{plant['estimate'+product.product_type+plant.id]?plant['estimate'+product.product_type+plant.id]:0}}KG</h2>\r\n                            <small class=\"px-1\" :class=\"plant.organic_status=='Organic'?'text-success':'text-primary'\">{{plant.organic_status}}</small>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </b-collapse>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"col-12\">\r\n              <h2 class=\" my-3\">OTHER HISTORY</h2>\r\n              <div class=\"row m-0\">\r\n                <div class=\"col pr-1 pl-0\">\r\n                <div class=\"card\" style=\"padding:12px;\"  @click=\"getInspections()\" v-b-toggle.accordion-6>\r\n                  <h2 class=\"text-center font-weight-500  \">Inspection  <i class=\"fa fa-angle-down\"></i></h2>\r\n                </div>\r\n                </div>\r\n                <div class=\"col pr-1 pl-0\">\r\n                <div class=\"card\" style=\"padding:12px;\" @click=\"getSamples()\" v-b-toggle.accordion-7 v-if=\"$root.user_type=='aeo'\">\r\n                  <h2 class=\"text-center font-weight-500  \">Sample  <i class=\"fa fa-angle-down\"></i></h2>\r\n                </div>\r\n                </div>\r\n                <div class=\"col pl-0 pr-0\">\r\n                <div class=\"card\" style=\"padding:12px;\" @click=\"getNormals()\" v-b-toggle.accordion-8 v-if=\"$root.user_type=='aeo'\">\r\n                  <h2 class=\"text-center font-weight-500  \">Normal  <i class=\"fa fa-angle-down\"></i></h2>\r\n                </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"\">\r\n                <b-collapse id=\"accordion-6\" accordion=\"my-accordion\" role=\"tabpanel\" class=\"mt-2\">\r\n                  <h2 class=\" my-3\">PREVIOUS INSPECTIONS </h2>\r\n                  <div class=\"card p-3 mb-1\" v-for=\"(inspection,key) in inspections\">\r\n                    <router-link :to=\"$root.user_type=='aeo'?'/inspection-detail/'+inspection.id:''\">\r\n                      <div class=\"row m-0\">\r\n                        <h3 class=\"text-16 font-weight-500  \">Year</h3>\r\n                        <h3 class=\"m-0 pl-2 text-16 text-darkgreen  font-weight-800\">\r\n                          {{inspection.year}}\r\n                        </h3>\r\n                      </div>\r\n                      <hr class=\"my-2\">\r\n                      <div class=\"row m-0\">\r\n                        <h4 class=\"text-15 pb-2 font-weight-500  \">Inspector:</h4>\r\n                        <h4 class=\"m-0 col text-15  font-weight-800\">\r\n                          {{inspection.internal_inspector}}\r\n                        </h4>\r\n                      </div>\r\n                      <div class=\"row m-0\">\r\n                        <h4 class=\"text-15 pb-2 font-weight-500  \">Date of Inspection:</h4>\r\n                        <h4 class=\"m-0 col text-15  font-weight-800\">\r\n                          {{dateFormate(inspection.date_of_inspection)}}\r\n                        </h4>\r\n                      </div>\r\n                      <div class=\"row m-0\">\r\n                        <h4 class=\"text-15 font-weight-500  \">Status:</h4>\r\n                        <h4 class=\"m-0 col text-15  font-weight-800\">\r\n                          <span class=\"badge\" :class=\"inspection.status=='Completed'?'badge-success':'badge-danger'\">{{inspection.status}}</span>\r\n                        </h4>\r\n                      </div>\r\n                    </router-link>\r\n                  </div>\r\n                  <div class=\"text-center pb-3\" v-if=\"inspections.length<=0\">\r\n                    {{is_loading?'Fetching...':'Empty Results!'}}\r\n                  </div>\r\n                </b-collapse>\r\n                <b-collapse id=\"accordion-7\" accordion=\"my-accordion\" role=\"tabpanel\" class=\"mt-2\">\r\n                  <h2 class=\" my-4\">SAMPLE PROCUREMENTS </h2>\r\n                  <div class=\"card p-3 mb-1\" v-for=\"(sample,key) in samples\">\r\n                    <router-link :to=\"'/sample-procurement-detail/'+sample.id\">\r\n                      <div class=\"row m-0\">\r\n                        <h4 class=\"font-weight-800\">\r\n                          {{sample.category}}/{{sample.product}}/{{sample.product_type}}\r\n                          <span class=\"badge badge-success\" v-if=\"sample.farmer_status=='Organic'\">ORG</span>\r\n                        </h4>\r\n                        <h4 class=\"font-weight-800 px-4 py-1 \" style=\"margin-right:-22px;\">{{dateFormate(sample.created_on)}}</h4>\r\n                      </div>\r\n                      <div class=\"hr-min\"></div>\r\n                      <div class=\"row m-0\">\r\n                        <h6 class=\" font-weight-500 text-success\">#{{sample.id}}/Sample  </h6>\r\n                        <h6 class=\"m-0 font-weight-800 pl-2 badge\" :class=\"sample.qc_status=='Waiting'?'badge-warning':sample.qc_status=='Approved'?'badge-success':sample.qc_status=='Rejected'?'badge-danger':'badge-primary'\">{{sample.qc_status}}</h6>\r\n                      </div>\r\n                    </router-link>\r\n                  </div>\r\n                  <div class=\"text-center pb-3\" v-if=\"samples.length<=0\">\r\n                    {{is_loading?'Fetching...':'Empty Results!'}}\r\n                  </div>\r\n                </b-collapse>\r\n                <b-collapse id=\"accordion-8\" accordion=\"my-accordion\" role=\"tabpanel\" class=\"mt-2\">\r\n                  <h2 class=\" my-4\">NORMAL PROCUREMENTS </h2>\r\n                  <div class=\"card p-3 mb-1\" v-for=\"(normal,key) in normals\">\r\n                    <router-link :to=\"'/normal-procurement-detail/'+normal.id\">\r\n                      <div class=\"row m-0\">\r\n                        <h4 class=\"font-weight-800\">\r\n                          {{normal.category}}/{{normal.product}}/{{normal.product_type}}\r\n                          <span class=\"badge badge-success\" v-if=\"normal.farmer_status=='Organic'\">ORG</span>\r\n                        </h4>\r\n                        <h4 class=\"font-weight-800 px-4  \" style=\"margin-right:-22px;\">{{dateFormate(normal.created_on)}}</h4>\r\n                      </div>\r\n                      <div class=\"hr-min\"></div>\r\n                      <div class=\"row m-0\">\r\n                        <h6 class=\" font-weight-500 \">Vehicle: <b>{{normal.vehicle_detail.vehicle_type}}</b>/ {{normal.vehicle_detail.name}}</h6>\r\n                        <h6 class=\"m-0 font-weight-800 pl-2  \">{{normal.vehicle_detail.vehicle_number}}</h6>\r\n                      </div>\r\n                      <div class=\"row m-0 \">\r\n                        <h6 class=\" font-weight-500 text-info\">{{normal.raw_number}}  </h6>\r\n                        <h6 class=\"m-0 font-weight-800 pl-2 badge\" :class=\"normal.status=='Collecting'?'badge-warning':normal.status=='On the Way'?'badge-primary':normal.status=='Qc Rejected'?'badge-danger':'badge-success'\">{{normal.status}}</h6>\r\n                      </div>\r\n                    </router-link>\r\n                  </div>\r\n                  <div class=\"text-center pb-3\" v-if=\"normals.length<=0\">\r\n                    {{is_loading?'Fetching...':'Empty Results!'}}\r\n                  </div>\r\n                </b-collapse>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          \r\n          \r\n          <div class=\"mt-3 mb-5 pb-3\">\r\n            <div>\r\n              <h2 class=\" my-3\">QUICK ACTIONS</h2>\r\n            </div>\r\n            <div class=\"row\" v-if=\"$root.user_type=='aeo'\">\r\n              <div class=\"col-6 pr-1\">\r\n                <router-link :to=\"'/inspection/create/?farmer='+farmer.id\">\r\n                  <button type=\"button\" class=\"quick-btns btn btn-light btn-lg btn-block text-18 font-weight-800  \">\r\n                    <img src=\"/images/inception.png\" >\r\n                  Inspection</button>\r\n                </router-link> \r\n              </div>\r\n              <div class=\"col-6 pl-1\">\r\n                <router-link :to=\"'/sample-procurement/create/?farmer='+farmer.id\">\r\n                  <button type=\"button\" class=\"quick-btns btn btn-light btn-lg btn-block text-18 font-weight-800  \">\r\n                    <img src=\"/images/sample.png\" >\r\n                  Sample</button>\r\n                </router-link> \r\n              </div>\r\n              <div class=\"col-6 pr-1 pt-2\">\r\n                <router-link :to=\"'/document/create/?farmer='+farmer.id\">\r\n                  <button type=\"button\" class=\"quick-btns btn btn-light btn-lg btn-block text-18 font-weight-800  \">\r\n                    <img src=\"/images/documents.png\" >\r\n                  Document</button>\r\n                </router-link> \r\n              </div>\r\n              <div class=\"col-6 pl-1 pt-2\">\r\n                <router-link :to=\"'/farmer-location?farmer='+farmer.id\">\r\n                  <button type=\"button\" class=\"quick-btns btn btn-light btn-lg btn-block text-18 font-weight-800  \">\r\n                    <img src=\"/images/location.png\" >\r\n                  Location</button>\r\n                </router-link> \r\n              </div>\r\n            </div> \r\n            <div class=\"row mx-2\" v-else>\r\n              <div class=\"col-12 px-1\">\r\n                <router-link :to=\"'/inspection/create/?farmer='+farmer.id\">\r\n                  <button type=\"button\" class=\"quick-btns btn btn-light btn-lg btn-block text-18 font-weight-800  \">\r\n                    <img src=\"/images/inception.png\" >\r\n                  Inspection</button>\r\n                </router-link> \r\n              </div>\r\n            </div> \r\n          </div>\r\n        </div>\r\n\r\n      <div class=\"filter-footer\">\r\n        <div class=\"footer-item2 d-flex align-items-center\" @click=\"$router.push('/')\">\r\n          <img src=\"/images/home.png\" class=\"\" width=\"25\">\r\n          <h5 class=\"m-0 pl-2 text-white\">Home</h5>\r\n        </div>\r\n      </div>\r\n    </main>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Farmer\"\r\n  },\r\n  data() {\r\n    return {\r\n      id:0,\r\n      not_start:true,\r\n      farmer:{\r\n        lands:{total_farming_area:0,organic_area:0,conventional_land_area:0, other_land: 0,wet_land: 0},\r\n        plants:{coffee_plant_number:0,pepper_plant_number:0},\r\n        location:{},\r\n      },\r\n      categories:[],\r\n      lands:[],\r\n      inspections:[],\r\n      samples:[],\r\n      normals:[],\r\n      farmer_files:[],\r\n      is_loading:true,\r\n    };\r\n  },\r\n  mounted(){\r\n    this.id = this.$route.params.id\r\n    this.getFarmer()\r\n  },\r\n  methods: {\r\n    getFarmer() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/employee/farmers/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.not_start =false\r\n        this.farmer = jsonData\r\n        this.getDocs()\r\n        this.getPlants()\r\n      })\r\n    },\r\n    getLands() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/land_details/?farmer='+this.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lands = jsonData\r\n      })\r\n    },\r\n    getDocs() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmer_files/?farmer='+this.id+'&category_wise=true', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmer_files = jsonData\r\n      })\r\n    },\r\n    getInspections() {\r\n      this.is_loading = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/inspections/?farmer='+this.id+'&history=true', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.is_loading = false\r\n        this.inspections = jsonData\r\n      })\r\n    },\r\n    getSamples() {\r\n      this.is_loading = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/employee/sample_procurement/?farmer='+this.id+'&history=true', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.is_loading = false\r\n        this.samples = jsonData\r\n      })\r\n    },\r\n    getNormals() {\r\n      this.is_loading = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token)\r\n      fetch(this.api_url+'/employee/normal_procured/?farmer='+this.id+'&history=true', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.is_loading = false\r\n        this.normals = jsonData\r\n      })\r\n    },\r\n    getPlants() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/plant_categories/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var self = this\r\n        jsonData.forEach(function(category,key1){\r\n          category.plants.forEach(function(plant,key2){\r\n            jsonData[key1].plants[key2]['number'] = ''\r\n            plant.products.forEach(function(product,key3){\r\n              jsonData[key1].plants[key2]['estimate'+product.product_type+plant.id] = ''\r\n              jsonData[key1].plants[key2]['land_unit'] = 'Number'\r\n              if(plant.plant_unit=='Area'){\r\n                jsonData[key1].plants[key2]['land_unit'] = 'Hectare'\r\n              }\r\n              self.setEstimate(plant,product)\r\n            })\r\n          })\r\n        })\r\n        this.categories = jsonData\r\n      })\r\n    },\r\n    setEstimate(plant,product){\r\n      if(this.farmer.plants.id){\r\n        var plants = this.farmer.plants.plants\r\n        plants.forEach(function(val){\r\n          if(product.id == val.product && val.organic_status==plant.organic_status){\r\n            plant['number'] = val.number\r\n            plant['estimate'+product.product_type+plant.id] = val.estimate\r\n            plant['land_unit'] = val.land_unit\r\n          }\r\n        })\r\n      }\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n.input-group-append{\r\n  padding-top: 0;\r\n  padding-bottom: 0;\r\n}\r\n.quick-btns{\r\n  display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    gap: 6px;\r\n    flex-direction: column;\r\n    /* height:44px; */\r\n    background-color: #fff;\r\n    color: #000;\r\n    padding:15px 5px;\r\n}\r\n.quick-btns img{\r\n  height:30px;\r\n}\r\n\r\n.farmer-details h2{\r\n  font-size:14.5px;\r\n  color:#3e3e3e;\r\n  line-height: 22px;\r\n}\r\n\r\n.farmer-details h3{\r\n  font-size:16px;\r\n  font-weight:600;\r\n}\r\n</style>\r\n"]}]}