area>\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n                        <div class=\" col-md-5\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input my-4\">\r\n                              <span style=\"width:120px;\"> Foul Odour: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"foul_odour\">\r\n                                <option value=\"\" selected disabled>Select an option</option>\r\n                                <option v-for=\"yn in yon\" :value=\"yn\" :selected=\"yn==form_data.foul_odour\">{{yn}}</option>\r\n                              </select>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input my-4\">\r\n                              <span style=\"width:120px;\"> Mould Infestation: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"mould_infestation\">\r\n                                <option value=\"\" selected disabled>Select an option</option>\r\n                                <option v-for=\"yn in yon\" :value=\"yn\" :selected=\"yn==form_data.mould_infestation\">{{yn}}</option>\r\n                              </select>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> AA Grade(%): </span>\r\n                              <textarea placeholder=\"Type here..\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" name=\"AA_grade\">{{form_data.AA_grade}}</textarea>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> AB Grade(%) : </span>\r\n                              <textarea placeholder=\"Type here..\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" name=\"AB_grade\">{{form_data.AB_grade}}</textarea>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input mt-5\">\r\n                              <span style=\"width:120px;\"> Percentage of Polish: </span>\r\n                              <input placeholder=\"Percentage of Polish\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.percentage_of_polish\" name=\"percentage_of_polish\">\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Moisture of Beans(%)  : </span>\r\n                              <input placeholder=\"Moisture of Beans\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.moisture_of_beans\" name=\"moisture_of_beans\">\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n                        <div class=\" col-md-1\"></div>\r\n                        <div class=\" col-md-1\"></div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2 mt-5\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Remarks : </span>\r\n                              <textarea placeholder=\"Write here..\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" name=\"remarks\">{{form_data.remarks}}</textarea>\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{date:new Date()},\r\n      id:null,\r\n      load_products:false,\r\n      product_id:null,\r\n      action:'create',\r\n      batchs:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'qc'\r\n    this.form_data.date = this.currentDate()\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n    }\r\n    this.getBatchs()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/inprocess_test/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_id = jsonData.product[0]\r\n        this.form_data = jsonData\r\n        this.load_products = true\r\n      })\r\n    },\r\n    getLots() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/batch_creation/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.batchs = jsonData\r\n      })\r\n    },\r\n    selectBatch() {\r\n      this.load_products = false\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/batch_creation/'+this.form_data.batch+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.batch_detail = jsonData\r\n        this.product_id = jsonData.product\r\n        this.load_products = true\r\n      })\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.form_data.product = id\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/inprocess_test/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/inprocess_test/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}