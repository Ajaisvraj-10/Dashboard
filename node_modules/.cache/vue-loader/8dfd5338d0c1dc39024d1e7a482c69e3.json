\n              </b-button></router-link>\r\n              <router-link tag=\"a\" class to=\"/app/oodo/import\"><b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Import\r\n              </b-button></router-link>\r\n            </div> -->\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: true,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: true,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows\"\r\n      >\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'weigh_bridge'\">\r\n            <b-button type=\"button\" variant=\"primary\" @click=\"selectLoad(props.row,'weigh_bridge')\" class=\" px-3 py-1 mx-1\">Weigh Bridge</b-button>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'unloading'\">\r\n            <b-button type=\"button\" variant=\"primary\" @click=\"selectLoad(props.row,'unloading')\" class=\" px-3 py-1 mx-1\" v-if=\"props.row.status=='Completed'\">Completed</b-button>\r\n            <b-button type=\"button\" variant=\"danger\" @click=\"selectLoad(props.row,'unloading')\" class=\" px-3 py-1 mx-1\" v-else-if=\"props.row.status!='Draft'\">Unloading</b-button>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'department'\">\r\n            {{props.row.product_detail.department}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'product'\">\r\n            {{props.row.product_detail.name}}\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n    <div>\r\n      <b-modal id=\"unloading-modal\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n           <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">{{slugToWord(action)}}</span></h5>\r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                <b-button type=\"button\" @click=\"dataForm()\" variant=\"success\" class=\" px-3 py-1 d-sm-block mx-1\">Save</b-button>\r\n                <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1 d-sm-block mx-1\" @click=\"$bvModal.hide('unloading-modal')\">Close</b-button>\r\n              </div>\r\n            </div>\r\n            <div class=\" my-3 border-top\"></div>\r\n        </div>\r\n        <b-row class=\"main-scroll\">\r\n          <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n            <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n              <b-card header-bg-variant=\"transparent\" class=\"mt-2 pt-0\" v-show=\"action=='weigh_bridge'\">\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\">Wb Slip : </span>\r\n                  <input placeholder=\" Wb Slip\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.Wb_slip\" name=\"Wb_slip\" required>\r\n                </div>\r\n                <div class=\"o_thread_search_input\" v-show=\"form_data.id\">\r\n                  <span style=\"width:120px;\"> Initial Wb (Kg): </span>\r\n                  <input placeholder=\"Initial Wb\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.initial_Wb\" name=\"initial_Wb\" required>\r\n                </div>\r\n                <div class=\"o_thread_search_input\" v-show=\"form_data.id\">\r\n                  <span style=\"width:120px;\"> Net Weight Wb (Kg): </span>\r\n                  <input placeholder=\"Initial Wb\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.net_weight_WB\" name=\"net_weight_WB\" @input=\"form_data.final_Wb=form_data.initial_Wb-form_data.net_weight_WB\" required>\r\n                </div>\r\n                <div class=\"o_thread_search_input\" v-if=\"form_data.id\">\r\n                  <span style=\"width:120px;\">Final Wb (Kg): </span>\r\n                  <input placeholder=\"Final Wb\" type=\"number\" min=\"0\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.final_Wb\" name=\"final_Wb\" required>\r\n                </div>\r\n                <hr class=\"my-2\">\r\n              </b-card>\r\n              <b-card header-bg-variant=\"transparent\" class=\"mt-2 pt-0\" v-show=\"action=='unloading'\">\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\"> Workers : </span>\r\n\r\n                  <div class=\"mx-1 workers_box\">\r\n                    <label class=\"workers_label\" v-for=\"worker in workers\" :for=\"'worker'+worker.id\" v-if=\"worker.is_contract\">\r\n                      <b-form-checkbox name=\"workers\" v-model=\"form_data.workers\" :id=\"'worker'+worker.id\" :value=\"worker.id\" @input=\"form_data.total_workers=form_data.workers.length\">\r\n                        <b>#{{worker.id}}/{{worker.name}}</b>\r\n                      </b-form-checkbox>\r\n                    </label>\r\n                  </div>\r\n                </div>\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\">Number of Bags : </span>\r\n                  <input placeholder=\"Number of Bags\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.number_of_bags\" name=\"number_of_bags\" required>\r\n                </div>\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\">Bag Weight (Kg) (1 bag): </span>\r\n                  <input placeholder=\" Bag Weight\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.bag_weight\" name=\"bag_weight\" required>\r\n                </div>\r\n                <div class=\"o_thread_search_input mt-2\">\r\n                  <span style=\"width:120px;\">Issues Observed: </span>\r\n                   <ol class=\"pl-4\" style=\"list-style: none;\">\r\n                      <li v-for=\"issue in form_data.issues_observed\"><input type=\"checkbox\" :value=\"issue.value\" class=\"mr-2\" :id=\"issue.value\" v-model=\"issue.checked\"><label :for=\"issue.value\">{{issue.value}}</label></li>\r\n                   </ol>\r\n                </div>\r\n              </b-card>\r\n            </form>\r\n          </b-col>\r\n        </b-row>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Vehicle Number\",\r\n          field: \"vehicle_number\"\r\n        },\r\n        {\r\n          label: \"Type\",\r\n          field: \"purchase_type\"\r\n        },\r\n        {\r\n          label: \"Department\",\r\n          field: \"department\"\r\n        },\r\n        {\r\n          label: \"Product\",\r\n          field: \"product\"\r\n        },\r\n        {\r\n          label: \"WB slip\",\r\n          field: \"Wb_slip\"\r\n        },\r\n        {\r\n          label: \"Total Bags\",\r\n          field: \"number_of_bags\"\r\n        },\r\n        {\r\n          label: \"Weigh Bridge\",\r\n          field: \"weigh_bridge\"\r\n        },\r\n        {\r\n          label: \"Unloading\",\r\n          field: \"unloading\"\r\n        },\r\n       \r\n      ],\r\n      rows: [],\r\n      form_data:{issues_observed:[\r\n        {value:'moister', checked:false},\r\n        {value:'fu', checked:false},\r\n        {value:'bag weight', checked:false},\r\n        {value:'coffee food hg', checked:false},\r\n        {value:'ineefs intent', checked:false},\r\n      ]},\r\n      id:null,\r\n      action:'weight_bridge',\r\n      workers:[],\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'unloading'\r\n    this.getUnloading()\r\n    this.getWorkers()\r\n  },\r\n  methods: {\r\n    getUnloading() {\r\n      this.form_data = {issues_observed:[\r\n        {value:'moister', checked:false},\r\n        {value:'fu', checked:false},\r\n        {value:'bag weight', checked:false},\r\n        {value:'coffee food hg', checked:false},\r\n        {value:'ineefs intent', checked:false},\r\n      ]}\r\n      this.id = null\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/unloading/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n    getWorkers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.workers = jsonData\r\n      })\r\n    },\r\n    selectLoad(unload,action){\r\n      this.action = action\r\n      this.form_data = unload\r\n      if(this.form_data.id!=this.id && this.form_data.issues_observed){\r\n        this.form_data.issues_observed = JSON.parse(this.form_data.issues_observed)\r\n      }\r\n      this.form_data.initial_Wb = unload.total_weight\r\n      this.id = unload.id\r\n      this.$bvModal.show('unloading-modal')\r\n    },\r\n    dataForm() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('issues_observed',JSON.stringify(this.form_data.issues_observed))\r\n      formdata.append('warehouse',this.form_data.warehouse)\r\n      if(this.form_data.status=='Draft'){\r\n        formdata.append('status','Unloading')\r\n      }else if(this.form_data.status=='Unloading' && this.action=='unloading'){\r\n        formdata.append('status','Completed')\r\n      }\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/unloading/'+this.form_data.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.$bvModal.hide('unloading-modal')\r\n          if(this.form_data.transfer && this.action=='unloading'){\r\n            this.transferItemStock()\r\n          }else{\r\n            this.getUnloading()\r\n            this.showAlert('Saved Successfully!')\r\n          }\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    transferItemStock() {\r\n      var formdata = new FormData()\r\n      formdata.append('transfer_id',this.form_data.transfer)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/transfer_item_stock/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.getUnloading()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}