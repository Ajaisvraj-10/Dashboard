;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;;;;;AAMA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;;;;;;;;AASA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"sign-in.vue","sourceRoot":"src/views/app/video_tv","sourcesContent":["\r\n<template>\r\n  <div class=\"main-content capcee-login\">\r\n    \r\n    <div class=\"site-wrapper\" v-if=\"start\"> \r\n        <img src=\"/video_tv/images/bg1.jpg\" class=\"signin-bg\">\r\n        \r\n        <div class=\"login-am signin-forms\" v-if=\"crnt_tab=='login'\">\r\n              \r\n            <div id=\"signInBorder\" class=\"signin-2\">\r\n              <router-link :to=\"edit?'':{name:'home'}\">\r\n                <i class=\"fa fa-close text-gray\" style=\"position:absolute;top:5%;right:5%;\"></i>\r\n              </router-link>\r\n              <div class=\"text-center\">\r\n                \r\n                <h4 class=\" text-18 font-weight-500 mb-4\" style=\"line-height:20px;\" @click=\"selectContent(blocks[1])\" :class=\"blocks[1].is_edit?'selected_content':''\" :id=\"'blocks_'+blocks[1].id\" v-show=\"blocks[1].status\">\r\n                  {{blocks[1].value}}\r\n                </h4>\r\n                \r\n              </div>\r\n\r\n              <form ref=\"loginUser\" @submit.prevent=\"loginUser()\">\r\n                <label>\r\n                  \r\n                  <input type=\"email\" placeholder=\"Emailo ID or Mobile Number\" name=\"email\" value=\"\" required>\r\n                </label>\r\n                <br>\r\n                <label>\r\n                  \r\n                  <input type=\"password\" placeholder=\"Password\" name=\"password\" required>\r\n                </label>\r\n                <br>\r\n                <div class=\"text-center mt-1 mb-2\">\r\n                  \r\n                    <span><a  style=\"color: #fff;\">Forgot password?</a></span>\r\n                  \r\n                </div>\r\n                <div>\r\n                  <button type=\"submit\" id=\"capcee\" >Login</button>\r\n                </div>\r\n              </form>\r\n             \r\n              <div class=\"text-center my-3\">\r\n                <span class=\"text-13 text-white mx-1\">New to capcee?</span>\r\n                <span class=\"text-13 cursor-pointer mx-1\" style=\"color: #fff;\" @click=\"crnt_tab='register'\">Register</span>\r\n\r\n                \r\n              </div>\r\n               <!--<div class=\"d-flex justify-content-center my-4\" style=\"gap:20px;\">\r\n                  <h6 class=\"text-10 text-white font-weight-600\" style=\"width: 35px;\r\n                    height: 35px;\r\n                    border-radius: 50%;\r\n                    display: flex;\r\n                    align-items: center;\r\n                    justify-content: center;\r\n                    background-color: #192b45;\">\r\n\r\n                    Or\r\n                  </h6>\r\n                  \r\n                </div>\r\n              <div class=\"d-flex justify-content-center my-3\" style=\"gap:20px;\">\r\n                  <img src=\"/video_tv/images/apple.svg\" style=\"width:40px;\">\r\n                  <img src=\"/video_tv/images/google.svg\" style=\"width:40px;\">\r\n                  <img src=\"/video_tv/images/fb.svg\" style=\"width:40px;\">\r\n                  <img src=\"/video_tv/images/twitter.svg\" style=\"width:40px;\">\r\n                </div>-->\r\n               \r\n\r\n\r\n\r\n            </div>\r\n        </div>\r\n        <!--<div class=\"otp-am signin-forms\" v-if=\"crnt_tab=='otp'\">\r\n             \r\n            <div id=\"signInBorder\" class=\"signin-2\">\r\n                <router-link :to=\"edit?'':{name:'home'}\">\r\n                  <i class=\"fa fa-close text-gray\" style=\"position:absolute;top:5%;right:5%;\"></i>\r\n                </router-link>\r\n                <div class=\"mb-3\">\r\n                  <p id=\"SignInTxt\" class=\"mb-3\">\r\n                    Two-Step Verification\r\n                  </p>\r\n                  <h6 class=\"text-white fontweight-400  text-13\">\r\n\r\n                    For added security, please enter the One Time Password (OTP) that has been sent to a phone number ending in 320\r\n                  </h6>\r\n              </div>\r\n\r\n              <label>\r\n                <strong>Enter OTP</strong>\r\n               \r\n                <input type=\"email\" name=\"email\" value=\"\" class=\"mb-1\">\r\n              </label>\r\n\r\n              <div class=\"flex-align3 text-12 mb-1 text-light font-weight-400\">\r\n                   <input type=\"checkbox\" class=\" m-0\" style=\"width:initial;\">\r\n                   <h6 class=\"text-12 text-light px-2 font-weight-500\">Don't require OTP on this browser</h6>\r\n              </div>\r\n              <div>\r\n                <router-link to=\"main\">\r\n                    <button id=\"capcee\">Sign in</button>\r\n                </router-link>\r\n              </div>\r\n              <div class=\"my-2\">\r\n                <h4 class=\"text-12 text-dark2 font-weight-500\" style=\"line-height:18px;\">\r\n                  <a class=\"text-info\"> Didn't receive the OTP?</a>\r\n                </h4>\r\n                \r\n              </div>\r\n             \r\n            </div>\r\n        </div> -->\r\n        <div class=\"signin-am signin-forms\" v-if=\"crnt_tab=='register'\">\r\n            <div id=\"signInBorder\" class=\"signin-2\">\r\n              <router-link :to=\"edit?'':{name:'home'}\">\r\n                <i class=\"fa fa-close text-gray\" style=\"position:absolute;top:5%;right:5%;\"></i>\r\n              </router-link>\r\n              <div class=\"text-center\">\r\n                <p id=\"SignInTxt\" class=\"mb-3\">\r\n                  Create a new account\r\n                </p>\r\n                \r\n                \r\n              </div>\r\n\r\n              <form ref=\"registerUser\" @submit.prevent=\"registerUser()\">\r\n                <label>\r\n                  \r\n                  <input type=\"text\" placeholder=\"Your Name\" name=\"username\" value=\"\" required>\r\n                </label>\r\n                <label>\r\n                  \r\n                  <input type=\"email\" placeholder=\"Email ID\" name=\"email\" value=\"\" required>\r\n                </label>\r\n                <br>\r\n                <label>\r\n                 \r\n                  <input type=\"password\" placeholder=\"Password\" name=\"password1\" required>\r\n                </label>\r\n                <br>\r\n                <label>\r\n                  \r\n                  <input type=\"password\" placeholder=\"Confirm Password\" name=\"password\" required>\r\n                </label>\r\n                <br>\r\n                <!--<div class=\"text-center  my-3\">\r\n                  \r\n                    <span class=\"text-13 text-white\">By proceeding you agree to our <a  style=\"color: #fff;\">Terms of service & privacy policy </a></span>\r\n                  \r\n                </div>-->\r\n                <div>\r\n                    <button id=\"capcee\" type=\"submit\">Register</button>\r\n                </div>\r\n              </form>\r\n             \r\n              <div class=\"text-center my-3\">\r\n                <span class=\"text-13 text-white mx-1\">Already registered?</span>\r\n                <span class=\"text-13 cursor-pointer mx-1\" style=\"color: #fff;\" @click=\"crnt_tab='login'\">Login</span>\r\n\r\n                \r\n              </div>\r\n             \r\n              <div class=\" my-3\" >\r\n                  \r\n                  <button  class=\" w-100 d-flex align-items-center justify-content-center\" style=\"border:1px solid #fff;cursor:pointer;border-radius:5px;background-color:initial;color:#fff;height:44px;\">\r\n                   \r\n                    Sign in with Google\r\n                  </button>\r\n                  \r\n                </div>\r\n                \r\n\r\n\r\n\r\n            </div>\r\n        </div>\r\n\r\n\r\n    </div>\r\n    \r\n    \r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  props: ['edit','blocks','current_block'],\r\n  data() {\r\n    return {\r\n      start:false,\r\n      current_page:{},\r\n      menu_click:false,\r\n      crnt_tab:\"login\",\r\n    };\r\n  },\r\n  mounted(){\r\n    if(this.edit){\r\n      this.start = true\r\n    }else{\r\n        this.edit=false\r\n        this.blocks = [{},{},{}]\r\n        this.getPage()\r\n    }\r\n  },\r\n  methods: {\r\n    registerUser() {\r\n      var formdata = new FormData(this.$refs['registerUser'])\r\n      fetch(this.api_url+'/users/users/', {\r\n          method : 'post',\r\n          body : formdata\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.is_active){\r\n          this.loginUser(true)\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    loginUser(new_user=false) {\r\n      if(new_user){\r\n        var formdata = new FormData(this.$refs['registerUser'])\r\n      }else{\r\n        var formdata = new FormData(this.$refs['loginUser'])\r\n      }\r\n      fetch(this.api_url+'/users/obtain_token/', {\r\n          method : 'post',\r\n          body : formdata\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.access){\r\n          localStorage.setItem(\"userInfo\", JSON.stringify(jsonData));\r\n          this.$root.token = jsonData.access\r\n          if(this.$root.token==''){\r\n              localStorage.removeItem(\"userInfo\");\r\n          }else{\r\n            window.location.href = '/';\r\n          }\r\n        }else{\r\n          this.showAlert(jsonData.detail)\r\n            localStorage.removeItem(\"userInfo\");\r\n        }\r\n      })\r\n    },\r\n    getPage() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/content/pages_contents/?theme='+theme_name+'&page=/sign-in', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.current_page = jsonData\r\n        this.blocks = jsonData.blocks\r\n        this.start = true\r\n      })\r\n    },\r\n    selectContent(block){\r\n        if(this.edit){\r\n            this.blocks.forEach(function(blk){\r\n                blk.is_edit = false\r\n            })\r\n            block.is_edit=true\r\n            this.$emit('selectedContent', block)\r\n        }\r\n    },\r\n  }\r\n\r\n};\r\n</script>\r\n<style scoped>\r\n  \r\n  @import '/video_tv/style/style.css';\r\n\r\n</style>\r\n<style>\r\n\r\n#capcee{\r\n  width:350px;\r\n  height:44px;\r\n  font-size:14px;\r\n  color:#fff;\r\n \r\n  border: 1px solid #de146d;\r\n  border-radius:5px;\r\n  margin-top: 10px;\r\n  cursor:pointer;\r\n  background: linear-gradient(270deg,#de146d,#d21f49);\r\n  font-weight:600;\r\n}\r\n\r\n\r\n.login-logo{\r\n    width:150px!important;\r\n}\r\n\r\n\r\n\r\n\r\n#signInBorder{\r\n \r\n  margin: 0 auto;\r\n \r\n  width: 390px;\r\n  padding: 20px;\r\n    background-color: #070d0e;\r\n    border-radius:6px;\r\n    position:relative;\r\n\r\n}\r\n\r\n.capcee-login input{\r\n  border:none;\r\n  border-bottom: 2px solid #757575!important;\r\n  background-color:initial;\r\n  border-radius: 0px;\r\n  border-width: 0.2px;\r\n  height: 35px;\r\n  width: 350px;\r\n  margin-bottom: 15px;\r\n  font-size:12px;\r\n  color:white;\r\n  padding:10px 0;\r\n    margin-top:5px;\r\n\r\n}\r\n\r\n.capcee-login input:focus{\r\n  -webkit-box-shadow: 0px 0px 1.5px 1.5px rgba(231,118,0,0.9);\r\n  -moz-box-shadow: 0px 0px 1.5px 1.5px rgba(231,118,0,0.9);\r\n  box-shadow: 0px 0px 1.5px 1.5px rgba(231,118,0,0.9);\r\n  outline:0;\r\n}\r\n\r\n\r\n#SignInTxt, #blocks_9{\r\n\r\n  font-size: 16px;\r\n  color:white;\r\n  font-weight:600;\r\n\r\n}\r\n.capcee-login h6{\r\n  \r\n    font-size:13px;\r\n    color:black;\r\n\r\n\r\n}\r\n.capcee-login a{\r\n    font-weight:600;\r\n    font-size:12px;\r\n\r\n    \r\n}\r\n.capcee-login h2 {\r\n   color: rgb(134, 134, 134);\r\n   font-weight: normal;\r\n   letter-spacing: 1px;\r\n   font-size: 11.5px;\r\n   width: 350px;\r\n   text-align: center;\r\n   border-top: 1px solid rgba(0, 0, 0, 0.1);\r\n   border-bottom: 1px solid rgba(255, 255, 255, 0.3);\r\n   line-height: 0.1em;\r\n   margin: 10px 0 20px;\r\n   margin-bottom: 5px;\r\n}\r\n\r\n.capcee-login h2 span {\r\n    background:#fff;\r\n    padding:0 10px;\r\n}\r\n\r\n#createAccount{\r\n  margin-top: 30px;\r\n}\r\n\r\n#newAccount{\r\n  background: linear-gradient(#f6f7f9, #e7e9ec);\r\n  margin-top: 10px;\r\n  margin-bottom: 20px;\r\n  width:350px;\r\n  height:33px;\r\n  font-size:14px;\r\n  border: 0.5px solid #aaaaaa;\r\n  border-radius:3px;\r\n}\r\n\r\n#newAccount:hover{\r\n  background: linear-gradient(#f5f6f8, #d9dce1);\r\n}\r\n\r\n.capcee-login #footer{\r\n  border: 0;\r\n   height: 0;\r\n   border-top: 1px solid rgba(0, 0, 0, 0.1);\r\n   border-bottom: 1px solid rgba(255, 255, 255, 0.3);\r\n   margin-top: 30px;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n .links{\r\n  font-size: 10px;\r\n  text-align: center;\r\n  padding-left: 15px;\r\n}\r\n\r\n\r\n#special{\r\n  font-size: 11px;\r\n}\r\n.signin-bg{\r\n  width:100%!important;\r\n  height:100vh!important;\r\n  filter: blur(5px);\r\n  opacity:.5;\r\n  object-fit:cover;\r\n}\r\n.signin-forms{\r\n  position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n}\r\n.capcee-login .site-wrapper {\r\n    \r\n       height:100vh;\r\n       overflow:auto;\r\n     \r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: flex-start;\r\n    align-items:center;\r\n    //filter: blur(8px);\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n@media screen and (max-width: 420px) {\r\n   #signInBorder {\r\n    \r\n    width: auto;\r\n    padding: 20px;\r\n   background-color: initial;\r\n    border-radius: 0px;\r\n \r\n    }\r\n    .signin-bg{\r\n    display:none;\r\n    }\r\n    .capcee-login input {\r\n\r\n    width: 330px;\r\n    }\r\n    #capcee {\r\n    width: 330px;\r\n    }\r\n}\r\n</style>\r\n"]}]}