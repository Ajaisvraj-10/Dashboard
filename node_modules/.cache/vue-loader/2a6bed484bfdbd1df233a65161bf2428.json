AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA","file":"indent-product-form.vue","sourceRoot":"src/views/app/oodo/logistics-department","sourcesContent":["<template>\r\n  <div class=\"main-content create-main\">\r\n    <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n      <div>\r\n         <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Indent</span>/ Product/ {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <b-button @click=\"goBack()\" type=\"button\"variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Discard\r\n                </b-button>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n\r\n        <b-col lg=\"4\" xl=\"4\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\" col-md-12\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Indent Number : </span>\r\n                              <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"'#'+form_data.main_indent+'/Indent'\" readonly>\r\n                              <input type=\"hidden\" name=\"main_indent\" v-model=\"form_data.main_indent\" readonly>\r\n                            </div>\r\n\r\n                            <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n                            <!--<div class=\"o_thread_search_input mt-2\">\r\n                              <span style=\"width:130px\">  Have parameter?: </span>\r\n                              <b-form-checkbox v-model=\"form_data.is_parameters\" name=\"is_parameters\" switch>\r\n                                <b>{{ form_data.is_parameters?'Yes':'No' }}</b>\r\n                              </b-form-checkbox>\r\n                            </div> -->\r\n                            <div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Parameter : </span>\r\n                                <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"parameter\" v-model=\"form_data.parameter\" required>\r\n                                    <option v-for=\"parameter in parameters\" :value=\"parameter.id\">{{parameter.name}}</option>\r\n                                </select>\r\n                              </div>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Quantity (Kg) : </span>\r\n                              <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.quantity\" name=\"quantity\" required>\r\n                            </div>\r\n                            <hr class=\"my-2\">\r\n                            <div class=\"mx-2 px-1\">\r\n                              <div style=\"width:100%;\">How to pack?: </div>\r\n                              <textarea placeholder=\"Packing Note\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" v-model=\"form_data.packing_note\" name=\"packing_note\" rows=\"7\"></textarea>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n        <b-col lg=\"8\" xl=\"8\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-4\">\r\n                    <h5>Packing Materials ({{form_data.values.length}})</h5>\r\n                  </div>\r\n                  <div class=\"col-md-8 text-right\">\r\n                    <b-button variant=\"success\" type=\"button\" class=\"px-3 py-1 mx-1\" @click=\"lineOperation('+')\" style=\"margin-top:-10px\">\r\n                      Packing material special request\r\n                    </b-button>\r\n                    <b-button variant=\"primary\" type=\"button\" class=\"px-3 py-1 mx-1\" @click=\"getPackingMaterialsList()\" style=\"margin-top:-10px\">\r\n                      Packing material regular\r\n                    </b-button>\r\n                    <input type=\"hidden\" name=\"keys\" :value=\"form_data.values.length\">\r\n                  </div>\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\" style=\"overflow-x:auto\">\r\n                        <div class=\"col-md-6 px-2\" v-for=\"(line,key) in form_data.values\">\r\n                          <div class=\"card p-2 my-1\">\r\n                            <fieldset :disabled=\"line.print_indent.id\">\r\n                              <div class=\"o_thread_search_input px-0\">\r\n                                <select class=\"ui-autocomplete-input w-100 ml-0\" v-model=\"line.material_detail\" @change=\"line.material=line.material_detail.id\">\r\n                                  <option v-for=\"material in materials\" :value=\"material\">{{material.name}}</option>\r\n                                </select>\r\n                                <input type=\"hidden\" :name=\"'material'+key\" v-model=\"line.material\">\r\n                                <div class=\"pb-3\" v-if=\"!line.id\">\r\n                                  <b-button variant=\"danger\" type=\"button\" class=\"ml-2 px-3 py-1 d-sm-block\" @click=\"lineOperation(key)\">\r\n                                    Remove\r\n                                  </b-button>\r\n                                </div>\r\n                                <div class=\"pb-3\" v-else>\r\n                                  <input type=\"hidden\" :name=\"'id'+key\" v-model=\"line.id\">\r\n                                </div>\r\n                              </div>\r\n                              <table class=\"table material_table mb-0\">\r\n                                <tbody>\r\n                                  <tr v-for=\"(specification,key2) in line.specifications\" v-if=\"specification.material_ids.includes(line.material)\">\r\n                                    <th>{{specification.name}}</th>\r\n                                    <td>\r\n                                      <div>\r\n                                        <select class=\"ui-autocomplete-input w-100 mx-1\" :name=\"specification.id+'value'+key\" v-model=\"specification.value\" v-if=\"specification.values\" @change=\"checkMaterialStock(line)\">\r\n                                          <option v-for=\"value in specification.values\" :value=\"value.value\">{{value.value}}</option>\r\n                                        </select>\r\n                                        <div v-else><input type=\"checkbox\" :name=\"specification.id+'value'+key\" v-model=\"specification.value\" :id=\"'check'+key\" @change=\"checkMaterialStock(line)\"> <label :for=\"'check'+key\"> YES</label></div>\r\n                                      </div>\r\n                                    </td>\r\n                                  </tr>\r\n                                  <tr>\r\n                                  <tr>\r\n                                    <th>Quantity (number)</th>\r\n                                    <td>\r\n                                      <input placeholder=\"QTY\" type=\"text\" class=\"\" autocomplete=\"off\" v-model=\"line.requested_quantity\" :name=\"'requested_quantity'+key\" @input=\"calcultion()\" required>\r\n                                    </td>\r\n                                  </tr>\r\n                                </tbody>\r\n                              </table>\r\n                            </fieldset>\r\n                            <table class=\"table material_table mb-0\">\r\n                                <tr>\r\n                                  <th> \r\n                                    Stock:\r\n                                    <input placeholder=\"QTY\" type=\"hidden\" class=\"\" autocomplete=\"off\" :value=\"checkMaterialStock(line)\" readonly>\r\n                                  </th>\r\n                                  <td>\r\n                                    <span v-if=\"line.stock_quantity>0 && line.id\">\r\n                                      <b-button variant=\"primary\" type=\"button\" class=\"ml-2 px-3 py-1 d-sm-block\" @click=\"openPrintModal(line)\" v-if=\"line.print_indent.id\">\r\n                                        Print (Requested)\r\n                                      </b-button>\r\n                                      <b-button variant=\"primary\" type=\"button\" class=\"ml-2 px-3 py-1 d-sm-block\" @click=\"openPrintModal(line)\" v-else-if=\"line.material_detail.is_printing\">\r\n                                        Print Request\r\n                                      </b-button>\r\n                                      <span class=\"text-success\" v-else-if=\"line.stock_quantity>0\">Stock is available!</span>\r\n                                    </span>\r\n                                    <span class=\"text-danger\" v-else-if=\"line.stock_quantity<=0\">Stock is empty!</span>\r\n                                    <span class=\"text-success\" v-else>Stock is available!</span>\r\n                                  </td>\r\n                                </tr>\r\n                              </tbody>\r\n                            </table>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n\r\n    <div>\r\n      <b-modal id=\"print-indent\" hide-header  hide-footer>\r\n        <form ref=\"printIndent\" @submit.prevent=\"printIndent()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Print Indent</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Print\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('print-indent')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Indent: </span>\r\n                            <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"'#'+form_data.main_indent+'/Indent'\" readonly>\r\n                            <input type=\"hidden\" name=\"indent_packing\" v-model=\"print_indent.indent_packing\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Packing Material: </span>\r\n                            <input placeholder=\"Packing Material\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"print_indent.material_detail.name\" readonly>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input my-3\">\r\n                            <span style=\"width:130px;\">Side : </span>\r\n                            <b-form-radio name=\"side\" v-model=\"print_indent.side\" :value=\"1\">\r\n                              <b>1 Side</b>\r\n                            </b-form-radio>\r\n                            <span style=\"width:20px;\"> </span>\r\n                            <b-form-radio name=\"side\" v-model=\"print_indent.side\" :value=\"2\">\r\n                              <b>2 Side</b>\r\n                            </b-form-radio>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">ICO Number: </span>\r\n                            <input placeholder=\"ICO Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"ICO_number\" v-model=\"print_indent.ICO_number\" required>\r\n                          </div>\r\n\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n      <b-modal id=\"select-material\" hide-header  hide-footer>\r\n        <div>\r\n           <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Select packing material</span></h5>\r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('select-material')\">\r\n                  Discard\r\n                </b-button>\r\n              </div>\r\n            </div>\r\n            <div class=\" my-3 border-top\"></div>\r\n        </div>\r\n        <!-- <div class=\"wrapper\"> -->\r\n        <b-row class=\"main-scroll\">\r\n          <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n            <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n              <div>\r\n                <div class=\"ul-contact-detail__timeline\">\r\n                   <div class=\"row\">\r\n                      <div class=\"col-md-4 pt-1\" style=\"overflow-y:auto;height:72vh\" v-for=\"material in packing_materials_lists\">\r\n                        <b-card  header-bg-variant=\"transparent\" class=\"mb-2\">\r\n                          <div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:90px;\">Material : </span>\r\n                              <input placeholder=\"Material\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"material.material_detail.name\" readonly>\r\n                            </div>\r\n                            <div class=\"\">\r\n                              <span>Specifications:</span>\r\n                              <table class=\"pt-2\">\r\n                                <tr v-for=\"specification in material.specifications\" v-if=\"specification.material_ids.includes(material.material)\">\r\n                                  <th>{{specification.name}}</th>\r\n                                  <th>:</th>\r\n                                  <td>{{specification.value}}</td>\r\n                                </tr>\r\n                              </table>\r\n                            </div>\r\n                            <div class=\"text-center pt-2\">\r\n                              <b-button variant=\"primary\" class=\"w-50 py-1\" @click=\"addToMaterial(material)\">Select</b-button>\r\n                            </div>\r\n                          </div>\r\n                        </b-card>\r\n                      </div>\r\n                   </div>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n          </b-col>\r\n        </b-row>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{id:null,parameter:null, parameter_detail:{}, expected_date:'2022-03',total:0,values:[],total_quantity:0, indent_number:'',is_parameters:false,},\r\n      id:null,\r\n      action:'create',\r\n      product_id:null,\r\n      load_products:false,\r\n      customers:[],\r\n      parameters:[],\r\n      materials:[],\r\n      specifications:[],\r\n      packing_materials:[],\r\n      packing_materials_lists:[],\r\n      print_indent:{material_detail:{}},\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'logistics-department'\r\n    this.action = this.$route.params.action\r\n    this.form_data.main_indent = this.$route.params.main_indent\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n    }\r\n    this.getPackingMaterials()\r\n    this.getCustomers()\r\n    this.getMaterials()\r\n    this.getSpecifications()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.product_id = jsonData.product\r\n        this.load_products = true\r\n        this.getParameters()\r\n      })\r\n    },\r\n    getCustomers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/customers/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.customers = jsonData\r\n      })\r\n    },\r\n    getMaterials() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/materials/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.materials = jsonData\r\n      })\r\n    },\r\n    getSpecifications() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/specifications/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.specifications = jsonData\r\n      })\r\n    },\r\n    lineOperation(operation){\r\n      if(operation=='+'){\r\n        var specification_values = [];\r\n        this.specifications.forEach(function(val,key){\r\n          // if(val.specification_for=='Packing Material'){\r\n          // }\r\n          specification_values.push({id:val.id, name:val.name ,value:'', is_values:val.is_values, values:val.values,material_ids:val.material_ids})\r\n        })\r\n        this.form_data.values.push({material:this.materials[0].id, requested_quantity:0, price:0,max_quantity:0,stock_quantity:0, print_indent:{id:null}, specifications:specification_values, material_detail:this.materials[0]})\r\n      }else{\r\n        this.form_data.values.splice(operation, 1)\r\n      }\r\n      this.calcultion()\r\n    },\r\n    addToMaterial(material){\r\n      material['print_indent'] = {id:null}\r\n      material.id = null\r\n      this.form_data.values.push(material)\r\n      this.$bvModal.hide('select-material')\r\n    },\r\n    getPackingMaterials(){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_stock/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.packing_materials = jsonData\r\n      })\r\n    },\r\n    getPackingMaterialsList(){\r\n      this.$bvModal.show('select-material')\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase_materials/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.packing_materials_lists = jsonData\r\n      })\r\n    },\r\n    checkMaterialStock(material){\r\n      var packing_materials = this.packing_materials\r\n      var quantity = 0\r\n      packing_materials.forEach(function(p1){\r\n        var is_pack = true\r\n        if(material.material==p1.material){\r\n          p1.specifications.forEach(function(s2){\r\n            var is_specs = true\r\n            if(p1.material_detail.specifications.includes(s2.id)){\r\n              var spec = true\r\n              material.specifications.forEach(function(s1){\r\n                if(s1.material_ids.includes(material.material)){\r\n                  if(s2.id==s1.id){\r\n                    if(s2.value==s1.value || (s1.values==null && s2.value=='on' && s1.value)){\r\n                    }else{\r\n                      spec = false\r\n                    }\r\n                  }\r\n                }\r\n              })\r\n              if(!spec){\r\n                is_specs = false\r\n              }\r\n            }\r\n            if(!is_specs){\r\n              is_pack = false\r\n            }\r\n          })\r\n        }else{\r\n          is_pack = false\r\n        }\r\n        if(is_pack){\r\n          quantity = quantity+p1.quantity\r\n        }\r\n      })\r\n      material.stock_quantity = quantity\r\n      return quantity\r\n    },\r\n    calcultion(){\r\n      var quantity = 0\r\n      var price = 0\r\n      this.form_data.values.forEach(function(val,key){\r\n        quantity = quantity + parseFloat(val.requested_quantity)\r\n      })\r\n      this.form_data.total_quantity = quantity\r\n    },\r\n    autoIndentNumber(){\r\n      var dateTime = new Date()\r\n      return dateTime.getTime()\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.getParameters()\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameters/?product='+this.product_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.parameters = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.id){\r\n        this.updateFormdata()\r\n      }else{\r\n        this.createFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.id = jsonData.id\r\n          this.form_data.product_detail = jsonData.product_detail\r\n          this.indentPackingDetails()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('product',this.product_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.indentPackingDetails()\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    indentPackingDetails() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('department',this.form_data.product_detail.category)\r\n      formdata.append('indent',this.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent_packing_details/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.getForm()\r\n          this.getPackingMaterials()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    printIndent(method='POST') {\r\n      var param=''\r\n      if(this.print_indent.id){\r\n        method='PUT'\r\n        param = this.print_indent.id+'/'\r\n      }\r\n      var formdata = new FormData(this.$refs['printIndent'])\r\n      formdata.append('indent',this.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/print_indent/'+param, {\r\n          method : method,\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.$bvModal.hide('print-indent')\r\n          this.getForm()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    openPrintModal(line){\r\n      this.print_indent=line.print_indent;\r\n      if(!this.print_indent.id){\r\n        this.print_indent.indent_packing=line.id;\r\n        this.print_indent.material_detail=line.material_detail;\r\n      }\r\n      this.$bvModal.show('print-indent')\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n<style>\r\n.despatch label{\r\n  font-weight: 700;\r\n  white-space: nowrap;\r\n    align-self: center;\r\n    margin-right: 10px;\r\n    min-width:140px;\r\n}\r\n.unloading .btn-secondary{\r\n  width:100%;\r\n  \r\n    border: 2px solid #efefef;\r\n    background-color: #f8f9fa!important;\r\n    border-radius: 2px;\r\n    text-align: left;\r\n    height: calc(1.9695rem + 2px);\r\n    padding: 5px;\r\n}\r\n\r\n   .table-3 th {\r\n    border: 1px solid #dcdfe6;\r\n    background-color: #f3f4f7;\r\n}\r\n   .table-3 th, .table-3 td {\r\n    border: 1px solid #dcdfe6;\r\n}\r\n  .table-3 th {\r\n    padding:0.75em 1.5em 0.75em 0.75em;\r\n  }\r\n.table-3 .tr-bottom td:nth-child(2){\r\n  border: 2px solid #dcdfe6;\r\n background-color: #f3f4f7;\r\n  \r\n}\r\n.table-3 .tr-bottom td:nth-child(1){\r\nborder: none;\r\n  \r\n}\r\n</style>\r\n"]}]}