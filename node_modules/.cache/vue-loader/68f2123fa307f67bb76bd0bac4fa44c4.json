ass=\" px-3 py-1  d-sm-block mx-1\" @click=\"form_data={};$bvModal.show('yard-form')\">+ Create\r\n              </b-button>\r\n              <router-link tag=\"a\" class to=\"/app/oodo/import\"><b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Import\r\n              </b-button></router-link>\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: true,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: true,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows\"\r\n      >\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <b-button  variant=\"danger\" class=\" px-3 py-1  mx-1\" @click=\"form_data=props.row;$bvModal.show('yard-form')\">Edit</b-button>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'no'\">\r\n            #{{props.row.id}}/Yard\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'status'\">\r\n              <span class=\"badge\" :class=\"props.row.status?'badge-success':'badge-danger'\">{{props.row.status?'Active':'Deactive'}}</span>\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n    <div>\r\n      <b-modal id=\"yard-form\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n          <div class=\"main-content create-main\">\r\n            <form ref=\"dataForm\" @submit.prevent=\"yardFormdata()\">\r\n              <div>\r\n                <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                    <slot name=\"header\">\r\n                       <h5 class=\"mb-0\"><span class=\"text-success\">Yard</span> / {{form_data.id?'Edit':'Create'}}</h5>\r\n                          \r\n                    </slot>\r\n                    <div class=\"d-flex\">\r\n                      <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\">Save\r\n                      </b-button>\r\n                      <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('yard-form')\"\r\n                        >Discard\r\n                      </b-button>\r\n                    </div>\r\n                   \r\n                </div>\r\n                <div class=\" my-3 border-top\"></div>\r\n              </div>\r\n\r\n              <b-row class=\"main-scroll\">\r\n                <b-col lg=\"12\" xl=\"12\" class=\"my-3\">\r\n                  <div>\r\n                    <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:90px;\"> Name: </span>\r\n                        <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" v-model=\"form_data.name\" name=\"name\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:90px;\"> Own type: </span>\r\n                        <select class=\"ui-autocomplete-input w-100 ml-1\" name=\"own_type\" v-model=\"form_data.own_type\">\r\n                          <option v-for=\"own_type in ['Rent','Own']\" :value=\"own_type\"> {{own_type}}</option>\r\n                        </select>\r\n                      </div>\r\n                      <div v-if=\"form_data.own_type=='Rent'\">\r\n                        <div class=\"o_thread_search_input\">\r\n                          <span style=\"width:90px;\"> Rent: </span>\r\n                          <button class=\"btn btn-white append_btn_left\" type=\"button\">Days</button>\r\n                          <input placeholder=\"Days\" type=\"number\" class=\"ui-autocomplete-input append_btn_input_left\" autocomplete=\"off\" v-model=\"form_data.days\" name=\"days\" style=\"margin-right:10px\">\r\n                          <button class=\"btn btn-white append_btn_left\" type=\"button\">Price</button>\r\n                          <input placeholder=\"Price\" type=\"number\" class=\"ui-autocomplete-input append_btn_input_left\" autocomplete=\"off\" v-model=\"form_data.price\" name=\"price\">\r\n                        </div>\r\n                        <div class=\"o_thread_search_input\">\r\n                          <span style=\"width:90px;\"> Rent (validity): </span>\r\n                          <input placeholder=\"Date\" type=\"date\" class=\"ui-autocomplete-input\" v-model=\"form_data.validity\" name=\"validity\" required>\r\n                        </div>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:90px;\"> Max Sheets: </span>\r\n                        <input placeholder=\"Max Sheets\" type=\"number\" class=\"ui-autocomplete-input\" v-model=\"form_data.max_boxes\" name=\"max_boxes\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input mt-2\">\r\n                        <b style=\"width:96px\"> Status: </b>\r\n                        <b-form-checkbox v-model=\"form_data.status\" name=\"status\" switch>\r\n                          <b>{{ form_data.status?'Active':'Deactive' }}</b>\r\n                        </b-form-checkbox>\r\n                      </div>\r\n                    </b-card>\r\n                  </div>\r\n                </b-col>\r\n              </b-row>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"Yards\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{},\r\n      columns: [\r\n        {\r\n          label: \"No\",\r\n          field: \"no\"\r\n        },\r\n        {\r\n          label: \"Name\",\r\n          field: \"name\"\r\n        },\r\n        {\r\n          label: \"Type\",\r\n          field: \"own_type\"\r\n        },\r\n        {\r\n          label: \"Max Sheets\",\r\n          field: \"max_boxes\"\r\n        },\r\n        {\r\n          label: \"Status\",\r\n          field: \"status\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: [\r\n       \r\n      ]\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'production'\r\n    this.getYards()\r\n  },\r\n  methods: {\r\n    getYards() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/yards/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n    yardFormdata(method='POST') {\r\n      var param = ''\r\n      if(this.form_data.id){\r\n        param = this.form_data.id+'/'\r\n        method = 'PUT'\r\n      }\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/yards/'+param, {\r\n          method : method,\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.getYards()\r\n          this.$bvModal.hide('yard-form')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n  #yard-form.modal.show .modal-dialog{\r\n    min-width: 550px!important;\r\n  }\r\n  #yard-form.modal.show .modal-dialog{\r\n    min-width: 500px!important;\r\n  }\r\n</style>\r\n"]}]}