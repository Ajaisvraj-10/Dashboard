           <td>\r\n                                  {{vehicle.vehicle_number}}\r\n                                </td>\r\n                                <td>\r\n                                  {{vehicle.name}}\r\n                                </td>\r\n                                <td>\r\n                                  {{vehicle.contact}}\r\n                                </td>\r\n                                <td>\r\n                                  {{vehicle.vehicle_type}}\r\n                                </td>\r\n                                <td>\r\n                                  <button type=\"button\" class=\"btn btn-primary btn-sm px-3 py-1 ml-4\" @click=\"chooseVehicle(vehicle)\">Select</button>\r\n                                </td>\r\n                              </tr>\r\n                            </tbody>\r\n                          </table>\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n         {\r\n          label: \"Id\",\r\n          field: \"current_status\"\r\n        },\r\n        {\r\n          label: \"Product\",\r\n          field: \"product\"\r\n        },\r\n        {\r\n          label: \"Quantity (Kg)\",\r\n          field: \"total_available_qty\"\r\n        },\r\n        {\r\n          label: \"Vendor\",\r\n          field: \"vendor\"\r\n        },\r\n        {\r\n          label: \"Date\",\r\n          field: \"created_on\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: [],\r\n      current_row:[],\r\n      vehicles:[],\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'production'\r\n    this.getRows()\r\n  },\r\n  methods: {\r\n    getRows() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/?purchase_type=2&status=Collecting', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n    selectMaterial(row){\r\n      this.current_row = row\r\n      this.$bvModal.show('choose-vehicle')\r\n      this.getVehicles()\r\n    },\r\n    getVehicles() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/vehicle/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.vehicles = jsonData\r\n      })\r\n    },\r\n    chooseVehicle(vehicle){\r\n      this.$bvModal.hide('choose-vehicle')\r\n      \r\n      var formdata = new FormData()\r\n      formdata.append('vehicle', vehicle.id)\r\n      formdata.append('normal_procurement_id', this.current_row.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/attach_vehicle_to_normal/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.getRows()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}