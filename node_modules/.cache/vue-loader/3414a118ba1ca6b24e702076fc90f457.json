CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"direct-machine-out-packing-form.vue","sourceRoot":"src/views/app/oodo/production","sourcesContent":["<template>\r\n  <div class=\"main-content create-main\">\r\n    <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n      <div>\r\n         <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">{{slugToWord('direct-machine-out-packing')}}</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/production/direct-machine-out-packing\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n\r\n        <b-col lg=\"4\" xl=\"4\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\" col-md-12\">\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Select Indent : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"indent\" v-model=\"form_data.indent\" @change=\"getIndent()\">\r\n                                <option v-for=\"indent in indents\" :value=\"indent.id\">{{indent.indent_number}}</option>\r\n                              </select>\r\n                            </div>\r\n                            <div v-if=\"form_data.indent\">\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Indent Number : </span>\r\n                                <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"indent_number\" v-model=\"form_data.indent_detail.indent_number\" readonly>\r\n                              </div>\r\n                              <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Parameter : </span>\r\n                                <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"parameter\" v-model=\"form_data.indent_detail.parameter\" required>\r\n                                    <option v-for=\"parameter in parameters\" :value=\"parameter.id\">{{parameter.name}}</option>\r\n                                </select>\r\n                              </div>\r\n\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Quantity (Kg) : </span>\r\n                                <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.indent_detail.quantity\" name=\"quantity\" required>\r\n                              </div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">PO Number : </span>\r\n                                <input placeholder=\"PO Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.indent_detail.PO_number\" name=\"PO_number\" required>\r\n                              </div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\"> Expected Date: </span>\r\n                                <input placeholder=\"Expected Date\" type=\"month\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.indent_detail.expected_date\" name=\"expected_date\" required>\r\n                              </div>  \r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\"> Remark : </span>\r\n                                <textarea placeholder=\"Remark\" rows=\"5\" class=\"ui-autocomplete-input w-100 mx-1\" autocomplete=\"off\" v-model=\"form_data.indent_detail.remark\" name=\"remark\"></textarea>\r\n                                <input type=\"hidden\" name=\"product\" :value=\"product_id\" v-if=\"id\">\r\n                              </div>\r\n                            </div>\r\n                          </div>\r\n                          <hr class=\"my-2\"/>\r\n                          <div>\r\n                            <h5>Requested Packing Materials</h5>\r\n                            <table class=\"table\">\r\n                              <tr>\r\n                                <th>Material</th>\r\n                                <th>Specs</th>\r\n                                <th>Quantity(Kg)</th>\r\n                              </tr>\r\n                              <tr v-for=\"packing_material in form_data.indent_detail.values\" :for=\"'packing_material'+packing_material.id\">\r\n                                <td>{{packing_material.material_detail.name}}</td>\r\n                                <td>\r\n                                  <small v-for=\"specification in packing_material.specifications\" v-if=\"specification.material_ids.includes(packing_material.material)\">\r\n                                    <b class=\"text-primary\"> {{specification.name}}</b>:{{specification.value}}, \r\n                                  </small>\r\n                                </td>\r\n                                <td>\r\n                                  {{packing_material.quantity}}\r\n                                </td>\r\n                              </tr>\r\n                            </table>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n        <b-col lg=\"8\" xl=\"8\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <h5>Packed Raws</h5>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <table class=\"table\">\r\n                  <tr>\r\n                    <th>Product</th>\r\n                    <th>Lot</th>\r\n                    <th>Quantity(Kg)</th>\r\n                    <th>Action</th>\r\n                  </tr>\r\n                  <tr v-for=\"raw in form_data.machine_out_raws\">\r\n                    <td>{{raw.parameter}}</td>\r\n                    <td>\r\n                      #Lot{{raw.process_output__process__lot_id}}\r\n                    </td>\r\n                    <td>\r\n                      {{raw.available_quantity}}\r\n                    </td>\r\n                          <td>\r\n                            <button type=\"button\" class=\"btn btn-sm py-1 px-2 btn-warning\" @click=\"$bvModal.show('packing-form'+raw.id)\" v-if=\"raw.packing_materials_count>0\">\r\n                              Packed ({{raw.packing_materials_count}})\r\n                            </button>\r\n                            <button type=\"button\" class=\"btn btn-sm py-1 px-2 btn-primary\" @click=\"$bvModal.show('packing-form'+raw.id)\" v-else>\r\n                              Packing\r\n                            </button>\r\n                            <div v-for=\"packing_material in raw.packing_materials\" v-if=\"raw.quantity>0\">\r\n                              <input type=\"hidden\" v-model=\"packing_material.packing_quantity\" :name=\"'packing_quantity'+packing_material.id+'parameter'+raw.id\">\r\n                              <input type=\"hidden\" :name=\"'is_added'+packing_material.id+'parameter'+raw.id\" :value=\"packing_material.is_added?'yes':'no'\">\r\n                              <input type=\"hidden\" :name=\"'number_of_bags'+packing_material.id+'parameter'+raw.id\" :value=\"packing_material.number_of_bags\">\r\n                            </div>\r\n                            <b-modal class=\"packing-form\" :id=\"'packing-form'+raw.id\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n                              <div>\r\n                                <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                                  <slot name=\"header\">\r\n                                     <h5 class=\"mb-0\">Packing/<span class=\"text-success\">{{parameter_detail.name}}</span></h5>\r\n                                  </slot>\r\n                                  <div class=\"d-flex\">\r\n                                    <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('packing-form'+raw.id)\">Save</b-button>\r\n                                  </div>\r\n                                </div>\r\n                                <div class=\" my-3 border-top\"></div>\r\n                              </div>\r\n                              <b-row class=\"main-scroll\">\r\n                                <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n                                  <b-card header-bg-variant=\"transparent\" class=\"mt-2\">\r\n                                    <div class=\"user-info pt-2\">\r\n                                      <div class=\"o_thread_search_input\">\r\n                                        <table class=\"table\">\r\n                                          <tr>\r\n                                            <th>Material</th>\r\n                                            <th>Specs</th>\r\n                                            <th>Lot</th>\r\n                                            <th>Quantity</th>\r\n                                            <th>Action</th>\r\n                                          </tr>\r\n                                          <tr v-for=\"packing_material in raw.packing_materials\" :for=\"'packing_material'+raw.id\" v-if=\"packing_material.quantity>0\">\r\n                                            <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">{{packing_material.material_detail.name}}</td>\r\n                                            <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">\r\n                                              <small v-for=\"specification in packing_material.specifications\" v-if=\"specification.material_ids.includes(packing_material.material)\">\r\n                                                <b class=\"text-primary\"> {{specification.name}}</b>:{{specification.value}}, \r\n                                              </small>\r\n                                            </td>\r\n                                            <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">#Lot{{packing_material.packing_material_purchase}}</td>\r\n                                            <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">\r\n                                              <input type=\"number\" class=\"ui-autocomplete-input w-100\" :max=\"packing_material.quantity\" v-model=\"packing_material.packing_quantity\" :disabled=\"!packing_material.is_added\" @change=\"changePackingMaterial(packing_material)\">\r\n                                              <input type=\"hidden\" :value=\"packing_material.is_added?'yes':'no'\">\r\n                                              <small v-if=\"packing_material.packing_quantity>0\">Number of bags: <b>{{packing_material.number_of_bags}}</b></small>\r\n                                              <input type=\"hidden\" :value=\"packing_material.number_of_bags\">\r\n                                            </td>\r\n                                            <td>\r\n                                              <button type=\"button\" class=\"btn btn-primary btn-sm px-3 py-1 ml-4\" @click=\"addMaterial(raw,packing_material)\" v-if=\"!packing_material.is_added\">Add</button>\r\n                                              <button type=\"button\" class=\"btn btn-danger btn-sm px-3 py-1 ml-4\" @click=\"removeMaterial(raw,packing_material)\" v-else>Remove</button>\r\n                                            </td>\r\n                                          </tr>\r\n                                        </table>\r\n                                      </div>\r\n                                    </div>\r\n                                  </b-card>\r\n                                </b-col>\r\n                              </b-row>\r\n                            </b-modal>\r\n                          </td>\r\n                  </tr>\r\n                </table>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{indent:null, indent_detail:{parameter:null, expected_date:'2022-03', total:0, values:[], total_quantity:0, indent_number:'', is_parameters:false}, machine_out_raws:[]},\r\n      id:null,\r\n      action:'create',\r\n      product_id:null,\r\n      load_products:false,\r\n      parameters:[],\r\n      indents:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n    this.getIndents()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.product_id = jsonData.product\r\n        this.load_products = true\r\n        this.getParameters()\r\n      })\r\n    },\r\n    getIndent() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+this.form_data.indent+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.indent_detail = jsonData\r\n        this.product_id = jsonData.product\r\n        this.load_products = true\r\n        this.getParameters()\r\n        this.getMachineOutProducts()\r\n      })\r\n    },\r\n    getIndents() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.indents = jsonData\r\n      })\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.getParameters()\r\n    },\r\n    getMachineOutProducts() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/packing/?parameter='+this.form_data.indent_detail.parameter+'&direct_packing=out', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.machine_out_raws = jsonData\r\n      })\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameters/?product='+this.product_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.parameters = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.id = jsonData.id\r\n          this.form_data.product_detail = jsonData.product_detail\r\n          this.indentPackingDetails()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.indentPackingDetails()\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    indentPackingDetails() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('department',this.form_data.product_detail.category)\r\n      formdata.append('indent',this.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent_packing_details/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n<style>\r\n.despatch label{\r\n  font-weight: 700;\r\n  white-space: nowrap;\r\n    align-self: center;\r\n    margin-right: 10px;\r\n    min-width:140px;\r\n}\r\n.unloading .btn-secondary{\r\n  width:100%;\r\n  \r\n    border: 2px solid #efefef;\r\n    background-color: #f8f9fa!important;\r\n    border-radius: 2px;\r\n    text-align: left;\r\n    height: calc(1.9695rem + 2px);\r\n    padding: 5px;\r\n}\r\n\r\n   .table-3 th {\r\n    border: 1px solid #dcdfe6;\r\n    background-color: #f3f4f7;\r\n}\r\n   .table-3 th, .table-3 td {\r\n    border: 1px solid #dcdfe6;\r\n}\r\n  .table-3 th {\r\n    padding:0.75em 1.5em 0.75em 0.75em;\r\n  }\r\n.table-3 .tr-bottom td:nth-child(2){\r\n  border: 2px solid #dcdfe6;\r\n background-color: #f3f4f7;\r\n  \r\n}\r\n.table-3 .tr-bottom td:nth-child(1){\r\nborder: none;\r\n  \r\n}\r\n</style>\r\n"]}]}