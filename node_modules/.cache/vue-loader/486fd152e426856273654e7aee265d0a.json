     var farmer_code = this.form_data.farmer_detail.farmer_code\r\n      if(farmer_code.length<4){\r\n        return false\r\n      }\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      formdata.append('farmer_code',farmer_code)\r\n      formdata.append('search_type','sample_test')\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/search_farmers/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmers = jsonData\r\n      })\r\n    },\r\n    selectFarmer(farmer=null){\r\n      if(farmer){\r\n        this.form_data.farmer_detail.farmer_code = farmer.farmer_code\r\n        this.form_data.farmer_detail.name = farmer.name\r\n        this.form_data.farmer = farmer.id\r\n      }\r\n      this.farmers = []\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_procurement/?farmer_id='+this.form_data.farmer, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.samples = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('purchase_type',this.purchase_types[0].id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_materials_test/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.updateStatus()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('purchase_type',this.purchase_types[0].id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_materials_test/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.updateStatus()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/physical_parameters/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var length = jsonData.length/3\r\n        this.form_data.parameters = this.pagination(jsonData,Math.round(length))\r\n      })\r\n    },\r\n    checkParam(param){\r\n      if(param.limit_type=='<'){\r\n        if(param.limit2 == 0){\r\n          if(param.limit < param.limit1){\r\n\r\n          }\r\n        }else{\r\n          if(param.limit > param.limit1 && param.limit < param.limit2){\r\n\r\n          }\r\n        }\r\n      }else if(param.limit_type=='>'){\r\n        if(param.limit2 == 0){\r\n          if(param.limit > param.limit1){\r\n\r\n          }\r\n        }else{\r\n          if(param.limit < param.limit1 && param.limit > param.limit2){\r\n\r\n          }\r\n        }\r\n      }else if(param.limit_type=='%'){\r\n        if(param.limit2 == 0){\r\n          if(param.limit < param.limit1){\r\n\r\n          }\r\n        }else{\r\n          if(param.limit >= param.limit1 && param.limit <= param.limit2){\r\n\r\n          }\r\n        }\r\n      }\r\n    },\r\n    updateStatus() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('id',this.form_data.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_materials_update_qc_status/', {\r\n          method : 'post',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.showAlert('Updated Successfully!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}