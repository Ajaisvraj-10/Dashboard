lass=\"d-flex\">\r\n          <h5 class=\"mb-0 mx-1 font-weight-600 cursor-pointer tab-breadcamp\" v-for=\"date in dates\" @click=\"current_date=currentDate(date);getRows()\" :class=\"dateFormate(current_date)==dateFormate(date)?'tab-breadcamp-active task-blue':''\">{{dateFormate(date)}}</h5>\r\n        </div>\r\n        <div class=\"d-flex\">\r\n          <span style=\"width:50px;\">Date : </span>\r\n          <input type=\"date\" v-model=\"current_date\" class=\"o_thread_search_input p-0\" @change=\"getRows()\">\r\n        </div>\r\n      </div>\r\n      <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-row class=\"main-scroll pt-2\">\r\n      <b-col lg=\"12\" xl=\"12\" v-for=\"group in groups\">\r\n        <b-card>\r\n          <h5 class=\"\">shift <b class=\"text-primary\">{{group.shift}}</b></h5>\r\n          <div class=\"row mb-2 mx-0\" v-for=\"warehouse in group.warehouses\">\r\n            <div class=\"col-md-12\"><b>{{warehouse.name}}</b></div>\r\n            <div class=\"col-md-3\" v-for=\"department in warehouse.departments\">\r\n              <div class=\"card p-2 mb-2 text-center\">\r\n                <div class=\"font-weight-600 text-primary pt-2\">{{department.department}}</div>\r\n                <div class=\"text-center\">\r\n                  <div>Labours: <b>{{department.labours_count}}</b></div>\r\n                  <router-link :to=\"'/app/production/workers-attendance/department/'+department.id+'/'+group.shift+'/'+warehouse.id+'?date='+current_date\" v-if=\"type=='attendance'\">\r\n                    <button class=\"btn btn-success py-1 my-2\">Add Attendance</button>\r\n                  </router-link>\r\n                  <router-link :to=\"'/app/production/workers-working-time/department/'+department.id+'/'+group.shift+'/'+warehouse.id+'?date='+current_date\" v-else>\r\n                    <button class=\"btn btn-success py-1 my-2 ml-1\">Set Time</button>\r\n                  </router-link>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"row mx-0\">\r\n            <div class=\"col-md-12\"><b>Yards</b></div>\r\n            <div class=\"col-md-3\" v-for=\"yard in group.yards\">\r\n              <div class=\"card p-2 mb-2 text-center\">\r\n                <div class=\"font-weight-600 text-primary pt-2\">{{yard.name}}</div>\r\n                <div class=\"text-center\">\r\n                  <div>Labours: <b>{{yard.labours_count}}</b></div>\r\n                  <router-link :to=\"'/app/production/workers-attendance/yard/'+yard.id+'/'+group.shift+'?date='+current_date\" v-if=\"type=='attendance'\">\r\n                    <button class=\"btn btn-success py-1 my-2\">Add Attendance</button>\r\n                  </router-link>\r\n                  <router-link :to=\"'/app/production/workers-working-time/yard/'+yard.id+'/'+group.shift+'?date='+current_date\" v-else>\r\n                    <button class=\"btn btn-success py-1 my-2\">Set Time</button>\r\n                  </router-link>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </b-card>\r\n      </b-col>\r\n    </b-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{date:'', shift:'DAY', attendance:'FULL TIME',workers:[]},\r\n      dates:[],\r\n      groups:[],\r\n      select_all:false,\r\n      tab:'DAY',\r\n      departments:[],\r\n      yards:[],\r\n      department_id:null,\r\n      department:'all',\r\n      type:'attendance',\r\n      current_date:this.currentDate(),\r\n      today:this.currentDate(),\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'production'\r\n    if(this.$route.params.type){\r\n      this.type = this.$route.params.type\r\n    }\r\n    var startDate = this.addDays(new Date(),-6)\r\n    var stopDate = this.addDays(new Date(),0)\r\n    this.dates = this.getDates(startDate, stopDate)\r\n    this.getWarehouses()\r\n    this.getPlanAccess()\r\n    this.getRows()\r\n  },\r\n  watch: {\r\n    '$route': 'changeParam'\r\n  },\r\n  methods: {\r\n    changeParam(){\r\n      if(this.$route.params.type){\r\n        this.type = this.$route.params.type\r\n      }\r\n    },\r\n    getRows() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers_department_groups/?date='+this.current_date, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.groups = jsonData\r\n      })\r\n    },\r\n    getDepartments() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/departments/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.departments = jsonData\r\n        this.getYards()\r\n      })\r\n    },\r\n    getYards() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/yards/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.yards = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}