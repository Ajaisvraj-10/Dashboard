Form()\">\r\n      <div>\r\n         <div class=\"d-flex mb-2 align-items-center justify-content-between\">\r\n            <slot name=\"header\">\r\n               <h5 class=\"mb-0\">Sun Dried /{{yard.name}}</h5>\r\n            </slot>\r\n            <div class=\"d-flex\">\r\n              <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Save\r\n              </b-button>\r\n              <router-link to=\"/app/production/sun-dried/loading\">\r\n                <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Discard\r\n                </b-button>\r\n              </router-link>\r\n            </div>\r\n          </div>\r\n          <div class=\"my-3 border-top\"></div>\r\n      </div>\r\n      <b-row class=\"main-scroll pt-2\">\r\n        <b-col lg=\"5\" xl=\"5\" class=\"\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <h5>Select Sheets</h5>\r\n            <div>\r\n              <div class=\"o_composer_container my-2 row mx-0\">\r\n                <div class=\"col-md-12\">\r\n                  <div class=\"pt-4 pb-2 px-4\" v-for=\"yard_spread_product in lots\" v-if=\" yard_spread_product['sheets'+yard_spread_product.stock.lot].length>0\">\r\n                    <div>\r\n                      <b-form-checkbox v-model=\"form_data.lots\" :value=\"yard_spread_product.stock.lot\" :name=\"'lots'\" @change=\"selectAllSheets(yard_spread_product)\">\r\n                        <b class=\"pt-2\">#{{yard_spread_product.stock.lot}}Lot {{yard_spread_product.stock.name}} </b>\r\n                      </b-form-checkbox>\r\n                    </div>\r\n                    <div v-if=\"form_data.lots.includes(yard_spread_product.stock.lot)\">\r\n                      <table class=\"w-100 ml-3\">\r\n                        <tr v-for=\"sheet in yard_spread_product['sheets'+yard_spread_product.stock.lot]\">\r\n                          <td>\r\n                            <b-form-checkbox :name=\"'sheets'\" v-model=\"form_data.sheets\" :id=\"'sheet'+sheet.id\" :value=\"sheet.id\" @input=\"totalQty(yard_spread_product)\">\r\n                              <b>#Sheet: {{sheet.sheet_number}}</b>\r\n                            </b-form-checkbox>\r\n                          </td>\r\n                          <td>{{sheet.quantity}}KG</td>\r\n                        </tr>\r\n                        <tr>\r\n                          <td>\r\n                            <div class=\"mt-2\">\r\n                               Total Quantity(KG):\r\n                            </div>\r\n                          </td>\r\n                          <td>\r\n                            <b>{{yard_spread_product.quantity}}KG </b>\r\n                          </td>\r\n                        </tr>\r\n                      </table>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"7\" xl=\"7\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll mb-2\">\r\n            <h5>Loading</h5>\r\n            <div class=\"o_composer_container my-2 row mx-0\">\r\n                <div class=\" col-md-12\">\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\">Warehouse: </span>\r\n                    <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.warehouse\" name=\"warehouse\">\r\n                      <option v-for=\"warehouse in warehouses\" :value=\"warehouse.id\">{{warehouse.name}}</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Vehicle: </span>\r\n                    <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"vehicle\" v-model=\"form_data.vehicle\" @change=\"changeVehicle()\">\r\n                      <option value=\"\" selected disabled>Select a vehicle</option>\r\n                      <option v-for=\"vehicle in vehicles\" :value=\"vehicle.id\">{{vehicle.name}}</option>\r\n                      <option value=\"create\" style=\"color:green!important\">Create New Vehicle</option>\r\n                    </select>\r\n                  </div>\r\n                </div>\r\n            </div>\r\n            <hr class=\"mb-2\">\r\n          </b-card>\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <h5>Loading Workers</h5>\r\n            <div class=\"o_composer_container my-2 row mx-0\">\r\n              <div class=\" col-md-12\">\r\n                <div>\r\n                  <b-form-group class=\"work_type_option\">\r\n                    <b-form-radio-group\r\n                      id=\"work_type\"\r\n                      v-model=\"form_data.work_type\"\r\n                      :options=\"['wages','contract']\"\r\n                      button-variant=\"outline-primary\"\r\n                      name=\"work_type\"\r\n                      @change=\"form_data.workers=[]\"\r\n                      buttons\r\n                      style=\"font-size:20px\"\r\n                    ></b-form-radio-group>\r\n                  </b-form-group>\r\n                  <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                    <div class=\"row\">\r\n                      <div class=\"col-md-12\">\r\n                        <div class=\"o_thread_search_input mt-1\">\r\n                          <span style=\"width:130px\"> Select Labours: </span>\r\n                          <div class=\"mx-2 workers_box\">\r\n                            <label class=\"workers_label\" v-for=\"worker in workers\" :for=\"'worker'+worker.id\" v-if=\"(form_data.work_type=='wages' && worker.work_type=='Wage') || (form_data.work_type=='contract' && worker.work_type=='Contract') \">\r\n                              <b-form-checkbox name=\"workers\" v-model=\"form_data.workers\" :id=\"'worker'+worker.id\" :value=\"worker.id\" @input=\"form_data.total_workers=form_data.workers.length\">\r\n                                <b>#{{worker.id}}/{{worker.name}}</b>\r\n                              </b-form-checkbox>\r\n                            </label>\r\n                          </div>\r\n                        </div>\r\n                        <div v-if=\"form_data.work_type=='wages'\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:130px;\"> Number of Labours : </span>\r\n                            <input placeholder=\"Labours\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.total_workers\" name=\"total_workers\" min=\"0\">\r\n                            <span style=\"width:5px;\">  </span>\r\n                            <input placeholder=\"Hourse\" type=\"number\" class=\"ui-autocomplete-input append_btn_input_right\" autocomplete=\"off\" v-model=\"form_data.working_hourse\" name=\"working_hourse\" min=\"0\">\r\n                            <button class=\"btn btn-white append_btn_right\" type=\"button\">Hours</button>\r\n                          </div>\r\n                          <div class=\"mx-2 px-1 text-right text-danger\" style=\"font-size:12px\" v-if=\"form_data.working_hourse>0 && form_data.total_workers\">\r\n                            Salary : ({{form_data.total_workers}}Labours x {{form_data.working_hourse}}Hr x Rs440)/8 = <b>Rs{{(form_data.total_workers*form_data.working_hourse*440)/8}}/-</b>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mt-2\">\r\n                            <b style=\"width:96px\"> Over Time : </b>\r\n                          </div>\r\n                          <div class=\"mx-2 px-1 mt-2\">\r\n                            <small style=\"font-weight:700\">Double over time</small>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input pt-0\">\r\n                            <span style=\"width:130px;\"> Number of Labours : </span>\r\n                            <input placeholder=\"Total Workers\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.overtime_labours_st1\" name=\"overtime_labours_st1\" min=\"0\">\r\n                            <span style=\"width:5px;\">  </span>\r\n                            <input placeholder=\"Working Hourse\" type=\"number\" class=\"ui-autocomplete-input append_btn_input_right\" autocomplete=\"off\" v-model=\"form_data.overtime_hourse_st1\" name=\"overtime_hourse_st1\" min=\"0\">\r\n                            <button class=\"btn btn-white append_btn_right\" type=\"button\">Hours</button>\r\n                          </div>\r\n                          <div class=\"mx-2 px-1 text-right text-danger\" style=\"font-size:12px\" v-if=\"form_data.overtime_labours_st1>0 && form_data.overtime_hourse_st1\">\r\n                            Salary : (Rs440/8Hr) x 2 x {{form_data.overtime_labours_st1}}Labours x {{form_data.overtime_hourse_st1}}Hr = <b>Rs{{(440/8)*2*form_data.overtime_labours_st1*form_data.overtime_hourse_st1}}/-</b>\r\n                          </div>\r\n                          <div class=\"mx-2 px-1 mt-2\">\r\n                            <small style=\"font-weight:700\">Single over time</small>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input pt-0\">\r\n                            <span style=\"width:130px;\"> Number of Labours : </span>\r\n                            <input placeholder=\"Labours\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.overtime_labours_st2\" name=\"overtime_labours_st2\" min=\"0\">\r\n                            <span style=\"width:5px;\">  </span>\r\n                            <input placeholder=\"Working Hourse\" type=\"number\" class=\"ui-autocomplete-input append_btn_input_right\" autocomplete=\"off\" v-model=\"form_data.overtime_hourse_st2\" name=\"overtime_hourse_st2\" min=\"0\">\r\n                            <button class=\"btn btn-white append_btn_right\" type=\"button\">Hours</button>\r\n                          </div>\r\n                          <div class=\"mx-2 px-1 text-right text-danger\" style=\"font-size:12px\" v-if=\"form_data.overtime_labours_st2>0 && form_data.overtime_hourse_st2\">\r\n                            Salary : (Rs440/8Hr) x {{form_data.overtime_labours_st2}}Labours x {{form_data.overtime_hourse_st2}}Hr = <b>Rs{{(440/8)*form_data.overtime_labours_st2*form_data.overtime_hourse_st2}}/-</b>\r\n                          </div>\r\n                        </div>\r\n\r\n                        <div v-else>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:130px;\"> Number of Labours: </span>\r\n                            <input placeholder=\"Labours\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.total_workers\" name=\"total_workers\" @input=\"form_data.labour_charge = (form_data.total_workers*400)/8\" min=\"0\">\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\" v-if=\"form_data.work_type=='contract'\">\r\n                            <span style=\"width:130px;\"> Labour Charge : </span>\r\n                            <input placeholder=\"Labour Charge \" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.contract_labour_charge\" name=\"contract_labour_charge\" min=\"0\">\r\n                          </div>\r\n                          <div class=\"mx-2 px-1 text-right text-danger\" style=\"font-size:12px\" v-if=\"form_data.total_workers>0 && form_data.contract_labour_charge\">\r\n                            Salary : {{form_data.total_workers}}Labours x {{total_weight}}Kg x {{form_data.contract_labour_charge}}Rs = <b>Rs{{form_data.total_workers*total_weight*form_data.contract_labour_charge}}/-</b>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input mt-2\">\r\n                            <div style=\"width:140px\"> Contract from wage? : </div>\r\n                            <b-form-checkbox v-model=\"form_data.contract_from_wage\" name=\"contract_from_wage\" value=\"1\" switch>\r\n                              <b>{{ form_data.contract_from_wage?'Yes':'No' }}</b>\r\n                            </b-form-checkbox>\r\n                          </div>\r\n                          <div v-if=\"form_data.contract_from_wage\">\r\n                            <div class=\"o_thread_search_input mt-3\">\r\n                              <span style=\"width:130px\"> Select Labours: </span>\r\n                              <div class=\"mx-2 workers_box\">\r\n                                <label class=\"workers_label\" v-for=\"worker in workers\" :for=\"'worker'+worker.id\" v-if=\"worker.work_type=='Wage'\">\r\n                                  <b-form-checkbox name=\"wage_workers\" v-model=\"form_data.wage_workers\" :id=\"'worker'+worker.id\" :value=\"worker.id\">\r\n                                    <b>#{{worker.id}}/{{worker.name}}</b>\r\n                                  </b-form-checkbox>\r\n                                </label>\r\n                              </div>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input mt-2\">\r\n                              <div style=\"width:140px\"> Is there payment for wage? : </div>\r\n                              <b-form-checkbox v-model=\"form_data.is_wage_payment\" name=\"is_wage_payment\" value=\"1\" switch>\r\n                                <b>{{ form_data.is_wage_payment?'Yes':'No' }}</b>\r\n                              </b-form-checkbox>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                        <div class=\"o_thread_search_input mt-2\">\r\n\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </b-card>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n    <div>\r\n      <b-modal id=\"create-vehicle\" hide-header  hide-footer>\r\n        <form ref=\"vehicleForm\" @submit.prevent=\"vehicleForm()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Vehicle</span> / Create</h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('create-vehicle')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Vehicle Type : </span>\r\n                            <input placeholder=\"Vehicle Type\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.vehicle_type\" name=\"vehicle_type\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Driver Name: </span>\r\n                            <input placeholder=\"Driver Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.name\" name=\"name\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Driver Contact: </span>\r\n                            <input placeholder=\"Driver Contact\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.contact\" name=\"contact\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Vehicle No : </span>\r\n                            <input placeholder=\"Vehicle No\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.vehicle_number\" name=\"vehicle_number\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Origin of the Vehicle  : </span>\r\n                            <input placeholder=\"Origin of the Vehicle\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.origin_of_the_vehicle\" name=\"origin_of_the_vehicle\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Destination: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.destination\" name=\"destination\">\r\n                              <option v-for=\"warehouse in warehouses\" :value=\"warehouse.id\">{{warehouse.name}}</option>\r\n                            </select>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{date:this.currentDate(), id:null, status:'On the Way', lots:[], sheets:[], warehouse:null, yard:null, work_type:'wages', total_workers:0, working_hourse:0, vehicle:'',workers:[], wage_workers:[], vehicle_detail:{name:'', phone:''}},\r\n      action:'create',\r\n      yard:{},\r\n      workers:[],\r\n      sheets:[],\r\n      warehouses:[],\r\n      vehicles:[],\r\n      current_yard_product:{},\r\n      lots:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.yard.id = this.$route.params.yard\r\n    if(this.$route.params.id){\r\n      this.form_data.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n    this.getYard()\r\n    this.getWarehouses()\r\n    this.getWorkers()\r\n    this.getVehicles()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/yard_loading/'+this.form_data.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n      })\r\n    },\r\n    getYard() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/yards/'+this.yard.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.yard = jsonData\r\n        var lots = []\r\n        var added_lot_ids = []\r\n        for(var y1=0;y1<jsonData.yard_spread_products.length;y1++){\r\n          var yard_spread_product = jsonData.yard_spread_products[y1]\r\n          if(!added_lot_ids.includes(yard_spread_product.stock.lot)){\r\n            yard_spread_product['sheets'+yard_spread_product.stock.lot] = []\r\n            added_lot_ids.push(yard_spread_product.stock.lot)\r\n            lots.push(yard_spread_product)\r\n          }\r\n          for(var l1=0;l1<lots.length;l1++){\r\n            for(var y2=0;y2<yard_spread_product.completed_sheets.length;y2++){\r\n              var sheet = yard_spread_product.completed_sheets[y2]\r\n              if(added_lot_ids.includes(yard_spread_product.stock.lot)){\r\n                var lot = lots[l1]\r\n                if(lot['sheets'+yard_spread_product.stock.lot]){\r\n                  if((sheet.is_active && !this.form_data.id) || (this.form_data.id && this.form_data.sheets.includes(sheet.id))){\r\n                    lot['sheets'+yard_spread_product.stock.lot].push(sheet)\r\n                  }\r\n                }\r\n              }\r\n            }\r\n          }\r\n          if(this.form_data.id){\r\n            this.totalQty(yard_spread_product)\r\n          }\r\n        }\r\n        this.lots = lots\r\n      })\r\n    },\r\n    selectAllSheets(yard_spread_product){\r\n      var self = this\r\n      yard_spread_product['sheets'+yard_spread_product.stock.lot].forEach(function(val){\r\n        if(!self.form_data.sheets.includes(val.id)){\r\n          self.form_data.sheets.push(val.id)\r\n        }\r\n      })\r\n      this.totalQty(yard_spread_product)\r\n    },\r\n    totalQty(yard_spread_product){\r\n      var self = this\r\n      var total_qty = 0\r\n      yard_spread_product['sheets'+yard_spread_product.stock.lot].forEach(function(val){\r\n        if(self.form_data.sheets.includes(val.id)){\r\n          total_qty = total_qty+val.quantity\r\n        }\r\n      })\r\n      yard_spread_product.quantity = total_qty\r\n    },\r\n    getYardProducts() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/transfer_yard_products/?yard='+this.yard.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.yard_products = jsonData\r\n      })\r\n    },\r\n    dataForm(method='POST') {\r\n      if(this.$root.user_type!='supervisor'){\r\n        this.showAlert('You cannot add!')\r\n        return false\r\n      }\r\n      var param = ''\r\n      if(this.form_data.id){\r\n        method = 'PUT'\r\n        param = this.form_data.id+'/'\r\n      }\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('supervisor',this.employeeId)\r\n      formdata.append('status',this.form_data.status)\r\n      formdata.append('yard',this.yard.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/yard_loading/'+param, {\r\n          method : method,\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.form_data.id = jsonData.id\r\n          this.$bvModal.hide('start-process')\r\n          if(method=='POST'){\r\n            this.dataForm()\r\n          }else{\r\n            this.getForm()\r\n          }\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getWorkers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.workers = jsonData\r\n      })\r\n    },\r\n    getVehicles() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/vehicle/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.vehicles = jsonData\r\n      })\r\n    },\r\n    changeVehicle(){\r\n      if(this.form_data.vehicle=='create'){\r\n        this.form_data.vehicle = ''\r\n        this.$bvModal.show('create-vehicle')\r\n      }else{\r\n        var self =this\r\n        this.vehicles.forEach(function(val,key){\r\n          if(val.id == self.form_data.vehicle){\r\n            self.form_data.vehicle_detail = val\r\n          }\r\n        })\r\n      }\r\n    },\r\n    vehicleForm() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['vehicleForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/vehicle/', {\r\n          method : 'POST',