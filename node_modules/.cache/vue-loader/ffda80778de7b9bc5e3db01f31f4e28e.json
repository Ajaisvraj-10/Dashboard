nk tag=\"a\" class to=\"/app/employee/workers/create\"><b-button variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >+ Create\r\n              </b-button></router-link>\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"true\"\r\n        :search-options=\"{\r\n          enabled: true,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: true,\r\n          perPage: 20,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows\"\r\n      >\r\n        <div slot=\"table-actions\" class=\"mb-3 filter-but\">\r\n\r\n        </div>\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'action'\">\r\n            <router-link :to=\"'/app/employee/workers/edit/'+props.row.id\">\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1 mx-1\">Edit</b-button>\r\n            </router-link>\r\n            <!-- <router-link :to=\"'/app/employee/workers/payment/history/'+props.row.id\">\r\n              <b-button  variant=\"warning\" class=\" px-3 py-1 mx-1\">Payment History</b-button>\r\n            </router-link> -->\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'id'\">\r\n            #{{props.row.id}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'status'\">\r\n            <span class=\"badge\" :class=\"props.row.status?'badge-success':'badge-danger'\">{{props.row.status?'Active':'Deactive'}}</span>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'job_position_detail'\">\r\n            {{props.row.job_position_detail?props.row.job_position_detail.name:''}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'department_detail'\">\r\n            {{props.row.department_detail?props.row.department_detail.name:''}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'distance'\">\r\n            {{props.row.distance}}KM\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'age'\">\r\n            <b>{{ ageCalculation(props.row.dob) }}</b>\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Id\",\r\n          field: \"id\"\r\n        },\r\n        {\r\n          label: \"Name\",\r\n          field: \"name\"\r\n        },\r\n        {\r\n          label: \"Status\",\r\n          field: \"status\"\r\n        },\r\n        {\r\n          label: \"Gender\",\r\n          field: \"gender\"\r\n        },\r\n        {\r\n          label: \"Phone\",\r\n          field: \"mobile\"\r\n        },\r\n        {\r\n          label: \"Location\",\r\n          field: \"location\"\r\n        },\r\n        {\r\n          label: \"Distance\",\r\n          field: \"distance\"\r\n        },\r\n        // {\r\n        //   label: \"Age\",\r\n        //   field: \"age\"\r\n        // },\r\n        {\r\n          label: \"Job Type\",\r\n          field: \"work_type\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"action\"\r\n        },\r\n       \r\n      ],\r\n      rows: [\r\n       \r\n      ]\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'employee'\r\n    this.getWorkers()\r\n  },\r\n  methods: {\r\n    getWorkers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}