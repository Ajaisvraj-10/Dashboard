;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"batch-creation-form.vue","sourceRoot":"src/views/app/oodo/production","sourcesContent":["<template>\r\n  <div class=\"main-content create-main\">\r\n    <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n      <div>\r\n         <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Batch Creation</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/production/batch-creation\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"7\" xl=\"7\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <h5>Batch Creation</h5>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\" col-md-12\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Total Weight: </span>\r\n                              <input placeholder=\"Total Weight\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_weight\" name=\"total_weight\" readonly>\r\n                            </div>\r\n                            \r\n                            <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Purchase Type  : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"purchase_type\" v-model=\"form_data.purchase_type\" @change=\"getLots()\">\r\n                                  <option v-for=\"purchase_type in purchase_types\" :value=\"purchase_type.id\">{{purchase_type.name}}</option>\r\n                              </select>\r\n                            </div>\r\n\r\n                            <select name=\"lots\" v-model=\"form_data.lots\" multiple v-show=\"false\">\r\n                              <option v-for=\"new_lot in new_lots\" :value=\"new_lot.id\">{{new_lot.id}}</option>\r\n                            </select>\r\n                            \r\n                          </div>\r\n                        </div>\r\n                        <div class=\"col-md-6 mt-4\">\r\n                          <table class=\"table\">\r\n                            <thead>\r\n                              <tr>\r\n                                <th>Lot Id</th>\r\n                                <th>Quantity</th>\r\n                                <th>Action</th>\r\n                              </tr>\r\n                            </thead>\r\n                            <tbody v-if=\"product_id\">\r\n                              <tr v-for=\"(line,key) in lots\">\r\n                                 <td>\r\n                                  Lot/{{line.id}}\r\n                                </td>\r\n                                <td>\r\n                                  {{line.total_weight}}\r\n                                </td>\r\n                                <td>\r\n                                  <button type=\"button\" class=\"btn btn-primary btn-sm px-3 py-1 ml-4\" @click=\"lineOperation('+',line)\">Add</button>\r\n                                </td>\r\n                              </tr>\r\n                            </tbody>\r\n                          </table>\r\n                        </div>\r\n                        <div class=\"col-md-6 mt-4\">\r\n                          <table class=\"table\">\r\n                            <thead>\r\n                              <tr>\r\n                                <th>Lot Id</th>\r\n                                <th>Quantity</th>\r\n                                <th>Action</th>\r\n                              </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                              <tr v-for=\"(line,key) in new_lots\">\r\n                                 <td>\r\n                                  Lot/{{line.id}}\r\n                                </td>\r\n                                <td>\r\n                                  {{line.total_weight}}\r\n                                </td>\r\n                                <td>\r\n                                  <button type=\"button\" class=\"btn btn-danger btn-sm px-3 py-1 ml-4\" @click=\"lineOperation(key,line)\">Remove</button>\r\n                                </td>\r\n                              </tr>\r\n                              <tr>\r\n                                <th>Total Qty</th>\r\n                                <td>{{form_data.total_weight}}</td>\r\n                              </tr>\r\n                            </tbody>\r\n                          </table>\r\n                        </div>\r\n\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"5\" xl=\"5\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <h5>Pending Lots</h5>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\" col-md-12\">\r\n                           <table class=\"table\">\r\n                            <thead>\r\n                              <tr>\r\n                                <th>Lot Id</th>\r\n                                <th>Department/Product/Type</th>\r\n                                <th>Purchase Type</th>\r\n                                <th>Quantity</th>\r\n                              </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                              <tr v-for=\"(line,key) in pending_lots\">\r\n                                 <td>\r\n                                  Lot/{{line.id}}\r\n                                </td>\r\n                                <td>\r\n                                  {{line.product_detail.category_detail.name}}/\r\n                                  {{line.product_detail.name}}/\r\n                                  {{line.product_detail.product_type_detail.name}}\r\n                                </td>\r\n                                <td>\r\n                                  {{line.purchase_type_detail.name}}\r\n                                </td>\r\n                                <td>\r\n                                  {{line.total_weight}}\r\n                                </td>\r\n                              </tr>\r\n                            </tbody>\r\n                          </table>\r\n                        </div>\r\n                     </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{date:new Date(),lots:[],total_weight:0,purchase_type:''},\r\n      id:null,\r\n      action:'create',\r\n      load_products:false,\r\n      product_id:null,\r\n      lots:[],\r\n      new_lots:[],\r\n      pending_lots:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.form_data.date = this.currentDate()\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products=true\r\n    }\r\n    this.getPurchaseTypes()\r\n    this.getPendingLots()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/batch_creation/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.new_lots = jsonData.added_lots\r\n        this.product_id = jsonData.product\r\n        this.form_data.product = jsonData.product\r\n        this.load_products=true\r\n        this.getLots()\r\n      })\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.form_data.product = id\r\n    },\r\n    getPendingLots() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/create_lots/?status=Create Lot', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.pending_lots = jsonData\r\n      })\r\n    },\r\n    getLots(){\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      formdata.append('product',this.product_id)\r\n      formdata.append('purchase_type',this.form_data.purchase_type)\r\n      formdata.append('status','Create Lot')\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/create_lots_product/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lots = jsonData\r\n        if(this.action == 'create'){\r\n          this.new_lots = []\r\n        }\r\n        this.totalSumQty()\r\n      })\r\n    },\r\n    totalSumQty(){\r\n      var total_qty = 0\r\n      this.new_lots.forEach(function(val){\r\n        total_qty = total_qty + val.total_weight\r\n      })\r\n      this.form_data.total_weight = total_qty\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    lineOperation(operation,line){\r\n      if(operation=='+'){\r\n        this.new_lots.push(line)\r\n        this.form_data.lots.push(line.id)\r\n        this.lots.splice(operation, 1)\r\n      }else{\r\n        this.lots.push(line)\r\n        this.new_lots.splice(operation, 1)\r\n        this.form_data.lots.splice.push(operation, 1)\r\n      }\r\n      this.totalSumQty()\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('product',this.product_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/batch_creation/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.updateLotStatus()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('product',this.product_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/batch_creation/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.updateLotStatus()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateLotStatus() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('status','Added to Batch')\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/update_lots_status/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.showAlert('Saved Successfully')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}