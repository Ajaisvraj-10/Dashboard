>\r\n                  \r\n            </slot>\r\n            <div class=\"d-flex\">\r\n              <b-button variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.show('start-process')\">+ Create\r\n              </b-button>\r\n              <router-link tag=\"a\" class to=\"/app/oodo/import\"><b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Import\r\n              </b-button></router-link>\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n\r\n    </b-card>\r\n    <div>\r\n      <b-modal id=\"start-process\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n          <div class=\"main-content create-main\">\r\n            <form ref=\"dataForm\" @submit.prevent=\"dataForm()\" v-if=\"is_start\">\r\n              <div>\r\n                <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                    <slot name=\"header\">\r\n                       <h5 class=\"mb-0\"><span class=\"text-success\">{{process_detail.name}}</span> / {{slugToWord(action)}}</h5>\r\n                          \r\n                    </slot>\r\n                    <div class=\"d-flex\">\r\n                      <div class=\"d-flex\" v-if=\"action=='input'\">\r\n                        <b-button variant=\"success\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"dataForm()\">Start Process\r\n                        </b-button>\r\n                      </div>\r\n                      <div class=\"d-flex\" v-else-if=\"action=='workers'\">\r\n                        <b-button variant=\"info\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"action='details'\">Back\r\n                        </b-button>\r\n                        <b-button variant=\"primary\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"form_data.status='Stop';dataForm()\">Next\r\n                        </b-button>\r\n                      </div>\r\n                      <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"closePopup()\"\r\n                        >Discard\r\n                      </b-button>\r\n                    </div>\r\n                   \r\n                </div>\r\n                <div class=\" my-3 border-top\"></div>\r\n              </div>\r\n              \r\n              <b-row class=\"main-scroll\">\r\n                <b-col lg=\"12\" xl=\"12\" class=\"my-3\" v-show=\"action=='input'\">\r\n                  <div>\r\n                    <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                      <div class=\"\">\r\n                        <input type=\"hidden\" name=\"status\" value=\"Starting\">\r\n                        <table class=\"table mt-2 w-100\">\r\n                          <thead>\r\n                            <tr>\r\n                              <th>Product</th>\r\n                              <th>Lot</th>\r\n                              <th>Available Qty</th>\r\n                              <th>Quantity</th>\r\n                              <th>Action</th>\r\n                            </tr>\r\n                          </thead>\r\n                          <tbody>\r\n                            <tr v-for=\"(stock_parameter,key) in stock_parameters\">\r\n                              <td :style=\"stock_parameter.is_added?'opacity:1':'opacity:0.5'\">\r\n                                {{stock_parameter.parameter_detail.name}}\r\n                              </td>\r\n                              <td :style=\"stock_parameter.is_added?'opacity:1':'opacity:0.5'\">\r\n                                #{{stock_parameter.lot}}/Lot\r\n                                <input type=\"hidden\" :name=\"'lot'+stock_parameter.id\" :value=\"stock_parameter.lot\">\r\n                              </td>\r\n                              <td :style=\"stock_parameter.is_added?'opacity:1':'opacity:0.5'\">\r\n                                {{stock_parameter.stock_quantity}}\r\n                              </td>\r\n                              <td :style=\"stock_parameter.is_added?'opacity:1':'opacity:0.5'\">\r\n                                <input type=\"number\" class=\"ui-autocomplete-input w-100\" :max=\"stock_parameter.stock_quantity\" v-model=\"stock_parameter.quantity\" :name=\"'quantity'+stock_parameter.id\" :disabled=\"!stock_parameter.is_added\">\r\n                                <input type=\"hidden\" :name=\"'is_added'+stock_parameter.id+'lot'\" :value=\"stock_parameter.is_added?'yes':'no'\">\r\n                              </td>\r\n                              <td>\r\n                                <button type=\"button\" class=\"btn btn-primary btn-sm px-3 py-1 ml-4\" @click=\"addProduct(stock_parameter)\" v-if=\"!stock_parameter.is_added\">Add</button>\r\n                                <button type=\"button\" class=\"btn btn-danger btn-sm px-3 py-1 ml-4\" @click=\"removeProduct(stock_parameter)\" v-else>Remove</button>\r\n                              </td>\r\n                            </tr>\r\n                          </tbody>\r\n                        </table>\r\n                      </div>\r\n                    </b-card>\r\n                  </div>\r\n                </b-col>\r\n                <b-col lg=\"12\" xl=\"12\" class=\"my-3\" v-show=\"action=='workers'\">\r\n                  <div>\r\n                    <b-form-group class=\"work_type_option\">\r\n                      <b-form-radio-group\r\n                        id=\"work_type\"\r\n                        v-model=\"form_data.work_type\"\r\n                        :options=\"['wages','contract']\"\r\n                        button-variant=\"outline-primary\"\r\n                        name=\"work_type\"\r\n                        buttons\r\n                        style=\"font-size:20px\"\r\n                      ></b-form-radio-group>\r\n                    </b-form-group>\r\n                    <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                      <div class=\"row\">\r\n                        <div class=\"col-md-12\">\r\n                          <div class=\"o_thread_search_input mt-3\">\r\n                            <span style=\"width:90px\"> Workers: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"workers\" v-model=\"form_data.workers\" multiple @change=\"form_data.total_workers=form_data.workers.length\">\r\n                              <option v-for=\"worker in workers\" :value=\"worker.id\" v-if=\"worker.job_position==5\"> #{{worker.id}}/{{worker.name}}</option>\r\n                            </select>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\" v-if=\"form_data.work_type=='wages'\">\r\n                            <span style=\"width:90px;\"> Total : </span>\r\n                            <input placeholder=\"Total Workers\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.total_workers\" name=\"total_workers\">\r\n                            <span style=\"width:5px;\">  </span>\r\n                            <input placeholder=\"Working Hourse\" type=\"number\" class=\"ui-autocomplete-input append_btn_input_right\" autocomplete=\"off\" v-model=\"form_data.working_hourse\" name=\"working_hourse\">\r\n                            <button class=\"btn btn-white append_btn_right\" type=\"button\">Hrs</button>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\" v-else>\r\n                            <span style=\"width:90px;\"> Total : </span>\r\n                            <input placeholder=\"Total Workers\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.total_workers\" name=\"total_workers\" @input=\"form_data.labour_charge = (form_data.total_workers*400)/8\">\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\" v-if=\"form_data.work_type=='contract'\">\r\n                            <span style=\"width:90px;\"> Labour Charge : </span>\r\n                            <input placeholder=\"Labour Charge \" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.labour_charge\" name=\"labour_charge\" >\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mt-2\" v-if=\"form_data.work_type=='wages'\">\r\n                            <b style=\"width:96px\"> Over Time? : </b>\r\n                            <b-form-checkbox v-model=\"form_data.is_over_time\" name=\"is_over_time\" switch>\r\n                              <b>{{ form_data.is_over_time?'Yes':'No' }}</b>\r\n                            </b-form-checkbox>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input\" v-if=\"form_data.is_over_time && form_data.work_type=='wages'\">\r\n                            <span style=\"width:90px;\"> Total (overtime) : </span>\r\n                            <input placeholder=\"Total Workers\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.overtime_total_workers\" name=\"overtime_total_workers\" >\r\n                            <span style=\"width:5px;\">  </span>\r\n                            <input placeholder=\"Working Hourse\" type=\"number\" class=\"ui-autocomplete-input append_btn_input_right\" autocomplete=\"off\" v-model=\"form_data.overtime_working_hourse\" name=\"overtime_working_hourse\">\r\n                            <button class=\"btn btn-white append_btn_right\" type=\"button\">Hrs</button>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input\">\r\n                            <input type=\"hidden\" name=\"lot\" :value=\"form_data.lot\">\r\n                            <select name=\"other_lots\" v-model=\"form_data.other_lots\" multiple v-show=\"false\">\r\n                              <option v-for=\"lot in form_data.other_lots\" :value=\"lot\">{{lot}}</option>\r\n                            </select>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input mt-2\">\r\n\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    </b-card>\r\n                  </div>\r\n                </b-col>\r\n              </b-row>\r\n            </form>\r\n            <div style=\"height:84vh\" v-else>\r\n              <div>\r\n                <div class=\"loader_logo\">\r\n                  <img src=\"/img/logo.png\" class=\"\" alt=\"logo\">\r\n                </div>\r\n\r\n                <div class=\"loading\"></div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{\r\n        id:null,\r\n      },\r\n      yard:[],\r\n      is_start:false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getEmployees()\r\n  },\r\n  methods: {\r\n    getProcess() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/processing/'+this.processing_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        \r\n        this.form_data = jsonData\r\n        this.action = 'output'\r\n        this.getProcessDetails()\r\n      })\r\n    },\r\n    getProcessDetails() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/process_details/'+this.process_detail_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.process_detail = jsonData\r\n        this.form_data.machines = jsonData.machines\r\n        if(this.form_data.process_outputs.length>0){\r\n          this.action = 'complete'\r\n          this.nextProcesses()\r\n        }else{\r\n          this.getParameters()\r\n        }\r\n        if(jsonData.have_barrel){\r\n          this.getBarrels()\r\n        }\r\n        this.getStartProcess()\r\n      })\r\n    },\r\n    getStartProcess() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/start_process/'+this.start_process_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.start_process_detail = jsonData\r\n        var last_process_id = null \r\n        jsonData.processes.forEach(function(val,key){\r\n          last_process_id = val.id\r\n        })\r\n        if(last_process_id!=this.processing_id && this.action=='complete'){\r\n          this.action = 'details'\r\n        }\r\n        if(jsonData.status=='Completed'){\r\n          this.action = 'details'\r\n        }\r\n        if(jsonData.process_products.length<=0){\r\n          this.action = 'input'\r\n          this.stockParameter()\r\n        }\r\n        var lots = [];\r\n        var lot = null;\r\n        var total = 0;\r\n        jsonData.process_products.forEach(function(val,key){\r\n          if(key==0)lot=val.lot\r\n          lots.push(val.lot)\r\n          total = total+val.quantity\r\n        })\r\n        this.total_weight = total\r\n        this.form_data.lot = lot\r\n        this.form_data.other_lots = lots\r\n        this.is_start = true\r\n      })\r\n    },\r\n    getBarrels() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/barrel/?product='+this.form_data.product, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var self = this\r\n        jsonData.forEach(function(val,key){\r\n          jsonData[key]['quantity'] = 0\r\n          jsonData[key]['quantity_persentage'] = 0\r\n          self.process_detail.consumable_items_list.forEach(function(val2,key2){\r\n            jsonData[key]['other'+val2.id] = 0\r\n          })\r\n        })\r\n        this.barrels = jsonData\r\n      })\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameters/?outputs='+this.process_detail.outputs, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.process_outputs = jsonData\r\n        var self=this\r\n        this.form_data.process_outputs.forEach(function(val,key){\r\n          self.form_data.process_outputs[key].quantity = 0\r\n          self.form_data.process_outputs[key].quantity_persentage = '0%'\r\n        })\r\n        this.calculation()\r\n      })\r\n    },\r\n    calculateQty(){\r\n      var self=this;\r\n      var total = 0;\r\n      this.barrels.forEach(function(val,key){\r\n        total = total+parseFloat(val.quantity)\r\n      })\r\n\r\n      this.form_data.process_outputs.forEach(function(val,key){\r\n        if(val.parameter_type=='product'){\r\n          self.form_data.process_outputs[key].quantity = total\r\n          // self.calculation(val)\r\n        }\r\n      })\r\n    },\r\n    calculation(parameter){\r\n      var self=this;\r\n      var total = 0;\r\n      var process_outputs = this.form_data.process_outputs\r\n      this.form_data.process_outputs = []\r\n      process_outputs.forEach(function(val,key){\r\n        if(val.quantity){\r\n          if(self.total_weight!=0){\r\n            var persentage = (val.quantity/self.total_weight)*100\r\n            persentage = persentage.toFixed(2)\r\n            process_outputs[key].quantity_persentage = persentage+'%'\r\n          }\r\n          if(val.parameter_type=='product'){\r\n            total = total+parseFloat(val.quantity)\r\n          }\r\n        }else{\r\n            process_outputs[key].quantity_persentage = '0%'\r\n        }\r\n      })\r\n      this.form_data.process_outputs = process_outputs\r\n      this.total_output = total\r\n    },\r\n    CalculatePersentage(value){\r\n      if(value){\r\n        if(this.total_weight!=0){\r\n          var persentage = (value/this.total_weight)*100\r\n          persentage = persentage.toFixed(2)\r\n          return persentage+'%'\r\n        }\r\n      }\r\n      return '0%'\r\n    },\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.workers = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action=='input'){\r\n        this.createNextProcess()\r\n      }\r\n      if(this.action == 'workers'){\r\n        this.createProcess()\r\n      }else if(this.action == 'output'){\r\n        this.createOutput()\r\n      }else if(this.action == 'complete'){\r\n        this.completeProcess()\r\n      }\r\n    },\r\n    createProcess(method='POST') {\r\n      var param = ''\r\n      if(this.form_data.id){\r\n        method = 'PUT'\r\n        param = this.form_data.id+'/'\r\n      }\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('supervisor',this.user_id)\r\n      formdata.append('status',this.form_data.status)\r\n      formdata.append('start_process',this.start_process_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/processing/'+param, {\r\n          method : method,\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.form_data.id = jsonData.id\r\n          this.action = 'output'\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    createOutput() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('process_detail',this.process_detail.id)\r\n      formdata.append('start_process',this.start_process_id)\r\n      formdata.append('process',this.form_data.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/process_outputs/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.action = 'complete'\r\n          this.nextProcesses()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    nextProcesses() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/process_details/?next_process_detail='+this.process_detail.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.next_processes = jsonData\r\n        if(jsonData.length<=0){\r\n          this.next_processes.push({id:this.default_packing_id, sort_order:'', name:'Packing'})\r\n        }\r\n        this.form_data.next_process = this.next_processes[0].id\r\n      })\r\n    },\r\n    completeProcess() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('supervisor',this.user_id)\r\n      formdata.append('process_detail',this.process_detail.id)\r\n      formdata.append('start_process',this.start_process_id)\r\n      formdata.append('process',this.form_data.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/complete_process/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.showAlert('Saved Successfully!')\r\n          this.closePopup()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    closePopup(){\r\n      this.$emit('closePopup', true)\r\n    },\r\n    stockParameter() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameter_stock/?inputs='+this.process_detail.inputs+'&warehouse='+this.form_data.start_process_detail.main_process_detail.warehouse, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.stock_parameters = jsonData\r\n      })\r\n    },\r\n    addProduct(stock_parameter){\r\n      stock_parameter.is_added = true\r\n      stock_parameter.quantity = stock_parameter.stock_quantity\r\n    },\r\n    removeProduct(stock_parameter){\r\n      stock_parameter.is_added = false\r\n      stock_parameter.quantity = ''\r\n    },\r\n    createNextProcess() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('inputs',this.process_detail.inputs)\r\n      formdata.append('start_process',this.start_process_id)\r\n      formdata.append('supervisor',this.user_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/process_products/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.action = 'detail'\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}