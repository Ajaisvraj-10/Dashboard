rn response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.rows = jsonData\r\n      })\r\n    },\r\n    selectFarmer(row){\r\n      this.current_row = row\r\n      this.total_payable = this.current_row.price-this.current_row.farmer_detail.balance_ics\r\n      this.$bvModal.show('choose-farmer')\r\n    },\r\n    updateStatus(status){\r\n      this.$bvModal.hide('choose-farmer')\r\n      \r\n      var formdata = new FormData()\r\n      formdata.append('purchase_farmer_farmers_id', this.current_row.id)\r\n      formdata.append('status', status)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/purchase/update_purchase_farmer_status/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n</style>\r\n"]}]}