       <div v-show=\"input_col==13\">\r\n              <div class=\"o_thread_search_input\">\r\n                <span style=\"width:120px;\"> Status: </span>\r\n                <div class=\"inspection-btn\" >\r\n                  <button type=\"button\" class=\"btn\" v-for=\"stat in status\" :class=\"form_data.status==stat?'active-btn':''\" @click=\"form_data.status=stat;nextCol()\">{{stat}} </button>\r\n                  <input type=\"hidden\" v-model=\"form_data.status\" name=\"status\" :class=\"'check_field_'+13\" required>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-show=\"input_col==14\">\r\n              <div class=\"o_thread_search_input\">\r\n                <span style=\"width:120px;\">Cluster: </span>\r\n                <input placeholder=\"Cluster\" type=\"text\" class=\"form-control\" autocomplete=\"off\" v-for=\"vfc in all_vfc\" v-if=\"vfc.id==form_data.vfc\" :value=\"vfc.cluster_type\" name=\"cluster_type\" :class=\"'check_field_2'+14\" readonly> \r\n              </div>\r\n            </div>\r\n            <div v-show=\"input_col==15\">\r\n              <table class=\"w-75 my-3\">\r\n                <tr>\r\n                  <th>\r\n                    <span style=\"width:120px;\">Fair Trade</span>\r\n                  </th>\r\n                  <th>:</th>\r\n                  <td>\r\n                    <input type=\"checkbox\" class=\"\" autocomplete=\"off\" value=\"1\" :checked=\"form_data.fire_trade\" name=\"fire_trade\" :readonly=\"action=='view'\"> {{form_data.fire_trade?'Yes':'NO'}}\r\n                  </td>\r\n                </tr>\r\n                <tr>\r\n                  <th>\r\n                    <span style=\"width:120px;\">UTZ/ RA </span>\r\n                  </th>\r\n                  <th>:</th>\r\n                  <td>\r\n                    <input type=\"checkbox\" class=\"\" autocomplete=\"off\" value=\"1\" :checked=\"form_data.utz_ra\" name=\"utz_ra\" :readonly=\"action=='view'\