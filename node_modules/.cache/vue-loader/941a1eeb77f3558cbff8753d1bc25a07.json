}}hrs + {{props.row.overtime_single_hours}}hrs\r\n                </span>\r\n              </template>\r\n\r\n              \r\n            </vue-good-table>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport LocationFields from '/src/views/app/oodo/components/location_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    LocationFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{gender:'M', job_positions:5, department:5},\r\n      id:null,\r\n      action:'create',\r\n      columns: [\r\n        {\r\n          label: \"Date\",\r\n          field: \"created_on\"\r\n        },\r\n        {\r\n          label: \"Shift\",\r\n          field: \"shift\"\r\n        },\r\n        {\r\n          label: \"Working Hours\",\r\n          field: \"hours\"\r\n        },\r\n        {\r\n          label: \"Over Time\",\r\n          field: \"overtime_double_hours\"\r\n        },\r\n        {\r\n          label: \"Payment\",\r\n          field: \"payment\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"action\"\r\n        },\r\n       \r\n      ],\r\n      payments:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'employee'\r\n    this.id = this.$route.params.id\r\n    this.getForm()\r\n    this.getWorkerPayments()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n      })\r\n    },\r\n    getWorkerPayments() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers_payment_history/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.payments = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}