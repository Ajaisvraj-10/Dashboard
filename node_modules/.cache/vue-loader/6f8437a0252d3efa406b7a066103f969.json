r  justify-content-between\">\r\n            <slot name=\"header\">\r\n              <h5 class=\"mb-0\"><span class=\"text-success\">Product Settings</span> / {{slugToWord(action)}}</h5>\r\n            </slot>\r\n            <div class=\"d-flex\">\r\n              <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Save\r\n              </b-button>\r\n              <router-link to=\"/app/product/product-price\">\r\n                <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Discard\r\n                </b-button>\r\n              </router-link>\r\n            </div>\r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll pt-2\">\r\n        <b-col lg=\"7\" xl=\"7\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n           <div class=\"o_composer_container my-2 row mx-0 pb-5\">\r\n              <div class=\"col-md-1 px-2\"></div>\r\n              <div class=\" col-md-10\">\r\n                <div class=\"user-info pt-2\">\r\n                  <ProductFields :product_id=\"product_id\" v-if=\"load_products\"></ProductFields>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:120px;\">Name: </span>\r\n                    <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.name\" name=\"name\" required>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:120px;\">Price (Kg): </span>\r\n                    <input placeholder=\"Price\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.price\" name=\"price\" required>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n           </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"5\" xl=\"5\" class=\"pl-0\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div class=\"o_composer_container my-2 row mx-0 pb-5\">\r\n              <div class=\" col-md-12\">\r\n                <div class=\"user-info pt-2\">\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:120px;\">Estimation (Constant): </span>\r\n                    <input placeholder=\"Constant\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.constant\" name=\"constant\">\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:130px;\">Qc auto aproval (Sample): </span>\r\n                    <b-form-checkbox v-model=\"form_data.qc_auto_approval\" name=\"qc_auto_approval\" >\r\n                      <b>{{ form_data.qc_auto_approval?'Yes':'No' }}</b>\r\n                    </b-form-checkbox>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:[],\r\n      id:null,\r\n      load_products:false,\r\n      lines:0,\r\n      product_id:null,\r\n      action:'create',\r\n      panchayaths:[],\r\n      vfcs:[],\r\n      farmers:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'product'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/products/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_id = jsonData.id\r\n        this.form_data = jsonData\r\n        this.load_products = true\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/products/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/products/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}