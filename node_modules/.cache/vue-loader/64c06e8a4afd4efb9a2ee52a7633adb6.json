tton type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">\r\n              Discard\r\n            </b-button>\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n      <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-row class=\"main-scroll pt-2\">\r\n      <b-col lg=\"6\" xl=\"6\" v-for=\"department in departments\">\r\n        <b-card body-class=\"p-2\">\r\n          <h5 class=\"font-weight-600 text-primary text-center\">{{department.name}} Department</h5>\r\n          <div class=\"\">\r\n            <div class=\"card mb-1 p-2\" v-for=\"heading in department.orders_headings\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-7\">\r\n                  <b class=\"py-1\">{{heading.name}}</b>\r\n                  <div class=\"text-12\" v-if=\"heading.answer.id\">@{{heading.answer.username}}</div>\r\n                </div>\r\n                <div class=\"col-md-5 text-right orders_headings_drpdwn\">\r\n                  <div v-if=\"heading.answer.id\">\r\n                    <span class=\"badge badge-primary cursor-pointer ml-1\" :style=\"'background-color:'+heading.answer.color+'!important;color:'+bgToTextColor(heading.answer.color)\">{{heading.answer.name}}</span>\r\n                    <div class=\"text-12\"><small>updated on</small>{{dateFormate(heading.answer.created_on,true)}}</div>\r\n                  </div>\r\n                  <div v-else-if=\"heading.employee_access.includes(job_type_id)\">\r\n                    <b-dropdown  no-caret>\r\n                      <template #button-content>\r\n                        <span class=\"badge badge-outline-primary p-1 cursor-pointer\">Complete Now</span>\r\n                      </template>\r\n                      <h6 class=\"text-center text-dark mb-0 p-3 pb-0\">Confirmation</h6>\r\n                      <b-dropdown-divider></b-dropdown-divider>\r\n                      <b-dropdown-item v-for=\"option in heading.options\" @click=\"changeStatus(heading, option)\">\r\n                        <div class=\"cursor-pointer\">\r\n                          <h6 class=\"mb-0 p-2\" style=\"border-radius:5px\" :style=\"'background-color:'+option.color+'!important;color:'+bgToTextColor(option.color)\" >Yes</h6>\r\n                        </div>\r\n                      </b-dropdown-item>\r\n                      <b-dropdown-divider></b-dropdown-divider>\r\n                    </b-dropdown>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </b-card>\r\n      </b-col>\r\n    </b-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      departments:[],\r\n      job_type_id:null,\r\n      indent_id:this.$route.params.indent_id\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'order-status'\r\n    this.getForm()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/'+this.employeeId+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.job_type_id = jsonData.job_position\r\n        this.getRows()\r\n      })\r\n    },\r\n    getRows() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/order_status/?order_status_indent='+this.indent_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.departments = jsonData\r\n      })\r\n    },\r\n    changeStatus(heaing,option) {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      var formdata = new FormData()\r\n      formdata.append('indent',this.indent_id)\r\n      formdata.append('heading',heaing.id)\r\n      formdata.append('answer',option.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      this.lock_btn = true\r\n      fetch(this.api_url+'/employee/order_status/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.getRows()\r\n          this.showAlert('Status Changed!')\r\n        }else{\r\n          this.showAlert('Alredy Added!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}