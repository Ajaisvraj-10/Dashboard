=\"d-flex mb-2 align-items-center justify-content-between\">\r\n        <slot name=\"header\">\r\n           <h5 class=\"mb-0\">{{slugToWord('labours_time')}} {{dateFormate(current_date)}} (shift {{shift}})</h5>\r\n        </slot>\r\n        <div class=\"d-flex\">\r\n          <h5 class=\"mb-0 mx-1\"></h5>\r\n        </div>\r\n        <div class=\"d-flex\">\r\n          <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n             @click=\"form_data={shift:shift,hours:8, overtime_double_hours:0, overtime_single_hours:0, workers:form_data.workers};$bvModal.show('worker-modal')\" v-if=\"form_data.workers.length>0 && current_date==today\">+ Set Time\r\n          </b-button>\r\n          <router-link to=\"/app/production/labours/time\">\r\n            <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">\r\n              Discard\r\n            </b-button>\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n      <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n\r\n    <b-row class=\"main-scroll pt-2\">\r\n      <b-col lg=\"9\" xl=\"9\">\r\n        <b-card>\r\n          <div>\r\n            <div class=\"main-scroll\" style=\"height:73vh\">\r\n              <table class=\"table table-striped\"> \r\n                <thead class=\"\">\r\n                  <tr >\r\n                    <th>\r\n                       <b-form-checkbox v-model=\"select_all\" @change=\"selectAll()\">\r\n                        <b>#Selct All</b>\r\n                      </b-form-checkbox>\r\n                    </th>\r\n                    <th>Gender</th>\r\n                    <th>Attendance</th>\r\n                    <th>Work Type</th>\r\n                    <th>Workig time(Hr)</th>\r\n                    <th>Double over time(Hr)</th>\r\n                    <th>Single over time(Hr)</th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"attendance in workers_attendances\">\r\n                    <th>\r\n                       <b-form-checkbox name=\"workers\" v-model=\"form_data.workers\" :value=\"attendance.id\">\r\n                        <b>#{{attendance.worker_detail.id}}/{{attendance.worker_detail.name}}</b>\r\n                      </b-form-checkbox>\r\n                    </th>\r\n                    <th>{{attendance.worker_detail.gender}}</th>\r\n                    <th>\r\n                      <span :class=\"attendance.attendance=='FULL TIME'?'text-success':attendance.attendance=='HALF TIME'?'text-warning':'text-danger'\">{{attendance.attendance}}</span>\r\n                    </th>\r\n                    <th>\r\n                      <span class=\"text-primary\">{{attendance.worker_detail.work_type}}</span>\r\n                    </th>\r\n                    <th>\r\n                      <input type=\"number\" min=\"0\" class=\"w-100\" v-model=\"attendance.hours\" @change=\"addAttedence(attendance)\">\r\n                    </th>\r\n                    <th>\r\n                      <input type=\"number\" min=\"0\" class=\"w-100\" v-model=\"attendance.overtime_double_hours\" @change=\"addAttedence(attendance)\">\r\n                    </th>\r\n                    <th>\r\n                      <input type=\"number\" min=\"0\" class=\"w-100\" v-model=\"attendance.overtime_single_hours\" @change=\"addAttedence(attendance)\">\r\n                    </th>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </div>\r\n          </div>\r\n        </b-card>\r\n      </b-col>\r\n      <b-col lg=\"3\" xl=\"3\" class=\"pl-0\">\r\n        <b-card>\r\n          <table class=\"table\">\r\n            <tr>\r\n              <td>Date</td>\r\n              <td>:</td>\r\n              <th>{{dateFormate(current_date)}}</th>\r\n            </tr>\r\n            <tr>\r\n              <td>Shift</td>\r\n              <td>:</td>\r\n              <th>{{shift}}</th>\r\n            </tr>\r\n            <tr v-if=\"department=='yard'\">\r\n              <td>{{'Yard'}}</td>\r\n              <td>:</td>\r\n              <th>{{department_name}}</th>\r\n            </tr>\r\n            <tr v-else>\r\n              <td>{{'Department'}}</td>\r\n              <td>:</td>\r\n              <th>{{department_name}} Department</th>\r\n            </tr>\r\n            <tr v-if=\"department=='department'\">\r\n              <td>Warehouse</td>\r\n              <td>:</td>\r\n              <th>{{warehouse_name}}</th>\r\n            </tr>\r\n            <tr>\r\n              <td>Labours</td>\r\n              <td>:</td>\r\n              <th>{{workers_attendances.length}}Labour{{checkMult(workers_attendances.length)}}</th>\r\n            </tr>\r\n          </table>\r\n        </b-card>\r\n      </b-col>\r\n    </b-row>\r\n\r\n    <div>\r\n      <b-modal id=\"worker-modal\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Set</span>/Time</h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                  <b-button type=\"submit\" variant=\"success\" class=\" px-3 py-1 d-sm-block mx-1\">Submit</b-button>\r\n                  <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1 d-sm-block mx-1\" @click=\"$bvModal.hide('worker-modal')\">Close</b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card header-bg-variant=\"transparent\" class=\"mt-2 pt-0\">\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\">Workig time(Hr) : </span>\r\n                  <input type=\"number\" min=\"0\" v-model=\"form_data.hours\" name=\"hours\">\r\n                </div>\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\">Double over time(Hr) : </span>\r\n                  <input type=\"number\" min=\"0\" v-model=\"form_data.overtime_double_hours\" name=\"overtime_double_hours\">\r\n                </div>\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\">Single over time(Hr) : </span>\r\n                  <input type=\"number\" min=\"0\" v-model=\"form_data.overtime_single_hours\" name=\"overtime_single_hours\">\r\n                </div>\r\n                <div class=\"o_thread_search_input mx-1 mt-2\">\r\n                  <span style=\"width:120px;\">Workers ({{form_data.workers.length}}): </span>\r\n                  <select v-model=\"form_data.workers\" multiple name=\"workers\" v-show=\"false\">\r\n                    <option v-for=\"w in form_data.workers\" :value=\"w\">{{w}}</option>\r\n                  </select>\r\n                  <input type=\"hidden\" name=\"shift\" :value=\"form_data.shift\">\r\n                  <table>\r\n                    <tr v-for=\"attendance in workers_attendances\">\r\n                      <th>\r\n                        <b>#{{attendance.worker_detail.id}}/{{attendance.worker_detail.name}}</b>\r\n                      </th>\r\n                      <th>({{attendance.worker_detail.gender}})</th>\r\n                    </tr>\r\n                  </table>\r\n                </div>\r\n                <hr class=\"my-2\">\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{hours:8, overtime_double_hours:0, overtime_single_hours:0, date:'', shift:'DAY',workers:[],},\r\n      workers_attendances:[],\r\n      select_all:false,\r\n      shift: this.$route.params.shift,\r\n      department: this.$route.params.department,\r\n      department_id: this.$route.params.department_id,\r\n      department_name:'',\r\n      current_date: this.$route.query.date,\r\n      warehouse_id:null,\r\n      warehouse_name:'',\r\n      select_all:false,\r\n      today:this.currentDate(),\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'production'\r\n    if(this.$route.params.warehouse){\r\n      this.warehouse_id=this.$route.params.warehouse\r\n    }\r\n    this.getPlanAccess()\r\n    this.getRows()\r\n  },\r\n  methods: {\r\n    getRows() {\r\n      var param = '?date='+this.current_date+'&department='+this.department+'&department_id='+this.department_id+'&shift='+this.shift\r\n      if(this.department=='department' && this.warehouse_id){\r\n        param += '&warehouse='+this.warehouse_id\r\n      }\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers_attendance/'+param, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.workers_attendances = jsonData.data\r\n        this.department_name = jsonData.department_name\r\n        this.warehouse_name = jsonData.warehouse_name\r\n      })\r\n    },\r\n    selectAll(){\r\n      var self = this\r\n      this.form_data.workers = []\r\n      if(!this.select_all){\r\n        this.workers_attendances.forEach(function(attendance){\r\n          self.form_data.workers.push(attendance.id)\r\n        })\r\n      }\r\n    },\r\n    dataForm() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers_attendance_mult/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.success){\r\n          this.$bvModal.hide('worker-modal')\r\n          this.getRows()\r\n          this.showAlert('Done!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    addAttedence(attendance) {\r\n      if(!attendance.attendance || !attendance.shift){\r\n        return false\r\n      }\r\n      var formdata = new FormData()\r\n      formdata.append('worker', attendance.worker)\r\n      formdata.append('attendance', attendance.attendance)\r\n      formdata.append('shift', attendance.shift)\r\n      formdata.append('hours', attendance.hours)\r\n      formdata.append('overtime_double_hours', attendance.overtime_double_hours)\r\n      formdata.append('overtime_single_hours', attendance.overtime_single_hours)\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers_attendance/'+attendance.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.showAlert('Done!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}