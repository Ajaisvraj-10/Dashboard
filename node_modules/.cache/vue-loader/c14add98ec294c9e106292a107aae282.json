"/app/qc/normal-materials-test/create\"><b-button variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >+ Create\r\n              </b-button></router-link>\r\n              <router-link tag=\"a\" class to=\"/app/oodo/import\"><b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Import\r\n              </b-button></router-link> -->\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: true,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: false,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows.results\"\r\n      >\r\n        <div slot=\"table-actions\" class=\"mb-3 filter-but\">\r\n\r\n        </div>\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <router-link :to=\"'/app/qc/vehicle-inspection/normal/create/'+props.row.id\">\r\n              <b-button  variant=\"primary\" class=\" px-3 py-1 mx-1\" v-if=\"props.row.is_vehicle_inspection<=0\">Vehicle Inspection</b-button>\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1 ml-2\" v-else>Tested (Vehicle)</b-button>\r\n            </router-link>\r\n            <router-link :to=\"'/app/qc/normal-materials-test/edit/'+props.row.id\">\r\n              <b-button  variant=\"success\" class=\" px-3 py-1 mx-1\" v-if=\"props.row.status=='Reached Factory'\">Test Now</b-button>\r\n              <b-button  variant=\"danger\" class=\" px-3 py-1 ml-2\" v-else>Tested</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'procure_id'\">\r\n            <b>{{props.row.raw_number}}</b>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'vehicle_detail'\">\r\n            <div>Number: <b>{{props.row.vehicle_detail.vehicle_number}}</b></div>\r\n            <div>Type: <b>{{props.row.vehicle_detail.vehicle_type}}</b></div>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'product'\">\r\n            {{props.row.category}}> {{props.row.product}}{{props.row.parameter?'> '+props.row.parameter:''}}\r\n            <sup>\r\n              <span class=\"badge badge-success\" v-if=\"props.row.farmer_status=='Organic'\">ORG</span>\r\n            </sup>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'created_on'\">\r\n            {{dateFormate(props.row.created_on,true)}}\r\n          </span>\r\n        </template>\r\n      </vue-good-table>\r\n      <TablePagination :page=\"page\" :rows=\"rows\" :load_first=\"load_first\" @changePage=\"changePage\" v-if=\"rows.count>0\"></TablePagination>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TablePagination from '/src/views/app/oodo/components/table_pagination'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Raw Test\"\r\n  },\r\n  components:{\r\n    TablePagination\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n         {\r\n          label: \"Dispatch No\",\r\n          field: \"procure_id\"\r\n        },\r\n        {\r\n          label: \"Vehicle\",\r\n          field: \"vehicle_detail\"\r\n        },\r\n        {\r\n          label: \"Product\",\r\n          field: \"product\"\r\n        },\r\n        {\r\n          label: \"Purchase Type\",\r\n          field: \"purchase_type\"\r\n        },\r\n        {\r\n          label: \"Status\",\r\n          field: \"status\"\r\n        },\r\n        {\r\n          label: \"Date\",\r\n          field: \"created_on\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: {results:[],count:0},\r\n      page:0,\r\n      load_first:true,\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'qc'\r\n    this.changePage(1)\r\n  },\r\n  methods: {\r\n    changePage(count){\r\n      this.page = count\r\n      this.load_first = true\r\n      this.getProcurements()\r\n    },\r\n    getProcurements() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/?page='+this.page+'&normal_status=reached_factory', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.detail=='Invalid page.'){\r\n          this.rows = {results:[],count:0}\r\n        }else{\r\n          this.load_first = false\r\n          this.rows = jsonData\r\n        }\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}