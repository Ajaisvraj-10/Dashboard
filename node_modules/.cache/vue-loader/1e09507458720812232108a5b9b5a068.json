dataForm()\">\r\n      <div>\r\n         <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Indent</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                  <b-button type=\"button\" variant=\"primary\" class=\"px-3 py-1  d-sm-block mx-1\" @click=\"openDispatchIndent()\" v-if=\"id\">\r\n                     {{form_data.dispatch_indent.id?'Dispatched':'Add to Dispatch'}}\r\n                  </b-button>\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/logistics-department/indent\">\r\n                  <b-button  type=\"button\"variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n\r\n        <b-col lg=\"4\" xl=\"4\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\" col-md-12\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Indent Number : </span>\r\n                              <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"indent_number\" v-model=\"form_data.indent_number\" readonly>\r\n                            </div>\r\n\r\n                            <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n                            <!--<div class=\"o_thread_search_input mt-2\">\r\n                              <span style=\"width:130px\">  Have parameter?: </span>\r\n                              <b-form-checkbox v-model=\"form_data.is_parameters\" name=\"is_parameters\" switch>\r\n                                <b>{{ form_data.is_parameters?'Yes':'No' }}</b>\r\n                              </b-form-checkbox>\r\n                            </div> -->\r\n                            <div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Parameter : </span>\r\n                                <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"parameter\" v-model=\"form_data.parameter\" required>\r\n                                    <option v-for=\"parameter in parameters\" :value=\"parameter.id\">{{parameter.name}}</option>\r\n                                </select>\r\n                              </div>\r\n                            </div>\r\n\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Quantity (Kg) : </span>\r\n                              <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.quantity\" name=\"quantity\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">PO Number : </span>\r\n                              <input placeholder=\"PO Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.PO_number\" name=\"PO_number\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Expected Date: </span>\r\n                              <input placeholder=\"Expected Date\" type=\"month\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.expected_date\" name=\"expected_date\" required>\r\n                            </div>  \r\n\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Remark : </span>\r\n                              <textarea placeholder=\"Remark\" rows=\"5\" class=\"ui-autocomplete-input w-100 mx-1\" autocomplete=\"off\" v-model=\"form_data.remark\" name=\"remark\"></textarea>\r\n                              <input type=\"hidden\" name=\"product\" :value=\"product_id\" v-if=\"id\">\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n        <b-col lg=\"8\" xl=\"8\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <h5>Packing Materials</h5>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-12\">\r\n                    <b-button variant=\"success\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"lineOperation('+')\">\r\n                      Add a material\r\n                    </b-button>\r\n                    <input type=\"hidden\" name=\"keys\" :value=\"form_data.values.length\">\r\n                  </div>\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\" style=\"overflow-x:auto\">\r\n                        <div class=\"col-md-6 px-2\" v-for=\"(line,key) in form_data.values\">\r\n                          <div class=\"card p-2 my-1\">\r\n                            <div class=\"o_thread_search_input px-0\">\r\n                              <select class=\"ui-autocomplete-input w-100 ml-0\" :name=\"'material'+key\" v-model=\"line.material\" @change=\"checkMaterialStock(line)\">\r\n                                <option v-for=\"material in materials\" :value=\"material.id\">{{material.name}}</option>\r\n                              </select>\r\n                              <div class=\"pb-3\" v-if=\"!line.id\">\r\n                                <b-button variant=\"danger\" type=\"button\" class=\"ml-2 px-3 py-1 d-sm-block\" @click=\"lineOperation(key)\">\r\n                                  Remove\r\n                                </b-button>\r\n                              </div>\r\n                              <div class=\"pb-3\" v-else>\r\n                                <input type=\"hidden\" :name=\"'id'+key\" v-model=\"line.id\">\r\n                              </div>\r\n                            </div>\r\n                            <table class=\"table material_table mb-0\">\r\n                              <tbody>\r\n                                <tr v-for=\"(specification,key2) in line.specifications\" v-if=\"specification.material_ids.includes(line.material)\">\r\n                                  <th>{{specification.name}}</th>\r\n                                  <td>\r\n                                    <div>\r\n                                      <select class=\"ui-autocomplete-input w-100 mx-1\" :name=\"specification.id+'value'+key\" v-model=\"specification.value\" v-if=\"specification.values\" @change=\"checkMaterialStock(line)\">\r\n                                        <option v-for=\"value in specification.values\" :value=\"value.value\">{{value.value}}</option>\r\n                                      </select>\r\n                                      <div v-else><input type=\"checkbox\" :name=\"specification.id+'value'+key\" v-model=\"specification.value\" :id=\"'check'+key\" @change=\"checkMaterialStock(line)\"> <label :for=\"'check'+key\"> YES</label></div>\r\n                                    </div>\r\n                                  </td>\r\n                                </tr>\r\n                                <tr>\r\n                                  <th>Max Qty (Kg)</th>\r\n                                  <td>\r\n                                    <input placeholder=\"Max QTY\" type=\"text\" class=\"\" autocomplete=\"off\" v-model=\"line.max_quantity\" :name=\"'max_quantity'+key\" required>\r\n                                    <input type=\"hidden\" v-model=\"line.max_quantity\" name=\"'max_quantity'+key\">\r\n                                  </td>\r\n                                </tr>\r\n                                <tr>\r\n                                <tr>\r\n                                  <th>Quantity (Kg)</th>\r\n                                  <td>\r\n                                    <input placeholder=\"QTY\" type=\"text\" class=\"\" autocomplete=\"off\" v-model=\"line.requested_quantity\" :name=\"'requested_quantity'+key\" @input=\"calcultion()\" required>\r\n                                  </td>\r\n                                </tr>\r\n                                <tr>\r\n                                  <th> \r\n                                    Stock:\r\n                                    <input placeholder=\"QTY\" type=\"hidden\" class=\"\" autocomplete=\"off\" :value=\"checkMaterialStock(line)\" readonly>\r\n                                  </th>\r\n                                  <td>\r\n                                    <b-button variant=\"primary\" type=\"button\" class=\"ml-2 px-3 py-1 d-sm-block\" @click=\"openPrintModal(line)\" v-if=\"line.stock_quantity>0 && line.id\">\r\n                                      {{line.print_indent.id?'Print (Requested)':'Print Request'}}\r\n                                    </b-button>\r\n                                    <span class=\"text-danger\" v-else-if=\"line.stock_quantity<=0\">Stock is empty!</span>\r\n                                    <span class=\"text-success\" v-else>Stock is available!</span>\r\n                                  </td>\r\n                                </tr>\r\n                              </tbody>\r\n                            </table>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n\r\n    <div>\r\n      <b-modal id=\"print-indent\" hide-header  hide-footer>\r\n        <form ref=\"printIndent\" @submit.prevent=\"printIndent()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Print Indent</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Print\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('print-indent')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Indent Number: </span>\r\n                            <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.indent_number\" name=\"indent_number\" readonly>\r\n                            <input type=\"hidden\" name=\"indent_packing\" v-model=\"print_indent.indent_packing\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Packing Material: </span>\r\n                            <input placeholder=\"Packing Material\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"print_indent.material_detail.name\" readonly>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Slides : </span>\r\n                            <input type=\"radio\" name=\"slide\" class=\"ui-autocomplete-input mx-1\" v-model=\"print_indent.slide\" value=\"1\"> 1\r\n                            <span style=\"width:20px;\"> </span>\r\n                            <input type=\"radio\" name=\"slide\" class=\"ui-autocomplete-input mx-1\" v-model=\"print_indent.slide\" value=\"2\"> 2\r\n                            <span style=\"width:50px;\"> </span>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">ICO Number: </span>\r\n                            <input placeholder=\"ICO Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"ICO_number\" v-model=\"print_indent.ICO_number\" required>\r\n                          </div>\r\n\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n\r\n      <b-modal id=\"dispatch\" hide-header  hide-footer>\r\n        <form ref=\"dispatchIndent\" @submit.prevent=\"dispatchIndent()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Dispatch</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('dispatch')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Date: </span>\r\n                            <input placeholder=\"Date\" type=\"date\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"date\" v-model=\"form_data.dispatch_indent.date\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:220px;\">Expected Vehicle Arraival Time : </span>\r\n                            <input placeholder=\"Time\" type=\"time\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"expected_vehicle_arraival_time\" v-model=\"form_data.dispatch_indent.expected_vehicle_arraival_time\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Customer: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"customer\" v-model=\"form_data.dispatch_indent.customer\">\r\n                              <option v-for=\"customer in customers\" :value=\"customer.id\">{{customer.customer_code}} - {{customer.name}}</option>\r\n                            </select>\r\n                          </div>\r\n\r\n                          <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n\r\n                          <div class=\"o_thread_search_input\" v-if=\"form_data.parameter\">\r\n                            <span style=\"width:120px;\">Parameter : </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"parameter\" v-model=\"form_data.dispatch_indent.parameter\" required>\r\n                                <option v-for=\"parameter in parameters\" :value=\"parameter.id\">{{parameter.name}}</option>\r\n                            </select>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input mt-4\">\r\n                            <span style=\"width:120px;\">ICO Number : </span>\r\n                            <input placeholder=\"ICO Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.dispatch_indent.ICO_number\" name=\"ICO_number\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Indent Number : </span>\r\n                            <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.dispatch_indent.indent_number\" name=\"indent_number\" required>\r\n                            <input type=\"hidden\" name=\"product\" :value=\"product_id\" v-if=\"id\">\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Quantity (Kg) : </span>\r\n                            <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.dispatch_indent.quantity\" name=\"quantity\" required>\r\n                          </div>\r\n                          \r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{parameter:null,expected_date:'2022-03',total:0,values:[],total_quantity:0, indent_number:'',is_parameters:false, dispatch_indent:{}},\r\n      id:null,\r\n      action:'create',\r\n      product_id:null,\r\n      load_products:false,\r\n      customers:[],\r\n      parameters:[],\r\n      materials:[],\r\n      specifications:[],\r\n      packing_materials:[],\r\n      print_indent:{material_detail:{}},\r\n    };\r\n  },\r\n  mounted() {\r\n    this.form_data.expected_date = this.currentMonth()\r\n    this.form_data.indent_number = this.autoIndentNumber()\r\n    this.$root.current_section = 'logistics-department'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n    }\r\n    this.getPackingMaterials()\r\n    this.getCustomers()\r\n    this.getMaterials()\r\n    this.getSpecifications()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.product_id = jsonData.product\r\n        this.load_products = true\r\n        this.getParameters()\r\n      })\r\n    },\r\n    getCustomers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/customers/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.customers = jsonData\r\n      })\r\n    },\r\n    getMaterials() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/materials/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.materials = jsonData\r\n      })\r\n    },\r\n    getSpecifications() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/specifications/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.specifications = jsonData\r\n      })\r\n    },\r\n    lineOperation(operation){\r\n      if(operation=='+'){\r\n        var specification_values = [];\r\n        this.specifications.forEach(function(val,key){\r\n          if(val.specification_for=='Packing Material'){\r\n            specification_values.push({id:val.id, name:val.name ,value:'', is_values:val.is_values, values:val.values,material_ids:val.material_ids})\r\n          }\r\n        })\r\n        this.form_data.values.push({material:this.materials[0].id, requested_quantity:0, price:0,max_quantity:0,stock_quantity:0, specifications:specification_values})\r\n        this.total_keys++\r\n      }else{\r\n        this.form_data.values.splice(operation, 1)\r\n        this.total_keys--\r\n      }\r\n      this.calcultion()\r\n    },\r\n    getPackingMaterials(){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase_materials/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.packing_materials = jsonData\r\n      })\r\n    },\r\n    checkMaterialStock(material){\r\n      var packing_materials = this.packing_materials\r\n      var quantity = 0\r\n      packing_materials.forEach(function(p1){\r\n        var is_pack = true\r\n        if(material.material==p1.material){\r\n          p1.specifications.forEach(function(s2){\r\n            var is_specs = true\r\n            if(p1.material_detail.specifications.includes(s2.id)){\r\n              var spec = true\r\n              material.specifications.forEach(function(s1){\r\n                if(s1.material_ids.includes(material.material)){\r\n                  if(s2.id==s1.id){\r\n                    if(s2.value==s1.value || (s1.values==null && s2.value=='on' && s1.value)){\r\n                    }else{\r\n                      spec = false\r\n                    }\r\n                  }\r\n                }\r\n              })\r\n              if(!spec){\r\n                is_specs = false\r\n              }\r\n            }\r\n            if(!is_specs){\r\n              is_pack = false\r\n            }\r\n          })\r\n        }else{\r\n          is_pack = false\r\n        }\r\n        if(is_pack){\r\n          quantity = quantity+p1.quantity\r\n        }\r\n      })\r\n      material.stock_quantity = quantity\r\n      return quantity\r\n    },\r\n    calcultion(){\r\n      var quantity = 0\r\n      var price = 0\r\n      this.form_data.values.forEach(function(val,key){\r\n        quantity = quantity + parseFloat(val.requested_quantity)\r\n      })\r\n      this.form_data.total_quantity = quantity\r\n    },\r\n    autoIndentNumber(){\r\n      var dateTime = new Date()\r\n      return dateTime.getTime()\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.getParameters()\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameters/?product='+this.product_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.parameters = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.id = jsonData.id\r\n          this.form_data.product_detail = jsonData.product_detail\r\n          this.indentPackingDetails()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.indentPackingDetails()\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    indentPackingDetails() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('department',this.form_data.product_detail.category)\r\n      formdata.append('indent',this.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent_packing_details/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.getForm()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    printIndent(method='POST') {\r\n      var param=''\r\n      if(this.print_indent.id){\r\n        method='PUT'\r\n        param = this.print_indent.id+'/'\r\n      }\r\n      var formdata = new FormData(this.$refs['printIndent'])\r\n      formdata.append('indent',this.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/print_indent/'+param, {\r\n          method : method,\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.$bvModal.hide('print-indent')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    openPrintModal(line){\r\n      this.print_indent=line.print_indent;\r\n      if(!this.print_indent.id){\r\n        this.print_indent.indent_packing=line.id;\r\n        this.print_indent.material_detail=line.material_detail;\r\n      }\r\n      this.$bvModal.show('print-indent')\r\n    },\r\n    openDispatchIndent(){\r\n      if(!this.form_data.dispatch_indent.id){\r\n        this.form_data.dispatch_indent.date = this.currentDate()\r\n        this.form_data.dispatch_indent.ICO_number = this.form_data.ICO_number\r\n        this.form_data.dispatch_indent.indent_number = this.form_data.indent_number\r\n        this.form_data.dispatch_indent.parameter = this.form_data.parameter\r\n        this.form_data.dispatch_indent.quantity = this.form_data.quantity\r\n      }\r\n      this.$bvModal.show('dispatch')\r\n    },\r\n    dispatchIndent(method='POST') {\r\n      var param=''\r\n      if(this.form_data.dispatch_indent.id){\r\n        method='PUT'\r\n        param = this.form_data.dispatch_indent.id+'/'\r\n      }\r\n      var formdata = new FormData(this.$refs['dispatchIndent'])\r\n      formdata.append('indent',this.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/dispatch_indent/'+param, {\r\n          method : method,\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.$bvModal.hide('dispatch')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n<style>\r\n.despatch label{\r\n  font-weight: 700;\r\n  white-space: nowrap;\r\n    align-self: center;\r\n    margin-right: 10px;\r\n    min-width:140px;\r\n}\r\n.unloading .btn-secondary{\r\n  width:100%;\r\n  \r\n    border: 2px solid #efefef;\r\n    background-color: #f8f9fa!important;\r\n    border-radius: 2px;\r\n    text-align: left;\r\n    height: calc(1.9695rem + 2px);\r\n    padding: 5px;\r\n}\r\n\r\n   .table-3 th {\r\n    border: 1px solid #dcdfe6;\r\n    background-color: #f3f4f7;\r\n}\r\n   .table-3 th, .table-3 td {\r\n    border: 1px solid #dcdfe6;\r\n}\r\n  .table-3 th {\r\n    padding:0.75em 1.5em 0.75em 0.75em;\r\n  }\r\n.table-3 .tr-bottom td:nth-child(2){\r\n  border: 2px solid #dcdfe6;\r\n background-color: #f3f4f7;\r\n  \r\n}\r\n.table-3 .tr-bottom td:nth-child(1){\r\nborder: none;\r\n  \r\n}\r\n</style>\r\n"]}]}