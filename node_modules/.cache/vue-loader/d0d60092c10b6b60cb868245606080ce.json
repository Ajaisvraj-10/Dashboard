<h4 class=\"font-weight-700 px-3\">Add Visitor</h4>\r\n        </div>\r\n      </div>\r\n      <div class=\"\" v-if=\"not_start\">\r\n        <div class=\"loading\" style=\"z-index:999999999999999\"></div>\r\n      </div>\r\n      <form class=\"form-tab\" ref=\"dataForm\" @submit.prevent=\"dataForm()\" :style=\"not_start?'opacity:0.5':''\">\r\n        \r\n        <div class=\"body-bio px-3 py-2 create\">\r\n          <div class=\"user-info pt-2\">\r\n            <div class=\"o_thread_search_input\" v-show=\"input_col==1\">\r\n              <span style=\"width:120px;\">Type : </span>\r\n              <div class=\"inspection-btn\" >\r\n                <button type=\"button\" class=\"btn\" v-for=\"security_type in security_types\" :class=\"form_data.vehicle_for==security_type?'active-btn':''\" :style=\"security_type=='Employee Vehicle'?'color:red;':security_type=='Government Vehicle'?'color:orange;':security_type=='Guest'?'color:blue;':security_type=='Diocese'?'color:green;':security_type=='Customer/Client'?'color:purple;':security_type=='Other'?'color:gray;':security_type=='Workers Vehicle'?'color:lightblue;':''\" @click=\"form_data.vehicle_for=security_type\">{{security_type}} </button>\r\n                <input type=\"hidden\" v-model=\"form_data.vehicle_for\" name=\"vehicle_for\" :class=\"'check_field_'+1\" required>\r\n              </div>\r\n            </div>\r\n            <div v-show=\"input_col==2\">\r\n              <div class=\"o_thread_search_input\">\r\n                <span style=\"width:120px;\">Name : </span>\r\n                <input placeholder=\"Name\" type=\"text\" class=\"form-control\" autocomplete=\"off\" v-model=\"form_data.name\" name=\"name\" :class=\"'check_field_'+2\" @keyup=\"seachBook()\" required>\r\n                <div class=\"inspection-btn\" >\r\n                  <button type=\"button\" class=\"btn\" v-for=\"security_book in security_books\" :class=\"form_data.a_id==security_book.id?'active-btn':''\" @click=\"form_data.a_id=security_book.id;form_data.name=security_book.name;form_data.contact=security_book.contact;form_data.vehicle_type=security_book.vehicle_type;form_data.vehicle_number=security_book.vehicle_number\" :style=\"security_book.vehicle_for=='Employee Vehicle'?'color:red;':security_book.vehicle_for=='Government Vehicle'?'color:orange;':security_book.vehicle_for=='Guest'?'color:blue;':security_book.vehicle_for=='Diocese'?'color:green;':security_book.vehicle_for=='Customer/Client'?'color:purple;':form_data.vehicle_for=='Other'?'color:gray;':security_book.vehicle_for=='Workers Vehicle'?'color:lightblue;':''\">{{security_book.name}} </button>\r\n                  <input type=\"hidden\" v-model=\"form_data.a_id\" name=\"employee\" v-if=\"form_data.vehicle_for=='Employee Vehicle'\">\r\n                  <input type=\"hidden\" v-model=\"form_data.a_id\" name=\"worker\" v-if=\"form_data.vehicle_for=='Workers Vehicle'\">\r\n\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-show=\"input_col==3\">\r\n              <div class=\"o_thread_search_input\">\r\n                <span style=\"width:120px;\">Contact : </span>\r\n                <input placeholder=\"Contact\" type=\"text\" class=\"form-control\" autocomplete=\"off\" v-model=\"form_data.contact\" name=\"contact\" :class=\"'check_field_'+3\" required>\r\n              </div>\r\n            </div>\r\n            <div v-show=\"input_col==4\">\r\n              <div class=\"o_thread_search_input\">\r\n                <span style=\"width:120px;\">Vehicle Type : </span>\r\n                <input placeholder=\"Vehicle Type\" type=\"text\" class=\"form-control\" autocomplete=\"off\" v-model=\"form_data.vehicle_type\" name=\"vehicle_type\" :class=\"'check_field_'+4\" required>\r\n\r\n                <div class=\"inspection-btn\" >\r\n                  <button type=\"button\" class=\"btn\" v-for=\"vehicle_type in vehicle_types\" :class=\"form_data.vehicle_type==vehicle_type?'active-btn':''\" @click=\"form_data.vehicle_type=vehicle_type\">{{vehicle_type}} </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div v-show=\"input_col==5\">\r\n              <div class=\"o_thread_search_input\">\r\n                <span style=\"width:120px;\"> Vehicle Number : </span>\r\n                <input placeholder=\"Vehicle Number\" type=\"text\" class=\"form-control\" autocomplete=\"off\" v-model=\"form_data.vehicle_number\" name=\"vehicle_number\" :class=\"'check_field_'+5\" required>\r\n              </div>\r\n              <div class=\"o_thread_search_input\">\r\n                <span style=\"width:120px;\"> Origin of the Vehicle : </span>\r\n                <input placeholder=\"Origin of the Vehicle\" type=\"text\" class=\"form-control\" autocomplete=\"off\" v-model=\"form_data.origin_of_the_vehicle\" name=\"origin_of_the_vehicle\" required>\r\n              </div>\r\n            </div>\r\n            <div v-show=\"input_col==6\">\r\n              <div class=\"o_thread_search_input\">\r\n                <span style=\"width:120px;\">Who came to see? : </span>\r\n                <div class=\"inspection-btn\" v-if=\"form_data.vehicle_for=='Government Vehicle' || form_data.vehicle_for=='Customer/Client' || form_data.vehicle_for=='Guest' || form_data.vehicle_for=='Diocese'\">\r\n                  <button type=\"button\" class=\"btn\" v-for=\"employee in employees\" :class=\"form_data.see_for==employee.id?'active-btn':''\" @click=\"form_data.see_for=employee.id\">{{employee.name}} ({{employee.job_position_detail.name}}) </button>\r\n                  <input type=\"hidden\" v-model=\"form_data.see_for\" name=\"see_for\" :class=\"'check_field_'+6\" required>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\" my-1 footer border-top\">\r\n          <div class=\"px-3\" v-if=\"not_start==false\">\r\n\r\n            <div v-if=\"input_col==6\">\r\n              <button type=\"submit\" class=\"btn btn-secondary btn-lg btn-block text-18 font-weight-700\">Save</button> \r\n            </div>\r\n            <div class=\"\" style=\"display: flex;justify-content: flex-end;gap: 10px;\" v-else>\r\n              <button type=\"button\" class=\"btn btn-light\" @click=\"input_col>1?backCol():''\"> <i class=\"fa fa-angle-left\" style=\"\"></i> Back</button>\r\n              <button type=\"button\" class=\"btn btn-secondary\" @click=\"nextCol()\">Next <i class=\"fa fa-angle-right\" style=\"color:white;\"></i></button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        \r\n        <b-modal id=\"confirmPopup\" title=\"Alert\"  hide-footer>\r\n          <div class=\"modal-body p-0 m-0\">\r\n              <!-- <div v-html=\"csrf\"></div> -->\r\n              <div class=\"row mx-0 mb-3\">\r\n                  <div class=\"col-md-12\">\r\n                    Are you sure want to this action?\r\n                  </div>\r\n              </div>\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n              <button type=\"button\" class=\"btn btn-success text-18  px-4\" @click=\"dataForm('yes')\" v-if=\"not_start==false\">Yes</button>\r\n              <button type=\"button\" class=\"btn btn-outline-success text-18  px-4\" @click=\"$bvModal.hide('confirmPopup')\">No</button>\r\n          </div>\r\n        </b-modal>\r\n      </form>\r\n\r\n    </main>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{date:'',vehicle_for:'', a_id:null, name:'', contact:'', vehicle_type:'', vehicle_number:'', see_for:''},\r\n      id:null,\r\n      input_col:1,\r\n      action:'create',\r\n      security_books:[],\r\n      employees:[],\r\n      vehicle_types:['Car','Bike','Auto Rickshaw','Pickup','Lorry','Goods Auto','Tipper Lorry','Truck','Other'],\r\n      not_start:true,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.form_data.date = this.currentDate()\r\n    this.$root.current_section = 'security-gate'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.not_start=false\r\n    }\r\n    this.getEmployees()\r\n  },\r\n  methods: {\r\n    nextCol(next_section=''){\r\n      var is_input = document.getElementsByClassName('check_field_'+this.input_col)\r\n      var next = true\r\n      for(var i=0;i<is_input.length;i++){\r\n        is_input[i].style.border = 'none'\r\n        if(is_input[i].value==''){\r\n          is_input[i].style.border = '1px red solid'\r\n          next = false\r\n        }\r\n      }\r\n      if(next){\r\n        this.input_col++\r\n      }else{\r\n        this.showAlert('Please select a coloumn!')\r\n      }\r\n    },\r\n    backCol(){\r\n      this.input_col--\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/security/security_book/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.not_start=false\r\n      })\r\n    },\r\n    dataForm(action='no') {\r\n      if(action=='no'){\r\n        this.$bvModal.show('confirmPopup')\r\n        return false\r\n      }\r\n      this.$bvModal.hide('confirmPopup')\r\n      this.not_start=true\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var d = new Date();\r\n      var current_time = this.currentDate()+' '+d.getHours()+':'+d.getMinutes();\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('gate_in', 1)\r\n      formdata.append('gate_in_time', current_time)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/security/security_book/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.$router.push('/visitor-book')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    seachBook() {\r\n      this.security_books = []\r\n      if(!this.form_data.name){\r\n        return false\r\n      }\r\n      var formdata = new FormData()\r\n      formdata.append('name',this.form_data.name)\r\n      formdata.append('vehicle_for',this.form_data.vehicle_for)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/security/search_security_book/', {\r\n          method : 'post',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.security_books = jsonData\r\n      })\r\n    },\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.employees = jsonData\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n.input-group-append{\r\n  padding-top: 0;\r\n  padding-bottom: 0;\r\n}\r\n</style>\r\n"]}]}