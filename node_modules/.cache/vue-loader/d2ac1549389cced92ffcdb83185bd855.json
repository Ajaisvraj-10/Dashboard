                   <option v-for=\"purchase_type in purchase_types\" :value=\"purchase_type.id\" v-if=\"purchase_type.id==1\">{{purchase_type.name}}</option>\r\n                              </select>\r\n                            </div>\r\n\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Sample Id: </span>\r\n                              <input type=\"text\" placeholder=\"Sample Id\" class=\"\" autocomplete=\"off\" :value=\"sample_id+'/Sample'\" readonly required>\r\n                              <input type=\"hidden\" name=\"sample_procurement\" :value=\"sample_id\">\r\n                            </div>\r\n\r\n                            <ProductFields :product_id=\"product_id\" :readonly=\"true\" v-if=\"load_products\"></ProductFields>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{date:new Date(),type:'Raw Material',sample_procurement_detail:{id:null,qc_status:null},'farmer_estimate':0,'farmer_detail':{'name':'', 'farmer_code':''},'farmer':null, parameters:{datas:[]},status:'Waiting',purchase_type:1},\r\n      id:null,\r\n      sample_id:null,\r\n      product_id:null,\r\n      load_products:false,\r\n      action:'create',\r\n      qc_statuses:['Waiting','Approved','Reject']\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'qc'\r\n    this.form_data.date = this.currentDate()\r\n    this.sample_id = this.$route.params.id\r\n    this.getPurchaseTypes()\r\n    this.checkSampleTest()\r\n    this.getParameters()\r\n\r\n  },\r\n  methods: {\r\n    checkSampleTest() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/check_sample_test/'+this.sample_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.id = jsonData.id\r\n          this.getForm()\r\n        }else{\r\n          this.selectSample()\r\n        }\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_materials_test/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.product_id = jsonData.sample_procurement_detail.product\r\n        this.load_products = true\r\n        if(jsonData.parameters){\r\n          var jparameters = JSON.parse(jsonData.parameters)\r\n          var length = jparameters.length/3\r\n          this.form_data.parameters = this.pagination(jparameters,Math.round(length))\r\n        }else{\r\n          this.form_data.parameters = {datas:[]}\r\n          this.getParameters()\r\n        }\r\n      })\r\n    },\r\n    selectSample(){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_procurement/'+this.sample_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.sample_procurement = jsonData.id\r\n        this.form_data.sample_procurement_detail = jsonData\r\n        this.form_data.farmer = jsonData.farmer\r\n        this.form_data.farmer_detail = jsonData.farmer_detail\r\n        this.product_id = jsonData.product\r\n        this.load_products = true\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.id){\r\n        this.updateFormdata()\r\n      }else{\r\n        this.createFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_materials_test/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.form_data = jsonData\r\n          this.updateStatus()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_materials_test/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.updateStatus()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/physical_parameters/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var length = jsonData.length/3\r\n        this.form_data.parameters = this.pagination(jsonData,Math.round(length))\r\n      })\r\n    },\r\n    checkParam(param){\r\n      if(param.limit_type=='<'){\r\n        if(param.limit2 == 0){\r\n          if(param.limit < param.limit1){\r\n\r\n          }\r\n        }else{\r\n          if(param.limit > param.limit1 && param.limit < param.limit2){\r\n\r\n          }\r\n        }\r\n      }else if(param.limit_type=='>'){\r\n        if(param.limit2 == 0){\r\n          if(param.limit > param.limit1){\r\n\r\n          }\r\n        }else{\r\n          if(param.limit < param.limit1 && param.limit > param.limit2){\r\n\r\n          }\r\n        }\r\n      }else if(param.limit_type=='%'){\r\n        if(param.limit2 == 0){\r\n          if(param.limit < param.limit1){\r\n\r\n          }\r\n        }else{\r\n          if(param.limit >= param.limit1 && param.limit <= param.limit2){\r\n\r\n          }\r\n        }\r\n      }\r\n    },\r\n    updateStatus() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('id',this.form_data.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_materials_update_qc_status/', {\r\n          method : 'post',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.showAlert('Saved Successfully!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}