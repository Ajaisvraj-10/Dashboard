\r\n                                  <div class=\"user-info pt-2\" v-for=\"param in data\">\r\n                                    <div class=\"o_thread_search_input mt-2\">\r\n                                      <span style=\"width:100px;\" :for=\"param.name\"> {{param.name}} : </span>\r\n                                      <span>\r\n                                        <input type=\"checkbox\" class=\"ui-autocomplete-input mx-2\" autocomplete=\"off\" value=\"1\" v-model=\"param.checked\" :name=\"param.name\" :id=\"param.name\">\r\n                                        <input placeholder=\"%\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input w-50\" autocomplete=\"off\" v-model=\"param.limit\" :name=\"param.name+'limit'\" v-if=\"param.checked\" @input=\"checkParam(param)\" required>\r\n                                      </span>\r\n                                    </div>\r\n                                  </div>\r\n                                </div>\r\n                                \r\n                              </div>\r\n                            </div> \r\n                        \r\n                            <div class=\"o_thread_search_input mt-4\" v-if=\"form_data.sample_procurement_detail.id!=null\">\r\n                              <span style=\"width:120px;\"> Qc Status: </span>\r\n                              <div>\r\n                                <input type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.status\" name=\"status\" readonly>\r\n                                <button type=\"button\" class=\"btn btn-success ml-4\" @click=\"form_data.status='Approved'\">Approve</button>\r\n                                <button type=\"button\" class=\"btn btn-danger ml-2\" @click=\"form_data.status='Reject'\">Reject</button>\r\n                              </div>\r\n                            </div>\r\n                            \r\n                          </div>\r\n             