                     <span style=\"width:120px;\">Purchase Type  : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"purchase_type\" v-model=\"form_data.purchase_type\">\r\n                                  <option v-for=\"purchase_type in purchase_types\" :value=\"purchase_type.id\" v-if=\"purchase_type.id==2\">{{purchase_type.name}}</option>\r\n                              </select>\r\n                            </div>\r\n\r\n                            <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n                            \r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">  Have parameters?: </span>\r\n                              <input type=\"checkbox\" class=\"ui-autocomplete-input mt-2\" autocomplete=\"off\" v-model=\"form_data.is_parameters\" name=\"is_parameters\">\r\n                              <span style=\"width:270px;\">  </span>\r\n                            </div>\r\n                            <div v-if=\"form_data.is_parameters\">\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Parameter : </span>\r\n                                <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"parameter\" v-model=\"form_data.parameter\">\r\n                                    <option v-for=\"parameter in parameters\" :value=\"parameter.id\">{{parameter.name}}</option>\r\n                                </select>\r\n                              </div>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                       \r\n                     </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Vendor  : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"vendor\" v-model=\"form_data.vendor\" @change=\"createData('vendor')\">\r\n                                  <option v-for=\"vendor in vendors\" :value=\"vendor.id\" v-if=\"vendor.vendor_type=='Raw Material'\">{{vendor.name}}</option>\r\n                                  <option value=\"create\" style=\"color:green!important\">Create New Vendor</option>\r\n                              </select>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Quantity: </span>\r\n                              <input placeholder=\"0\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_available_qty\" name=\"total_available_qty\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Price: </span>\r\n                              <input placeholder=\"0\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.price\" name=\"price\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">  Status: </span>\r\n                              <input placeholder=\"\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.status\" name=\"status\" readonly>\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n\r\n        <b-col lg=\"12\" xl=\"12\" class=\"my-3\" v-if=\"form_data.is_parameters\">\r\n         <!--  <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <h5>Parameters</h5>\r\n              <div class=\"ul-contact-detail__timeline \">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-12 px-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n\r\n                            <div class=\"o_composer_container my-2 row mx-0\">\r\n                              <div class=\" col-md-6\" v-for=\"data in form_data.parameters.datas\">\r\n                                <div class=\"user-info pt-2\" v-for=\"param in data\">\r\n                                  <div class=\"o_thread_search_input mt-2\">\r\n                                    <span style=\"width:100px;\"> {{param.name}} : </span>\r\n                                    <span>\r\n                                      <input type=\"checkbox\" class=\"ui-autocomplete-input mx-2\" autocomplete=\"off\" value=\"1\" v-model=\"param.checked\" :name=\"param.name\">\r\n                                      <input placeholder=\"Quantity\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input mx-1\" autocomplete=\"off\" v-model=\"param.quantity\" :name=\"param.name+'quantity'\" v-if=\"param.checked\" @input=\"checkParam(param)\" required>\r\n                                      <input placeholder=\"Price\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input mx-1\" autocomplete=\"off\" v-model=\"param.price\" :name=\"param.name+'price'\" v-if=\"param.checked\" @input=\"checkParam(param)\" required>\r\n                                    </span>\r\n                                  </div>\r\n                                </div>\r\n                              </div>\r\n                              \r\n                            </div>\r\n                          </div> \r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card> -->\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n    <div>\r\n      <b-modal id=\"create-vendor\" hide-header  hide-footer>\r\n        <form ref=\"createVendor\" @submit.prevent=\"createVendor()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Create Vendor</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('create-vendor')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2 pb-5\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <div style=\"width:50px;\">Name: </div>\r\n                            <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"name\" required>\r\n                          </div>\r\n                          <div>\r\n                            <input type=\"hidden\" name=\"vendor_type\" value=\"Raw Material\">\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{purchase_type:2,date:new Date(),is_parameters:false,sample_procurement:null,status:'Collecting',total_farmers:0,parameters:[]},\r\n      id:null,\r\n      load_products:false,\r\n      product_id:null,\r\n      action:'create',\r\n      vehicles:[],\r\n      parameters:[],\r\n      vendors:[],\r\n      total_keys:0,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'procurement'\r\n    this.form_data.date = this.currentDate()\r\n    this.action = this.$route.params.action\r\n    this.getPurchaseTypes()\r\n    this.getVendors()\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n    }\r\n    this.getParameters()\r\n  },\r\n  watch: {\r\n    '$route': 'changeURL'\r\n  },\r\n  methods: {\r\n    changeURL(){\r\n      this.form_data.date = this.currentDate()\r\n      this.action = this.$route.params.action\r\n      this.getPurchaseTypes()\r\n      this.getVehicles()\r\n      if(this.action == 'edit'){\r\n        this.id = this.$route.params.id\r\n        this.getForm()\r\n      }\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_id= jsonData.product\r\n        this.form_data = jsonData\r\n        this.load_products=true\r\n      })\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.form_data.product = id\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameters/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.parameters = jsonData\r\n      })\r\n    },\r\n    checkParam() {\r\n\r\n    },\r\n    getVendors() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/vendors/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.vendors = jsonData\r\n      })\r\n    },\r\n    createData(model){\r\n      if(model=='vendor'){\r\n        if(this.form_data.vendor=='create'){\r\n          this.form_data.vendor = ''\r\n          this.$bvModal.show('create-vendor')\r\n        }\r\n      }\r\n    },\r\n    createVendor() {\r\n      var formdata = new FormData(this.$refs['createVendor'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/vendors/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.getVendors()\r\n          this.form_data.vendor = jsonData.id\r\n          this.$bvModal.hide('create-vendor')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('aeo',2)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.form_data = jsonData\r\n          this.id = jsonData.id\r\n          this.updateFormdata()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n</style>\r\n"]}]}