n-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Normal Procured</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/procurement/normal-procurement\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n          <h5></h5>\r\n        </b-col>\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Purchase Type  : </span>\r\n                              <input placeholder=\"Purchase Type\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" value=\"Farmer Purchase\" readonly>\r\n                            </div>\r\n                         <!--    <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Organic Status: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"organic_status\" :disabled=\"action=='edit'\">\r\n                                  <option v-for=\"organic_statuse in organic_statuses\" :value=\"organic_statuse\" :selected=\"organic_statuse==form_data.organic_statuse\">{{organic_statuse}}</option>\r\n                              </select>\r\n                            </div> -->\r\n\r\n                            <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n\r\n                            <div class=\"o_thread_search_input mt-5\">\r\n                              <span style=\"width:120px;\"> Vehicle No: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"vehicle\" v-model=\"form_data.vehicle\" @change=\"changeVehicle()\" :disabled=\"action=='edit'\">\r\n                                <option value=\"\" selected disabled>Select a vehicle</option>\r\n                                <option v-for=\"vehicle in vehicles\" :value=\"vehicle.id\">{{vehicle.name}}</option>\r\n                                <option value=\"create\" style=\"color:green!important\">Create New Vehicle</option>\r\n                              </select>\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n                       \r\n                     </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Date: </span>\r\n                              <input placeholder=\"Date\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.date\" name=\"date\" required readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Total Estimate Qty: </span>\r\n                              <input placeholder=\"0\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_estimate_qty\" name=\"total_estimate_qty\" readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Total Quantity: </span>\r\n                              <input placeholder=\"0\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_available_qty\" name=\"total_available_qty\" readonly>\r\n                            </div>\r\n                       <!--      <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Total Panchayath: </span>\r\n                              <input placeholder=\"0\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_panchayath\" readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Total VFC: </span>\r\n                              <input placeholder=\"0\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_vfc\" readonly>\r\n                            </div> -->\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Total Farmers: </span>\r\n                              <input placeholder=\"0\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_farmers\" readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">  Status: </span>\r\n                              <input placeholder=\"\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.status\" name=\"status\" readonly>\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        \r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n          <h5>Farmer Lines</h5>\r\n        </b-col>\r\n\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mb-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-12 px-2\">\r\n                          <table class=\"table\">\r\n                            <thead>\r\n                              <tr>\r\n                                <th>Farmer Code</th>\r\n                                <th>Farmer</th>\r\n                                <th>Estimate QTY (KG)</th>\r\n                                <th>Estimate QTY Balance (KG)</th>\r\n                                <!-- <th>Farmer Estimate Qty (KG)</th> -->\r\n                                <!-- <th>Last Procure Qty (KG)</th> -->\r\n                                <th>Procure Qty (KG)</th>\r\n                                <!-- <th>Final Procured Qty (KG)</th> -->\r\n                                <!-- <th>Vehicle Status</th> -->\r\n                                <th>Qc Status</th>\r\n                                <th>Action</th>\r\n                              </tr>\r\n                            </thead>\r\n                            <tbody v-if=\"product_id\">\r\n                              <tr v-for=\"(line,key) in lines\">\r\n                                 <td>\r\n                                  <input type=\"text\" placeholder=\"Search (Farmer code)\" class=\"\" autocomplete=\"off\" v-model=\"line.farmer_detail.farmer_code\" @input=\"searchFarmers(line)\" required>\r\n                                  <div class=\"farmer_search_drpdwn\" v-show=\"line.farmers_list.length>0\">\r\n                                    <div class=\"farmer_heading lines\">\r\n                                      Farmers Result <span @click=\"line.farmers_list=[]\"><i class=\"fa fa-times-circle\" aria-hidden=\"true\"></i></span>\r\n                                    </div>\r\n                                    <div class=\"farmer_results\">\r\n                                      <p v-for=\"farmer in line.farmers_list\" @click=\"selectFarmer(farmer,line)\">{{farmer.farmer_code}}-{{farmer.name}}</p>\r\n                                    </div>\r\n                                  </div>\r\n                                </td>\r\n                                <td>\r\n                                  <input type=\"text\" placeholder=\"Farmer name\" class=\"\" autocomplete=\"off\" :value=\"line.farmer_detail.name\" readonly required>\r\n                                  <input type=\"hidden\" :name=\"'farmer'+key\" :value=\"line.farmer\">\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"0\" type=\"text\" class=\"\" autocomplete=\"off\" :name=\"'estimate_qty'+key\" v-model=\"line.estimate_qty\" readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"0\" type=\"text\" class=\"\" autocomplete=\"off\" :name=\"'estimate_qty_balance'+key\" v-model=\"line.estimate_qty_balance\" readonly></td>\r\n                              <!--   <td>\r\n                                  <input placeholder=\"0\" type=\"text\" class=\"\" autocomplete=\"off\" :name=\"'farmer_estimate'+key\" required v-model=\"line.farmer_estimate\" @input=\"calcultion()\">\r\n                                </td> -->\r\n                                <!-- <td>\r\n                                  <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" :name=\"'last_procure_qty'+key\" v-model=\"line.last_procure_qty\" readonly>\r\n                                </td> -->\r\n                                <td>\r\n                                  <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" :name=\"'procure_qty'+key\" v-model=\"line.procure_qty\" @input=\"line.estimate_qty_balance=line.estimate_qty-line.procure_qty;calcultion()\" required>\r\n                                </td>\r\n                              <!--   <td>\r\n                                  <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" :name=\"'final_procured_qty'+key\" v-model=\"line.final_procured_qty\" readonly>\r\n                                </td> -->\r\n                             <!--    <td>\r\n                                  <input placeholder=\"\" type=\"text\" class=\"\" autocomplete=\"off\" :value=\"line.vehicle_status\" :name=\"'vehicle_status'+key\"></td>-->\r\n                                <td>\r\n                                  <input placeholder=\"\" type=\"text\" class=\"\" autocomplete=\"off\" :value=\"line.qc_status\" :name=\"'qc_status'+key\" readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <b-button variant=\"danger\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\"@click=\"lineOperation(key)\">\r\n                                    Remove\r\n                                  </b-button>\r\n                                </td>\r\n                              </tr>\r\n                              <tr>\r\n                                <td>\r\n                                  <b-button variant=\"success\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\"@click=\"lineOperation('+')\">\r\n                                    Add a line\r\n                                  </b-button>\r\n                                  <input type=\"hidden\" name=\"keys\" :value=\"lines.length\">\r\n                                </td>\r\n                                <td colspan=\"8\"></td>\r\n                              </tr>\r\n                            </tbody>\r\n                          </table>\r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n    <div>\r\n      <b-modal id=\"create-vehicle\" hide-header  hide-footer>\r\n        <form ref=\"vehicleForm\" @submit.prevent=\"vehicleForm()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Vehicle</span> / Create</h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('create-vehicle')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Purchase Type: </span>\r\n                            <input placeholder=\"Purchase Type\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.purchase_type\" name=\"purchase_type\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Vehicle Type : </span>\r\n                            <input placeholder=\"Vehicle Type\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.vehicle_type\" name=\"vehicle_type\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Driver Name: </span>\r\n                            <input placeholder=\"Driver Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.name\" name=\"name\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Vehicle No : </span>\r\n                            <input placeholder=\"Vehicle No\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.vehicle_number\" name=\"vehicle_number\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Origin of the Vehicle  : </span>\r\n                            <input placeholder=\"Origin of the Vehicle\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.origin_of_the_vehicle\" name=\"origin_of_the_vehicle\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Destination: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"destination\">\r\n                              <option v-for=\"warehouse in warehouses\" :value=\"warehouse.id\" :selected=\"warehouse.id==form_data.warehouse\">{{warehouse.name}}</option>\r\n                            </select>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mt-4\">\r\n                            <span style=\"width:120px;\">Transport Date : </span>\r\n                            <input placeholder=\"Transport Date\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.date\" name=\"transport_date\" readonly>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Transport Cost : </span>\r\n                            <input placeholder=\"Transport Cost\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.transport_cost\" name=\"transport_cost\" required>\r\n                            <input type=\"hidden\" name=\"vehicle_for\" value=\"vendor\">\r\n                          </div>\r\n                          \r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{date:new Date(),sample_procurement:null,status:'Collecting',total_farmers:0},\r\n      id:null,\r\n      load_products:false,\r\n      lines:[],\r\n      product_id:null,\r\n      action:'create',\r\n      vehicles:[],\r\n      samples:[],\r\n      panchayaths:[],\r\n      vfcs:[],\r\n      farmers:[],\r\n      total_keys:0,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'procurement'\r\n    this.form_data.date = this.currentDate()\r\n    this.action = this.$route.params.action\r\n    this.getPurchaseTypes()\r\n    this.getVehicles()\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n    }\r\n  },\r\n  watch: {\r\n    '$route': 'changeURL'\r\n  },\r\n  methods: {\r\n    changeURL(){\r\n      this.form_data.date = this.currentDate()\r\n      this.action = this.$route.params.action\r\n      this.getPurchaseTypes()\r\n      this.getVehicles()\r\n      if(this.action == 'edit'){\r\n        this.id = this.$route.params.id\r\n        this.getForm()\r\n      }\r\n    },\r\n    lineOperation(operation){\r\n      if(operation=='+'){\r\n        this.lines.push({'estimate_qty':0,'estimate_qty_balance':0,'farmer':null,'farmer_detail':{'name':'', 'farmer_code':''},'vfc':null,'procure_qty':0,'qc_status':'Waiting','farmers_list':[]})\r\n        this.total_keys++\r\n      }else{\r\n        this.lines.splice(operation, 1)\r\n        this.total_keys--\r\n      }\r\n      this.calcultion()\r\n    },\r\n    calcultion(){\r\n      var self =this\r\n      var total_farmers = []\r\n      var total_available_qty = 0\r\n      var total_estimate_qty = 0\r\n      this.lines.forEach(function(val,key){\r\n        if(!total_farmers.includes(val.farmer) && val.farmer){\r\n          total_farmers.push(val.farmer)\r\n        }\r\n        total_available_qty = total_available_qty + parseFloat(val.procure_qty)\r\n        total_estimate_qty = total_estimate_qty + parseFloat(val.estimate_qty)\r\n      })\r\n      this.form_data.total_farmers = total_farmers.length\r\n      this.form_data.total_available_qty = total_available_qty\r\n      this.form_data.total_estimate_qty = total_estimate_qty\r\n\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.form_data.product = id\r\n    },\r\n    searchFarmers(line){\r\n      var farmer_code = line.farmer_detail.farmer_code\r\n      if(farmer_code.length<4){\r\n        return false\r\n      }\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      formdata.append('farmer_code',farmer_code)\r\n      formdata.append('search_type','normal_procurement')\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/search_farmers/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        line.farmers_list = jsonData\r\n      })\r\n    },\r\n    selectFarmer(farmer=null,line){\r\n      if(farmer){\r\n        line.farmer_detail.farmer_code = farmer.farmer_code\r\n        line.farmer_detail.name = farmer.name\r\n        line.farmer = farmer.id\r\n        line.farmers_list = []\r\n      }\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      formdata.append('farmer', line.farmer)\r\n      formdata.append('product', this.product_id)\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmer_full_details/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        line.estimate_qty = jsonData.estimate_qty\r\n      })\r\n    },\r\n    changeVehicle(){\r\n      if(this.form_data.vehicle=='create'){\r\n        this.getWarehouses()\r\n        this.$bvModal.show('create-vehicle')\r\n      }\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_id= jsonData.product\r\n        this.form_data = jsonData\r\n        this.lines = jsonData.farmer_lines\r\n        this.load_products=true\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('aeo',2)\r\n      formdata.append('current_status','sample_procurement')\r\n      formdata.append('purchase_type',this.purchase_types[0].id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.$router.push('/app/procurement/normal-procurement/edit/'+jsonData.id)\r\n          this.updateFormdata()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getVehicles() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/vehicle/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.vehicles = jsonData\r\n        this.getWarehouses()\r\n      })\r\n    },\r\n    vehicleForm() {\r\n      var formdata = new FormData(this.$refs['vehicleForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/vehicle/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.getVehicles()\r\n          this.$bvModal.hide('create-vehicle')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n#create-vehicle.modal.show .modal-dialog{\r\n  min-width:600px!important;\r\n}\r\n\r\n</style>\r\n"]}]}