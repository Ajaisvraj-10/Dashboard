             <b-button type=\"button\" variant=\"outline-info\" class=\"w-100 px-3 py-1 ml-1 mr-2\" @click=\"cancelFilter()\">Cancel</b-button>\r\n              <b-button @click=\"farmer_file_types=[]\" type=\"submit\" variant=\"success\" class=\"w-100 px-3 py-1 mr-1 ml-2\">Submit</b-button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n\r\n\r\n      </b-modal>\r\n\r\n      <div class=\"main-body4\" id=\"main_body\">\r\n\r\n        <div class=\"my-3 px-3\">\r\n          <div class=\"card\">\r\n            <table>\r\n              <tr>\r\n                <td class=\"pl-2\">FPO</td>\r\n                <td>:</td>\r\n                <th class=\"text-right\">\r\n                  <span class=\"badge badge-success py-1 ml-1 \">{{$root.userInfo.fpo==1?'WOFFA':'KAFFE'}}</span>\r\n                </th>\r\n              </tr>\r\n              <tr v-if=\"filter.ics\">\r\n                <td class=\"pl-2\">ICS</td>\r\n                <td>:</td>\r\n                <th class=\"text-right\">\r\n                  <span class=\"badge badge-primary py-1 ml-1\" v-for=\"ics in all_ics\" v-if=\"ics.id==filter.ics\">{{ics.name}}</span>\r\n                </th>\r\n              </tr>\r\n              <tr v-if=\"filter.vfc\">\r\n                <td class=\"pl-2\">VFC</td>\r\n                <td>:</td>\r\n                <th class=\"text-right\">\r\n                  <span class=\"badge badge-success py-1 ml-1\" v-for=\"vfc in all_vfc\" v-if=\"vfc.id==filter.vfc\">{{vfc.name}}</span>\r\n                </th>\r\n              </tr>\r\n              <tr v-if=\"filter.panchayath\">\r\n                <td class=\"pl-2\">Panchayath</td>\r\n                <td>:</td>\r\n                <th class=\"text-right\">\r\n                  <span class=\"badge badge-primary py-1 ml-1\" v-for=\"panchayath in all_panchayaths\" v-if=\"panchayath.id==filter.panchayath\">{{panchayath.name}}</span>\r\n                </th>\r\n              </tr>\r\n            </table>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"my-3 px-3\" v-for=\"farmer_file_type in farmer_file_types\">\r\n          <div class=\"card\">\r\n            <div>\r\n              <div class=\"flex-align\">\r\n                <div>\r\n                  <h4 class=\"font-weight-500 text-17 pr-2\">{{farmer_file_type.file_type}}</h4>\r\n                </div>\r\n                <div class=\"text-right\">\r\n                  <router-link :to=\"'/document/files/'+farmer_file_type.file_type+'/not_recieved'+filter_params\">\r\n                    <h4 class=\"badge badge-info p-1\">Not Recieved (<b>{{farmer_file_type.not_recieved}}</b>)</h4>\r\n                  </router-link>\r\n                </div>\r\n              </div>\r\n              <div class=\"hr-min\"></div>\r\n              <div style=\"display:flex;gap:8px;\">\r\n                <div>\r\n                  <router-link :to=\"'/document/files/'+farmer_file_type.file_type+'/Approved'+filter_params\">\r\n                    <h4 class=\"text-13 text-success py-1\">Approved (<b>{{farmer_file_type.Approved}}</b>)</h4>\r\n                  </router-link>\r\n                </div>\r\n                <div>\r\n                  <router-link :to=\"'/document/files/'+farmer_file_type.file_type+'/Pending'+filter_params\">\r\n                    <h4 class=\"text-14 text-warning py-1\">Pending (<b>{{farmer_file_type.Pending}}</b>)</h4>\r\n                  </router-link>\r\n                </div>\r\n                <div>\r\n                  <router-link :to=\"'/document/files/'+farmer_file_type.file_type+'/Decline'+filter_params\">\r\n                    <h4 class=\"text-14 text-danger py-1\">Decline (<b>{{farmer_file_type.Decline}}</b>)</h4>\r\n                  </router-link>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n      </div>\r\n      <div class=\"filter-footer\">\r\n        <div class=\"footer-item2 d-flex align-items-center\" @click=\"$router.push