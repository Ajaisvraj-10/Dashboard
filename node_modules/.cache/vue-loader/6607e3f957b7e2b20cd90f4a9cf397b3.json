     <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Indent</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                  <b-button type=\"button\" variant=\"primary\" class=\"px-3 py-1  d-sm-block mx-1\" @click=\"openDispatchIndent()\" v-if=\"id\">\r\n                     {{form_data.dispatch_indent.id?'Dispatched':'Add to Dispatch'}}\r\n                  </b-button>\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/logistics-department/indent\">\r\n                  <b-button  type=\"button\"variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n\r\n        <b-col lg=\"5\" xl=\"5\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\" col-md-12\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\" v-if=\"id\">\r\n                              <span style=\"width:120px;\">Indent Number : </span>\r\n                              <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"'#'+form_data.id+'/Indent'\" readonly>\r\n                            </div>\r\n\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">PO Number : </span>\r\n                              <input placeholder=\"PO Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.PO_number\" name=\"PO_number\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Expected Date: </span>\r\n                              <input placeholder=\"Expected Date\" type=\"month\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.expected_date\" name=\"expected_date\" required>\r\n                            </div>  \r\n\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Remark : </span>\r\n                              <textarea placeholder=\"Remark\" rows=\"5\" class=\"ui-autocomplete-input w-100 mx-1\" autocomplete=\"off\" v-model=\"form_data.remark\" name=\"remark\"></textarea>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n        <b-col lg=\"7\" xl=\"7\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div v-if=\"id\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-9\">\r\n                  <h5>Products</h5>\r\n                </div>\r\n                <div class=\"col-md-3 text-right\">\r\n                  <router-link :to=\"'/app/logistics-department/indent/product/'+form_data.id+'/create'\">\r\n                    <b-button variant=\"success\" type=\"button\" class=\" px-3 py-1 d-sm-block mx-1\">\r\n                      Create Product\r\n                    </b-button>\r\n                  </router-link>\r\n                </div>\r\n              </div>\r\n              <div class=\"o_thread_composer  \">\r\n                <vue-good-table\r\n                  :columns=\"columns\"\r\n                  :line-numbers=\"false\"\r\n                  :search-options=\"{\r\n                    enabled: true,\r\n                    placeholder: 'Search'\r\n                  }\"\r\n                  :pagination-options=\"{\r\n                    enabled: true,\r\n                    mode: 'records'\r\n                  }\"\r\n                  styleClass=\"tableOne vgt-table\"\r\n                  :rows=\"form_data.products\"\r\n                >\r\n                  <div slot=\"table-actions\" class=\"mb-3 filter-but\">\r\n                    \r\n                  </div>\r\n                  <template slot=\"table-row\" slot-scope=\"props\">\r\n                    <span v-if=\"props.column.field == 'id'\">\r\n                      <router-link :to=\"'/app/logistics-department/indent/product/'+form_data.id+'/edit/'+props.row.id\">\r\n                        <b-button  variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">Edit</b-button>\r\n                      </router-link>\r\n                    </span>\r\n                    <span v-else-if=\"props.column.field == 'quantity'\">\r\n                      {{props.row.quantity}}KG\r\n                    </span>\r\n                    <span v-else-if=\"props.column.field == 'product'\">\r\n                      {{props.row.product_detail.category_detail.name}}> {{props.row.product_detail.name}}> {{props.row.parameter_detail.name}}\r\n                    </span>\r\n                  </template>\r\n                </vue-good-table>\r\n              </div>\r\n              <hr>\r\n            </div>\r\n            <div v-else>\r\n              Fill data and click save\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n\r\n    <div>\r\n      <b-modal id=\"dispatch\" hide-header  hide-footer>\r\n        <form ref=\"dispatchIndent\" @submit.prevent=\"dispatchIndent()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Dispatch</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('dispatch')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Date: </span>\r\n                            <input placeholder=\"Date\" type=\"date\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"date\" v-model=\"form_data.dispatch_indent.date\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:220px;\">Expected Vehicle Arraival Time : </span>\r\n                            <input placeholder=\"Time\" type=\"time\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"expected_vehicle_arraival_time\" v-model=\"form_data.dispatch_indent.expected_vehicle_arraival_time\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Customer: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"customer\" v-model=\"form_data.dispatch_indent.customer\">\r\n                              <option v-for=\"customer in customers\" :value=\"customer.id\">{{customer.customer_code}} - {{customer.name}}</option>\r\n                            </select>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input mt-4\">\r\n                            <span style=\"width:120px;\">ICO Number : </span>\r\n                            <input placeholder=\"ICO Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.dispatch_indent.ICO_number\" name=\"ICO_number\" required>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Indent Number : </span>\r\n                            <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"'#'+form_data.id+'/Indent'\" readonly>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{id:null,expected_date:'2022-03',total_quantity:0, products:[], dispatch_indent:{}},\r\n      id:null,\r\n      action:'create',\r\n      customers:[],\r\n      columns: [\r\n        {\r\n          label: \"Product\",\r\n          field: \"product\"\r\n        },\r\n        {\r\n          label: \"Quantity (Kg)\",\r\n          field: \"quantity\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.form_data.expected_date = this.currentMonth()\r\n    this.$root.current_section = 'logistics-department'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n    }\r\n    this.getCustomers()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/main_indent/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n      })\r\n    },\r\n    getCustomers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/customers/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.customers = jsonData\r\n      })\r\n    },\r\n    calcultion(){\r\n      var quantity = 0\r\n      var price = 0\r\n      this.form_data.values.forEach(function(val,key){\r\n        quantity = quantity + parseFloat(val.requested_quantity)\r\n      })\r\n      this.form_data.total_quantity = quantity\r\n    },\r\n    autoIndentNumber(){\r\n      var dateTime = new Date()\r\n      return dateTime.getTime()\r\n    },\r\n    dataForm() {\r\n      if(this.id){\r\n        this.updateFormdata()\r\n      }else{\r\n        this.createFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/main_indent/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.id = jsonData.id\r\n          this.form_data = jsonData\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/main_indent/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.form_data = jsonData\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    openDispatchIndent(){\r\n      if(!this.form_data.dispatch_indent.id){\r\n        this.form_data.dispatch_indent.date = this.currentDate()\r\n        this.form_data.dispatch_indent.ICO_number = this.form_data.ICO_number\r\n        this.form_data.dispatch_indent.indent_number = this.form_data.indent_number\r\n        this.form_data.dispatch_indent.parameter = this.form_data.parameter\r\n        this.form_data.dispatch_indent.quantity = this.form_data.quantity\r\n      }\r\n      this.$bvModal.show('dispatch')\r\n    },\r\n    dispatchIndent(method='POST') {\r\n      var param=''\r\n      if(this.form_data.dispatch_indent.id){\r\n        method='PUT'\r\n        param = this.form_data.dispatch_indent.id+'/'\r\n      }\r\n      var formdata = new FormData(this.$refs['dispatchIndent'])\r\n      formdata.append('indent',this.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/dispatch_indent/'+param, {\r\n          method : method,\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.$bvModal.hide('dispatch')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n<style>\r\n.despatch label{\r\n  font-weight: 700;\r\n  white-space: nowrap;\r\n    align-self: center;\r\n    margin-right: 10px;\r\n    min-width:140px;\r\n}\r\n.unloading .btn-secondary{\r\n  width:100%;\r\n  \r\n    border: 2px solid #efefef;\r\n    background-color: #f8f9fa!important;\r\n    border-radius: 2px;\r\n    text-align: left;\r\n    height: calc(1.9695rem + 2px);\r\n    padding: 5px;\r\n}\r\n\r\n   .table-3 th {\r\n    border: 1px solid #dcdfe6;\r\n    background-color: #f3f4f7;\r\n}\r\n   .table-3 th, .table-3 td {\r\n    border: 1px solid #dcdfe6;\r\n}\r\n  .table-3 th {\r\n    padding:0.75em 1.5em 0.75em 0.75em;\r\n  }\r\n.table-3 .tr-bottom td:nth-child(2){\r\n  border: 2px solid #dcdfe6;\r\n background-color: #f3f4f7;\r\n  \r\n}\r\n.table-3 .tr-bottom td:nth-child(1){\r\nborder: none;\r\n  \r\n}\r\n</style>\r\n"]}]}