ontent-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Raw Material Test</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <b-button variant=\"primary\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  @click=\"action='edit'\" v-if=\"action=='view'\">Edit\r\n                </b-button>\r\n                <b-button variant=\"primary\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  @click=\"action='view'\" v-else-if=\"action=='edit'\">View\r\n                </b-button>\r\n                <router-link to=\"/app/qc/normal-materials-test\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n              </div>\r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n\r\n        <b-col lg=\"4\" xl=\"4\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <h5>Raw Details</h5>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\" col-md-12\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Raw ID : </span>\r\n                              <input placeholder=\"\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.normal_procurement_detail.raw_number\" name=\"raw_number\" readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Purchase Type  : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.normal_procurement_detail.purchase_type\" disabled>\r\n                                  <option v-for=\"purchase_type in purchase_types\" :value=\"purchase_type.id\" >{{purchase_type.name}}</option>\r\n                              </select>\r\n                              <input type=\"hidden\" name=\"normal_procurement\" v-model=\"form_data.normal_procurement\">\r\n                              <input type=\"hidden\" name=\"purchase_type\" v-model=\"form_data.normal_procurement_detail.purchase_type\" required>\r\n                              <input type=\"hidden\" v-model=\"form_data.total_quantity\" name=\"total_quantity\" required>\r\n                              <input type=\"hidden\" v-model=\"form_data.rejected_quantity\" name=\"rejected_quantity\" required>\r\n                            </div>\r\n\r\n                            <ProductFields :product_id=\"product_id\" :readonly=\"true\" v-if=\"load_products\"></ProductFields>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Vehicle: </span>\r\n                                <input type=\"text\" class=\"ui-autocomplete-input \" autocomplete=\"off\" :value=\"form_data.normal_procurement_detail.vehicle_detail.name+' ('+form_data.normal_procurement_detail.vehicle_detail.vehicle_number+')'\" readonly style=\"width:100%\">\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Product Status: </span>\r\n                                <input type=\"text\" class=\"ui-autocomplete-input \" autocomplete=\"off\" v-model=\"form_data.normal_procurement_detail.farmer_status\" name=\"farmer_status\" readonly style=\"width:100%\">\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Status: </span>\r\n                                <input type=\"text\" class=\"ui-autocomplete-input \" autocomplete=\"off\" v-model=\"form_data.status\" readonly style=\"width:100%\">\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Procurement Status: </span>\r\n                                <input type=\"text\" class=\"ui-autocomplete-input \" autocomplete=\"off\" v-model=\"form_data.normal_procurement_detail.status\" readonly style=\"width:100%\">\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n                       \r\n                     </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"8\" xl=\"8\" class=\"my-3\" v-if=\"form_data.normal_procurement_detail.purchase_type==1\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <h5>Farmer Lines</h5>\r\n              <div class=\"ul-contact-detail__timeline \">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-12 px-2\">\r\n                          <table class=\"table\">\r\n                            <thead>\r\n                              <tr>\r\n                                <th>Farmer Code</th>\r\n                                <th>Farmer</th>\r\n                                <th>Procure Qty (KG)</th>\r\n                                <th>Cutting (%)</th>\r\n                                <th>Qc Status</th>\r\n                                <th>Action</th>\r\n                              </tr>\r\n                            </thead>\r\n                            <tbody v-if=\"product_id\">\r\n                              <tr v-for=\"(line,key) in lines\">\r\n                                 <td>\r\n                                  <input type=\"text\" placeholder=\"Search (Farmer code)\" class=\"\" autocomplete=\"off\" v-model=\"line.farmer_detail.farmer_code\" required readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <input type=\"text\" placeholder=\"Farmer name\" class=\"\" autocomplete=\"off\" :value=\"line.farmer_detail.name\" readonly required>\r\n                                  <input type=\"hidden\" :name=\"'farmer'+key\" :value=\"line.farmer\">\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" :name=\"'procure_qty'+key\" v-model=\"line.procure_qty\" readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\" \" type=\"number\" class=\"\" autocomplete=\"off\" v-model=\"line.cutting\" :name=\"'cutting'+key\" readonly>\r\n                                  <div v-for=\"data in line.physical_parameter\" v-show=\"false\">\r\n                                    <div v-for=\"param in data\">\r\n                                      <input type=\"checkbox\" value=\"1\" v-model=\"param.checked\" :name=\"param.name+'farmer'+key\">\r\n                                      <input type=\"number\" v-model=\"param.limit\" :name=\"param.name+'limit'+key\" v-if=\"param.checked\" required>\r\n                                    </div>\r\n                                  </div>\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"\" type=\"text\" class=\"\" autocomplete=\"off\" :value=\"line.qc_status\" :name=\"'qc_status'+key\" readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <button type=\"button\" class=\"btn btn-primary btn-sm px-3 py-1 ml-1\" @click=\"$bvModal.show('select-params'+key)\">Check</button>\r\n                                  <!-- <button type=\"button\" class=\"btn btn-danger btn-sm px-3 py-1 ml-2\" @click=\"line.qc_status='Reject'\">Reject</button> -->\r\n                                  <input placeholder=\"0\" type=\"hidden\" class=\"\" autocomplete=\"off\" :name=\"'estimate_qty'+key\" v-model=\"line.estimate_qty\" readonly>\r\n                                  <input placeholder=\"0\" type=\"hidden\" class=\"\" autocomplete=\"off\" :name=\"'estimate_qty_balance'+key\" v-model=\"line.estimate_qty_balance\" readonly>\r\n                                  <div class=\"bio_std_modal\">\r\n                                    \r\n                                    <b-modal class=\"bio_std_modal\" :id=\"'select-params'+key\" hide-header  hide-footer>\r\n                                      <div>\r\n                                         <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                                          <slot name=\"header\">\r\n                                             <h5 class=\"mb-0\"><span class=\"text-success\">{{line.farmer_detail.name}}</span> / Check</h5>\r\n                                          </slot>\r\n                                            <div class=\"d-flex\">\r\n                                              <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('select-params'+key)\">\r\n                                                Close\r\n                                              </b-button>\r\n                                            </div>\r\n                                          </div>\r\n                                          <div class=\" my-3 border-top\"></div>\r\n                                      </div>\r\n                                      <!-- <div class=\"wrapper\"> -->\r\n                                      <b-row class=\"main-scroll\">\r\n                                        <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n                                          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                                            <div>\r\n                                              <div class=\"ul-contact-detail__timeline\">\r\n                                                 <div class=\"\">\r\n                                                    <div class=\"user-info pt-2\">\r\n                                                      <div class=\"o_thread_search_input\">\r\n                                                        <span style=\"width:120px;\"> Cutting: </span>\r\n                                                        <input placeholder=\" \" type=\"number\" class=\"\" autocomplete=\"off\" v-model=\"line.cutting\" @input=\"calcultion()\" required>\r\n                                                      </div>\r\n\r\n                                                      <div class=\"o_thread_search_input\">\r\n                                                        <span style=\"width:100px;\"> Pysical Parameters: </span>\r\n\r\n                                                        <div class=\"my-2 row mx-0 w-100\">\r\n                                                          <div class=\" col-md-6\" v-for=\"data in line.physical_parameter\">\r\n                                                            <div class=\"user-info pt-2\" v-for=\"param in data\">\r\n                                                              <div class=\"o_thread_search_input mt-2\">\r\n                                                                <span style=\"width:100px;\"> {{param.name}} : </span>\r\n                                                                <span>\r\n                                                                  <input type=\"checkbox\" class=\"ui-autocomplete-input mx-2\" autocomplete=\"off\" value=\"1\" v-model=\"param.checked\" >\r\n                                                                  <input :placeholder=\"param.limit_type\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input w-50\" autocomplete=\"off\" v-model=\"param.limit\" v-if=\"param.checked\" @input=\"checkParam(param)\" required>\r\n                                                                </span>\r\n                                                              </div>\r\n                                                            </div>\r\n                                                          </div>\r\n                                                          \r\n                                                        </div>\r\n                                                      </div> \r\n\r\n                                                      <div class=\"o_thread_search_input mt-1\">\r\n                                                        <span style=\"width:120px;\"> Qc Status: </span>\r\n                                                        <div>\r\n                                                          <input type=\"text\" class=\"ui-autocomplete-input py-1\" autocomplete=\"off\" :value=\"line.qc_status\" name=\"status\" readonly style=\"width:80px\">\r\n                                                          <button type=\"button\" class=\"btn btn-success py-1 mx-2\" @click=\"line.qc_status='Approved';$bvModal.hide('select-params'+key);calcultion()\">Approve</button>\r\n                                                          <button type=\"button\" class=\"btn btn-danger py-1\" @click=\"line.qc_status='Rejected';$bvModal.hide('select-params'+key);calcultion()\">Rejected</button>\r\n                                                        </div>\r\n                                                      </div>\r\n\r\n                                                    \r\n                                                    </div>\r\n                                                  </div>\r\n                                              </div>\r\n                                            </div>\r\n                                          </b-card>\r\n                                        </b-col>\r\n                                      </b-row>\r\n                                    </b-modal>\r\n                                  </div>\r\n                                </td>\r\n                              </tr>\r\n                              <tr>\r\n                                <th>Total</th>\r\n                                <td>{{form_data.total_farmers}}Farmer{{checkMult(form_data.total_farmers)}}</td>\r\n                                <td>{{form_data.normal_procurement_detail.total_procured_qty}}KG</td>\r\n                                <td class=\"text-danger\">{{form_data.total_cutting}}%</td>\r\n                                <td>{{form_data.status}}</td>\r\n                                <td>\r\n                                  <input type=\"hidden\" name=\"keys\" :value=\"lines.length\">\r\n                                  <input placeholder=\"0\" type=\"hidden\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.normal_procurement_detail.total_estimate_qty\" name=\"total_estimate_qty\" readonly>\r\n                                  <input placeholder=\"0\" type=\"hidden\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.normal_procurement_detail.total_procured_qty\" name=\"total_procured_qty\" readonly>\r\n                                </td>\r\n                              </tr>\r\n                            </tbody>\r\n                            <tfoot>\r\n                              <tr>\r\n                                <td colspan=\"3\"></td>\r\n                                <td><b>Total Procured Quantity</b> </td>\r\n                                <td>:</td>\r\n                                <td>{{form_data.normal_procurement_detail.total_procured_qty}}KG</td>\r\n                              </tr>\r\n                              <tr>\r\n                                <td colspan=\"3\"></td>\r\n                                <td><b>Rejected Quantity</b> </td>\r\n                                <td>:</td>\r\n                                <td class=\"text-danger\">{{form_data.rejected_quantity}}KG</td>\r\n                              </tr>\r\n                              <tr>\r\n                                <td colspan=\"3\"></td>\r\n                                <td><b>Available Quantity</b> </td>\r\n                                <td>:</td>\r\n                                <td><h4>{{form_data.total_quantity}}KG</h4></td>\r\n                              </tr>\r\n                            </tfoot>\r\n                          </table>\r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n        <b-col lg=\"8\" xl=\"8\" class=\"my-3\" v-if=\"form_data.normal_procurement_detail.purchase_type==2\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <h5>Physical Parameters</h5>\r\n              <div class=\"ul-contact-detail__timeline \">\r\n                <div class=\"row\">\r\n                  <div class=\" col-md-4\" v-for=\"data in form_data.physical_parameter\">\r\n                    <div class=\"user-info pt-2\" v-for=\"param in data\">\r\n                      <div class=\"o_thread_search_input mt-2\">\r\n                        <span style=\"width:100px;\"> {{param.name}} : </span>\r\n                        <span>\r\n                          <input type=\"checkbox\" class=\"ui-autocomplete-input mx-2\" autocomplete=\"off\" value=\"1\" v-model=\"param.checked\" :name=\"param.name+'param'\">\r\n                          <input :placeholder=\"param.limit_type\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input w-50\" autocomplete=\"off\" v-model=\"param.limit\" v-if=\"param.checked\" :name=\"param.name+'limit'\" @input=\"checkParam(param)\" required>\r\n                        </span>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-md-12\">\r\n                    <hr>\r\n                    <div class=\"o_thread_search_input mt-1\">\r\n                      <span style=\"width:120px;\"> Qc Status: </span>\r\n                      <div>\r\n                        <input type=\"text\" class=\"ui-autocomplete-input py-1\" autocomplete=\"off\" :value=\"form_data.status\" name=\"status\" readonly style=\"width:80px\">\r\n                        <button type=\"button\" class=\"btn btn-success py-1 mx-2\" @click=\"form_data.status='Approved'\">Approve</button>\r\n                        <button type=\"button\" class=\"btn btn-danger py-1\" @click=\"form_data.status='Rejected'\">Rejected</button>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input mt-1\">\r\n                      <input type=\"hidden\" name=\"keys\" :value=\"0\">\r\n                      <input placeholder=\"0\" type=\"hidden\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.normal_procurement_detail.total_estimate_qty\" name=\"total_estimate_qty\" readonly>\r\n                      <input placeholder=\"0\" type=\"hidden\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.normal_procurement_detail.total_procured_qty\" name=\"total_procured_qty\" readonly>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{created_on:'', rejected_quantity:0, total_quantity:0, status:'Waiting', normal_procurement_detail:{type:'Raw Material',product:null, purchase_type:null,warehouse:'',total_estimate_qty:0, total_procured_qty:0,total_quantity:0,status:'',vehicle_detail:{},farmer_status:''},physical_parameter:[]},\r\n      id:null,\r\n      normal_id:null,\r\n      load_products:false,\r\n      lines:[],\r\n      product_id:null,\r\n      physical_parameters:[],\r\n      action:'create',\r\n      farmers:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'qc'\r\n    this.form_data.created_on = this.currentDate()\r\n    this.action = this.$route.params.action\r\n    this.getPurchaseTypes()\r\n    this.normal_id = this.$route.params.id\r\n    this.checkNormalTest()\r\n  },\r\n  methods: {\r\n    checkNormalTest() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/check_normal_test/'+this.normal_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.id = jsonData.id\r\n          this.getForm()\r\n        }else{\r\n          this.selectNormal()\r\n        }\r\n      })\r\n    },\r\n    selectNormal(){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/'+this.normal_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.normal_procurement = jsonData.id\r\n        this.form_data.normal_procurement_detail = jsonData\r\n        this.form_data.total_quantity = jsonData.total_quantity\r\n        this.product_id = jsonData.product\r\n        this.load_products = true\r\n        this.lines = jsonData.farmer_lines\r\n        this.getPhysicalParameters()\r\n        this.calcultion()\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/raw_material/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.id = jsonData.id\r\n        this.form_data = jsonData\r\n        this.lines = jsonData.normal_procurement_detail.farmer_lines\r\n        this.product_id = jsonData.normal_procurement_detail.product\r\n        this.load_products = true\r\n        this.getPhysicalParameters()\r\n        this.calcultion()\r\n      })\r\n    },\r\n    calcultion(){\r\n      var self =this\r\n      var rejected_quantity = 0\r\n      var total_cutting = 0\r\n      this.lines.forEach(function(line,key){\r\n        if(line.qc_status=='Approved'){\r\n          var cutting = 0\r\n          total_cutting = total_cutting+parseFloat(line.cutting)\r\n          if(line.cutting!=0){\r\n            cutting = (parseFloat(line.cutting)/100)*parseFloat(line.procure_qty)\r\n          }\r\n        }else if(line.qc_status=='Rejected'){\r\n          rejected_quantity = rejected_quantity + parseFloat(line.procure_qty)\r\n        }\r\n      })\r\n      this.form_data.rejected_quantity = rejected_quantity\r\n      this.form_data.total_cutting = total_cutting\r\n      this.form_data.total_quantity = this.form_data.normal_procurement_detail.total_quantity-rejected_quantity\r\n      if(this.form_data.total_quantity<0)this.form_data.total_quantity=0\r\n      this.form_data.total_farmers = this.lines.length\r\n\r\n    },\r\n    getPhysicalParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/physical_parameters/?parameter_for=qc&product='+this.product_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var length = jsonData.length/3\r\n        this.physical_parameters = jsonData\r\n        if(this.form_data.physical_parameter!=null){\r\n          this.form_data.physical_parameter = this.pagination(this.form_data.physical_parameter,Math.round(length)).datas\r\n        }else{\r\n          this.form_data.physical_parameter = this.pagination(jsonData,Math.round(length)).datas\r\n        }\r\n        this.setParams()\r\n      })\r\n    },\r\n    setParams(){\r\n      var lines = this.lines\r\n      this.lines = []\r\n      var self = this\r\n      lines.forEach(function(line){\r\n        if(line.physical_parameter!=null){\r\n          var jparameters = JSON.parse(line.physical_parameter)\r\n          var length = jparameters.length/2\r\n          line.physical_parameter = self.pagination(jparameters,Math.round(length)).datas\r\n        }else{\r\n          var length = self.physical_parameters.length/2\r\n          line.physical_parameter = self.pagination(self.physical_parameters,Math.round(length)).datas\r\n        }\r\n      })\r\n      this.lines = lines\r\n    },\r\n    dataForm() {\r\n      this.updateFarmerFormdata()\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('status', this.form_data.status)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/raw_material/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('status', this.form_data.status)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/raw_material/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFarmerFormdata() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('warehouse', this.form_data.normal_procurement_detail.warehouse)\r\n      var status = this.form_data.normal_procurement_detail.status\r\n      formdata.append('status', status)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/'+this.form_data.normal_procurement+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          if(jsonData.rejected){\r\n            this.updateNormalStatus(jsonData.id,'Rejected')\r\n          }else{\r\n            this.updateNormalStatus(jsonData.id,'Approved')\r\n          }\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateNormalStatus(normal_id,status) {\r\n      this.form_data.status = status\r\n      var formdata = new FormData()\r\n      formdata.append('normal_id',normal_id)\r\n      formdata.append('status',status)\r\n      formdata.append('raw_test',1)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/update_normal_status/', {\r\n          method : 'post',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          if(this.id){\r\n            this.updateFormdata()\r\n          }else{\r\n            this.createFormdata()\r\n          }\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n  .bio_std_modal .modal.show .modal-dialog{\r\n    min-width: 700px!important;\r\n  }\r\n</style>\r\n"]}]}