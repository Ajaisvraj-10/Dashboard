\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.balance_ics\" name=\"balance_ics\" required>\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        \r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\" v-if=\"id\">\r\n          <h5>Farm Information</h5>\r\n        </b-col>\r\n\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mb-3\" v-if=\"id\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n                          <div class=\"oe_button_box\">\r\n                            <router-link :to=\"'/app/fpo/farmer-lands/'+id\">\r\n                              <button type=\"button\" name=\"action_get_by_farmer\" class=\"btn oe_stat_button\">\r\n                                <i class=\"fa fa-fw o_button_icon fa-book\"></i>\r\n                                <div name=\"farmer_count\" class=\"o_field_widget o_stat_info o_readonly_modifier\">\r\n                                  <span class=\"o_stat_value\">{{form_data.farmers_count}}</span>\r\n                                    <span class=\"o_stat_text\">Land Details</span>\r\n                                </div>\r\n                              </button>\r\n                            </router-link>\r\n                            <router-link :to=\"'/app/fpo/farmer-plant/'+id\">\r\n                              <button type=\"button\" name=\"action_get_by_vfc\" class=\"btn oe_stat_button\">\r\n                                <i class=\"fa fa-fw o_button_icon fa-book\"></i>\r\n                                <div name=\"vfc_count\" class=\"o_field_widget o_stat_info o_readonly_modifier\">\r\n                                    <span class=\"o_stat_value\">{{form_data.vfc_count}}</span>\r\n                                    <span class=\"o_stat_text\">Plant Records</span>\r\n                                </div>\r\n                              </button>\r\n                            </router-link>\r\n                            <router-link :to=\"'/app/fpo/farmer-crops/'+id\">\r\n                              <button type=\"button\" name=\"action_get_by_procurement\" class=\"btn oe_stat_button\">\r\n                                <i class=\"fa fa-fw o_button_icon fa-book\"></i>\r\n                                <div name=\"procurement_count\" class=\"o_field_widget o_stat_info o_readonly_modifier\">\r\n                                  <span class=\"o_stat_value\">{{form_data.porcurment_count}}</span>\r\n                                  <span class=\"o_stat_text\">Crop Records</span>\r\n                                </div>\r\n                              </button>\r\n                            </router-link>\r\n                            <router-link :to=\"'/app/fpo/farmer-ics-payments/'+id\">\r\n                              <button type=\"button\" name=\"action_get_by_procurement\" class=\"btn oe_stat_button\">\r\n                                <i class=\"fa fa-fw o_button_icon fa-book\"></i>\r\n                                <div name=\"procurement_count\" class=\"o_field_widget o_stat_info o_readonly_modifier\">\r\n                                  <span class=\"o_stat_value\">{{form_data.porcurment_count}}</span>\r\n          