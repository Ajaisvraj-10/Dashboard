rm>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport LocationFields from '/src/views/app/oodo/components/location_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    LocationFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{farmers_count:0,vfc_count:0,porcurment_count:0},\r\n      states:[],\r\n      districts:[],\r\n      panchayaths:[],\r\n      revenue_villages:[],\r\n      local_villages:[],\r\n      id:null,\r\n      post_office_id:null,\r\n      load_locations:false,\r\n      action:'create',\r\n      types:[],\r\n      employees:[],\r\n      all_ics:[],\r\n      all_vfc:[],\r\n      all_aeo:[],\r\n      religions:['HINDHU','CHIRSTIAN','MUSLIM'],\r\n      card_types:['APL','BPL'],\r\n      genders:['M','F'],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'fpo'\r\n    this.getTypes()\r\n    this.getVfc()\r\n    this.getIcs()\r\n    this.getEmployees()\r\n    this.getAeo()\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_locations = true\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getTypes() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/types/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.types = jsonData\r\n      })\r\n    },\r\n    getIcs() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/ics/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.all_ics = jsonData\r\n      })\r\n    },\r\n    getVfc() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/vfc/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.all_vfc = jsonData\r\n      })\r\n    },\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.employees = jsonData\r\n      })\r\n    },\r\n    getAeo() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/aeo/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.all_aeo = jsonData\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmers/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.post_office_id = jsonData.post_office\r\n        this.form_data = jsonData\r\n        this.load_locations = true\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmers/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmers/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\ninput[type=\"date\"]::-webkit-datetime-edit, input[type=\"date\"]::-webkit-inner-spin-button, input[type=\"date\"]::-webkit-clear-button {\r\n  color: #fff;\r\n  position: relative;\r\n}\r\n\r\ninput[type=\"date\"]::-webkit-datetime-edit-year-field{\r\n  position: absolute !important;\r\n  border-left:1px solid #b8b8b8;\r\n  padding: 2px;\r\n  padding-left: 6px;\r\n  color:#495057;\r\n  left: 60px;\r\n}\r\n\r\ninput[type=\"date\"]::-webkit-datetime-edit-month-field{\r\n  position: absolute !important;\r\n  border-left:1px solid #b8b8b8;\r\n  padding: 2px;\r\n  padding-left: 6px;\r\n  color:#495057;\r\n  left: 26px;\r\n}\r\n\r\n\r\ninput[type=\"date\"]::-webkit-datetime-