                <th>Cutting </th>\r\n                                <th>Cutting Qc (%)</th>\r\n                                <th>Final Quantity</th>\r\n                                <th>Final Price</th>\r\n                              </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                              <tr v-for=\"(farmer,key) in farmers\">\r\n                                 <td>\r\n                                  {{farmer.farmer_detail.farmer_code}}\r\n                                </td>\r\n                                <td>\r\n                                  {{farmer.farmer_detail.name}}\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"Procure Quantity\" type=\"text\" :name=\"'procure_qty'+farmer.id\" v-model=\"farmer.procure_qty\" class=\"ui-autocomplete-input\" readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"Cutting\" type=\"text\" :name=\"'cutting_mngr'+farmer.id\" v-model=\"farmer.cutting_mngr\" class=\"ui-autocomplete-input\" required @input=\"calcultion('cutting')\">\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"Cutting (Qc)\" type=\"text\" :name=\"'cutting'+farmer.id\" v-model=\"farmer.cutting\" class=\"ui-autocomplete-input\" required @input=\"calcultion('cutting')\">\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"Final Quantity\" type=\"text\" :name=\"'quantity'+farmer.id\" v-model=\"farmer.quantity\" class=\"ui-autocomplete-input\" required @input=\"calcultion()\">\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"Price\" type=\"text\" :name=\"'price'+farmer.id\" v-model=\"farmer.price\" class=\"ui-autocomplete-input\" @input=\"calcultion('price')\" required>\r\n                                </td>\r\n                              </tr>\r\n                              <tr>\r\n                                <th></th>\r\n                                <th class=\"text-right\">Total</th>\r\n                                <th>\r\n                                  <input placeholder=\"Quantity\" type=\"text\" name=\"total_procure_qty\" v-model=\"form_data.total_procure_qty\" class=\"ui-autocomplete-input\" readonly>\r\n                                </th>\r\n                                <th>\r\n                                  <input placeholder=\"Cutting\" type=\"text\" name=\"total_cutting_mngr\" v-model=\"form_data.total_cutting_mngr\" class=\"ui-autocomplete-input\" readonly>\r\n                                </th>\r\n                                <th>\r\n                                  <input placeholder=\"Cutting (Qc)\" type=\"text\" name=\"total_cutting\" v-model=\"form_data.total_cutting\" class=\"ui-autocomplete-input\" readonly>\r\n                                </th>\r\n                                <th>\r\n                                  <input placeholder=\"Final Quantity\" type=\"text\" name=\"total_quantity\" v-model=\"form_data.total_quantity\" class=\"ui-autocomplete-input\" readonly>\r\n                                </th>\r\n                                <th>\r\n                                  <input placeholder=\"Price\" type=\"text\" name=\"total_price\" v-model=\"form_data.total_price\" class=\"ui-autocomplete-input\" readonly>\r\n                                </th>\r\n                              </tr>\r\n                            </tbody>\r\n                          </table>\r\n\r\n                          <div class=\"o_thread_search_input mt-3\">\r\n                            <span style=\"width:120px;\"> Note : </span>\r\n                            <textarea placeholder=\"Note\" class=\"ui-autocomplete-input w-100 mx-1\" autocomplete=\"off\" v-model=\"form_data.note\" rows=\"5\" name=\"note\"></textarea>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"4\" xl=\"4\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div class=\"ul-contact-detail__timeline\">\r\n              <div class=\"row\">\r\n                <div class=\"o_thread_composer  \">\r\n                   <div class=\"o_composer_container my-2 row mx-0\">\r\n                      <div class=\"col-md-1\"></div>\r\n                      <div class=\"col-md-12\">\r\n                        <div class=\"o_thread_search_input mb-3\">\r\n                          <span style=\"width:120px;\"> Lot: </span>\r\n                          <input type=\"text\" class=\"ui-autocomplete-input\" :value=\"lot_id+'/Lot'\" readonly>\r\n                          <input type=\"hidden\" name=\"lot\" :value=\"lot_id\">\r\n                        </div>\r\n                        <ProductFields :product_id=\"product_id\" :readonly=\"true\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n                        <input type=\"hidden\" name=\"product\" :value=\"product_id\">\r\n                        <div class=\"o_thread_search_input mb-3\">\r\n                          <span style=\"width:120px;\"> Product Price: </span>\r\n                          <input type=\"text\" class=\"ui-autocomplete-input\" :value=\"product_price\" readonly>\r\n                        </div>\r\n\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              <hr>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{lot_detail:{},total_procure_qty:0,total_cutting:0,total_cutting_qc:0,total_quantity:0,total_price:0},\r\n      id:null,\r\n      load_products:false,\r\n      product_id:null,\r\n      action:'create',\r\n      lot_id:null,\r\n      farmers:[],\r\n      product_price:0,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'purchase-manager'\r\n    this.lot_id = this.$route.params.id\r\n    this.checkLotPurchase()\r\n  },\r\n  methods: {\r\n    checkLotPurchase() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/purchase/check_lot_purchase/'+this.lot_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.id = jsonData.id\r\n          this.getForm()\r\n        }else{\r\n          this.selectLot()\r\n        }\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/purchase/farmer_purchase/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.product_id = jsonData.product\r\n        this.product_price = jsonData.product_detail.price\r\n        this.load_products = true\r\n        this.farmers = jsonData.farmers\r\n        this.calcultion()\r\n      })\r\n    },\r\n    selectLot() {\r\n      this.load_products = false\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/create_lots/'+this.lot_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.lot_detail = jsonData\r\n        this.product_id = jsonData.product\r\n        this.product_price = jsonData.product_detail.price\r\n        this.load_products = true\r\n        var farmer_lines = []\r\n        jsonData.normal_procurements.forEach(function(val,key){\r\n          val.farmer_lines.forEach(function(farm_l){\r\n            farmer_lines.push(farm_l)\r\n          })\r\n        })\r\n        this.farmers = farmer_lines\r\n        this.calcultion()\r\n      })\r\n    },\r\n    calcultion(field=''){\r\n      var self = this\r\n      var total_procure_qty = 0\r\n      var total_cutting_mngr = 0\r\n      var total_cutting = 0\r\n      var total_quantity = 0\r\n      var total_price = 0\r\n      this.farmers.forEach(function(val,key){\r\n        if(field=='cutting'){\r\n          var qty_total = parseFloat(val.procure_qty) - parseFloat(val.cutting_mngr)\r\n          var qc_cutting = (parseFloat(val.cutting)/100)*qty_total\r\n          self.farmers[key].quantity =  qty_total - qc_cutting\r\n        }\r\n        if(field!='price'){\r\n          var qty = val.quantity\r\n          if(!qty && qty!=0){\r\n            qty = val.procure_qty\r\n          }\r\n          self.farmers[key].price = self.product_price * parseFloat(qty)\r\n        }\r\n        total_procure_qty = total_procure_qty + parseFloat(val.procure_qty)\r\n        total_cutting_mngr = total_cutting_mngr + parseFloat(val.cutting_mngr)\r\n        total_cutting = total_cutting + parseFloat(val.cutting)\r\n        total_quantity = total_quantity + parseFloat(val.quantity)\r\n        total_price = total_price + parseFloat(val.price)\r\n      })\r\n      this.form_data.total_procure_qty = total_procure_qty\r\n      this.form_data.total_cutting_mngr = (total_cutting_mngr)?total_cutting_mngr:0\r\n      this.form_data.total_cutting = total_cutting\r\n      this.form_data.total_quantity = (total_quantity)?total_quantity:0\r\n      this.form_data.total_price = total_price\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.form_data.product = id\r\n    },\r\n    dataForm() {\r\n      if(this.id){\r\n        this.updateFormdata()\r\n      }else{\r\n        this.createFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/purchase/farmer_purchase/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/purchase/farmer_purchase/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}