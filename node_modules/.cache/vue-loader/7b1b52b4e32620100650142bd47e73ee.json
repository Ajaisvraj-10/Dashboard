sm-block mx-1\" v-if=\"action!='view'\">\r\n            Save\r\n          </b-button>\r\n          <router-link to=\"/app/procurement/sample-procurement\">\r\n            <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\">\r\n              Discard\r\n            </b-button>\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n      <div class=\" my-3 border-top\"></div>\r\n      <fieldset :disabled=\"action=='view'?true:false\">\r\n        <b-row class=\"main-scroll pt-2\">\r\n          <b-col lg=\"5\" xl=\"5\">\r\n            <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n              <div>\r\n                <h5>Product Details</h5>\r\n                <div class=\"ul-contact-detail__timeline\">\r\n                  <div class=\"row\">\r\n                    <div class=\" col-md-12\">\r\n\r\n                      <div class=\"user-info pt-2\">\r\n                        <div class=\"o_thread_search_input mb-2\">\r\n                          <span style=\"width:120px;\"> Date: </span>\r\n                          <input placeholder=\"Date\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"dateFormate(form_data.created_on,true)\" required readonly>\r\n                        </div>\r\n\r\n                        <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n\r\n                        <div class=\"o_thread_search_input mt-2\">\r\n                          <span style=\"width:120px;\"> Farmer Status: </span>\r\n                          <input type=\"text\" name=\"farmer_status\" v-model=\"form_data.farmer_status\" readonly>\r\n                        </div>\r\n\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <hr>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n          </b-col>\r\n          <b-col lg=\"7\" xl=\"7\">\r\n            <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n              <div>\r\n                <h5>Farmer Details</h5>\r\n                <div class=\"ul-contact-detail__timeline\">\r\n                  <div class=\"row\">\r\n                    <div class=\" col-md-12\">\r\n\r\n                      <div class=\"user-info pt-2\">\r\n                        <div class=\"o_thread_search_input\">\r\n                          <span style=\"width:120px;\"> Farmer Code: </span>\r\n                          <input type=\"text\" placeholder=\"Search (Farmer code)\" class=\"\" autocomplete=\"off\" v-model=\"form_data.farmer_detail.farmer_code\" @input=\"searchFarmers()\" required>\r\n                              <div class=\"farmer_search_drpdwn\" v-show=\"farmers.length>0\">\r\n                                <div class=\"farmer_heading\">\r\n                                  Farmers Result <span @click=\"farmers=[]\"><i class=\"fa fa-times-circle\" aria-hidden=\"true\"></i></span>\r\n                                </div>\r\n                                <div class=\"farmer_results\">\r\n                                  <p v-for=\"farmer in farmers\" @click=\"selectFarmer(farmer)\">{{farmer.farmer_code}}-{{farmer.name}}</p>\r\n                                </div>\r\n                              </div>\r\n                        </div>\r\n                        <div class=\"o_thread_search_input\">\r\n                          <span style=\"width:120px;\"> Farmer name: </span>\r\n                          <input type=\"text\" placeholder=\"Farmer name\" class=\"\" autocomplete=\"off\" :value=\"form_data.farmer_detail.name\" readonly required>\r\n                          <input type=\"hidden\" name=\"farmer\" :value=\"form_data.farmer\">\r\n                        </div>\r\n                        <div class=\"o_thread_search_input\">\r\n                          <span style=\"width:120px;\"> Estimate QTY (Kg): </span>\r\n                          <input placeholder=\"Qty\" type=\"text\" :value=\"form_data.estimate_qty\" class=\"\" autocomplete=\"off\" name=\"estimate_qty\" readonly>\r\n                        </div>\r\n                        <div class=\"o_thread_search_input\">\r\n                          <span style=\"width:120px;\"> Farmer Estimate QTY (Kg): </span>\r\n                          <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" name=\"farmer_estimate\" required v-model=\"form_data.farmer_estimate\">\r\n                        </div>\r\n                        <div class=\"o_thread_search_input\">\r\n                          <span style=\"width:120px;\"> Available QTY (KG): </span>\r\n                          <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" name=\"available_qty\" v-model=\"form_data.available_qty\" required>\r\n                        </div>\r\n                        <div class=\"o_thread_search_input\">\r\n                          <span style=\"width:120px;\"> QC Status: </span>\r\n                          <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" :value=\"form_data.qc_status\" readonly></td>\r\n                        </div>\r\n                        \r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <hr>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n          </b-col>\r\n        </b-row>\r\n      </fieldset>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{purchase_type:1,created_on:'','estimate_qty':0,'farmer_estimate':0,'farmer_detail':{'name':'', 'farmer_code':''},'farmer':null, 'available_qty':0,'qc_status':'Waiting','farmer_status':''},\r\n      id:null,\r\n      load_products:false,\r\n      product_id:null,\r\n      action:'create',\r\n      farmers:[],\r\n      total_keys:0,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.form_data.created_on = this.currentDate()\r\n    this.$root.current_section = 'procurement'\r\n    this.action = this.$route.params.action\r\n    this.getPurchaseTypes()\r\n    if(this.action != 'create'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    searchFarmers(line=''){\r\n      var farmer_code = this.form_data.farmer_detail.farmer_code\r\n      if(farmer_code.length<4){\r\n        return false\r\n      }\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      formdata.append('farmer_code',farmer_code)\r\n      formdata.append('search_type','sample_procurement')\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/search_farmers/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmers = jsonData\r\n      })\r\n    },\r\n    selectFarmer(farmer=null){\r\n      if(farmer){\r\n        this.form_data.farmer_detail = farmer\r\n        this.form_data.farmer_status = 'Conventional'\r\n        if(farmer.status=='ORG'){\r\n          this.form_data.farmer_status = 'Organic'\r\n        }\r\n        this.form_data.farmer = farmer.id\r\n      }\r\n      this.farmers = []\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      formdata.append('farmer', this.form_data.farmer)\r\n      formdata.append('product', this.form_data.product)\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmer_full_details/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.estimate_qty = jsonData.estimate_qty\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_procurement/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_id = jsonData.product\r\n        this.form_data = jsonData\r\n        this.load_products = true\r\n      })\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.form_data.product = id\r\n      this.selectFarmer()\r\n    },\r\n    dataForm() {\r\n      if(this.id){\r\n        this.updateFormdata()\r\n      }else{\r\n        this.createFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('aeo',2)\r\n      formdata.append('purchase_type',1)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_procurement/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.id = jsonData.id\r\n          this.form_data = jsonData\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('aeo',2)\r\n      formdata.append('purchase_type',1)\r\n      formdata.append('product', this.form_data.product)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_procurement/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n@import '../styles/style.css';\r\n</style>\r\n"]}]}