\r\n                            </select>\r\n                          </div>\r\n                          \r\n                          <div class=\"o_thread_search_input mb-3\">\r\n                            <span style=\"width:120px;\"> Packing Methods: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"packing_method\" v-model=\"form_data.packing_method\">\r\n                              <option v-for=\"packing_method in packing_methods\" :value=\"packing_method\"> {{packing_method}}</option>\r\n                            </select>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input mb-3\" v-if=\"form_data.packing_method=='Stock deserved bags'\">\r\n                            <span style=\"width:120px;\"> Indent: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"indent\" v-model=\"form_data.indent\" @change=\"selectIndent()\">\r\n                              <option v-for=\"indent in indents\" :value=\"indent.id\"> Indent/{{indent.id}}/{{indent.PO_number}}</option>\r\n                            </select>\r\n                          </div>\r\n\r\n                          <ProductFields :product_id=\"product_id\" :readonly=\"true\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n\r\n                          <div class=\"o_thread_search_input\" v-if=\"form_data.indent_detail.is_parameters\">\r\n                            <span style=\"width:120px;\">Parameter : </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"parameter\" v-model=\"form_data.indent_detail.parameter\">\r\n                                <option v-for=\"parameter in parameters\" :value=\"parameter.id\">{{parameter.name}}</option>\r\n                            </select>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input\" v-if=\"form_data.packing_method=='Stock deserved bags'\">\r\n                            <span style=\"width:120px\"> Workers: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"workers\" v-model=\"form_data.workers\" multiple>\r\n                              <option v-for=\"worker in workers\" :value=\"worker.id\" v-if=\"worker.job_position==5\"> #{{worker.id}}/{{worker.name}}</option>\r\n                            </select>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input\" v-if=\"form_data.lot=='Stock deserved bags'\">\r\n                            <span style=\"width:120px\"> Workers: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"workers\" v-model=\"form_data.workers\" multiple>\r\n                              <option v-for=\"worker in workers\" :value=\"worker.id\" v-if=\"worker.job_position==5\"> #{{worker.id}}/{{worker.name}}</option>\r\n                            </select>\r\n                          </div>\r\n\r\n                          <div class=\"o_thread_search_input col-md-3\">\r\n                            <span style=\"width:120px;\"> Status: </span>\r\n                            <input type=\"checkbox\" name=\"status\" v-model=\"form_data.status\" class=\"ui-autocomplete-input mr-2\" value=\"1\"> {{form_data.status?'Active':'Deactive'}}\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n                        <div class=\"col-md-5 border-left pt-1\" style=\"overflow-y:auto;height:72vh\">\r\n                          <b-card  header-bg-variant=\"transparent\" class=\"mb-2\" v-for=\"material in values\">\r\n                            <div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Material : </span>\r\n                                <input placeholder=\"Material\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"material.material_detail.name\" readonly>\r\n                              </div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px\">Specifications:</span>\r\n                                <table class=\"pt-2\">\r\n                                  <tr v-for=\"specification in material.specifications\" v-if=\"specification.material_ids.includes(material.material)\">\r\n                                    <th>{{specification.name}}</th>\r\n                                    <th>:</th>\r\n                                    <td>{{specification.value}}</td>\r\n                                  </tr>\r\n                                </table>\r\n                              </div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Max Quantity : </span>\r\n                                <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"material.quantity\" readonly>\r\n                              </div>\r\n                              <div class=\"o_thread_search_input\">\r\n                                <span style=\"width:120px;\">Quantity: </span>\r\n                                <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"material.fill_count\" :name=\"'fill_count'+material.id\" required>\r\n                                <input type=\"hidden\" :name=\"'material_id'+material.id\" :value=\"material.id\">\r\n                              </div>\r\n                            </div>\r\n                          </b-card>\r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"PRODUCTION - Machines\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{packing_method:'Stock deserved bags', lot_detail:{}, indent_detail:{is_parameters:false}},\r\n      id:null,\r\n      action:'create',\r\n      product_id:null,\r\n      load_products:false,\r\n      indents:[],\r\n      lots:[],\r\n      packing_methods:['Direct machine out','Stock deserved bags'],\r\n      values:[],\r\n      workers:[],\r\n      parameters:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n    this.getBatchs()\r\n    this.getEmployees()\r\n    this.getParameters()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/packing/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n      })\r\n    },\r\n    getBatchs() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/lot_creation/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lots = jsonData\r\n      })\r\n    },\r\n    selectBatch() {\r\n      this.load_products = false\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/lot_creation/'+this.form_data.lot+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.lot_detail = jsonData\r\n        this.product_id = jsonData.product\r\n        this.load_products = true\r\n        this.getIndents()\r\n      })\r\n    },\r\n    getIndents() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/?product='+this.product_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.indents = jsonData\r\n      })\r\n    },\r\n    selectIndent() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+this.form_data.indent+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.indent_detail = jsonData\r\n        this.product_id = jsonData.product\r\n        this.values = jsonData.values\r\n      })\r\n    },\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.workers = jsonData\r\n      })\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameters/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.parameters = jsonData\r\n      })\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.form_data.product = id\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/packing/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/packing/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}