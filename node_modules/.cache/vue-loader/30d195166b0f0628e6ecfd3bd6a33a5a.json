v class=\"menu-wrapper  d-flex align-items-center justify-content-between\"> \r\n                <div class=\"flex-align3\" style=\"gap:15px;\">   \r\n                       \r\n                    <div class=\"header-logo\">\r\n                        <router-link :to=\"edit?'':{name:'home'}\">\r\n                            <img :src=\"frontent_settings.main_logo\" class=\"logo\">\r\n                        </router-link>             \r\n                    </div>\r\n                    <div class=\"menu-home\" >\r\n                        <ul class=\"home-nav\" >\r\n                            <router-link v-for=\"header in site_header\" :to=\"edit?'':header.link\" v-if=\"header.header_type=='Menu' && header.status\">\r\n                                <li :class=\"active_menu==header.name?'active-nav':''\" @click=\"active_menu=header.name\">{{header.name}}</li>\r\n                            </router-link>\r\n                            \r\n                        </ul>\r\n\r\n                    </div>\r\n                    \r\n                    \r\n                </div>\r\n                <div class=\"text-center header-right flex-align3\" style=\"gap:20px;height: 40px;\">\r\n                    \r\n                    \r\n                    <div class=\"search-input\">\r\n                        <img src=\"/mini_tv/images/search.png\" class=\" cursor-pointer\" >\r\n                        <input placeholder=\"Movies, series, Etc\">\r\n                       \r\n                    </div>\r\n                    \r\n                    <div class=\"profile-sec\">\r\n                        <img src=\"/mini_tv/images/menubar.png\" style=\"cursor:pointer;width:18px;\">\r\n                        <div class=\"profile-nav\">\r\n                            \r\n                                <div class=\"profile\" v-if=\"$root.token\">\r\n                                    <router-link :to=\"edit?'':{name:'profile'}\">\r\n                                        <h6 class=\"\">Profile</h6>\r\n                                    </router-link>\r\n                                    <router-link :to=\"edit?'':{name:'plan'}\">\r\n                                        <h6 >Upgrade Plan</h6>\r\n                                    </router-link>\r\n                                </div>\r\n                                <div class=\"profile\" v-else>\r\n                                    <router-link :to=\"edit?'':{name:'sign-in'}\">\r\n                                        <h6 class=\"\">Login</h6>\r\n                                    </router-link>\r\n                                    <router-link :to=\"edit?'':{name:'plan'}\">\r\n\r\n                                        <h6 >Buy Plan</h6>\r\n                                    </router-link>\r\n                                </div>\r\n                            \r\n                        </div>\r\n                    </div>\r\n                      \r\n                </div>\r\n            </div>\r\n        </header> \r\n    </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  props: ['edit','blocks','current_block'],\r\n  data() {\r\n    return {\r\n        active_menu:'home',\r\n        search:'',\r\n        site_header:[],\r\n        \r\n    };\r\n  },\r\n  mounted(){\r\n    this.active_menu = ''\r\n    if(this.$route.params.type){\r\n        this.active_menu = this.$route.params.type\r\n    }\r\n    this.getHeader()\r\n  },\r\n  methods: {\r\n    searchResult(){\r\n        this.$router.push({name:'videos', params: { type: 'search'}, query: { tag: this.search}})\r\n    },\r\n    getHeader() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/content/site_header/?theme='+theme_name, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n          this.site_header = jsonData\r\n      })\r\n    },\r\n  }\r\n\r\n};\r\n</script>\r\n<style scoped>\r\n  @import '/mini_tv/style/style.css';\r\n  @import '/mini_tv/style/style-2.css';\r\n\r\n\r\n\r\n</style>\r\n"]}]}