    <slot name=\"header\">\r\n               <h5 class=\"mb-0\"><span class=\"text-success\">Consumables Purchase</span> / {{slugToWord(action)}}</h5>\r\n                  \r\n            </slot>\r\n            <div class=\"d-flex\">\r\n               <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Save\r\n              </b-button>\r\n              <b-button type=\"button\" @click=\"goBack()\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Discard\r\n              </b-button>\r\n            </div>\r\n          </div>\r\n        <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll pt-2\">\r\n        <b-col lg=\"4\" xl=\"4\" class=\"pr-0\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div class=\"ul-contact-detail__timeline\">\r\n              <div class=\"row\">\r\n                <div class=\" col-md-12\">\r\n\r\n                  <div class=\"user-info pt-2\">\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\"> Type: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"item_type\" v-model=\"current_item_type\">\r\n                        <!-- <option :value=\"null\">All types</option> -->\r\n                        <option v-for=\"item_type in item_types\" :value=\"item_type\">{{slugToWord(item_type)}}</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\"> Vehicle No: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"vehicle\" v-model=\"form_data.vehicle\" @change=\"changeVehicle()\">\r\n                        <option value=\"\" selected disabled>Select a vehicle</option>\r\n                        <option v-for=\"vehicle in vehicles\" :value=\"vehicle.id\">{{vehicle.name}}</option>\r\n                        <option value=\"create\" style=\"color:green!important\">Create New Vehicle</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Driver Name : </span>\r\n                      <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.vehicle_detail.name\" readonly>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Driver Phone : </span>\r\n                      <input placeholder=\"Phone\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.vehicle_detail.contact\" readonly>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Warehouse: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.warehouse\" name=\"warehouse\">\r\n                        <option v-for=\"warehouse in warehouses\" :value=\"warehouse.id\">{{warehouse.name}}</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Total Price (Rs): </span>\r\n                      <input placeholder=\"Price\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.total_price\" name=\"total_price\">\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <hr>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"8\" xl=\"8\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div class=\"ul-contact-detail__timeline\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-8\">\r\n                  <h5>Items ({{form_data.items.length}})</h5>\r\n                </div>\r\n                <div class=\"col-md-4 text-right\">\r\n                  <b-button variant=\"success\" type=\"button\" class=\"px-3 py-1 mx-1\" @click=\"lineOperation('+')\" style=\"margin-top:-10px\">\r\n                    Add item\r\n                  </b-button>\r\n                  <input type=\"hidden\" name=\"keys\" :value=\"form_data.items.length\">\r\n                </div>\r\n                <div class=\" col-md-6\" v-for=\"(consumable_item,key) in form_data.items\">\r\n                  <div class=\"card p-2 pb-2 mb-2\">\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:50px;\"> Item: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 mx-1\" :name=\"'item'+key\" v-model=\"consumable_item.item\" @change=\"changeItem(consumable_item)\">\r\n                        <option value=\"\" selected disabled>Select Consumable</option>\r\n                        <option v-for=\"item in items\" :value=\"item.id\" v-if=\"!item.is_free && (current_item_type==item.item_type || !current_item_type) \">{{item.name}}</option>\r\n                        <option value=\"create\" style=\"color:green!important\">Create New Item</option>\r\n                      </select>\r\n                      <div class=\"\">\r\n                        <b-button variant=\"danger\" type=\"button\" class=\"ml-2 px-2 py-1 d-sm-block\" @click=\"lineOperation(key)\">\r\n                          Remove\r\n                        </b-button>\r\n                      </div>\r\n                    </div>\r\n                    <hr class=\"my-1\">\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Unit : </span>\r\n                      <input placeholder=\"Unit\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"consumable_item.item_detail.unit\" :name=\"'unit'+key\" readonly>\r\n                    </div>\r\n                    <div class=\"\" v-if=\"consumable_item.item_detail.is_numbers\">\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\">Quantity 1Item ({{consumable_item.item_detail.unit}}): </span>\r\n                        <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"consumable_item.qty_single\" @input=\"consumable_item.quantity=consumable_item.number*consumable_item.qty_single\" :name=\"'qty_single'+key\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\">Quantity (Number): </span>\r\n                        <input placeholder=\"Number\" type=\"number\" min=\"0\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"consumable_item.number\" :name=\"'number'+key\" @input=\"consumable_item.quantity=consumable_item.number*consumable_item.qty_single\" required>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Total Purchased Quantity ({{consumable_item.item_detail.unit}}) : </span>\r\n                      <input placeholder=\"Quantity\" type=\"number\" min=\"0\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"consumable_item.quantity\" :name=\"'quantity'+key\" required>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\" v-if=\"consumable_item.item_detail.is_volume\">\r\n                      <span style=\"width:120px;\">Volume (M<sup>3</sup>): </span>\r\n                      <input placeholder=\"Volume\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"consumable_item.volume\" :name=\"'volume'+key\" required>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:120px;\">Total Price (Rs): </span>\r\n                      <input placeholder=\"Price\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"consumable_item.price\" :name=\"'price'+key\" @input=\"calcultion()\" required>\r\n                      <input type=\"hidden\" :name=\"'available_quantity'+key\" v-model=\"consumable_item.quantity\">\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\" v-if=\"consumable_item.id\">\r\n                      <span style=\"width:120px;\">Stock Quantity ({{consumable_item.item_detail.unit}}): </span>\r\n                      <input placeholder=\"Price\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"consumable_item.available_quantity\" readonly>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <hr>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n\r\n    <div>\r\n      <b-modal id=\"create-vehicle\" hide-header  hide-footer>\r\n        <form ref=\"vehicleForm\" @submit.prevent=\"vehicleForm()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Vehicle</span> / Create</h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('create-vehicle')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Vehicle Type : </span>\r\n                            <input placeholder=\"Vehicle Type\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.vehicle_type\" name=\"vehicle_type\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Driver Name: </span>\r\n                            <input placeholder=\"Driver Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.name\" name=\"name\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Driver Contact: </span>\r\n                            <input placeholder=\"Driver Contact\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.contact\" name=\"contact\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Vehicle No : </span>\r\n                            <input placeholder=\"Vehicle No\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.vehicle_number\" name=\"vehicle_number\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Origin of the Vehicle  : </span>\r\n                            <input placeholder=\"Origin of the Vehicle\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.origin_of_the_vehicle\" name=\"origin_of_the_vehicle\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Destination: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.warehouse\" name=\"destination\">\r\n                              <option v-for=\"warehouse in warehouses\" :value=\"warehouse.id\">{{warehouse.name}}</option>\r\n                            </select>\r\n                          </div>\r\n                          \r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n      <b-modal id=\"create-item\" hide-header  hide-footer>\r\n        <form ref=\"itemForm\" @submit.prevent=\"itemForm()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Item</span> / Create</h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('create-item')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:150px;\">Name: </span>\r\n                            <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"name\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:150px;\">Unit : </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"unit\">\r\n                              <option v-for=\"unit in units\" :value=\"unit\">{{unit}}</option>\r\n                            </select>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:150px;\">Item Type : </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"item_type\" v-model=\"current_item_type\">\r\n                              <option v-for=\"item_type in item_types\" :value=\"item_type\">{{slugToWord(item_type)}} item</option>\r\n                            </select>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mt-2\">\r\n                            <div style=\"width:160px\"> Is lot wise? : </div>\r\n                            <b-form-checkbox v-model=\"form_data.is_lot_wise\" name=\"is_lot_wise\" value=\"1\" switch>\r\n                              <b>{{ form_data.is_lot_wise?'Yes':'No' }}</b>\r\n                            </b-form-checkbox>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mt-2\">\r\n                            <div style=\"width:160px\"> Quantity with number? : </div>\r\n                            <b-form-checkbox v-model=\"form_data.is_numbers\" name=\"is_numbers\" value=\"1\" switch>\r\n                              <b>{{ form_data.is_numbers?'Yes':'No' }}</b>\r\n                            </b-form-checkbox>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{date:'',vehicle:'', warehouse:'', vehicle_detail:{name:'', phone:''}, items:[], total_price:0},\r\n      id:null,\r\n      action:'create',\r\n      vehicles:[],\r\n      items:[],\r\n      item_types:['other','spare','food','wood'],\r\n      current_item_type:null,\r\n      current_item:{},\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'inventory'\r\n    if(this.$route.query.item_type){\r\n      this.current_item_type = this.$route.query.item_type\r\n    }\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n    this.getVehicles()\r\n    this.getItems()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/consumable_purchases/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.current_item_type = this.form_data.item_type\r\n      })\r\n    },\r\n    getItems() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/other_items/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.items = jsonData\r\n      })\r\n    },\r\n    lineOperation(operation){\r\n      if(operation=='+'){\r\n        this.form_data.items.push({item:'', price:0, item_detail:{unit:''}})\r\n      }else{\r\n        this.form_data.items.splice(operation, 1)\r\n      }\r\n      this.calcultion()\r\n    },\r\n    calcultion(){\r\n      var price = 0\r\n      this.form_data.items.forEach(function(val,key){\r\n        price = price + parseFloat(val.price)\r\n      })\r\n      // this.form_data.total_quantity = quantity\r\n      this.form_data.total_price = price\r\n    },\r\n    changeItem(consumable_item){\r\n      this.current_item = consumable_item\r\n      if(consumable_item.item=='create'){\r\n        consumable_item.item = ''\r\n        this.$bvModal.show('create-item')\r\n      }else{\r\n        this.items.forEach(function(val,key){\r\n          if(val.id == consumable_item.item){\r\n            consumable_item.item_detail = val\r\n          }\r\n        })\r\n      }\r\n    },\r\n    itemForm() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['itemForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/other_items/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.getItems()\r\n          this.current_item.item = jsonData.id\r\n          this.current_item.item_detail = jsonData\r\n          this.$bvModal.hide('create-item')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getVehicles() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/vehicle/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.vehicles = jsonData\r\n        this.getWarehouses()\r\n      })\r\n    },\r\n    changeVehicle(){\r\n      if(this.form_data.vehicle=='create'){\r\n        this.form_data.vehicle = ''\r\n        this.$bvModal.show('create-vehicle')\r\n      }else{\r\n        var self =this\r\n        this.vehicles.forEach(function(val,key){\r\n          if(val.id == self.form_data.vehicle){\r\n            self.form_data.vehicle_detail = val\r\n            self.form_data.warehouse = val.destination\r\n          }\r\n        })\r\n      }\r\n    },\r\n    vehicleForm() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['vehicleForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/vehicle/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.getVehicles()\r\n          this.form_data.vehicle = jsonData.id\r\n          this.form_data.vehicle_detail = jsonData\r\n          this.$bvModal.hide('create-vehicle')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.form_data.id){\r\n        this.updateFormdata()\r\n      }else{\r\n        this.createFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      if(this.form_data.items.length<=0){\r\n        this.showAlert('Please add items!','red')\r\n        return false\r\n      }\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/consumable_purchases/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        this.id = jsonData.id\r\n        if(jsonData.id>0){\r\n          this.setConsumableItems()\r\n        }else{\r\n          this.showAlert('Something Wrong!','red')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/consumable_purchases/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.setConsumableItems()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    setConsumableItems() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('consumable_purchase',this.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/consumables/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.success){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!','red')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import '../styles/style.css';\r\n#create-item.modal.show .modal-dialog, #create-vehicle.modal.show .modal-dialog{\r\n  min-width: 470px!important;\r\n}\r\n\r\n</style>\r\n"]}]}