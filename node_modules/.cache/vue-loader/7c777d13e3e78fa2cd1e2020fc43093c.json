\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Packing Material Specifications</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                <b-button variant=\"primary\" type=\"button\" class=\"px-3 py-1  d-sm-block mx-1\" @click=\"selectMaterial()\">\r\n                  Create new Material\r\n                </b-button>\r\n                <b-button variant=\"success\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"selectSpecification()\">\r\n                   Add Specification\r\n                </b-button>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <table class=\"table\">\r\n              <thead>\r\n                <tr>\r\n                  <th style=\"background-color:#a1aaff\" class=\"border-right\">Specification</th>\r\n                  <th>Type</th>\r\n                  <th v-for=\"material in materials\"><button type=\"button\" class=\"btn btn-primary btn-sm py-1 px-2\" @click=\"selectMaterial(material.id)\">{{material.name}} </button> </th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"specification in specifications\">\r\n                  <th class=\"border-right\">{{specification.name}} <span class=\"badge badge-primary\" @click=\"selectSpecification(specification.id)\" style=\"cursor:pointer\">Edit</span></th>\r\n                  <td>{{specification.specification_for}}</td>\r\n                  <td v-for=\"material in materials\">\r\n                    <span v-if=\"material.specifications.includes(specification.id)\" class=\"badge badge-success\">Yes</span>\r\n                    <span v-else class=\"badge badge-danger\">No</span>\r\n                  </td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n\r\n    <div>\r\n      <b-modal id=\"create-material\" hide-header  hide-footer>\r\n        <form ref=\"dataMaterial\" @submit.prevent=\"dataMaterial()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\" v-if=\"form_data.id\"><span class=\"text-success\">Update Material</span></h5>\r\n                   <h5 class=\"mb-0\" v-else><span class=\"text-success\">Create Material</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('create-material')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <div style=\"width:50px;\">Name: </div>\r\n                            <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"name\" v-model=\"form_data.name\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mt-2\">\r\n                            <span style=\"width:100px;\"> Specifications: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"specifications\" v-model=\"form_data.specifications\" multiple style=\"height:250px\">\r\n                              <option v-for=\"specification in specifications\" :value=\"specification.id\"> {{specification.name}}</option>\r\n                            </select>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n      <b-modal id=\"add-specification\" hide-header  hide-footer>\r\n        <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\" v-if=\"specification_form.id\"><span class=\"text-success\">Update Specification</span></h5>\r\n                   <h5 class=\"mb-0\" v-else><span class=\"text-success\">Create Specification</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('add-specification')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2 pb-5\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <div style=\"width:70px;\">Name: </div>\r\n                            <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"name\" v-model=\"specification_form.name\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:56px;\"> Type: </span>\r\n                              <select class=\"ui-autocomplete-input w-100\" name=\"specification_for\" v-model=\"specification_form.specification_for\">\r\n                                <option v-for=\"specification_type in specification_types\" :value=\"specification_type\"> {{specification_type}}</option>\r\n                              </select>\r\n                            </div>\r\n                          <div class=\"o_thread_search_input mt-2\">\r\n                            <span style=\"width:100px;\">Have Values?: </span>\r\n                            <input type=\"checkbox\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"is_values\" v-model=\"specification_form.is_values\" value=\"1\">\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mt-2\" v-if=\"specification_form.is_values\">\r\n                            <span style=\"width:70px;\">Values: </span>\r\n                            <ul style=\"padding-left: 5px\">\r\n                              <li v-for=\"(value,key) in specification_form.values\" class=\"mt-1\">\r\n                                <input type=\"text\" :name=\"'value'+key\" v-model=\"value.value\" class=\"ui-autocomplete-input\" required>\r\n                                <button class=\"btn btn-danger btn-sm p-1\" @click=\"specification_form.values.splice(key, 1)\">Remove</button>\r\n                              </li>\r\n                              <li class=\"mt-1\">\r\n                                <button type=\"button\" class=\"btn btn-primary btn-sm p-1\" @click=\"specification_form.values.push('')\">Add Value</button>\r\n                                <input type=\"hidden\" name=\"keys\" :value=\"specification_form.values.length\">\r\n                              </li>\r\n                            </ul>\r\n                          </div>\r\n\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{name:'', specifications:[]},\r\n      action:'Manage',\r\n      id:null,\r\n      materials:[],\r\n      specifications:[],\r\n      specification_types:['Packing Material','Certificate'],\r\n      specification_form:{name:'', is_values:false, specification_type:'Packing Material',values:[{value:''}]},\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.getMaterials()\r\n    this.getSpecifications()\r\n  },\r\n  methods: {\r\n    getMaterials() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/materials/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.materials = jsonData\r\n      })\r\n    },\r\n    getSpecifications() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/specifications/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.specifications = jsonData\r\n      })\r\n    },\r\n    selectMaterial(material_id=null){\r\n      var self = this\r\n      this.form_data = {name:'', specifications:[]};\r\n      if(material_id){\r\n        this.materials.forEach(function(val,index){\r\n          if(val.id == material_id){\r\n            self.form_data = val\r\n          }\r\n        })\r\n      }\r\n      this.$bvModal.show('create-material')\r\n    },\r\n    dataMaterial() {\r\n      if(this.form_data.id){\r\n        this.updateMaterial()\r\n      }else{\r\n        this.createMaterial()\r\n      }\r\n    },\r\n    createMaterial() {\r\n      var formdata = new FormData(this.$refs['dataMaterial'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/materials/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.$bvModal.hide('create-material')\r\n          this.getMaterials()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateMaterial() {\r\n      var formdata = new FormData(this.$refs['dataMaterial'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/materials/'+this.form_data.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.form_data = jsonData\r\n          this.getMaterials()\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    selectSpecification(specification_id=null){\r\n      var self = this\r\n      this.specification_form = {name:'', is_values:false, specification_for:'Packing Material',values:[{value:''}]};\r\n      if(specification_id){\r\n        this.specifications.forEach(function(val,index){\r\n          if(val.id == specification_id){\r\n            if(!val.values){\r\n              val.values = []\r\n            }\r\n            self.specification_form = val\r\n          }\r\n        })\r\n      }\r\n      this.$bvModal.show('add-specification')\r\n    },\r\n    dataForm() {\r\n      if(this.specification_form.id){\r\n        this.updateFormdata()\r\n      }else{\r\n        this.createFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/specifications/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.$bvModal.hide('add-specification')\r\n          this.getSpecifications()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/specifications/'+this.specification_form.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.specification_form = jsonData\r\n          this.getSpecifications()\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n<style>\r\n   .table-3 th {\r\n    background-color: #f3f4f7;\r\n}\r\n   .table-3 td {\r\n    border: 1px solid #dcdfe6;\r\n}\r\n  .table-3 th {\r\n    padding:0.75em 1.5em 0.75em 0.75em;\r\n  }\r\n.table-3 .tr-bottom td:nth-child(2){\r\n  border: 2px solid #dcdfe6;\r\n  background-color: #f3f4f7;\r\n  \r\n}\r\n.table-3 .tr-bottom td:nth-child(1){\r\nborder: none;\r\n  \r\n}\r\n</style>\r\n"]}]}