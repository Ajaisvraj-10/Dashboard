t>\r\nimport {basicDoughnut} from \"@/data/echarts\";\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Dashboard\"\r\n  },\r\n  components:{\r\n  },\r\n  data() {\r\n    return {\r\n      basicDoughnut,\r\n      dashboard:{\r\n        farmers:[],\r\n        inspections:[],\r\n      },\r\n    };\r\n  },\r\n  mounted(){\r\n   this.$root.current_section = 'fpo'\r\n   this.getDashboard()\r\n  },\r\n  methods:{\r\n    getDashboard() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/dashboard/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.dashboard = jsonData\r\n        this.setBasicDoughnut()\r\n      })\r\n    },\r\n    setBasicDoughnut(){\r\n      var colors = []\r\n      this.dashboard.statuses.forEach(function(color){\r\n        colors.push(color.color)\r\n      })\r\n      this.basicDoughnut.color = colors\r\n      this.basicDoughnut.series[0].data = this.dashboard.statuses\r\n      this.basicDoughnut.series[0].name = 'Farmers Status'\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}