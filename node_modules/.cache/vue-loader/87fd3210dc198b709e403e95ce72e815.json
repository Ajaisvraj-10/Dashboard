      Locations\r\n                  </div>\r\n                  <div class=\"d-flex mt-2\" style=\"flex-wrap:wrap;\">\r\n                  \r\n                    <router-link to=\"/app/locations/state\">\r\n                      <h6 class=\"dashboard-tab-child\"><span class=\"dot-icon bg-success mr-1\"></span>State </h6>\r\n                    </router-link>\r\n                    <router-link to=\"/app/locations/district\">\r\n                      <h6 class=\"dashboard-tab-child\"><span class=\"dot-icon bg-success mr-1\"></span> District</h6>\r\n                    </router-link>\r\n                    <router-link to=\"/app/locations/panchayath\">\r\n                      <h6 class=\"dashboard-tab-child\"><span class=\"dot-icon bg-success mr-1\"></span> Panchayath</h6>\r\n                    </router-link>\r\n                    <router-link to=\"/app/locations/revenue-village\">\r\n                      <h6 class=\"dashboard-tab-child\"><span class=\"dot-icon bg-success mr-1\"></span> Revenue Village</h6>\r\n                    </router-link>\r\n                    <router-link to=\"/app/locations/local-village\">\r\n                      <h6 class=\"dashboard-tab-child\"><span class=\"dot-icon bg-success mr-1\"></span> Local Villages</h6>\r\n                    </router-link>\r\n                    <router-link to=\"/app/locations/post-office\">\r\n                      <h6 class=\"dashboard-tab-child\"><span class=\"dot-icon bg-success mr-1\"></span> Post Office</h6>\r\n                    </router-link>\r\n                  \r\n                   \r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\" col-auto px-1 pb-3\">\r\n              <div  class=\"card p-3\">\r\n                <div class=\"\">\r\n                  <div class=\"d-flex align-items-center justify-content-center\" style=\"gap:15px;\">\r\n                    <img src=\"/img/updation.jpg\">\r\n                     \r\n                     Aeo Updations\r\n                  </div>\r\n                  <div class=\"d-flex mt-2\" style=\"flex-wrap:wrap;\">\r\n                  \r\n                    <router-link to=\"/app/fpo/aeo-updations/farmers\">\r\n                      <h6 class=\"dashboard-tab-child\"><span class=\"dot-icon bg-success mr-1\"></span>Farmer</h6>\r\n                    </router-link>\r\n                    <router-link to=\"/app/fpo/aeo-updations/farmer-files\">\r\n                      <h6 class=\"dashboard-tab-child\"><span class=\"dot-icon bg-success mr-1\"></span> Farmer Files</h6>\r\n                    </router-link>\r\n                    \r\n                   \r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            \r\n\r\n          </div>\r\n        </div>\r\n        <div class=\"col-md-4\">\r\n          <div class=\"card p-3 m-0 text-left\">\r\n            <h4>Farmers Status</h4>\r\n            <div class=\"chart-wrapper\" style=\"height: 351px\">\r\n              <v-chart :options=\"basicDoughnut\" :autoresize=\"true\" v-if=\"dashboard.farmers.length>0\"></v-chart>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"row\">\r\n        <div class=\"col-md-6 px-2 mb-3\">\r\n          <b-card class=\"w-100\">\r\n            <div class=\"flex-align border-bottom pb-2\">\r\n              <h5 class=\"\">Inspections</h5>\r\n              <router-link to=\"/app/fpo/inspections\">\r\n                <button class=\"btn btn-info px-2 py-1 btn-sm\">Vies All</button>\r\n              </router-link>\r\n            </div>\r\n            <table class=\"dashboard-table mt-3\">\r\n              <tr>\r\n                <th>Name</th>\r\n                <th>Farmer Code</th>\r\n                <th>AEO</th>\r\n                <th>Year</th>\r\n                <th>Date</th>\r\n              </tr>\r\n              <tr v-for=\"inspection in dashboard.inspections\" @click