der\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">{{slugToWord('printing')}}</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                <router-link to=\"/app/production/printing/\">\r\n                  <b-button variant=\"danger\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n         <b-col lg=\"6\" xl=\"6\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <h5>Print Request</h5>\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div v-if=\"form_data.id\">\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:120px;\">Indent Number : </span>\r\n                    <input placeholder=\"Indent Number\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" name=\"indent_number\" v-model=\"form_data.indent_number\" readonly>\r\n                  </div>\r\n                  <ProductFields :product_id=\"product_id\" :readonly=\"true\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:120px;\">Parameter : </span>\r\n                    <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"parameter\" v-model=\"form_data.parameter\" disabled>\r\n                        <option v-for=\"parameter in parameters\" :value=\"parameter.id\">{{parameter.name}}</option>\r\n                    </select>\r\n                  </div>\r\n\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:120px;\">Quantity (Kg) : </span>\r\n                    <input placeholder=\"Quantity\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.quantity\" name=\"quantity\" readonly>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <hr class=\"my-3\"/>\r\n\r\n          </b-card>\r\n        </b-col>\r\n         <b-col lg=\"6\" xl=\"6\" class=\"my-3\">\r\n          <div class=\"card p-0 pb-2 my-1\">\r\n            <div class=\"card p-2 m-0 flex-row border\">\r\n              <div class=\"card col-md-1 align-self-center p-1\">\r\n                <img src=\"/img/website.png\" style=\"vertical-align:top;height:40px;width:40px\">\r\n              </div>\r\n              <div class=\"col-md-2 align-self-center\">\r\n                <b>Printing  </b>\r\n              </div>\r\n              <div class=\"col-md-4 align-self-center\">\r\n                <small>Status</small> \r\n                <span :class=\"form_data.status=='Not Start'?'text-danger':'text-success'\"> {{form_data.status}}</span>\r\n              </div>\r\n              <div class=\"col-md-5 align-self-center text-right\">\r\n                <span class=\"badge badge-danger p-1 cursor-pointer\" v-if=\"form_data.total_prints>=form_data.indent_packing_detail\" @click=\"startProcess('stop')\" >Stop Process</span>\r\n                <span class=\"badge badge-danger p-1 cursor-pointer\" v-else-if=\"printing_processes.length>0\" @click=\"startProcess('stop')\" >Stop Process</span>\r\n                <span class=\"badge badge-success p-1 cursor-pointer\" v-else @click=\"startProcess('shift')\" >Start Process</span>\r\n              </div>\r\n            </div>\r\n            <div class=\"process_kanban\">\r\n              <div class=\"m-1\">\r\n                <div class=\"px-2 card py-2\">\r\n                  <div class=\"mx-1 mb-2\">\r\n                    <table class=\"w-100\">\r\n                      <tr class=\"\">\r\n                        <th class=\"text-primary\">#{{form_data.id}}/Indent</th>\r\n                        <th class=\"text-right\">\r\n                          LOT\r\n                          <span v-for=\"(process_product,key) in form_data.workers\">\r\n                            {{key==0?'':', '}}{{process_product.lot}}\r\n                          </span>\r\n                        </th>\r\n                      </tr>\r\n                      <tr v-for=\"(process_product,key) in form_data.workers\">\r\n                        <th>{{key==0?'MATERIAL'+slugToWord(checkMult(start_process.process_products.length)):''}}</th>\r\n                        <th class=\"text-right\">\r\n                          {{key==0?'':', '}}{{process_product.parameter_detail.name}}({{process_product.quantity}}KG)\r\n                        </th>\r\n                      </tr>\r\n                    </table>\r\n                  </div>\r\n                  <div class=\"row\">\r\n                    <div class=\"col-md-6\">\r\n                      <b class=\"mx-1\">Shifts</b>\r\n                    </div>\r\n                    <div class=\"col-md-6 text-right\">\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"card px-2 py-2 mt-1 \" v-for=\"process in printing_processes\">\r\n                    <table class=\"\">\r\n                      <tr class=\"border border-rounded cursor-pointer\" @click=\"startProcess('shift',process_detail,start_process,process.id,true)\">\r\n                        <th class=\"text-info\">\r\n                          #{{process.supervisor_detail.name}}\r\n                        </th>\r\n                        <th>\r\n                          {{process.shift}}\r\n                        </th>\r\n                        <th>{{dateFormate(process.created_on)}}</th>\r\n                        <th class=\"text-right\">\r\n                          <span class=\"badge\" :class=\"process.status=='Completed'?'badge-success':process.status=='Draft'?'badge-secondary':process.status=='Starting'?'badge-success':process.status=='Stop'?'badge-danger':'badge-warning'\">{{process.status}}</span>\r\n                        </th>\r\n                      </tr>\r\n                      <tr>\r\n                        <th colspan=\"2\">\r\n                          Material\r\n                        </th>\r\n                        <th>\r\n                          {{process.printed_materials.length>0?process.printed_materials.length+' Material'+checkMult(process.printed_materials.length):'No materials'}}\r\n                        </th>\r\n                        <th class=\"text-right\">\r\n                          <span class=\"badge badge-warning cursor-pointer\" @click=\"startProcess('material',process.id,true)\" v-if=\"process.printed_materials.length\">View</span>\r\n                          <span class=\"badge badge-primary cursor-pointer\" @click=\"startProcess('material',process.id)\" v-else>Add</span>\r\n                        </th>\r\n                      </tr>\r\n                      <tr>\r\n                        <th colspan=\"2\">\r\n                          Workers\r\n                        </th>\r\n                        <th>\r\n                          {{process.workers.length>0?process.workers.length+' worker'+checkMult(process.workers.length):'No workers'}}\r\n                        </th>\r\n                        <th class=\"text-right\">\r\n                          <span class=\"badge badge-warning cursor-pointer\" @click=\"startProcess('workers',process.id,true)\" v-if=\"process.workers.length\">View</span>\r\n                          <span class=\"badge badge-primary cursor-pointer\" @click=\"startProcess('workers',process.id)\" v-else>Add</span>\r\n                        </th>\r\n                      </tr>\r\n                      <tr>\r\n                        <th colspan=\"2\">\r\n                          Consumable\r\n                        </th>\r\n                        <th>\r\n                          {{process.consumables_count>0?process.consumables_count+' consumable'+checkMult(process.consumables_count):'No consumables'}}\r\n                        </th>\r\n                        <th class=\"text-right\">\r\n                          <span class=\"badge badge-warning cursor-pointer\" @click=\"startProcess('consumables',process.id,true)\" v-if=\"process.consumables_count\">View</span>\r\n                          <span class=\"badge badge-primary cursor-pointer\" @click=\"startProcess('consumables',process.id)\" v-else>Add</span>\r\n                        </th>\r\n                      </tr>\r\n                    </table>\r\n                  </div>\r\n                </div>\r\n                <hr class=\"my-2\">\r\n              </div>\r\n              <div class=\"text-center\"> </div>\r\n            </div>\r\n          </div>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n    <div>\r\n      <b-modal id=\"start-process\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n       <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n          <slot name=\"header\">\r\n             <h5 class=\"mb-0\"><span class=\"text-success\">Printing</span> /{{action}}</h5>\r\n          </slot>\r\n          <div class=\"d-flex\">\r\n            <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"dataForm()\">Save</b-button>\r\n            <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('start-process')\">Close</b-button>\r\n          </div>\r\n        </div>\r\n        <div class=\" my-3 border-top\"></div>\r\n        <form ref=\"dataForm\" @submit.prevent=\"dataForm()\" >\r\n          <fieldset :disabled=\"is_view\">\r\n            <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\" v-show=\"action=='shift'\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-12\">\r\n                  <div class=\"user-info pt-2 mb-5\">\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:90px;\"> Shift: </span>\r\n                      <select class=\"ui-autocomplete-input w-100 ml-1\" name=\"shift\" v-model=\"process_data.shift\">\r\n                        <option v-for=\"shift in shifts\" :value=\"shift\"> {{shift}}</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <span style=\"width:90px;\"> Date : </span>\r\n                      <input placeholder=\"Date\" type=\"date\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"process_data.date\" name=\"date\" required>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input\">\r\n                      <input type=\"hidden\" name=\"indent\" :value=\"form_data.id\">\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n            <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\" v-show=\"action=='material'\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-12\">\r\n                  <h5>Input Materials</h5>\r\n                  <table class=\"table\">\r\n                    <tr>\r\n                      <th>Material</th>\r\n                      <th>Specs</th>\r\n                      <th>Lot</th>\r\n                      <th>Quantity</th>\r\n                      <th>Action</th>\r\n                    </tr>\r\n                    <tr v-for=\"packing_material in form_data.print_requested_materials\" :for=\"'packing_material'+form_data.id\" v-if=\"packing_material.quantity>0\">\r\n                      <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">{{packing_material.material_detail.name}}</td>\r\n                      <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">\r\n                        <small v-for=\"specification in packing_material.specifications\" v-if=\"specification.material_ids.includes(packing_material.material)\">\r\n                          <b class=\"text-primary\"> {{specification.name}}</b>:{{specification.value}}, \r\n                        </small>\r\n                      </td>\r\n                      <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">#Lot{{packing_material.packing_material_purchase}}</td>\r\n                      <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">\r\n                        <input type=\"number\" class=\"ui-autocomplete-input w-100\" :max=\"packing_material.quantity\" v-model=\"packing_material.packing_quantity\" :disabled=\"!packing_material.is_added\" @change=\"changePackingMaterial(packing_material)\">\r\n                        <input type=\"hidden\" :value=\"packing_material.is_added?'yes':'no'\">\r\n                        <small v-if=\"packing_material.packing_quantity>0\">Number of bags: <b>{{packing_material.number_of_bags}}</b></small>\r\n                        <input type=\"hidden\" :value=\"packing_material.number_of_bags\">\r\n                      </td>\r\n                      <td>\r\n                        <button type=\"button\" class=\"btn btn-primary btn-sm px-3 py-1 ml-4\" @click=\"addMaterial(packing_material)\" v-if=\"!packing_material.is_added\">Add</button>\r\n                        <button type=\"button\" class=\"btn btn-danger btn-sm px-3 py-1 ml-4\" @click=\"removeMaterial(packing_material)\" v-else>Remove</button>\r\n                      </td>\r\n                    </tr>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n            <b-card  header-bg-variant=\"transparent\" class=\"card-scroll my-3\" v-show=\"action=='workers'\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-12\">\r\n                  <h5>Workers</h5>\r\n                  <div>\r\n                    <b-form-group class=\"work_type_option\">\r\n                      <b-form-radio-group\r\n                        id=\"work_type\"\r\n                        v-model=\"process_data.work_type\"\r\n                        :options=\"['wages','contract']\"\r\n                        button-variant=\"outline-primary\"\r\n                        name=\"work_type\"\r\n                        buttons\r\n                        style=\"font-size:20px\"\r\n                      ></b-form-radio-group>\r\n                    </b-form-group>\r\n                    <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                      <div class=\"row\">\r\n                        <div class=\"col-md-12\">\r\n                          <div class=\"o_thread_search_input my-0 py-0\">\r\n                            <b style=\"width:96px\"> Workers : </b>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mt-3\">\r\n                            <span style=\"width:130px\"> Select Workers: </span>\r\n                            <div class=\"mx-2 workers_box\">\r\n                              <label class=\"workers_label\" v-for=\"worker in workers\" :for=\"'worker'+worker.id\" v-if=\"(process_data.work_type=='wages' && worker.is_wage) || (process_data.work_type=='contract' && process_data.is_contract) \">\r\n                                <b-form-checkbox name=\"workers\" v-model=\"process_data.workers\" :id=\"'worker'+worker.id\" :value=\"worker.id\" @input=\"process_data.total_workers=process_data.workers.length\">\r\n                                  <b>#{{worker.id}}/{{worker.name}}</b>\r\n                                </b-form-checkbox>\r\n                              </label>\r\n                            </div>\r\n                          </div>\r\n                          <div v-if=\"process_data.work_type=='wages'\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:130px;\"> Number of Labours : </span>\r\n                              <input placeholder=\"Labours\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"process_data.total_workers\" name=\"total_workers\" min=\"0\">\r\n                              <span style=\"width:5px;\">  </span>\r\n                              <input placeholder=\"Hourse\" type=\"number\" class=\"ui-autocomplete-input append_btn_input_right\" autocomplete=\"off\" v-model=\"process_data.working_hourse\" name=\"working_hourse\" min=\"0\">\r\n                              <button class=\"btn btn-white append_btn_right\" type=\"button\">Hours</button>\r\n                            </div>\r\n                            <div class=\"mx-2 px-1 text-right text-danger\" style=\"font-size:12px\" v-if=\"process_data.working_hourse>0 && process_data.total_workers\">\r\n                              Salary : ({{process_data.total_workers}}Labours x {{process_data.working_hourse}}Hr x Rs440)/8 = <b>Rs{{(process_data.total_workers*process_data.working_hourse*440)/8}}/-</b>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input mt-2\">\r\n                              <b style=\"width:96px\"> Over Time : </b>\r\n                            </div>\r\n                            <div class=\"mx-2 px-1 mt-2\">\r\n                              <small style=\"font-weight:700\">Stage 1</small>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input pt-0\">\r\n                              <span style=\"width:130px;\"> Number of Labours : </span>\r\n                              <input placeholder=\"Total Workers\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"process_data.overtime_labours_st1\" name=\"overtime_labours_st1\" min=\"0\">\r\n                              <span style=\"width:5px;\">  </span>\r\n                              <input placeholder=\"Working Hourse\" type=\"number\" class=\"ui-autocomplete-input append_btn_input_right\" autocomplete=\"off\" v-model=\"process_data.overtime_hourse_st1\" name=\"overtime_hourse_st1\" min=\"0\">\r\n                              <button class=\"btn btn-white append_btn_right\" type=\"button\">Hours</button>\r\n                            </div>\r\n                            <div class=\"mx-2 px-1 text-right text-danger\" style=\"font-size:12px\" v-if=\"process_data.overtime_labours_st1>0 && process_data.overtime_hourse_st1\">\r\n                              Salary : (Rs440/8Hr) x 2 x {{process_data.overtime_labours_st1}}Labours x {{process_data.overtime_hourse_st1}}Hr = <b>Rs{{(440/8)*2*process_data.overtime_labours_st1*process_data.overtime_hourse_st1}}/-</b>\r\n                            </div>\r\n                            <div class=\"mx-2 px-1 mt-2\">\r\n                              <small style=\"font-weight:700\">Stage 2</small>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input pt-0\">\r\n                              <span style=\"width:130px;\"> Number of Labours : </span>\r\n                              <input placeholder=\"Labours\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"process_data.overtime_labours_st2\" name=\"overtime_labours_st2\" min=\"0\">\r\n                              <span style=\"width:5px;\">  </span>\r\n                              <input placeholder=\"Working Hourse\" type=\"number\" class=\"ui-autocomplete-input append_btn_input_right\" autocomplete=\"off\" v-model=\"process_data.overtime_hourse_st2\" name=\"overtime_hourse_st2\" min=\"0\">\r\n                              <button class=\"btn btn-white append_btn_right\" type=\"button\">Hours</button>\r\n                            </div>\r\n                            <div class=\"mx-2 px-1 text-right text-danger\" style=\"font-size:12px\" v-if=\"process_data.overtime_labours_st2>0 && process_data.overtime_hourse_st2\">\r\n                              Salary : (Rs440/8Hr) x {{process_data.overtime_labours_st2}}Labours x {{process_data.overtime_hourse_st2}}Hr = <b>Rs{{(440/8)*process_data.overtime_labours_st2*process_data.overtime_hourse_st2}}/-</b>\r\n                            </div>\r\n                          </div>\r\n\r\n                          <div v-else>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:130px;\"> Total Workers: </span>\r\n                              <input placeholder=\"Labours\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"process_data.total_workers\" name=\"total_workers\" @input=\"process_data.labour_charge = (process_data.total_workers*400)/8\" min=\"0\">\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\" v-if=\"process_data.work_type=='contract'\">\r\n                              <span style=\"width:130px;\"> Labour Charge : </span>\r\n                              <input placeholder=\"Labour Charge \" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"process_data.contract_labour_charge\" name=\"contract_labour_charge\" min=\"0\">\r\n                            </div>\r\n                            <div class=\"mx-2 px-1 text-right text-danger\" style=\"font-size:12px\" v-if=\"process_data.total_workers>0 && process_data.contract_labour_charge\">\r\n                              Salary : {{process_data.total_workers}}Labours x {{process_data.contract_labour_charge}}Rs = <b>Rs{{process_data.total_workers*process_data.contract_labour_charge}}/-</b>\r\n                            </div>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mt-2\">\r\n\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    </b-card>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n          </fieldset>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  components:{\r\n    ProductFields,\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{id:null, parameter:null, expected_date:'2022-03', total:0, indent_packing_detail:{}, total_quantity:0, indent_number:'', is_parameters:false, status:''\r\n      },\r\n      process_data:{\r\n        indent:null, \r\n        id:null,\r\n        date:this.currentDate(),\r\n        product:null,\r\n        status:'Draft',\r\n        shift:'DAY',\r\n        workers:[],\r\n        work_type:'wages',\r\n        overtime_hourse_st1:2,\r\n        consumable_items_list:[],\r\n      },\r\n      printing_processes:[],\r\n      id:null,\r\n      action:'',\r\n      product_id:null,\r\n      load_products:false,\r\n      parameters:[],\r\n      user_id:6,\r\n      workers:[],\r\n      is_view:false,\r\n      packing_materials:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.id = this.$route.params.id\r\n    this.getForm()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/indent/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.product_id = jsonData.product\r\n        this.load_products = true\r\n        this.getParameters()\r\n        this.getPrintingProcess()\r\n      })\r\n    },\r\n    getPrintingProcess() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/printing_process/?indent='+this.form_data.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.printing_processes = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action=='material'){\r\n        this.createNextProcess()\r\n      }\r\n      if(this.action == 'shift'){\r\n        this.process_data.status = 'Starting'\r\n        this.printProcessForm()\r\n      }else if(this.action == 'workers'){\r\n        this.process_data.status = 'Completed'\r\n        this.printProcessForm()\r\n      }else if(this.action == 'consumables'){\r\n        this.consumableStockCreate()\r\n      }else if(this.action == 'stop'){\r\n        this.completeProcess()\r\n      }\r\n    },\r\n    printProcessForm(method='POST') {\r\n      var param = ''\r\n      if(this.process_data.id){\r\n        method = 'PUT'\r\n        param = this.process_data.id+'/'\r\n      }\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('supervisor',this.user_id)\r\n      formdata.append('status',this.process_data.status)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/printing_process/'+param, {\r\n          method : method,\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.process_data.id = jsonData.id\r\n          this.$bvModal.hide('start-process')\r\n          this.getPrintingProcess()\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    startProcess(action,processing_id=null,is_view=false){\r\n      this.action = action\r\n      var false_message = ''\r\n      if(!processing_id && (action=='shift' || action=='stop')){\r\n        this.printing_processes.forEach(function(process_){\r\n          if(process_.workers.length<=0){\r\n            false_message = 'Please complete this shift!'\r\n          }\r\n        })\r\n      }\r\n      if(action=='shift' && this.printing_processes.length>0 && !is_view){\r\n        false_message = 'The input product is over!'\r\n      }\r\n      if(false_message){\r\n        this.showAlert(false_message)\r\n        return false\r\n      }\r\n      if(action=='shift' && !is_view){\r\n        processing_id = null\r\n      }\r\n      this.is_view = is_view\r\n      this.$bvModal.show('start-process')\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameters/?product='+this.product_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.parameters = jsonData\r\n        this.getWorkers()\r\n      })\r\n    },\r\n    getWorkers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.workers = jsonData\r\n      })\r\n    },\r\n    addMaterial(packing_material){\r\n      packing_material.is_added = true\r\n    },\r\n    removeMaterial(packing_material){\r\n      packing_material.is_added = false\r\n    },\r\n    getPackingMaterials(){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase_materials/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        jsonData.forEach(function(val,key){\r\n          jsonData[key].is_added = false\r\n          jsonData[key].packing_quantity = 0\r\n          jsonData[key].number_of_bags = 0\r\n        })\r\n        this.packing_materials = jsonData\r\n      })\r\n    },\r\n    changePackingMaterial(packing_material){\r\n      packing_material.number_of_bags = parseInt(packing_material.packing_quantity/packing_material.max_quantity)\r\n      if(packing_material.number_of_bags == (packing_material.packing_quantity/packing_material.max_quantity)){\r\n        packing_material.number_of_bags = packing_material.number_of_bags\r\n      }else if(packing_material.number_of_bags < (packing_material.packing_quantity/packing_material.max_quantity)){\r\n        packing_material.number_of_bags = packing_material.number_of_bags+1\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n</style>\r\n"]}]}