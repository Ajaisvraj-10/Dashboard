.png\" class=\"mb-3\" style=\"width:200px;\" />\r\n              <div class=\" prepend-1\" v-if=\"login_step=='login'\">\r\n                <h3 class=\"font-weight-600\">Please login to continue</h3>\r\n                <b-input-group class=\"mb-3 mt-5 pt-4\">\r\n                  <template #prepend>\r\n                   <b-dropdown :text=\"current_code\" variant=\"white\">\r\n                      <b-dropdown-item v-for=\"country_code in country_codes\" :active=\"current_code==country_code.code\" @click=\"current_code=country_code.code\">{{country_code.code}}</b-dropdown-item>\r\n                    </b-dropdown>\r\n                  </template>\r\n                  <input type=\"hidden\" name=\"dial\" :value=\"current_code.replace('+','')\">\r\n                  <b-form-input type=\"number\" v-model=\"phone\" placeholder=\"Mobile number\" name=\"username\" style=\"height:50px\"></b-form-input>\r\n\r\n                  \r\n                </b-input-group>\r\n\r\n              </div>\r\n\r\n              <div v-else>\r\n                <h3 class=\"font-weight-600 mt-4'o\" >Verify mobile number</h3>\r\n                <p class=\"text-11\">We have sent OTP To <span class=\"text-dark px-1\">{{current_code}} {{phone}}</span> <span class=\"rose\" @click=\"login_step='login'\">CHANGE</span></p>\r\n\r\n                <div class=\"mb-3 mt-5 otp\">\r\n                 \r\n                  <div id=\"otp\" class=\"inputs d-flex flex-row justify-content-center\">\r\n                    <input class=\"m-2 text-center form-control rounded\" type=\"text\" id=\"input1\" @focus=\"inputenter(1)\" @keyup=\"onKeyUpEvent(1,event)\" maxlength=\"1\" />\r\n                    <input class=\"m-2 text-center form-control rounded\" @focus=\"inputenter(2)\" @keyup=\"onKeyUpEvent(2,event)\" type=\"text\" id=\"input2\" maxlength=\"1\" />\r\n                    <input class=\"m-2 text-center form-control rounded\" @focus=\"inputenter(3)\" @keyup=\"onKeyUpEvent(3,event)\" type=\"text\" id=\"input3\" maxlength=\"1\" /> \r\n                    <input class=\"m-2 text-center form-control rounded\" @focus=\"inputenter(4)\" @keyup=\"onKeyUpEvent(4,event)\" type=\"text\" id=\"input4\" maxlength=\"1\" /> \r\n                    <input class=\"m-2 text-center form-control rounded\" @focus=\"inputenter(5)\" @keyup=\"onKeyUpEvent(5,event)\" type=\"text\" id=\"input5\" maxlength=\"1\" /> \r\n                  </div>\r\n                \r\n\r\n                </div>\r\n                \r\n                <p class=\"text-center text-11\">Resend OTP <span class=\"rose\"> </span></p>\r\n              </div>\r\n            </div>\r\n            \r\n        \r\n    </div>\r\n\r\n\r\n   \r\n    <footer class=\"footer-login mb-2\">\r\n      <button class=\"btn btn-secondary btn-lg btn-block font-weight-700\" v-if=\"login_step=='login'\" @click=\"login_step='otp'\" style=\"height:50px\">CONTINUE</button>\r\n      <button class=\"btn btn-secondary btn-lg  btn-block font-weight-700\" style=\"height:50px\" v-else>VERIFY</button>\r\n       \r\n    </footer>\r\n\r\n\r\n  </main>\r\n\r\n\r\n\r\n\r\n\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Home\"\r\n  },\r\n  data() {\r\n    return {\r\n      current_code:'+91',\r\n      eye_show:false,\r\n      otp:'',\r\n      phone:'',\r\n      login_step:'login',\r\n    };\r\n  },\r\n  mounted(){\r\n\r\n  },\r\n  methods : {\r\n    goBack(){\r\n      if(this.login_step=='login'){\r\n        this.$router.push('/login')\r\n      }else{\r\n        this.login_step = 'login'\r\n      }\r\n    },\r\n    inputenter(index) {\r\n      for (var item = 1; item < index; item++) {\r\n        const currentElement = this.getCodeBoxElement(item);\r\n        if (!currentElement.value) {\r\n            currentElement.focus();\r\n            break;\r\n        }\r\n      }\r\n    },\r\n\r\n    onKeyUpEvent(index, event) {\r\n      if (this.getCodeBoxElement(index).value.length === 1) {\r\n          if (index !== 5) {\r\n            this.getCodeBoxElement(index+ 1).focus();\r\n          } else {\r\n            this.getCodeBoxElement(index).blur();\r\n          }\r\n        }\r\n        this.otp = ''\r\n        for (var i = 1; i <= 5; i++) \r\n        {\r\n          this.otp = this.otp + document.getElementById('input'+i).value;\r\n        }\r\n    },\r\n    getCodeBoxElement(index) {\r\n        return document.getElementById('input' + index);\r\n    },\r\n    loginUser() {\r\n      var formdata = new FormData(this.$refs['login_form'])\r\n      fetch(this.api_url+'/driver/login/', {\r\n          method : 'post',\r\n          body : formdata\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.status){\r\n            localStorage.setItem(\"userInfo\", JSON.stringify(jsonData));\r\n            this.checkLogin()\r\n            if(this.$root.token==''){\r\n                localStorage.removeItem(\"userInfo\");\r\n            }\r\n            else{\r\n                var self = this\r\n                setTimeout(function(){\r\n                    window.location.href = '/';\r\n                },500);\r\n            }\r\n        }else{\r\n            localStorage.removeItem(\"userInfo\");\r\n        }\r\n      })\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n  \r\n</style>\r\n"]}]}