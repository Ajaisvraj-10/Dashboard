op\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n   \r\n    <div class=\"main-scroll\">\r\n      <div class=\"row mx-0\"  v-for=\"department in departments\">\r\n        <div class=\"col-md-12 mt-3\">\r\n          <h5>{{department.name}} Department</h5>\r\n        </div>\r\n        <div class=\"col-md-4 col-sm-5 col-6\" v-for=\"proces in department.production_processes\" v-if=\"proces.qc_test\">\r\n          <b-card class=\"mt-2\">\r\n            <h6 class=\"font-weight-700 mb-0 \" style=\"min-height:20px;\">{{proces.name}}</h6>\r\n            <div class=\"row\" v-for=\"production_process in proces.production_processes\" v-if=\"production_process.qc_test\">\r\n              <div class=\"col-2 align-self-center\">\r\n                <img src=\"/img/blank-img.png\" >\r\n              </div>\r\n              <div class=\"col-7 align-self-center\">\r\n                <b>{{production_process.name}} </b>\r\n              </div>\r\n              <div class=\"col-3 align-self-center\">\r\n                <div>\r\n                  <router-link :to=\"'/app/qc/inprocess-test/process/'+production_process.id\">\r\n                    <button class=\"btn btn-success py-0 btn-sm\">\r\n                      View\r\n                    </button>\r\n                  </router-link>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{},\r\n      departments:[],\r\n      department_id:null,\r\n      current_process:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'qc'\r\n    if(this.$route.query.department){\r\n      this.department_id = this.$route.query.department\r\n    }\r\n    this.getProcesses()\r\n  },\r\n  methods: {\r\n    getProcesses() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/departments/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.departments = jsonData\r\n        if(jsonData.length>0 && !this.$route.query.department){\r\n          this.department_id = jsonData[0].id\r\n        }\r\n      })\r\n    },\r\n    getProcess(proces){\r\n      this.current_process = proces\r\n      this.$bvModal.show('list-main-process')\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n</style>\r\n"]}]}