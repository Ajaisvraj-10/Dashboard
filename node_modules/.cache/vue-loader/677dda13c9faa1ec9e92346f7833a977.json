n            <router-link :to=\"'/app/qc/machine-cleaning-test'\">\r\n              <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Discard\r\n              </b-button>\r\n            </router-link>\r\n          </div>\r\n           \r\n      </div>\r\n      <div class=\"my-3 border-top\"></div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll my-2\">\r\n            <h5>Machine List</h5>\r\n            <table class=\"table\">\r\n              <tr>\r\n                <th>No</th>\r\n                <th>Machine</th>\r\n                <th>Process</th>\r\n                <th>Status</th>\r\n                <th>Action</th>\r\n              </tr>\r\n              <tr v-for=\"(machine,key) in form_data.machines_list\">\r\n                <td>\r\n                  {{key+1}}\r\n                </td>\r\n                <td>\r\n                  {{machine.name}}\r\n                </td>\r\n                <td>\r\n                  <span class=\"badge badge-danger mr-1\" v-for=\"pro_pros in machine.process_list\" v-if=\"pro_pros[0]==production_process.id\">\r\n                    {{pro_pros[1]}}\r\n                  </span>\r\n                </td>\r\n                <td>\r\n                  <span class=\"py-1\" :class=\"machine.status=='Not Cleaned'?'text-danger':'text-success'\">\r\n                    {{machine.status=='Waiting'?'Cleaned':machine.status}}\r\n                    <input type=\"checkbox\" name=\"machines\" :value=\"machine.machine\" v-model=\"form_data.machines\"  :id=\"'machine'+machine.machine\" v-show=\"false\" />\r\n                  </span>\r\n                </td>\r\n                <td>\r\n                  <button class=\"btn py-1\" :class=\"current_machine_id==machine.id?'btn-success':'btn-primary'\" type=\"button\" @click=\"current_machine_id=machine.id\">Check</button>\r\n                </td>\r\n              </tr>\r\n            </table>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll my-2\">\r\n            <h5>QC Test </h5>\r\n            <div>\r\n              <div class=\"o_thread_search_input my-2\">\r\n                <span style=\"width:120px\"> Cleaning Type: </span>\r\n                <div>{{form_data.clean_type}}</div>\r\n              </div>\r\n              <div class=\"row mx-0\" v-for=\"(machine,key) in form_data.machines_list\" v-show=\"current_machine_id==machine.id\">\r\n                <div class=\"col-md-12\"><b>{{machine.name}}</b></div>\r\n                <div class=\"col-md-6\" v-for=\"data in machine.physical_parameter.datas\">\r\n                  <div class=\"user-info pt-2\" v-for=\"param in data\">\r\n                    <div class=\"d-flex mt-2\">\r\n                      <span style=\"width:80px;\" :for=\"param.name\"> {{param.name}} : </span>\r\n                      <input type=\"checkbox\" class=\"ui-autocomplete-input mx-2\" autocomplete=\"off\" value=\"1\" v-model=\"param.checked\" :name=\"param.name+machine.machine\" :id=\"param.name+machine.machine\">\r\n                      <span>\r\n                        <input :placeholder=\"param.limit_type\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" v-model=\"param.limit\" :name=\"param.name+'limit'+machine.machine\" v-if=\"param.checked && (param.limit_type=='%' || param.limit_type=='<' || param.limit_type=='>')\" @input=\"checkParam(param)\" required>\r\n                        <input :placeholder=\"'Type here..'\" type=\"text\" step=\"0.1\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" :name=\"param.name+'limit'+machine.machine\" v-else-if=\"param.checked && param.limit_type=='Typing'\" v-model=\"param.limit\" required>\r\n                        <select class=\"ui-autocomplete-input w-100\" :name=\"param.name+'limit'+machine.machine\" v-else-if=\"param.checked && param.limit_type=='YES/NO'\" v-model=\"param.limit\">\r\n                          <option value=\"YES\">YES</option>\r\n                          <option value=\"NO\">NO</option>\r\n                        </select>\r\n                        <select class=\"ui-autocomplete-input w-100\" :name=\"param.name+'limit'+machine.machine\" v-else-if=\"param.checked && param.limit_type=='Options'\" v-model=\"param.limit\">\r\n                          <option v-for=\"option in param.options\" :value=\"option.id\">{{option.name}}</option>\r\n                        </select>\r\n                      </span>\r\n                      <div class=\"px-2\" v-if=\"param.limit\">\r\n                        <span v-if=\"param.limit_type=='%' || param.limit_type=='<' || param.limit_type=='>'\">\r\n                          {{param.limit}}{{param.limit_type}}\r\n                        </span>\r\n                        <span v-else-if=\"param.limit_type=='Typing'\">\r\n                          {{param.limit}}\r\n                        </span>\r\n                        <span v-else-if=\"param.limit_type=='YES/NO'\" class=\"badge\" :class=\"param.limit=='YES'?'badge-success':'badge-danger'\">\r\n                          {{param.limit}}\r\n                        </span>\r\n                        <span v-else-if=\"param.limit_type=='Options'\" class=\"badge badge-light p-1\" :style=\"'background-color:'+param.color+'!important;color:'+bgToTextColor(param.color)\">\r\n                          {{param.limit}}\r\n                        </span>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-md-12\">\r\n                  <div class=\"o_thread_search_input mt-4\">\r\n                    <span style=\"width:120px;\"> Qc Status: </span>\r\n                    <div>\r\n                      <input type=\"hidden\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"machine.status\" :name=\"'qc_status'+machine.machine\" readonly>\r\n                      <button type=\"button\" class=\"btn ml-2\" :class=\"machine.status=='Cleaned'?'btn-success':'btn-outline-success'\" @click=\"machine.status='Cleaned'\">Cleaned</button>\r\n                      <button type=\"button\" class=\"btn ml-2\" :class=\"machine.status=='Not Cleaned'?'btn-danger':'btn-outline-danger'\" @click=\"machine.status='Not Cleaned'\">Not Cleaned</button>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div> \r\n              <hr>\r\n              <div class=\"o_thread_search_input my-2\">\r\n                <span style=\"width:120px\"> Remark: </span>\r\n                <textarea class=\"ui-autocomplete-input w-100\" rows=\"5\" placeholder=\"Write here..\" v-model=\"form_data.remark\" name=\"remark\"></textarea>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport WorkersForm from '/src/views/app/oodo/components/workers-form'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Machine\"\r\n  },\r\n  components:{\r\n    WorkersForm\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{\r\n        machines:[],\r\n        workers:[],\r\n        wage_workers:[],\r\n        work_type:'wages',\r\n        overtime_hourse_st1:2,\r\n      },\r\n      production_process:{id:null},\r\n      current_machine_id:null,\r\n      physical_parameters: [],\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'qc'\r\n    this.form_data.id = this.$route.params.id\r\n    this.getForm()\r\n    this.getWarehouses()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/machine_cleaning/'+this.form_data.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n        return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var self = this\r\n        jsonData.machines_list.forEach(function(ma){\r\n          if(ma.physical_parameter.length>0){\r\n            var length = ma.physical_parameter.length/2\r\n            ma.physical_parameter = self.pagination(ma.physical_parameter,Math.round(length))\r\n          }else{\r\n            ma.physical_parameter = {datas:[]}\r\n          }\r\n        })\r\n        this.form_data = jsonData\r\n        this.getProductionProcess()\r\n        this.getPhysicalParameters()\r\n      })\r\n    },\r\n    getProductionProcess() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/production_process/'+this.form_data.production_process+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n        return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.production_process = jsonData\r\n      })\r\n    },\r\n    getPhysicalParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/physical_parameters/?parameter_for=machine_cleaning&category='+this.form_data.category, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.physical_parameters = jsonData\r\n        var self = this\r\n        var machines_list = this.form_data.machines_list\r\n        this.form_data.machines_list = []\r\n        machines_list.forEach(function(ma){\r\n          var pp = JSON.stringify(jsonData)\r\n          jsonData = JSON.parse(pp)\r\n          if(ma.physical_parameter.datas.length<=0){\r\n            var length = jsonData.length/2\r\n            ma.physical_parameter = self.pagination(jsonData,Math.round(length))\r\n          }\r\n        })\r\n        this.form_data.machines_list = machines_list\r\n      })\r\n    },\r\n    dataForm() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('qc', this.employeeId)\r\n      formdata.append('production_process', this.production_process.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/machine_cleaning/'+this.form_data.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}