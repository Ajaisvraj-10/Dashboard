ACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA","file":"App.vue","sourceRoot":"src","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"common_alert\" id=\"common_alerti\">\r\n      <transition name=\"fade\">\r\n        <div class=\"Message\" :class=\"$root.alert_message.type\" v-if=\"$root.alert_message.show\">\r\n          <!-- <div class=\"Message-icon\">\r\n            <i class=\"fa fa-check\" v-if=\"$root.alert_message.type=='su'\"></i>\r\n            <i class=\"fa fa-times\" v-if=\"$root.alert_message.type=='red'\"></i>\r\n            <i class=\"fa fa-exclamation\" v-if=\"$root.alert_message.type=='yellow'\"></i>\r\n            <i class=\"fa fa-check\" v-if=\"$root.alert_message.type=='delete'\"></i>\r\n          </div> -->\r\n          <div class=\"Message-body\">\r\n            <p>{{$root.alert_message.message}}</p>\r\n          </div>\r\n          <button class=\"Message-close js-messageClose\" @click=\"$root.alert_message.show=0\"><i class=\"fa fa-times\"></i></button>\r\n        </div>\r\n      </transition>\r\n      <transition name=\"fade\">\r\n        <div class=\"Message\" :class=\"$root.alert_message2.type\" v-if=\"$root.alert_message2.show\">\r\n          <!-- <div class=\"Message-icon\">\r\n            <i class=\"fa fa-check\" v-if=\"$root.alert_message2.type=='green'\"></i>\r\n            <i class=\"fa fa-times\" v-if=\"$root.alert_message2.type=='red'\"></i>\r\n            <i class=\"fa fa-exclamation\" v-if=\"$root.alert_message2.type=='yellow'\"></i>\r\n            <i class=\"fa fa-times\" v-if=\"$root.alert_message2.type=='delete'\"></i>\r\n          </div> -->\r\n          <div class=\"Message-body\">\r\n            <p>{{$root.alert_message2.message}}</p>\r\n          </div>\r\n          <button class=\"Message-close js-messageClose\" @click=\"$root.alert_message2.show=0\"><i class=\"fa fa-times\"></i></button>\r\n        </div>\r\n      </transition>\r\n    </div>\r\n    <router-view></router-view>\r\n    <!-- <customizer></customizer> -->\r\n\r\n    <!-- maintenance -->\r\n    <div style=\"position:absolute;top:0;left:0;width:100%;height:100vh;background:#00000099;text-align:center;z-index:999999999999999999999999999999999\" v-if=\"is_maintenance\">\r\n      <div class=\"text-danger\" style=\"width:70%;margin:5% 15%;height:80vh;background-color:#f7f7f7;border-radius:15px\">\r\n        <img src=\"/img/maintens.png\" style=\"width:90%\">\r\n        <h3 class=\"text-danger font-weight-700\" style=\"margin-top:-10px\">ERP Under Maintenance</h3>\r\n        <p>We will be back soon!</p>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"complaint_box\" v-if=\"userId\">\r\n      <div class=\"issu_chat\">\r\n        <img class=\"open-button\" id=\"open_chat\" @click=\"chatbox=true\" src=\"/img/chat-icon.png\" width=\"120\" v-if=\"!chatbox\">\r\n        <img id=\"close_chat\" @click=\"chatbox=false\" src=\"/img/chat-icon-x.png\" width=\"120\" v-else>\r\n        \r\n      </div>\r\n      <div class=\"chat-popup\" id=\"myForm\" style=\"z-index: 99999999999999999999;\" v-if=\"chatbox\">\r\n        <div class=\"container\">\r\n          <div class=\"chat_box\">\r\n            <div class=\"head\">\r\n              <div class=\"user\">\r\n                <div class=\"avatar mr-2\">\r\n                  <img src=\"/img/website.png\" width=\"35\"  style=\"background-color:white;padding:2px;\">\r\n                </div>\r\n                <div class=\"name\">\r\n                  <b>Biowin ERP</b>\r\n                  <a>We are happy to help you</a>\r\n                </div>\r\n\r\n              </div>\r\n            </div>\r\n            <form ref=\"complaintForm\" @submit.prevent=\"complaintForm()\">\r\n              <div class=\"body\">\r\n                <div class=\"incoming flex\">\r\n                  <img  src=\"/favicon.png\" width=\"25\" height=\"25\" style=\"align-self: flex-end; margin-bottom: -30px;padding: 5px;\">\r\n                  <div class=\"bubble box arrow-top\">\r\n                    <p>Hey, we are here.</p>\r\n                  </div>\r\n                </div>\r\n                <div class=\"outgoing\">\r\n                  <div class=\"bubble lower\" v-for=\"issue_type in issue_types\" @click=\"form_data.issue_type=issue_type\" :class=\"form_data.issue_type==issue_type?'active':''\">\r\n                    <p>{{issue_type}}</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"foot\">\r\n                <input type=\"text\" name=\"complaint\" v-model=\"form_data.complaint\" class=\"msg\" placeholder=\"Type a message...\" required />\r\n                <input type=\"hidden\" name=\"issue_type\" v-model=\"form_data.issue_type\">\r\n                <button type=\"submit\"><i class=\"fa fa-paper-plane\"></i></button>\r\n              </div>\r\n              <div class=\"foot foot2\">\r\n                <div class=\"float-right\">\r\n                  <button @click=\"chatbox=false;$router.push('/')\" type=\"button\"><i class=\"fa fa-home\"></i></button>\r\n                  <button  @click=\"chatbox=false\" type=\"button\"><i class=\"fa fa-times\"></i></button>\r\n                </div>\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      is_maintenance:false,\r\n      chatbox:false,\r\n      form_data:{issue_type:'', page:'', complaint:''},\r\n      issue_types:['UI/UX Issue','Bug/Error Found','Other Issues'],\r\n      current_issue_type:'',\r\n    }\r\n  },\r\n  mounted(){\r\n    this.is_maintenance = is_maintenance_mode\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"getThemeMode\"]),\r\n    themeName() {\r\n      return this.getThemeMode.dark ? \"dark-theme\" : \" \";\r\n    },\r\n    rtl() {\r\n      return this.getThemeMode.rtl ? \"rtl\" : \" \";\r\n    }\r\n  },\r\n  methods: {\r\n    complaintForm() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['complaintForm'])\r\n      formdata.append('user', this.userId)\r\n      formdata.append('page', this.$route.path)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/erp_complaints/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.chatbox = false\r\n          this.showAlert('Added Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n  metaInfo() {\r\n    return {\r\n      // if no subcomponents specify a metaInfo.title, this title will be used\r\n      title: \"Biowin\",\r\n      // all titles will be injected into this template\r\n      titleTemplate: \"%s | Kerala\",\r\n      bodyAttrs: {\r\n        class: [this.themeName, \"text-left\"]\r\n      },\r\n      htmlAttrs: {\r\n        dir: this.rtl\r\n      }\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.common_alert{\r\n  z-index: 9999999;\r\n  position: fixed;\r\n  top: 30px;\r\n  right: 20px;\r\n}\r\n\r\n$c-black: #111;\r\n$c-white: #fff;\r\n$c-blues: #2980B9, #0074D9, #009FE3;\r\n$c-greys: #343C3F, #A2ACB0, #D9E1E4;\r\n$c-greens: #27AE60, #2ECC40, #01FF70;\r\n$c-reds: #C0392B, #FF4136, #E74C3C;\r\n$c-yellows: #C0392B, #FF4136, #E74C3C;\r\n$c-purples: #532E62, #8E44AD, #9B59B6;\r\n$c-rose: #00ad00, #18db18, #4cfc55;\r\n\r\n.common_alert * {\r\n  box-sizing: border-box;\r\n  border-radius:10px;\r\n}\r\n\r\n.common_alert .Message {\r\n  display: table;\r\n  position: relative;\r\n  margin: 10px auto 0;\r\n  min-width: 200px;\r\n  max-width: 400px;\r\n  background-color: nth($c-blues, 2);\r\n  color: #fff;\r\n  transition: all 0.2s ease;\r\n  \r\n  &.is-hidden {\r\n    opacity: 0;\r\n    height: 0;\r\n    font-size: 0;\r\n    padding: 0;\r\n    margin: 0 auto;\r\n    display: block;\r\n  }\r\n}\r\n\r\n.common_alert .green {\r\n  background-color: nth($c-rose, 1);\r\n}\r\n.common_alert .yellow {\r\n  background-color: nth($c-yellows, 1);\r\n}\r\n\r\n.common_alert .red {\r\n  background-color: nth($c-reds, 2);\r\n}\r\n.common_alert .blue {\r\n  background-color: nth($c-blues, 2);\r\n}\r\n\r\n.common_alert .Message-icon {\r\n  display: table-cell;\r\n  vertical-align: middle;\r\n  width: 70px;\r\n  padding: 15px 0 10px 0;\r\n  text-align: center;\r\n  background-color: rgba(black, 0.25);\r\n  \r\n  >i {\r\n    width: 20px;\r\n    font-size: 30px;\r\n  }\r\n}\r\n  \r\n.common_alert .Message-body {\r\n  display: table-cell;\r\n  vertical-align: middle;\r\n  font-weight:600;\r\n  padding: 7px 50px 0 17px;\r\n  \r\n  > p {\r\n    line-height: 1.2;\r\n    margin-top: 10px;\r\n  }\r\n}\r\n\r\n.common_alert .Message-button {\r\n  position: relative;\r\n  margin: 15px 5px -10px;\r\n  background-color: rgba(black, 0.25);\r\n  box-shadow: 0 3px rgba(black, 0.4);\r\n  border:none;\r\n  padding: 10px 15px;\r\n  font-size: 20px;\r\n  font-family: 'Source Sans Pro';\r\n  color: #fff;\r\n  outline: none;\r\n  cursor: pointer;\r\n  \r\n  &:hover {\r\n    background: rgba(black, 0.3);\r\n  }\r\n\r\n  &:active {\r\n    background: rgba(black, 0.3);\r\n    box-shadow: 0 0px rgba(black, 0.4);\r\n    top: 3px;\r\n  }\r\n}\r\n\r\n.common_alert .Message-close {\r\n  position: absolute;\r\n  background-color: transparent;\r\n  color: #fff;\r\n  border: none;\r\n  outline: none;\r\n  font-size: 12px;\r\n  right: 15px;\r\n  top: 10px;\r\n  opacity: 1;\r\n  cursor: pointer;\r\n  \r\n  .common_alert .Message:hover & {\r\n    opacity: 1;\r\n  }\r\n  \r\n  &:hover {\r\n    opacity: .7;\r\n  }\r\n}\r\n\r\n.common_alert .fade-enter-active, .common_alert .fade-leave-active {\r\n  transition: opacity .5s;\r\n}\r\n.common_alert .fade-enter, .common_alert .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\r\n  opacity: 0;\r\n}\r\n\r\n@media all and (max-width:768px){\r\n\r\n  .common_alert{\r\n    top: 10px;\r\n    right: 10px;\r\n  }\r\n  .common_alert .Message {\r\n    margin: 5px auto 0;\r\n    width: 200px;\r\n  }\r\n\r\n  .common_alert .Message-icon {\r\n    width: 50px;\r\n    padding: 7px 0 5px 0;\r\n    \r\n    >i {\r\n      width: 10px;\r\n      font-size: 20px;\r\n    }\r\n  }\r\n\r\n  .common_alert .Message-body {\r\n    font-weight:600;\r\n    padding: 10px 10px 5px 15px;\r\n    \r\n    > p {\r\n      line-height: 1.2;\r\n      margin-top: 10px;\r\n    }\r\n  }\r\n\r\n  .common_alert .Message-close {\r\n    right: 10px;\r\n    top: 6px;\r\n  }\r\n\r\n}\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n"]}]}