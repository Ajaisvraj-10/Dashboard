"src/views/app/oodo/qc","sourcesContent":["<template>\r\n  <div class=\"main-content create-main\">\r\n    <div>\r\n       <div class=\"d-flex mb-2 align-items-center justify-content-between\">\r\n          <slot name=\"header\">\r\n            <h5 class=\"mb-0\">Inprocess Test /<span class=\"text-success cursor-pointer\">{{production_process.main_process_name}}</span> / {{production_process.name}}</h5>\r\n                \r\n          </slot>\r\n          <div>\r\n\r\n          </div>\r\n          <div class=\"d-flex\">\r\n            <router-link :to=\"'/app/qc/inprocess-test'\">\r\n              <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Discard\r\n              </b-button>\r\n            </router-link>\r\n          </div>\r\n           \r\n        </div>\r\n        <div class=\"my-2 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n   \r\n    <div class=\"d-flex main-scroll\" style=\"overflow-y:hidden;min-width:101%;\">\r\n\r\n      <div class=\"p-1\" v-for=\"process_detail in process_details\" :style=\"'max-width:'+max_width+';flex: 0 0 '+max_width\" :class=\"is_active_process==process_detail.id?'active_process':''\" v-if=\"process_detail.qc_test\">\r\n        <div class=\"card p-0 pb-2 my-1\">\r\n          <div class=\"card p-2 m-0 flex-row border\" @click=\"is_active_process=process_detail.id\">\r\n            <div class=\"card col-md-2 align-self-center p-1\">\r\n              <img src=\"/img/procees.png\" style=\"vertical-align:top\">\r\n            </div>\r\n            <div class=\" col-md-10 align-self-center\">\r\n              <b>{{process_detail.name}}  </b>\r\n              <div>\r\n                <b-button type=\"button\" variant=\"primary\" class=\" px-1 py-0\" style=\"font-size:12px\" @click=\"testAction('new',process_detail)\">New Test</b-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"process_kanban\">\r\n            <b class=\"px-1 mt-1\" v-if=\"process_detail.not_attached_inprocess.length>0\">Not attached Inprocess</b>\r\n            <div class=\"m-1\" v-for=\"not_attached_inprocess in process_detail.not_attached_inprocess\">\r\n              <div class=\"px-2 card py-2\">\r\n                <div class=\"mx-1 mb-2\">\r\n                  <table class=\"w-100\">\r\n                    <tr class=\"\">\r\n                      <th class=\"text-info\">\r\n                        #{{not_attached_inprocess.supervisor_name}}\r\n                      </th>\r\n                      <th>\r\n                        {{not_attached_inprocess.shift}}\r\n                      </th>\r\n                      <th class=\"text-right\">{{dateFormate(not_attached_inprocess.created_on)}}</th>\r\n                    </tr>\r\n                    <tr class=\"\">\r\n                      <th class=\"text-info\">\r\n                        {{not_attached_inprocess.parameter_name}}\r\n                      </th>\r\n                      <th>\r\n                        <span class=\"text-success\">\r\n                          #<b>{{not_attached_inprocess.lot}}</b>/Lot\r\n                        </span>\r\n                      </th>\r\n                      <th class=\"text-right pb-1 cursor-pointer\">\r\n                        <span class=\"badge badge-primary\" @click=\"testAction('attach',process_detail, not_attached_inprocess)\">\r\n                          Attach to process\r\n                        </span>\r\n                      </th>\r\n                    </tr>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <b class=\"px-1 mt-1\">Process</b>\r\n            <div class=\"m-1\" v-for=\"(start_process,key) in process_detail.start_processes\" v-if=\"start_process.main_process_detail.current_process_detail==start_process.process_detail\">\r\n              <div class=\"px-2 card py-2\">\r\n                <div class=\"mx-1 mb-2\">\r\n                  <table class=\"w-100\" v-if=\"process_detail.barrel_input && !process_detail.is_sorting\">\r\n                    <tr class=\"\">\r\n                      <th class=\"text-primary\">#{{start_process.main_process}}/PROCESS</th>\r\n                      <th class=\"text-right pr-3\">\r\n                        LOT\r\n                        <span v-for=\"(barrel_washing_product,key) in start_process.barrel_washing_products\">\r\n                          {{key==0?'':', '}}{{barrel_washing_product.lot}}\r\n                        </span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr v-for=\"(barrel_washing_product,key) in start_process.barrel_washing_products\">\r\n                      <th>{{key==0?'BARREL'+slugToWord(checkMult(start_process.barrel_washing_products.length)):''}}</th>\r\n                      <th class=\"text-right\">\r\n                        {{key==0?'':', '}}{{barrel_washing_product.name}} <sup v-if=\"barrel_washing_product.is_organic\"><span class=\"badge badge-success\">ORG</span></sup> <span>({{barrel_washing_product.available_quantity}}KG)</span>\r\n                      </th>\r\n                    </tr>\r\n                  </table>\r\n                  <table class=\"w-100\" v-else>\r\n                    <tr class=\"\">\r\n                      <th class=\"text-primary\">#{{start_process.main_process}}/PROCESS</th>\r\n                      <th class=\"text-right pr-3\">\r\n                        LOT\r\n                        <span v-for=\"(process_product,key) in start_process.process_products\">\r\n                          {{key==0?'':', '}}{{process_product.lot}}\r\n                        </span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr v-for=\"(process_product,key) in start_process.process_products\">\r\n                      <th>{{key==0?'INPUT'+slugToWord(checkMult(start_process.process_products.length)):''}}</th>\r\n                      <th class=\"text-right\">\r\n                        {{key==0?'':', '}}{{process_product.parameter_detail.name}} <sup v-if=\"process_product.is_organic\"><span class=\"badge badge-success\">ORG</span></sup><span v-show=\"!process_detail.is_dryer\">({{process_product.quantity}}KG)</span>\r\n                      </th>\r\n                    </tr>\r\n                  </table>\r\n                  <table class=\"w-100\">\r\n                    <tr>\r\n                      <th colspan=\"2\">STATUS</th>\r\n                      <th class=\"text-right\">\r\n                        <span class=\"text-success\">{{start_process.status}}</span>\r\n                      </th>\r\n                    </tr>\r\n                  </table>\r\n                </div>\r\n                <div class=\"row\" v-if=\"start_process.processes.length>0\">\r\n                  <div class=\"col-md-12\">\r\n                    <b class=\"mx-1\">{{'Shifts'}}({{start_process.processes.length}})</b>\r\n                  </div>\r\n                </div>\r\n                <div class=\"card px-2 py-2 mt-1 \" v-for=\"process in start_process.processes\">\r\n                  <table class=\"\">\r\n                    <tr class=\"\">\r\n                      <th class=\"text-info\">\r\n                        #{{process.supervisor_detail.name}}\r\n                      </th>\r\n                      <th>\r\n                        {{process.shift}}\r\n                      </th>\r\n                      <th class=\"text-right\">{{dateFormate(process.created_on)}}</th>\r\n                    </tr>\r\n                    <tr class=\"\">\r\n                      <th class=\"text-info\">\r\n                        QC Check\r\n                      </th>\r\n                      <th>\r\n                        <span class=\"text-success\" v-if=\"process.qc_test_count>0\">\r\n                          QC <b>{{process.qc_test_count}}</b>time{{checkMult(process.qc_test_count)}} checked\r\n                        </span>\r\n                        <span class=\"text-danger\" v-else>\r\n                          QC not checked\r\n                        </span>\r\n                      </th>\r\n                      <th class=\"text-right pb-1 cursor-pointer\">\r\n                        <span class=\"badge badge-warning\" v-if=\"process.qc_test_count>0\" @click=\"testAction('view',process_detail, process,)\">\r\n                          View\r\n                        </span>\r\n                        <span class=\"badge badge-primary\" @click=\"testAction('new',process_detail,process)\" v-else>\r\n                          Add\r\n                        </span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr>\r\n                      <td colspan=\"3\" class=\"text-center border-top\">\r\n                        <b-button variant=\"primary\" class=\"py-0 mt-2 mb-1\" @click=\"testAction('new',process_detail,process)\">\r\n                          Add New Inprocess Test\r\n                        </b-button>\r\n                      </td>\r\n                    </tr>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n              <hr class=\"my-2\">\r\n            </div>\r\n            <div class=\"text-center\"> </div>\r\n          </div>\r\n        </div>\r\n    \r\n      </div>\r\n    </div>\r\n    <div>\r\n      <div style=\"position:fixed;top:45%;right:1px;cursor:pointer\" @mousedown=\"scroll_click=true;scrollRight()\"  @mouseup=\"scroll_click=false\">\r\n        <img src=\"/img/arrow-right.png\" style=\"width:20px;aspect-ratio: 1 / 2.5;\" v-if=\"scroll_pos.right\">\r\n      </div>\r\n      <div style=\"position:fixed;top:45%;left:105px;cursor:pointer\" @mousedown=\"scroll_click=true;scrollLeft()\"  @mouseup=\"scroll_click=false\">\r\n        <img src=\"/img/arrow-left.png\" style=\"width:20px;aspect-ratio: 1 / 2.5;\" v-if=\"scroll_pos.left\">\r\n      </div>\r\n    </div>\r\n    <div>\r\n      <b-modal id=\"qc-test\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n          <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n            <slot name=\"header\">\r\n               <h5 class=\"mb-0\"><span class=\"text-success\">Add Test</span></h5>\r\n            </slot>\r\n            <div class=\"d-flex\">\r\n              <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"dataForm()\">Save</b-button>\r\n              <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('qc-test')\">Close</b-button>\r\n            </div>\r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\">\r\n              <b-card class=\"mt-2\">\r\n                <div class=\"user-info border-bottom pb-2 mb-2\" v-if=\"!current_process.id\">\r\n                  <div class=\"o_thread_search_input \">\r\n                    <span style=\"width:120px;\"> Product: </span>\r\n                    <select class=\"ui-autocomplete-input w-75 ml-2\" name=\"parameter\" v-model=\"form_data.parameter\" @change=\"getLots()\">\r\n                      <option v-for=\"parameter in parameters\" :value=\"parameter.id\">{{parameter.name}}</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input \">\r\n                    <span style=\"width:120px;\"> Lot: </span>\r\n                    <select class=\"ui-autocomplete-input w-75 ml-2\" name=\"lot\" v-model=\"form_data.lot\">\r\n                      <option v-for=\"lot in lots\" :value=\"lot.id\">#{{lot.id}}/Lot {{lot.product}}</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:120px;\"> Shift: </span>\r\n                    <select class=\"ui-autocomplete-input w-100 ml-2\" name=\"shift\" v-model=\"form_data.shift\">\r\n                      <option v-for=\"shift in shifts\" :value=\"shift\"> {{shift}}</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input \">\r\n                    <span style=\"width:120px;\"> Supervisor: </span>\r\n                    <select class=\"ui-autocomplete-input w-75 ml-2\" name=\"supervisor\" v-model=\"form_data.supervisor\">\r\n                      <option v-for=\"supervisor in supervisors\" :value=\"supervisor.id\">{{supervisor.name}}</option>\r\n                    </select>\r\n                  </div>\r\n                </div>\r\n                <div v-else>\r\n                  <input type=\"hidden\" name=\"process\" v-model=\"current_process.id\">\r\n                </div>\r\n                <div class=\"user-info\" v-if=\"current_process.id || (!current_process.id && form_data.parameter && form_data.lot && form_data.supervisor && form_data.shift)\">\r\n                  <div class=\"row mx-0\">\r\n                    <div class=\"col-md-4\" v-for=\"data in physical_parameter.datas\">\r\n                      <div class=\"user-info pt-2\" v-for=\"param in data\">\r\n                        <div class=\"d-flex mt-2\">\r\n                          <span style=\"width:80px;\" :for=\"param.name\"> {{param.name}} : </span>\r\n                          <input type=\"checkbox\" class=\"ui-autocomplete-input mx-2\" autocomplete=\"off\" value=\"1\" v-model=\"param.checked\" :name=\"param.name\" :id=\"param.name\">\r\n                          <span>\r\n                            <input :placeholder=\"param.limit_type\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" v-model=\"param.limit\" :name=\"param.name+'limit'\" v-if=\"param.checked && (param.limit_type=='%' || param.limit_type=='<' || param.limit_type=='>')\" @input=\"checkParam(param)\" required>\r\n                            <input :placeholder=\"'Type here..'\" type=\"text\" step=\"0.1\" class=\"ui-autocomplete-input w-100\" autocomplete=\"off\" :name=\"param.name+'limit'\" v-else-if=\"param.checked && param.limit_type=='Typing'\" required>\r\n                            <select class=\"ui-autocomplete-input w-100\" :name=\"param.name+'limit'\" v-else-if=\"param.checked && param.limit_type=='YES/NO'\">\r\n                              <option value=\"YES\">YES</option>\r\n                              <option value=\"NO\">NO</option>\r\n                            </select>\r\n                            <select class=\"ui-autocomplete-input w-100\" :name=\"param.name+'limit'\" v-else-if=\"param.checked && param.limit_type=='Options'\">\r\n                              <option v-for=\"option in param.options\" :value=\"option.id\">{{option.name}}</option>\r\n                            </select>\r\n                          </span>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                  </div> \r\n                  <hr>\r\n                  <div>\r\n                    <div class=\"o_thread_search_input mt-4\">\r\n                      <span style=\"width:120px;\"> Qc Status: </span>\r\n                      <div>\r\n                        <input type=\"hidden\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.status\" name=\"status\" readonly>\r\n                        <button type=\"button\" class=\"btn ml-2\" :class=\"form_data.status=='Approved'?'btn-success':'btn-outline-success'\" @click=\"form_data.status='Approved'\">Approved</button>\r\n                        <button type=\"button\" class=\"btn ml-2\" :class=\"form_data.status=='Rejected'?'btn-danger':'btn-outline-danger'\" @click=\"form_data.status='Rejected'\">Rejected</button>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"o_thread_search_input \">\r\n                      <span style=\"width:120px;\"> Remark: </span>\r\n                      <textarea class=\"ui-autocomplete-input w-75 ml-2\" name=\"remarks\" placeholder=\"Write something here\"></textarea>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n\r\n      <b-modal id=\"attach-to-process\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n          <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n            <slot name=\"header\">\r\n               <h5 class=\"mb-0\"><span class=\"text-success\">{{current_process_detail.name}}</span> /Attach</h5>\r\n            </slot>\r\n            <div class=\"d-flex\">\r\n              <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"attachToProcess()\">Save</b-button>\r\n              <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('attach-to-process')\">Close</b-button>\r\n            </div>\r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n        </div>\r\n        <b-row class=\"main-scroll\">\r\n          <b-col lg=\"12\" xl=\"12\" class=\"pt-2\">\r\n              <div class=\"row mx-0\">\r\n                <div class=\"px-1 col-md-4\" v-for=\"(start_process,key) in current_process_detail.start_processes\" v-if=\"start_process.main_process_detail.current_process_detail==start_process.process_detail\">\r\n                  <div class=\"px-2 card py-2\">\r\n                    <div class=\"mx-1 mb-2\">\r\n                      <table class=\"w-100\" v-if=\"current_process_detail.barrel_input && !current_process_detail.is_sorting\">\r\n                        <tr class=\"\">\r\n                          <th class=\"text-primary\">#{{start_process.main_process}}/PROCESS</th>\r\n                          <th class=\"text-right pr-3\">\r\n                            LOT\r\n                            <span v-for=\"(barrel_washing_product,key) in start_process.barrel_washing_products\">\r\n                              {{key==0?'':', '}}{{barrel_washing_product.lot}}\r\n                            </span>\r\n                          </th>\r\n                        </tr>\r\n                        <tr v-for=\"(barrel_washing_product,key) in start_process.barrel_washing_products\">\r\n                          <th>{{key==0?'BARREL'+slugToWord(checkMult(start_process.barrel_washing_products.length)):''}}</th>\r\n                          <th class=\"text-right\">\r\n                            {{key==0?'':', '}}{{barrel_washing_product.name}} <sup v-if=\"barrel_washing_product.is_organic\"><span class=\"badge badge-success\">ORG</span></sup> <span>({{barrel_washing_product.available_quantity}}KG)</span>\r\n                          </th>\r\n                        </tr>\r\n                      </table>\r\n                      <table class=\"w-100\" v-else>\r\n                        <tr class=\"\">\r\n                          <th class=\"text-primary\">#{{start_process.main_process}}/PROCESS</th>\r\n                          <th class=\"text-right pr-3\">\r\n                            LOT\r\n                            <span v-for=\"(process_product,key) in start_process.process_products\">\r\n                              {{key==0?'':', '}}{{process_product.lot}}\r\n                            </span>\r\n                          </th>\r\n                        </tr>\r\n                        <tr v-for=\"(process_product,key) in start_process.process_products\">\r\n                          <th>{{key==0?'INPUT'+slugToWord(checkMult(start_process.process_products.length)):''}}</th>\r\n                          <th class=\"text-right\">\r\n                            {{key==0?'':', '}}{{process_product.parameter_detail.name}} <sup v-if=\"process_product.is_organic\"><span class=\"badge badge-success\">ORG</span></sup><span v-show=\"!current_process_detail.is_dryer\">({{process_product.quantity}}KG)</span>\r\n                          </th>\r\n                        </tr>\r\n                      </table>\r\n                      <table class=\"w-100\">\r\n                        <tr>\r\n                          <th colspan=\"2\">STATUS</th>\r\n                          <th class=\"text-right\">\r\n                            <span class=\"text-success\">{{start_process.status}}</span>\r\n                          </th>\r\n                        </tr>\r\n                      </table>\r\n                    </div>\r\n                    <div class=\"row\" v-if=\"start_process.processes.length>0\">\r\n                      <div class=\"col-md-12\">\r\n                        <b class=\"mx-1\">{{'Shifts'}}({{start_process.processes.length}})</b>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"card px-2 py-2 mt-1 \" v-for=\"process in start_process.processes\" v-if=\"current_process.shift==process.shift && current_process.lot==process.lot\">\r\n                      <table class=\"\">\r\n                        <tr class=\"\">\r\n                          <th class=\"text-info\">\r\n                            #{{process.supervisor_detail.name}}\r\n                          </th>\r\n                          <th>\r\n                            {{process.shift}}\r\n                          </th>\r\n                          <th class=\"text-right\">{{dateFormate(process.created_on)}}</th>\r\n                        </tr>\r\n                        <tr class=\"\">\r\n                          <th class=\"text-info\">\r\n                            QC Check\r\n                          </th>\r\n                          <th colspan=\"2\" class=\"text-right\">\r\n                            <span class=\"text-success\" v-if=\"process.qc_test_count>0\">\r\n                              QC <b>{{process.qc_test_count}}</b>time{{checkMult(process.qc_test_count)}} checked\r\n                            </span>\r\n                            <span class=\"text-danger\" v-else>\r\n                              QC not checked\r\n                            </span>\r\n                          </th>\r\n                        </tr>\r\n                        <tr>\r\n                          <td colspan=\"3\" class=\"text-center border-top\">\r\n                            <b-button variant=\"danger\" class=\"py-1 mt-2 mb-1\" @click=\"attach_process_id=null\" v-if=\"attach_process_id==process.id\">\r\n                              Attached\r\n                            </b-button>\r\n                            <b-button variant=\"success\" class=\"py-1 mt-2 mb-1\" @click=\"attach_process_id=process.id\" v-else>\r\n                              Attach here\r\n                            </b-button>\r\n                          </td>\r\n                        </tr>\r\n                      </table>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n          </b-col>\r\n        </b-row>\r\n      </b-modal>\r\n\r\n      <b-modal id=\"qc-test-list\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n          <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n            <slot name=\"header\">\r\n               <h5 class=\"mb-0\"><span class=\"text-success\">{{current_process_detail.name}}</span> /Inprocess tests</h5>\r\n            </slot>\r\n            <div class=\"d-flex\">\r\n              <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('qc-test-list')\">Close</b-button>\r\n            </div>\r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n        </div>\r\n        <b-row class=\"main-scroll\">\r\n          <b-col lg=\"12\" xl=\"12\">\r\n            <b-card class=\"mt-2\">\r\n              <table class=\"table table-striped\">\r\n                <thead>\r\n                  <tr>\r\n                    <th>Parameters</th>\r\n                    <th v-for=\"(inprocess_test,ikey) in inprocess_tests\">\r\n                      {{dateFormate(inprocess_test.created_on, true)}}\r\n                    </th>\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"(physical_parameter,pkey) in physical_parameters\">\r\n                    <td>{{physical_parameter.name}}</td>\r\n                    <td v-for=\"(inprocess_test,ikey) in inprocess_tests\">\r\n                      <div v-for=\"param in inprocess_test.physical_parameter\" v-if=\"param.id==physical_parameter.id\">\r\n                        <span v-if=\"param.limit_type=='%' || param.limit_type=='<' || param.limit_type=='>'\">\r\n                          {{param.limit}}{{param.limit_type}}\r\n                        </span>\r\n                        <span v-else-if=\"param.limit_type=='Typing'\">\r\n                          {{param.limit}}\r\n                        </span>\r\n                        <span v-else-if=\"param.limit_type=='YES/NO'\" class=\"badge\" :class=\"param.limit=='YES'?'badge-success':'badge-danger'\">\r\n                          {{param.limit}}\r\n                        </span>\r\n                        <span v-else-if=\"param.limit_type=='Options'\" class=\"badge badge-light p-1\" :style=\"'background-color:'+param.color+'!important;color:'+bgToTextColor(param.color)\">\r\n                          {{param.limit}}\r\n                        </span>\r\n                      </div>\r\n                    </td>\r\n                  </tr>\r\n                  <tr>\r\n                    <td>Remark</td>\r\n                    <td v-for=\"(inprocess_test,ikey) in inprocess_tests\">\r\n                      <p v-html=\"inprocess_test.remarks\"></p>\r\n                    </td>\r\n                  </tr>\r\n                </tbody>\r\n              </table>\r\n            </b-card>\r\n          </b-col>\r\n        </b-row>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - QC\"\r\n  },\r\n  components:{\r\n  },\r\n  data() {\r\n    return {\r\n      production_process:{id:null},\r\n      process_details:[],\r\n      current_process_detail:{id:null},\r\n      warehouse_id:'',\r\n      max_width:'400px',\r\n      is_active_process:null,\r\n      scroll_pos:{left:false,right:true,current_scroll:0},\r\n      scroll_click:false,\r\n      current_process:{id:null},\r\n      inprocess_tests:[],\r\n      attach_process_id:null,\r\n      form_data:{id:null, status:'', parameter:null},\r\n      physical_parameter:{datas:[]},\r\n      physical_parameters:[],\r\n      parameters:[],\r\n      lots:[],\r\n      supervisors:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'qc'\r\n    this.production_process.id = this.$route.params.process_id\r\n    this.getProductionProcess()\r\n    this.getWarehouses()\r\n  },\r\n  methods: {\r\n    getProductionProcess() {\r\n      this.scroll_pos = {left:false,right:true,current_scroll:0}\r\n      this.scroll_click = false\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/production_process/'+this.production_process.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n        return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.production_process = jsonData\r\n        this.getProcessDetails()\r\n      })\r\n    },\r\n    getProcessDetails() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/process_details/?production_process='+this.production_process.id+'&qc_test=true', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n        return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.process_details = jsonData\r\n        this.$bvModal.hide('list-main-process')\r\n        this.setColSize()\r\n      })\r\n    },\r\n    setColSize(){\r\n      var cols = this.process_details.length\r\n      this.max_width = '400px'\r\n      if(cols<3){\r\n      this.scroll_pos = {left:false,right:false,current_scroll:0}\r\n      }\r\n      // if(cols>4){\r\n      //   this.max_width = (100/cols)+'%'\r\n      // }\r\n    },\r\n    testAction(action='new',process_detail,process_={id:null}){\r\n      this.current_process_detail = process_detail\r\n      this.warehouse_id = process_detail.warehouse\r\n      this.current_process = process_\r\n      this.form_data = {id:null, status:''}\r\n      this.getPhysicalParameters()\r\n      if(!this.form_data.id){\r\n        this.getParameters()\r\n      }\r\n      if(action=='new'){\r\n        this.$bvModal.show('qc-test')\r\n      }else if(action=='view'){\r\n        this.viewTest()\r\n      }else if(action=='attach'){\r\n        this.$bvModal.show('attach-to-process')\r\n      }\r\n    },\r\n    viewTest() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/inprocess_test/?parameter_for=inprocess&process_detail='+this.current_process_detail.id+'&process='+this.current_process.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.inprocess_tests = jsonData\r\n        this.$bvModal.show('qc-test-list')\r\n      })\r\n    },\r\n    getPhysicalParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/physical_parameters/?parameter_for=inprocess&process_detail='+this.current_process_detail.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var length = jsonData.length/3\r\n        this.physical_parameters = jsonData\r\n        this.physical_parameter = this.pagination(jsonData,Math.round(length))\r\n      })\r\n    },\r\n    dataForm(){\r\n      if(this.form_data.id){\r\n        this.updateTest()\r\n      }else{\r\n        this.createTest()\r\n      }\r\n    },\r\n    createTest() {\r\n      if(this.$root.user_type!='qc'){\r\n        this.showAlert('You cannot add to this process!','red')\r\n        return false\r\n      }\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('qc',this.employeeId)\r\n      formdata.append('process_detail',this.current_process_detail.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/inprocess_test/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.form_data = jsonData\r\n          this.updateTest()\r\n        }else{\r\n          this.showAlert('Something Wrong!','red')\r\n        }\r\n      })\r\n    },\r\n    updateTest() {\r\n      if(this.$root.user_type!='qc'){\r\n        this.showAlert('You cannot add to this process!','red')\r\n        return false\r\n      }\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('qc',this.employeeId)\r\n      formdata.append('process_detail',this.current_process_detail.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/inprocess_test/'+this.form_data.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.$bvModal.hide('qc-test')\r\n          this.showAlert('Saved Successfully!')\r\n          this.getProcessDetails()\r\n        }else{\r\n          this.showAlert('Something Wrong!','red')\r\n        }\r\n      })\r\n    },\r\n    attachToProcess() {\r\n      if(this.$root.user_type!='qc'){\r\n        this.showAlert('You cannot add to this process!','red')\r\n        return false\r\n      }\r\n      if(!this.attach_process_id || !this.current_process.id){\r\n        this.showAlert('Please attach to a process!','red')\r\n        return false\r\n      }\r\n      var formdata = new FormData()\r\n      formdata.append('process',this.attach_process_id)\r\n      formdata.append('inprocess',this.current_process.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/attach_to_process/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.$bvModal.hide('attach-to-process')\r\n          this.showAlert('Attached Successfully!')\r\n          this.getProcessDetails()\r\n        }else{\r\n          this.showAlert('Something Wrong!','red')\r\n        }\r\n      })\r\n    },\r\n    getParameters() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameters/?outputs='+this.current_process_detail.outputs, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.parameters = jsonData\r\n      })\r\n    },\r\n    getLots() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/create_lots/?parameter='+this.form_data.parameter, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lots = jsonData\r\n        this.getEmployees()\r\n      })\r\n    },\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/?job_position=4', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.supervisors = jsonData\r\n      })\r\n    },\r\n    scrollLeft() {\r\n      if(!this.scroll_click){\r\n        return false\r\n      }\r\n      var self = this\r\n      let content = document.querySelector(\".main-scroll\");\r\n      content.scrollTo({left: content.scrollLeft -= 15,behavior: 'smooth',})\r\n      if(this.scroll_pos.current_scroll==content.scrollLeft){\r\n        this.scroll_pos.left = false\r\n      }else{\r\n        this.scroll_pos.right = true\r\n        this.scroll_pos.left = true\r\n        setTimeout(function(){\r\n          self.scrollLeft()\r\n        },10)\r\n      }\r\n      this.scroll_pos.current_scroll = content.scrollLeft\r\n    },\r\n    scrollRight() {\r\n      if(!this.scroll_click){\r\n        return false\r\n      }\r\n      var self = this\r\n      let content = document.querySelector(\".main-scroll\");\r\n      content.scrollTo({left: content.scrollLeft += 15,behavior: 'smooth',})\r\n      if(this.scroll_pos.current_scroll==content.scrollLeft){\r\n        this.scroll_pos.right = false\r\n      }else{\r\n        this.scroll_pos.right = true\r\n        this.scroll_pos.left = true\r\n        setTimeout(function(){\r\n          self.scrollRight()\r\n        },10)\r\n      }\r\n      this.scroll_pos.current_scroll = content.scrollLeft\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n  #list-process.modal.show .modal-dialog{\r\n    min-width: 60%!important;\r\n  }\r\n  #start-process.modal.show .modal-dialog{\r\n    min-width: 70%!important;\r\n  }\r\n  /*.active_process{\r\n    position: absolute;\r\n    top: -45px;\r\n    left:50%;\r\n    transform: translate(-50px);\r\n    z-index: 999;\r\n    width:350px;\r\n    max-width: 400px!important;\r\n    flex: 0 0 400px!important;\r\n  }\r\n  .active_process .process_kanban{\r\n    height:87vh;\r\n  }*/\r\n</style>\r\n"]}]}