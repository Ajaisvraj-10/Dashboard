AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"basic-report.vue","sourceRoot":"src/views/app/pages","sourcesContent":["\r\n<template>\r\n  <div class=\"main-content\">\r\n    <main >\r\n   \r\n      <div class=\"header-bio\">\r\n           \r\n        <div class=\"container d-flex justify-content-between align-items-center px-4\">\r\n          <div class=\"d-flex\">\r\n            <img src=\"/images/left-arrow.png\" width=\"10\" @click=\"backSection()\">\r\n            <h4 class=\"font-weight-700 px-3\"> Report</h4>\r\n          </div>\r\n        </div>\r\n\r\n      </div>\r\n      <div class=\"main-body\" id=\"main_body\">\r\n            \r\n        <div class=\"mt-4 flex-align px-3\" style=\"gap:10px;\">\r\n          <div  class=\"input-group mb-0 input-group-lg\">\r\n            <button class=\"btn btn-outline-dark w-100 text-left\" v-b-modal.filter-product>\r\n              {{ product_name?product_name:'Select Product' }}\r\n              <span class=\"badge float-right p-1\" :class=\"product_status=='Organic'?'badge-success':'badge-primary'\">{{product_name?product_status:''}}</span>\r\n            </button>\r\n          </div>\r\n          <b-modal id=\"filter-data\" title=\"Filter\" hide-footer>\r\n            <div>\r\n              <form ref=\"filterData\" @submit.prevent=\"filterData()\">\r\n                <div class=\"filter-item flex-align\">\r\n                  <label class=\"check-container\">ICS\r\n                  </label>\r\n                  <b-form-select v-model=\"filter.ics\" name=\"ics\" class=\"mb-3\" style=\"max-width: 120px;min-height: 35px;\" @change=\"changeICS()\">\r\n                    <b-form-select-option v-for=\"ics in all_ics\" :value=\"ics.id\" v-if=\"ics.type==$root.userInfo.fpo\">{{ics.name}}</b-form-select-option>\r\n                  </b-form-select>\r\n                </div>\r\n                <div class=\"filter-item flex-align\">\r\n                  <label class=\"check-container\">Panchayath\r\n                  </label>\r\n                  <b-form-select v-model=\"filter.panchayath\" name=\"panchayath\" class=\"mb-3\" style=\"max-width: 120px;min-height: 35px;\">\r\n                    <b-form-select-option v-for=\"panchayath in all_panchayaths\" :value=\"panchayath.id\">{{panchayath.name}}</b-form-select-option>\r\n                  </b-form-select>\r\n                </div>\r\n                <div class=\"filter-item flex-align\">\r\n                  <label class=\"check-container\">VFC\r\n                  </label>\r\n                  <b-form-select v-model=\"filter.vfc\" name=\"vfc\" class=\"mb-3\" style=\"max-width: 120px;min-height: 35px;\">\r\n                    <b-form-select-option v-for=\"vfc in all_vfc\" :value=\"vfc.id\" v-if=\"vfc.location.revenue_village_detail.panchayath  == filter.panchayath\">{{vfc.name}}</b-form-select-option>\r\n                  </b-form-select>\r\n                </div>\r\n                <hr>\r\n                <div class=\"filter-item flex-align\">\r\n                  <b-button type=\"button\" variant=\"outline-info\" class=\"w-100 px-3 py-1 ml-1 mr-2\" @click=\"cancelFilter()\">Cancel</b-button>\r\n                  <b-button type=\"submit\" variant=\"success\" class=\"w-100 px-3 py-1 mr-1 ml-2\">Submit</b-button>\r\n                </div>\r\n              </form>\r\n            </div>\r\n\r\n\r\n          </b-modal>\r\n\r\n          <b-modal id=\"filter-product\" title=\"Select Product\" hide-footer>\r\n            <div>\r\n              <ProductFields :product_id=\"product_id\" :input_col=\"input_col\" :readonly=\"false\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n\r\n              <div v-show=\"input_col==5\">\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\"> Product Status: </span>\r\n                  <div class=\"inspection-btn\" >\r\n                    <button type=\"button\" class=\"btn\" v-for=\"farmer_status in farmer_statuses\" :class=\"product_status==farmer_status?'active-btn':''\" @click=\"product_status=farmer_status;nextCol()\">{{farmer_status}}</button>\r\n                    <input type=\"hidden\" v-model=\"product_status\" name=\"farmer_status\" :class=\"'check_field_'+5\" required>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"o_thread_search_input\" v-show=\"input_col==6\">\r\n                <h4 class=\"text-light pr-4 py-2\">Inspection Year: </h4>\r\n                <select class=\"form-control w-100 mx-1\" v-model=\"inspection_year\" name=\"inspection_year\">\r\n                    <option v-for=\"year in this_year\" :value=\"year\" v-if=\"year>2000\">{{year}}</option>\r\n                </select>\r\n              </div>\r\n              <div class=\"o_thread_search_input\" v-show=\"input_col==6\">\r\n                <h4 class=\"text-light pr-4 py-2\">Procurement Year: </h4>\r\n                <select class=\"form-control w-100 mx-1\" v-model=\"procurement_year\" name=\"procurement_year\">\r\n                    <option v-for=\"year in this_year\" :value=\"year\" v-if=\"year>2000\">{{year}}</option>\r\n                </select>\r\n              </div>\r\n              <hr class=\"my-4\">\r\n              <div class=\"filter-item flex-align float-right\">\r\n                <div class=\"\" style=\"display: flex;justify-content: flex-end;gap: 10px;\">\r\n                  <button type=\"button\" class=\"btn btn-light\" @click=\"input_col>2?backCol():''\"> <i class=\"fa fa-angle-left\" style=\"\"></i> Back</button>\r\n                  <button type=\"button\" class=\"btn btn-secondary\" @click=\"nextCol();reports={success:false}\">{{input_col==6?'Select':'Next'}} <i class=\"fa fa-angle-right\" style=\"color:white;\"></i></button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n\r\n          </b-modal>\r\n         \r\n        </div>\r\n\r\n        <div class=\"card px-2 py-1 pb-2 mx-3 mt-3\" v-if=\"filter.ics || filter.vfc || filter.panchayath\">\r\n          <table>\r\n            <tr v-if=\"filter.ics\">\r\n              <td class=\"pl-2\">ICS</td>\r\n              <td>:</td>\r\n              <th>\r\n                <span class=\"badge badge-primary py-1 ml-1\" v-for=\"ics in all_ics\" v-if=\"ics.id==filter.ics\">{{ics.name}}</span>\r\n              </th>\r\n            </tr>\r\n            <tr v-if=\"filter.vfc\">\r\n              <td class=\"pl-2\">VFC</td>\r\n              <td>:</td>\r\n              <th>\r\n                <span class=\"badge badge-success py-1 ml-1\" v-for=\"vfc in all_vfc\" v-if=\"vfc.id==filter.vfc\">{{vfc.name}}</span>\r\n              </th>\r\n            </tr>\r\n            <tr v-if=\"filter.panchayath\">\r\n              <td class=\"pl-2\">Panchayath</td>\r\n              <td>:</td>\r\n              <th>\r\n                <span class=\"badge badge-primary py-1 ml-1\" v-for=\"panchayath in all_panchayaths\" v-if=\"panchayath.id==filter.panchayath\">{{panchayath.name}}</span>\r\n              </th>\r\n            </tr>\r\n          </table>\r\n        </div>\r\n           \r\n        <div class=\"mt-3 mb-5 pb-3\">\r\n          \r\n          <div v-if=\"reports.success\"> \r\n            <div class=\"px-3\">\r\n\r\n              <div class=\"card d-flex justify-content-between p-2 mb-3\">\r\n                <table class=\"table\">\r\n                  <tr>\r\n                    <th class=\"border-0\">\r\n                      <h4 class=\"font-weight-500 pr-2\">{{reports.name}} <span class=\"text-success font-weight-300 text-12\">{{reports.type}}</span></h4>\r\n                    </th>\r\n                    <th class=\"border-0\" colspan=\"2\">\r\n                      <h4 class=\"font-weight-500 text-right ml-3 py-1 text-green text-14\" style=\"white-space: pre;\">{{inspection_year}}</h4>\r\n                    </th>\r\n                  </tr>\r\n                  <tr>\r\n                    <th>Total estimate Qty</th>\r\n                    <th class=\"px-0\">:</th>\r\n                    <td class=\"text-right\">{{reports.total_estimate}}KG</td>\r\n                  </tr>\r\n                  <tr>\r\n                    <th>\r\n                      <h4 class=\"font-weight-500 pr-2\">Procurement</h4>\r\n                    </th>\r\n                    <th colspan=\"2\">\r\n                      <h4 class=\"font-weight-500 text-right ml-3 py-1 text-green text-14\" style=\"white-space: pre;\">{{procurement_year}}</h4>\r\n                    </th>\r\n                  </tr>\r\n                  <tr>\r\n                    <th>Total procured Qty</th>\r\n                    <th class=\"px-0\">:</th>\r\n                    <td class=\"text-right\">{{reports.total_procured}}KG</td>\r\n                  </tr>\r\n                  <tr>\r\n                    <th>Balance procurement Qty</th>\r\n                    <th class=\"px-0\">:</th>\r\n                    <td class=\"text-right\">{{reports.balance_procurement}}KG</td>\r\n                  </tr>\r\n                </table>\r\n              </div>\r\n\r\n              <div class=\"card p-3 m-0 text-left mb-5\">\r\n                <div class=\"chart-wrapper\" style=\"height: 250px\">\r\n                  <v-chart :options=\"echartPie\" :autoresize=\"true\"></v-chart>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"my-3 px-3\" style=\"display: flex;flex-direction: column;gap: 16px;\" v-if=\"filter.vfc\">\r\n              <h5 class=\"pb-0\">FARMERS LIST ({{farmers_groups.length}}):</h5>\r\n              <div v-for=\"farmers_group in farmers_groups\">\r\n                <div class=\" borderless-card\">\r\n                  <div class=\"d-flex justify-content-between align-items-center  px-0 py-1\">\r\n                    <h4 class=\"font-weight-700\">\r\n                      {{farmers_group.name}}\r\n                    </h4>\r\n                    <h4 class=\"font-weight-700 px-4 py-1 \" style=\"margin-right:-22px;white-space:nowrap;\">\r\n                      <span class=\"badge badge-success\">{{farmers_group.farmer_code}}</span>\r\n                    </h4>\r\n                  </div>\r\n                  <div class=\"hr-min\"></div>\r\n                  <div class=\"d-flex justify-content-between px-0 py-1 \">\r\n                    <h6 class=\" font-weight-500 text-light\">Estimate Qty: </h6>\r\n                    <h6 class=\"m-0 font-weight-700 pl-2 text-light \"><b>{{farmers_group.total_estimate}}KG</b></h6>\r\n                  </div>\r\n                 \r\n                  <div class=\"d-flex justify-content-between px-0 py-1 \">\r\n                    <h6 class=\" font-weight-500\">Procured Qty: </h6>\r\n                    <h6 class=\"m-0 font-weight-700\">{{farmers_group.total_procured}}KG</h6>\r\n                  </div>\r\n                  <div class=\"d-flex justify-content-between px-0 py-1 \">\r\n                    <h6 class=\" font-weight-500 text-light\">Estimate Qty Balance: </h6>\r\n                    <h6 class=\"m-0 font-weight-700 pl-2 text-light \"><b>{{farmers_group.balance_procurement}}KG</b></h6>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>  \r\n\r\n            <div class=\"my-3 px-3\" style=\"display: flex;flex-direction: column;gap: 16px;\" v-else-if=\"filter.ics || filter.panchayath\">\r\n              <h5 class=\"pb-0\">VFC LIST ({{vfc_groups.length}}):</h5>\r\n              <div v-for=\"vfc_group in vfc_groups\">\r\n                <div class=\" borderless-card\" @click=\"selectVfc(vfc_group)\">\r\n                  <div class=\"d-flex justify-content-between align-items-center  px-0 py-1\">\r\n                    <h4 class=\"font-weight-700\">\r\n                      {{vfc_group.name}}\r\n                    </h4>\r\n                    <h4 class=\"font-weight-700 px-4 py-1 \" style=\"margin-right:-22px;white-space:nowrap;\">{{vfc_group.total_farmers}}Farmer{{checkMult(vfc_group.total_farmers)}}</h4>\r\n                  </div>\r\n                  <div class=\"hr-min\"></div>\r\n                  <div class=\"d-flex justify-content-between px-0 py-1 \">\r\n                    <h6 class=\" font-weight-500 text-light\">Estimate Qty: </h6>\r\n                    <h6 class=\"m-0 font-weight-700 pl-2 text-light\"><b>{{vfc_group.total_estimate}}KG</b></h6>\r\n                  </div>\r\n                 \r\n                  <div class=\"d-flex justify-content-between px-0 py-1 \">\r\n                    <h6 class=\" font-weight-500\">Procured Qty: </h6>\r\n                    <h6 class=\"m-0 font-weight-700 pl-2 text-light\">{{vfc_group.total_procured}}KG</h6>\r\n                  </div>\r\n                  <div class=\"d-flex justify-content-between px-0 py-1 \">\r\n                    <h6 class=\" font-weight-500 text-light\">Estimate Qty Balance: </h6>\r\n                    <h6 class=\"m-0 font-weight-700 pl-2 text-light \"><b>{{vfc_group.balance_procurement}}KG</b></h6>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>  \r\n            \r\n          </div>  \r\n        </div>   \r\n              \r\n          \r\n      </div>\r\n      <div class=\"filter-footer\">\r\n        <div class=\"footer-item2 d-flex align-items-center\" @click=\"$router.push('/')\">\r\n          <img src=\"/images/home.png\" class=\"\" width=\"25\">\r\n          <h5 class=\"m-0 pl-2 text-white\">Home</h5>\r\n        </div>\r\n        <div class=\"footer-item2 filter-icon d-flex align-items-center\"  @click=\"selectFilter()\" v-b-modal.filter-data>\r\n         \r\n            <img src=\"/images/settings.png\" class=\"\" width=\"25\">\r\n            <div class=\"filter-number\" v-if=\"filter.count>0\">{{filter.count}}</div>\r\n            <h5 class=\"m-0 pl-2 text-white\">Filter</h5>\r\n         \r\n        </div>\r\n      </div>\r\n\r\n\r\n    </main>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport {echartPie} from \"@/data/echarts\";\r\nimport ProductFields from '/src/views/app/pages/product_fields';\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Home\"\r\n  },\r\n  components:{\r\n    ProductFields,\r\n  },\r\n  data() {\r\n    return {\r\n      echartPie,\r\n      input_col:2,\r\n      product_name:'',\r\n      product_id:null,\r\n      inspection_year:2022,\r\n      procurement_year:2022,\r\n      product_status:'Organic',\r\n      reports:{success:false},\r\n      filter:{ics:null,panchayath:null,vfc:null,count:0},\r\n      vfc_groups:[],\r\n      farmers_groups:[],\r\n      load_products:false,\r\n      lock_btn:false,\r\n    };\r\n  },\r\n  mounted(){\r\n    this.inspection_year = this.this_year-1\r\n    this.procurement_year = this.this_year\r\n    if(localStorage.getItem(\"basic_report_filter\")){\r\n      this.filter = JSON.parse(localStorage[\"basic_report_filter\"])\r\n      this.selectFilter()\r\n    }\r\n    this.load_products = true\r\n    this.$bvModal.show('filter-product')\r\n  },\r\n  methods: {\r\n    backSection(){\r\n      if(this.filter.vfc){\r\n        this.filter.vfc = null\r\n        this.filterData()\r\n      }else if(this.filter.ics || this.filter.panchayath){\r\n        this.filter.ics = null\r\n        this.filter.panchayath = null\r\n        this.filterData()\r\n      }else{\r\n        this.$router.push('/')\r\n      }\r\n    },\r\n    nextCol(){\r\n      var self = this\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      setTimeout(function(){\r\n        self.lock_btn = false\r\n        var is_input = document.getElementsByClassName('check_field_'+self.input_col)\r\n        var next = true\r\n        for(var i=0;i<is_input.length;i++){\r\n          is_input[i].style.border = 'none'\r\n          if(is_input[i].value==''){\r\n            is_input[i].style.border = '1px red solid'\r\n            next = false\r\n          }\r\n        }\r\n        if(next){\r\n          self.input_col++\r\n          if(self.input_col>6){\r\n            self.input_col = 2\r\n            self.filterData()\r\n            self.$bvModal.hide('filter-product')\r\n          }else{\r\n            // this.product_name = ''\r\n          }\r\n        }else{\r\n          self.showAlert('Please select a coloumn!')\r\n        }\r\n      },300)\r\n    },\r\n    backCol(){\r\n      this.input_col--\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.input_col=4\r\n      this.nextCol()\r\n      this.getProduct()\r\n    },\r\n    getProduct() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/products/'+this.product_id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_name = jsonData.name+' ('+jsonData.product_type_detail.name+')'\r\n      })\r\n    },\r\n    selectVfc(vfc_group){\r\n      this.filter.ics=vfc_group.ics;\r\n      this.filter.panchayath=vfc_group.panchayath;\r\n      this.filter.vfc=vfc_group.id;\r\n      this.filterData()\r\n    },\r\n    selectFilter(){\r\n      this.getTypes()\r\n      this.getIcs()\r\n      if(this.filter.ics){\r\n        this.changeICS()\r\n      }\r\n    },\r\n    cancelFilter(){\r\n      this.reports={success:false}\r\n      this.$bvModal.hide('filter-data');\r\n      this.filter={ics:null,panchayath:null,vfc:null,count:0}\r\n      localStorage.removeItem(\"basic_report_filter\");\r\n      if(this.product_id){\r\n        this.filterData()\r\n      }\r\n    },\r\n    calcFilterCount(){\r\n      var count = 0\r\n      if(this.filter.ics){\r\n        count++\r\n      }\r\n      if(this.filter.panchayath){\r\n        count++\r\n      }\r\n      if(this.filter.vfc){\r\n        count++\r\n      }\r\n      this.filter.count = count\r\n      localStorage.setItem(\"basic_report_filter\", JSON.stringify(this.filter));\r\n      this.$bvModal.hide('filter-data')\r\n    },\r\n    filterData() {\r\n      this.reports = {success:false}\r\n      this.calcFilterCount()\r\n      var formdata = new FormData()\r\n      formdata.append('fpo',this.$root.userInfo.fpo);\r\n      if(this.filter.ics)formdata.append('ics',this.filter.ics);\r\n      if(this.filter.panchayath)formdata.append('panchayath',this.filter.panchayath);\r\n      if(this.filter.vfc)formdata.append('vfc',this.filter.vfc);\r\n      formdata.append('product',this.product_id)\r\n      formdata.append('inspection_year',this.inspection_year)\r\n      formdata.append('procurement_year',this.procurement_year)\r\n      formdata.append('product_status',this.product_status)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/aeo_report/', {\r\n          method : 'post',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.reports = jsonData\r\n          this.vfc_groups = jsonData.vfc_group_datas\r\n          this.farmers_groups = jsonData.farmers_group_datas\r\n          this.setEchartPie()\r\n        }else{\r\n          this.showAlert('no data found!')\r\n        }\r\n      })\r\n    },\r\n    setEchartPie(){\r\n      this.echartPie.color = [this.reports.data[0].color,this.reports.data[1].color]\r\n      this.echartPie.series[0].data = this.reports.data\r\n      this.echartPie.series[0].name = 'Procurement'\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n  \r\n</style>\r\n"]}]}