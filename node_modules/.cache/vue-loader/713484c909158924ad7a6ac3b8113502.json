idth:120px;\"> Mobile Number: </span>\r\n                              <input placeholder=\"Mobile\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.mobile\" name=\"mobile\" required>\r\n                            </div>\r\n                             <div class=\"o_thread_search_input\">\r\n                                  <span style=\"width:120px;\"> Department: </span>\r\n                                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"department\">\r\n                                      <option selected disabled>Select a department</option>\r\n                                      <option v-for=\"department in departments\" :value=\"department.id\" :selected=\"department.id==form_data.department\" @click=\"job_positions=department.job_positions\">{{department.name}}</option>\r\n                                  </select>\r\n                             </div>\r\n                             <div class=\"o_thread_search_input\">\r\n                                  <span style=\"width:120px;\"> Job Position: </span>\r\n                                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"job_position\">\r\n                                      <option v-for=\"job_position in job_positions\" :value=\"job_position.id\" :selected=\"job_position.id==form_data.job_position\">{{job_position.name}}</option>\r\n                                  </select>\r\n                             </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Job Title: </span>\r\n                              <input placeholder=\"Job Title\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.job_title\" name=\"job_title\" required>\r\n                            </div>\r\n                             <div class=\"o_thread_search_input\">\r\n                                  <span style=\"width:120px;\"> Manager: </span>\r\n                                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"manager\">\r\n                                      <option v-for=\"manager in employees\" :value=\"manager.id\" :selected=\"manager.id==form_data.manager\">{{manager.name}}</option>\r\n                                  </select>\r\n                             </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Information: </span>\r\n                              <textarea placeholder=\"Write something\" class=\"ui-autocomplete-input w-100 mx-1\" autocomplete=\"off\" name=\"information\" required>{{form_data.information}}</textarea>\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport LocationFields from '/src/views/app/oodo/components/location_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    LocationFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:[],\r\n      id:null,\r\n      action:'create',\r\n      types:[],\r\n      departments:[],\r\n      job_positions:[],\r\n      employees:[],\r\n      load_locations:false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'employee'\r\n    this.getTypes()\r\n    this.getEmployees()\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_locations = true\r\n      this.getDepartments()\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getTypes() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/types/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.types = jsonData\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.getDepartments()\r\n        this.load_locations = true\r\n      })\r\n    },\r\n    getDepartments() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/jobs/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.departments = jsonData\r\n        var self= this\r\n        this.departments.forEach(function(val,key){\r\n          if(val.id==self.form_data.department){\r\n            self.job_positions = val.job_positions\r\n          }\r\n        })\r\n      })\r\n    },\r\n    getEmployees() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.employees = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/employees/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}