\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">QC</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/qc/normal-materials-test\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n          <h5></h5>\r\n        </b-col>\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">WareHouse  : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"warehouse\" v-model=\"form_data.warehouse\">\r\n                                  <option v-for=\"warehouse in warehouses\" :value=\"warehouse.id\">{{warehouse.name}}</option>\r\n                              </select>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Purchase Type  : </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"purchase_type\" v-model=\"form_data.purchase_type\">\r\n                                  <option v-for=\"purchase_type in purchase_types\" :value=\"purchase_type.id\">{{purchase_type.name}}</option>\r\n                              </select>\r\n                            </div>\r\n\r\n                            <div class=\"o_thread_search_input mt-2\">\r\n                              <span style=\"width:120px;\"> Normal ID: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"normal_procurement\" v-model=\"form_data.normal_procurement\" @change=\"getQCFarmers()\">\r\n                                <option value=\"\" selected disabled>Select normal id</option>\r\n                                <option v-for=\"normal in normals\" :value=\"normal.id\">{{normal.id}}/{{normal.product_detail.name}}/{{normal.date}}</option>\r\n                              </select>\r\n                            </div>\r\n                            <ProductFields :product_id=\"product_id\" :readonly=\"true\" v-if=\"load_products\"></ProductFields>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Receiving Date: </span>\r\n                              <input placeholder=\"Date\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.receiving_date\" name=\"receiving_date\" required readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Type  : </span>\r\n                              <input placeholder=\"Type\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.type\" name=\"type\" readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Total Weight : </span>\r\n                              <input placeholder=\"\" type=\"number\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_weight\" name=\"total_weight\" required>\r\n                            </div>\r\n\r\n                            <div class=\"o_thread_search_input mt-4\">\r\n                              <span style=\"width:120px;\"> Status: </span>\r\n                                <input type=\"text\" class=\"ui-autocomplete-input \" autocomplete=\"off\" v-model=\"form_data.status\" name=\"status\" readonly style=\"width:100%\">\r\n                                <!-- <button type=\"button\" class=\"btn btn-success btn-sm ml-4\" @click=\"form_data.status='Approved'\">Approve</button>\r\n                                <button type=\"button\" class=\"btn btn-danger btn-sm ml-2\" @click=\"form_data.status='Reject'\">Reject</button> -->\r\n                            </div>\r\n                        <!--     <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Organic Status: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"organic_status\" v-model=\"form_data.organic_status\">\r\n                                  <option v-for=\"organic_status in organic_statuses\" :value=\"organic_status\">{{organic_status}}</option>\r\n                              </select>\r\n                            </div> -->\r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        \r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n          <h5>Farmer Lines</h5>\r\n        </b-col>\r\n\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mb-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-12 px-2\">\r\n                          <table class=\"table\">\r\n                            <thead>\r\n                              <tr>\r\n                                <th>Farmer Code</th>\r\n                                <th>Farmer</th>\r\n                               <!--  <th>Estimate QTY (KG)</th>\r\n                                <th>Estimate QTY Balance (KG)</th> -->\r\n                                <th>Procure Qty (KG)</th>\r\n                                <th>Cutting (%)</th>\r\n                                <th>Qc Status</th>\r\n                                <th>Action</th>\r\n                              </tr>\r\n                            </thead>\r\n                            <tbody v-if=\"product_id\">\r\n                              <tr v-for=\"(line,key) in lines\">\r\n                                 <td>\r\n                                  <input type=\"text\" placeholder=\"Search (Farmer code)\" class=\"\" autocomplete=\"off\" v-model=\"line.farmer_detail.farmer_code\" required readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <input type=\"text\" placeholder=\"Farmer name\" class=\"\" autocomplete=\"off\" :value=\"line.farmer_detail.name\" readonly required>\r\n                                  <input type=\"hidden\" :name=\"'farmer'+key\" :value=\"line.farmer\">\r\n                                </td>\r\n                                <!-- <td>\r\n                                  <input placeholder=\"0\" type=\"text\" class=\"\" autocomplete=\"off\" :name=\"'estimate_qty'+key\" v-model=\"line.estimate_qty\" readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"0\" type=\"text\" class=\"\" autocomplete=\"off\" :name=\"'estimate_qty_balance'+key\" v-model=\"line.estimate_qty_balance\" readonly>\r\n                                </td> -->\r\n                                <td>\r\n                                  <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" :name=\"'procure_qty'+key\" v-model=\"line.procure_qty\" readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\" \" type=\"number\" class=\"\" autocomplete=\"off\" :name=\"'cutting'+key\" v-model=\"line.cutting\" required>\r\n                                </td>\r\n                                <td>\r\n                                  <input placeholder=\"\" type=\"text\" class=\"\" autocomplete=\"off\" :value=\"line.qc_status\" :name=\"'qc_status'+key\" readonly>\r\n                                </td>\r\n                                <td>\r\n                                  <button type=\"button\" class=\"btn btn-success btn-sm px-3 py-1 ml-4\" @click=\"selectAction(line,key)\">Action</button>\r\n                                  <!-- <button type=\"button\" class=\"btn btn-danger btn-sm px-3 py-1 ml-2\" @click=\"line.qc_status='Reject'\">Reject</button> -->\r\n                                  <input placeholder=\"0\" type=\"hidden\" class=\"\" autocomplete=\"off\" :name=\"'estimate_qty'+key\" v-model=\"line.estimate_qty\" readonly>\r\n                                  <input placeholder=\"0\" type=\"hidden\" class=\"\" autocomplete=\"off\" :name=\"'estimate_qty_balance'+key\" v-model=\"line.estimate_qty_balance\" readonly>\r\n\r\n                                  <b-modal :id=\"'select-params'+key\" hide-header  hide-footer>\r\n                                    <div>\r\n                                       <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                                        <slot name=\"header\">\r\n                                           <h5 class=\"mb-0\"><span class=\"text-success\">{{line.farmer_detail.name}}</span> / Action</h5>\r\n                                        </slot>\r\n                                          <div class=\"d-flex\">\r\n                                             <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                                              >Save\r\n                                            </b-button>\r\n                                            <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('select-params'+key)\">\r\n                                              Discard\r\n                                            </b-button>\r\n                                          </div>\r\n                                        </div>\r\n                                        <div class=\" my-3 border-top\"></div>\r\n                                    </div>\r\n                                    <!-- <div class=\"wrapper\"> -->\r\n                                    <b-row class=\"main-scroll\">\r\n                                      <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n                                        <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                                          <div>\r\n                                            <div class=\"ul-contact-detail__timeline\">\r\n                                               <div class=\"\">\r\n                                                  <div class=\"user-info pt-2\">\r\n\r\n                                                      <div class=\"o_thread_search_input mt-2\">\r\n                                                        <span style=\"width:120px;\"> Cutting: </span>\r\n                                                        <input placeholder=\" \" type=\"number\" class=\"\" autocomplete=\"off\" :name=\"'cutting'+key\" v-model=\"line.cutting\" required>\r\n                                                      </div>\r\n\r\n                                                      <div class=\"o_thread_search_input\"  v-if=\"line.parameters\">\r\n                                                        <span style=\"width:100px;\"> Parameters: </span>\r\n\r\n                                                        <div class=\"o_composer_container my-2 row mx-0\">\r\n                                                          <div class=\" col-md-6\" v-for=\"data in line.parameters.datas\">\r\n                                                            <div class=\"user-info pt-2\" v-for=\"param in data\">\r\n                                                              <div class=\"o_thread_search_input mt-2\">\r\n                                                                <span style=\"width:100px;\"> {{param.name}} : </span>\r\n                                                                <span>\r\n                                                                  <input type=\"checkbox\" class=\"ui-autocomplete-input mx-2\" autocomplete=\"off\" value=\"1\" v-model=\"param.checked\" :name=\"param.name\">\r\n                                                                  <input placeholder=\"%\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input w-50\" autocomplete=\"off\" v-model=\"param.limit\" :name=\"param.name+'limit'\" v-if=\"param.checked\" @input=\"checkParam(param)\" required>\r\n                                                                </span>\r\n                                                              </div>\r\n                                                            </div>\r\n                                                          </div>\r\n                                                          \r\n                                                        </div>\r\n                                                      </div> \r\n                                                  \r\n                                                  </div>\r\n                                                </div>\r\n                                            </div>\r\n                                          </div>\r\n                                        </b-card>\r\n                                      </b-col>\r\n                                    </b-row>\r\n                                  </b-modal>\r\n                                </td>\r\n                              </tr>\r\n                              <tr>\r\n                                <th>Total</th>\r\n                                <td>{{form_data.total_farmers}}</td>\r\n                                <td>{{form_data.total_available_qty}}</td>\r\n                                <td colspan=\"3\">\r\n                                  <input type=\"hidden\" name=\"keys\" :value=\"lines.length\">\r\n                                  <input placeholder=\"0\" type=\"hidden\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_estimate_qty\" name=\"total_estimate_qty\" readonly>\r\n                                  <input placeholder=\"0\" type=\"hidden\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.total_available_qty\" name=\"total_available_qty\" readonly>\r\n                                </td>\r\n                              </tr>\r\n                            </tbody>\r\n                          </table>\r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{receiving_date:new Date(),type:'Raw Material',total_weight:0,status:'Reached Factory'},\r\n      id:null,\r\n      load_products:false,\r\n      lines:0,\r\n      product_id:null,\r\n      action:'create',\r\n      normals:[],\r\n      panchayaths:[],\r\n      vfcs:[],\r\n      farmers:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'qc'\r\n    this.form_data.receiving_date = this.currentDate()\r\n    this.action = this.$route.params.action\r\n    this.getWarehouses()\r\n    this.getPurchaseTypes()\r\n    this.getNormals()\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      // this.getParameters()\r\n    }\r\n\r\n  },\r\n  methods: {\r\n    getQCFarmers(){\r\n      var self = this\r\n      var normal_id = this.form_data.normal_procurement\r\n      this.normals.forEach(function(val,key){\r\n        if(normal_id==val.id)\r\n        {\r\n          self.lines = val.farmer_lines\r\n          self.product_id = val.product\r\n          self.load_products = true\r\n          self.calcultion()\r\n        }\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/raw_material/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_id = jsonData.product\r\n        this.form_data = jsonData\r\n        this.lines = jsonData.normal_procurement_detail.farmer_lines\r\n        this.load_products = true\r\n        this.calcultion()\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      if(!formdata.has('product')){\r\n        formdata.append('product',this.product_id)\r\n      }\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/raw_material/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.updateFarmerFormdata()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      if(!formdata.has('product')){\r\n        formdata.append('product',this.product_id)\r\n      }\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/raw_material/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.updateFarmerFormdata()\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFarmerFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/'+this.form_data.normal_procurement+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    getNormals() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.normals = jsonData\r\n      })\r\n    },\r\n    selectAction(line,key){\r\n      if(line.parameters && line.parameters.length>0){\r\n        var jparameters = JSON.parse(line.parameters)\r\n        var length = jparameters.length/3\r\n        line.parameters = this.pagination(jparameters,Math.round(length))\r\n      }else{\r\n        line.parameters = {datas:[]}\r\n        this.getParameters(line)\r\n      }\r\n      this.$bvModal.show('select-params'+key);\r\n    },\r\n    getParameters(line) {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/physical_parameters/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        var length = jsonData.length/2\r\n        line.parameters = this.pagination(jsonData,Math.round(length))\r\n      })\r\n    },\r\n    calcultion(){\r\n      var self =this\r\n      var total_farmers = []\r\n      var total_available_qty = 0\r\n      var total_estimate_qty = 0\r\n      this.lines.forEach(function(val,key){\r\n        if(!total_farmers.includes(val.farmer) && val.farmer){\r\n          total_farmers.push(val.farmer)\r\n        }\r\n        total_available_qty = total_available_qty + parseFloat(val.procure_qty)\r\n        total_estimate_qty = total_estimate_qty + parseFloat(val.estimate_qty)\r\n      })\r\n      this.form_data.total_farmers = total_farmers.length\r\n      this.form_data.total_available_qty = total_available_qty\r\n      this.form_data.total_weight = total_available_qty\r\n      this.form_data.total_estimate_qty = total_estimate_qty\r\n\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n#select-params.modal.show .modal-dialog{\r\n  min-width:800px!important;\r\n}\r\n\r\n</style>\r\n"]}]}