odel=\"form_data.brand\" class=\"ui-autocomplete-input w-100 mx-1\" required>\r\n                            \r\n                          </div>\r\n                          <div class=\"o_thread_search_input mb-3\">\r\n                            <span style=\"width:90px;\"> Dpartment: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"department\" v-model=\"form_data.department\" @change=\"getProcessingDetails()\">\r\n                              <option v-for=\"category in categories\" :value=\"category.id\"> {{category.name}} Department</option>\r\n                            </select>\r\n                          </div>\r\n                        </div>\r\n                        <div class=\"col-md-5\">\r\n                          <div class=\"o_thread_search_input mb-3\">\r\n                            <span style=\"width:90px;\"> Power(HP): </span>\r\n                            <input placeholder=\"Machine Power\" step=\"0.1\" type=\"number\" name=\"power\" v-model=\"form_data.power\" class=\"ui-autocomplete-input w-100 mx-1\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mb-3\">\r\n                            <span style=\"width:90px;\"> Energy(KW): </span>\r\n                            <input placeholder=\"Machine Energy\" type=\"number\" step=\"0.1\" name=\"energy\" v-model=\"form_data.energy\" class=\"ui-autocomplete-input w-100 mx-1\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input mb-3\">\r\n                            <span style=\"width:90px;\"> Capacity(KG/HR): </span>\r\n                            <input placeholder=\"Capacity\" type=\"number\" name=\"capacity\" v-model=\"form_data.capacity\" class=\"ui-autocomplete-input w-100 mx-1\" required>\r\n                          </div>\r\n                          \r\n                          <div class=\"o_thread_search_input col-md-3 mb-3\">\r\n                            <span style=\"width:90px;\"> Status: </span>\r\n                            <input type=\"checkbox\" name=\"status\" v-model=\"form_data.status\" class=\"ui-autocomplete-input mr-2\" value=\"1\"> {{form_data.status?'Active':'Deactive'}}\r\n                            \r\n                          </div>\r\n                        </div>\r\n                        <div class=\"col-md-12\">\r\n                          <hr class=\"my-0\">\r\n                          <div class=\"o_thread_search_input my-3\">\r\n                            <span style=\"width:90px;\"> Process: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.process_detail\" name=\"process_detail\" multiple>\r\n                              <option v-for=\"process in processes\" :value=\"process.id\">{{process.production_process_detail.main_name}}> {{process.production_process_detail.sub_name}}> {{process.name}}</option>\r\n                            </select>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input col-md-3\">\r\n                            <span style=\"width:90px;\"> Note Stock: </span>\r\n                            <input type=\"checkbox\" name=\"note_stock\" v-model=\"form_data.note_stock\" class=\"ui-autocomplete-input mr-2\" value=\"1\"> {{form_data.note_stock?'Yes':'No'}}\r\n                          </div>\r\n                          <div v-if=\"form_data.note_stock\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Consumable items: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" v-model=\"form_data.consumable_items\" name=\"consumable_items\" multiple>\r\n                                <option v-for=\"consumable in consumables\" :value=\"consumable.id\">{{consumable.name}} ({{consumable.unit}})</option>\r\n                              </select>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input mb-3\">\r\n                              <span style=\"width:120px;\"> Max Quantity (L): </span>\r\n                              <input placeholder=\"Max Quantity\" type=\"number\" step=\"0.1\" name=\"max_quantity\" v-model=\"form_data.max_quantity\" class=\"ui-autocomplete-input w-100 mx-1\" required>\r\n                            </div>\r\n                          </div>\r\n                        </div>\r\n                     </div>\r\n\r\n                  </div>\r\n\r\n                </div>\r\n              </div>\r\n              <hr>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"5\" xl=\"5\" class=\"mb-3\">\r\n          <h5>Machine Motors</h5>\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll my-2\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                    <table class=\"table\">\r\n                      <thead>\r\n                        <tr>\r\n                          <th class=\"border-top-0\">#</th>\r\n                          <th class=\"border-top-0\">Name</th>\r\n                          <th class=\"border-top-0\">Number</th>\r\n                          <th class=\"border-top-0\">Power(HP)</th>\r\n                          <th class=\"border-top-0\">Energy(KW)</th>\r\n                          <th class=\"border-top-0\">Action</th>\r\n                        </tr>\r\n                      </thead>\r\n                      <tbody>\r\n                        <tr v-for=\"(motor,key) in form_data.motors\">\r\n                          <td>{{key+1}}</td>\r\n                          <td>{{motor.name}}</td>\r\n                          <td>{{motor.motor_number}}</td>\r\n                          <td>{{motor.power}}</td>\r\n                          <td>{{motor.energy}}</td>\r\n                          <td>\r\n                            <b-button variant=\"danger\" type=\"button\" @click=\"addMotor(motor)\" class=\" px-3 py-1  d-sm-block mx-1\">Edit</b-button>\r\n                          </td>\r\n                        </tr>\r\n                      </tbody>\r\n                    </table>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n\r\n    <div>\r\n      <b-modal id=\"motor-form\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n          <div class=\"main-content create-main\">\r\n            <form ref=\"motorFormdata\" @submit.prevent=\"motorFormdata()\">\r\n              <div>\r\n                <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                    <slot name=\"header\">\r\n                       <h5 class=\"mb-0\"><span class=\"text-success\">Machine Motor</span> / {{current_motor.id?'Edit':'Add'}}</h5>\r\n                          \r\n                    </slot>\r\n                    <div class=\"d-flex\">\r\n                      <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\">Save\r\n                      </b-button>\r\n                      <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('motor-form')\"\r\n                        >Discard\r\n                      </b-button>\r\n                    </div>\r\n                   \r\n                </div>\r\n                <div class=\" my-3 border-top\"></div>\r\n              </div>\r\n\r\n              <b-row class=\"main-scroll\">\r\n                 <b-col lg=\"12\" xl=\"12\" class=\"my-3\">\r\n                  <div>\r\n                    <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:90px;\"> Name: </span>\r\n                        <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" v-model=\"current_motor.name\" name=\"name\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:90px;\"> Motor Number: </span>\r\n                        <input placeholder=\"Motor Number\" type=\"text\" class=\"ui-autocomplete-input\" v-model=\"current_motor.motor_number\" name=\"motor_number\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:90px;\"> Power (HP): </span>\r\n                        <input placeholder=\"Power\" type=\"number\" class=\"ui-autocomplete-input\" v-model=\"current_motor.power\" step=\"0.1\" name=\"power\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:90px;\"> Energy (KW): </span>\r\n                        <input placeholder=\"Energy\" type=\"number\" class=\"ui-autocomplete-input\" v-model=\"current_motor.energy\" step=\"0.1\" name=\"energy\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input mt-2\">\r\n                        <b style=\"width:96px\"> Status: </b>\r\n                        <b-form-checkbox v-model=\"current_motor.status\" name=\"status\" switch>\r\n                          <b>{{ current_motor.status?'Active':'Deactive' }}</b>\r\n                        </b-form-checkbox>\r\n                      </div>\r\n                    </b-card>\r\n                  </div>\r\n                </b-col>\r\n              </b-row>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"PRODUCTION - Machines\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{status:true, category:null,motors:[],process_detail:[],consumable_items:[]},\r\n      current_motor:{},\r\n      id:null,\r\n      action:'create',\r\n      employees:[],\r\n      categories:[],\r\n      processes:[],\r\n      consumables:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n    this.getCategories()\r\n\r\n  },\r\n  methods: {\r\n    getCategories() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/categories/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.categories = jsonData\r\n      })\r\n    },\r\n    getProcessingDetails() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/list_process/?category='+this.form_data.department, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.processes = jsonData\r\n        this.getConsumables()\r\n      })\r\n    },\r\n    getConsumables() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/other_items/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.consumables = jsonData\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/machines/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.getProcessingDetails()\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/machines/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/machines/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    addMotor(motor={status:true}){\r\n      this.current_motor = motor\r\n      this.$bvModal.show('motor-form')\r\n    },\r\n    motorFormdata(method='POST') {\r\n      var param = ''\r\n      if(this.current_motor.id){\r\n        param = this.current_motor.id+'/'\r\n        method = 'PUT'\r\n      }\r\n      var formdata = new FormData(this.$refs['motorFormdata'])\r\n      formdata.append('machine',this.form_data.id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/machine_mototrs/'+param, {\r\n          method : method,\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.getForm()\r\n          this.$bvModal.hide('motor-form')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}