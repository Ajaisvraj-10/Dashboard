               \r\n            </slot>\r\n            <div class=\"d-flex\">\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: true,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: false,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows.results\"\r\n      >\r\n        <div slot=\"table-actions\" class=\"mb-3 filter-but\">\r\n\r\n        </div>\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <b-button  variant=\"primary\" class=\" px-3 py-1 mx-1\" @click=\"selectMaterial(props.row)\">Select Vehicle</b-button>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'normal_id'\">\r\n            {{props.row.raw_number}} \r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'product'\">\r\n            {{props.row.category}}> {{props.row.product}}{{props.row.parameter?'> '+props.row.parameter:''}}\r\n            <sup>\r\n              <span class=\"badge badge-success\" v-if=\"props.row.farmer_status=='Organic'\">ORG</span>\r\n            </sup>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'total_procured_qty'\">\r\n            {{props.row.total_procured_qty}}KG\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'date'\">\r\n            {{dateFormate(props.row.created_on,true)}}\r\n          </span>\r\n        </template>\r\n      </vue-good-table>\r\n      <TablePagination :page=\"page\" :rows=\"rows\" :load_first=\"load_first\" @changePage=\"changePage\" v-if=\"rows.count>0\"></TablePagination>\r\n    </b-card>\r\n    <div>\r\n      <b-modal id=\"choose-vehicle\" hide-header  hide-footer>\r\n        <form ref=\"chooseVehicle\" @submit.prevent=\"chooseVehicle()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Choose Vehicle</span></h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.show('create-vehicle')\">Create new vehicle\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('choose-vehicle')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <table class=\"table\">\r\n                            <thead>\r\n                              <tr>\r\n                                <th>Vehicle Number</th>\r\n                                <th>Name</th>\r\n                                <th>Contact</th>\r\n                                <th>Vehicle Type</th>\r\n                                <th>Action</th>\r\n                              </tr>\r\n                            </thead>\r\n                            <tbody>\r\n                              <tr v-for=\"(vehicle,key) in vehicles\">\r\n                                 <td>\r\n                                  {{vehicle.vehicle_number}}\r\n                                </td>\r\n                                <td>\r\n                                  {{vehicle.name}}\r\n                                </td>\r\n                                <td>\r\n                                  {{vehicle.contact}}\r\n                                </td>\r\n                                <td>\r\n                                  {{vehicle.vehicle_type}}\r\n                                </td>\r\n                                <td>\r\n                                  <button type=\"button\" class=\"btn btn-primary btn-sm px-3 py-1 ml-4\" @click=\"chooseVehicle(vehicle)\">Select</button>\r\n                                </td>\r\n                              </tr>\r\n                            </tbody>\r\n                          </table>\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n\r\n      <b-modal id=\"create-vehicle\" hide-header  hide-footer>\r\n        <form ref=\"vehicleForm\" @submit.prevent=\"vehicleForm()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Vehicle</span> / Create</h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                   <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Save\r\n                  </b-button>\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('create-vehicle')\">\r\n                    Discard\r\n                  </b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <!-- <div class=\"wrapper\"> -->\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n                <div>\r\n                  <div class=\"ul-contact-detail__timeline\">\r\n                     <div class=\"\">\r\n                        <div class=\"user-info pt-2\">\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Vehicle Type : </span>\r\n                            <input placeholder=\"Vehicle Type\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.vehicle_type\" name=\"vehicle_type\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Driver Name: </span>\r\n                            <input placeholder=\"Driver Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.name\" name=\"name\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Driver Contact: </span>\r\n                            <input placeholder=\"Driver Contact\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.contact\" name=\"contact\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Vehicle No : </span>\r\n                            <input placeholder=\"Vehicle No\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.vehicle_number\" name=\"vehicle_number\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Origin of the Vehicle  : </span>\r\n                            <input placeholder=\"Origin of the Vehicle\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.origin_of_the_vehicle\" name=\"origin_of_the_vehicle\" required>\r\n                          </div>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <span style=\"width:120px;\">Warehouse: </span>\r\n                            <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"destination\" v-model=\"form_data.warehouse\">\r\n                              <option v-for=\"warehouse in warehouses\" :value=\"warehouse.id\">{{warehouse.name}}</option>\r\n                            </select>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                  </div>\r\n                </div>\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TablePagination from '/src/views/app/oodo/components/table_pagination'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"OUTSIDE PROCUREMENT\"\r\n  },\r\n  components:{\r\n    TablePagination\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Dispatch Id\",\r\n          field: \"normal_id\"\r\n        },\r\n        {\r\n          label: \"Product\",\r\n          field: \"product\"\r\n        },\r\n        {\r\n          label: \"Quantity (KG)\",\r\n          field: \"total_procured_qty\"\r\n        },\r\n        {\r\n          label: \"Vendor\",\r\n          field: \"vendor\"\r\n        },\r\n        {\r\n          label: \"Date\",\r\n          field: \"date\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: {results:[],count:0},\r\n      page:0,\r\n      load_first:true,\r\n      current_row:[],\r\n      form_data:{},\r\n      vehicles:[],\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'production'\r\n    this.getWarehouses()\r\n    this.changePage(1)\r\n  },\r\n  methods: {\r\n    changePage(count){\r\n      this.page = count\r\n      this.load_first = true\r\n      this.getRows()\r\n    },\r\n    getRows() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/normal_procured/?page='+this.page+'&purchase_type=2&status=Collecting', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.detail=='Invalid page.'){\r\n          this.rows = {results:[],count:0}\r\n        }else{\r\n          this.load_first = false\r\n          this.rows = jsonData\r\n        }\r\n      })\r\n    },\r\n    selectMaterial(row){\r\n      this.current_row = row\r\n      this.$bvModal.show('choose-vehicle')\r\n      this.getVehicles()\r\n    },\r\n    getVehicles() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/vehicle/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.vehicles = jsonData\r\n      })\r\n    },\r\n    vehicleForm() {\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var formdata = new FormData(this.$refs['vehicleForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/vehicle/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.getVehicles()\r\n          this.$bvModal.hide('create-vehicle')\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    chooseVehicle(vehicle){\r\n      this.$bvModal.hide('choose-vehicle')\r\n      var formdata = new FormData()\r\n      formdata.append('vehicle', vehicle.id)\r\n      formdata.append('normal_procurement_id', this.current_row.id)\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/attach_vehicle_to_normal/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.success){\r\n          this.getRows()\r\n          this.showAlert('Selected Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}