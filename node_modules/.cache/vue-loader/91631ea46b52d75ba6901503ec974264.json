span class=\"text-success\">Processing</span> / {{slugToWord(action)}}</h5>\r\n                  \r\n            </slot>\r\n            <div class=\"d-flex\">\r\n               <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\">Save\r\n              </b-button>\r\n              <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                @click=\"goBack()\">Discard\r\n              </b-button>\r\n              <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                <b-dropdown-header id=\"dropdown-header-label\">\r\n                    <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                    <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                    \r\n                   \r\n                </b-dropdown-header>\r\n              </b-dropdown>\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      \r\n      <b-row class=\"main-scroll\">\r\n\r\n        <b-col lg=\"12\" xl=\"12\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div class=\"row mt-2\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"user-info mb-2\">\r\n\r\n                  <table class=\"table\">\r\n                    <thead>\r\n                      <tr>\r\n                        <th>Parameter</th>\r\n                        <th>Quantity(Kg)</th>\r\n                        <th style=\"width:25%\">Direct Machine out</th>\r\n                        <th>Indent Based</th>\r\n                        <th>No:Bags (direct packing)</th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr v-for=\"process_output in form_data.process_outputs\" v-if=\"process_output.quantity!=0 && process_output.parameter_type!='empty'\">\r\n                        <td>\r\n                          {{process_output.parameter_detail.name}}\r\n                          <input type=\"hidden\" :name=\"'parameter_type'+process_output.id\" :value=\"process_output.parameter_type\">\r\n                          <input type=\"hidden\" :name=\"'process_output'+process_output.id\" :value=\"process_output.id\">\r\n                          <input type=\"hidden\" :name=\"'parameter'+process_output.id\" :value=\"process_output.parameter\">\r\n                        </td>\r\n                        <td>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <input :placeholder=\"process_output.parameter_detail.name\" type=\"number\" step=\"0.1\" class=\"ui-autocomplete-input ops\" autocomplete=\"off\" v-model=\"process_output.quantity\" :name=\"'quantity'+process_output.id\" readonly>\r\n                          </div>\r\n                        </td>\r\n                        <!-- <td>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <select class=\"ui-autocomplete-input\" v-model=\"process_output.packing_material\" :name=\"'packing_material'+process_output.id\" @change=\"changePackingMaterial(process_output)\">\r\n                              <option value=\"\" disabled>Select Material</option>\r\n                              <option value=\"\">No Material</option>\r\n                              <option :value=\"packing_material.id\" v-for=\"packing_material in packing_materials\">\r\n                                {{packing_material.material_detail.name}} \r\n                                (<span v-for=\"specification in packing_material.specifications\" v-if=\"specification.material_ids.includes(packing_material.material)\">\r\n                                  <b>{{specification.name}}</b>:{{specification.value}}/\r\n                                </span>)\r\n                              </option>\r\n                            </select>\r\n                            <input type=\"number\" :name=\"'direct_quantity'+process_output.id\" class=\"ui-autocomplete-input ops\" :value=\"process_output.direct_quantity\" readonly>\r\n                          </div>\r\n                        </td> -->\r\n                        <td>\r\n                          <button type=\"button\" class=\"btn btn-sm py-1 px-2 btn-primary\" @click=\"$bvModal.show('packing-form'+process_output.id)\">\r\n                            Direct Packing\r\n                          </button>\r\n                          <div v-for=\"packing_material in packing_materials\" v-if=\"packing_material.quantity>0\">\r\n                            <input type=\"hidden\" v-model=\"packing_material.packing_quantity\" :name=\"'packing_quantity'+packing_material.id+'process_output'+process_output.id\">\r\n                            <input type=\"hidden\" :name=\"'is_added'+packing_material.id+'process_output'+process_output.id\" :value=\"packing_material.is_added?'yes':'no'\">\r\n                            <input type=\"hidden\" :name=\"'number_of_bags'+packing_material.id+'process_output'+process_output.id\" :value=\"packing_material.number_of_bags\">\r\n                          </div>\r\n                          <b-modal class=\"packing-form\" :id=\"'packing-form'+process_output.id\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n                            <div>\r\n                              <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                                <slot name=\"header\">\r\n                                   <h5 class=\"mb-0\">Packing/<span class=\"text-success\">{{process_output.parameter_detail.name}}</span></h5>\r\n                                </slot>\r\n                                <div class=\"d-flex\">\r\n                                  <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('packing-form'+process_output.id)\">Save</b-button>\r\n                                </div>\r\n                              </div>\r\n                              <div class=\" my-3 border-top\"></div>\r\n                            </div>\r\n                            <b-row class=\"main-scroll\">\r\n                              <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n                                <b-card header-bg-variant=\"transparent\" class=\"mt-2\">\r\n                                  <div class=\"user-info pt-2\">\r\n                                    <div class=\"o_thread_search_input\">\r\n                                      <table class=\"table\">\r\n                                        <tr>\r\n                                          <th>Material</th>\r\n                                          <th>Specs</th>\r\n                                          <th>Lot</th>\r\n                                          <th>Quantity</th>\r\n                                          <th>Action</th>\r\n                                        </tr>\r\n                                        <tr v-for=\"packing_material in packing_materials\" :for=\"'packing_material'+packing_material.id\" v-if=\"packing_material.quantity>0\">\r\n                                          <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">{{packing_material.material_detail.name}}</td>\r\n                                          <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">\r\n                                            <small v-for=\"specification in packing_material.specifications\" v-if=\"specification.material_ids.includes(packing_material.material)\">\r\n                                              <b class=\"text-primary\"> {{specification.name}}</b>:{{specification.value}}, \r\n                                            </small>\r\n                                          </td>\r\n                                          <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">#Lot{{packing_material.packing_material_purchase}}</td>\r\n                                          <td :style=\"packing_material.is_added?'opacity:1':'opacity:0.5'\">\r\n                                            <input type=\"number\" class=\"ui-autocomplete-input w-100\" :max=\"packing_material.quantity\" v-model=\"packing_material.packing_quantity\" :disabled=\"!packing_material.is_added\" @change=\"changePackingMaterial(packing_material)\">\r\n                                            <input type=\"hidden\" :value=\"packing_material.is_added?'yes':'no'\">\r\n                                            <small v-if=\"packing_material.packing_quantity>0\">Number of bags: <b>{{packing_material.number_of_bags}}</b></small>\r\n                                            <input type=\"hidden\" :value=\"packing_material.number_of_bags\">\r\n                                          </td>\r\n                                          <td>\r\n                                            <button type=\"button\" class=\"btn btn-primary btn-sm px-3 py-1 ml-4\" @click=\"addProduct(packing_material)\" v-if=\"!packing_material.is_added\">Add</button>\r\n                                            <button type=\"button\" class=\"btn btn-danger btn-sm px-3 py-1 ml-4\" @click=\"removeProduct(packing_material)\" v-else>Remove</button>\r\n                                          </td>\r\n                                        </tr>\r\n                                      </table>\r\n                                    </div>\r\n                                  </div>\r\n                                </b-card>\r\n                              </b-col>\r\n                            </b-row>\r\n                          </b-modal>\r\n                        </td>\r\n                        <td>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <button type=\"button\" class=\"btn btn-sm py-1 px-2\" :class=\"process_output.indent?'btn-success':'btn-primary'\" @click=\"current_output=process_output;$bvModal.show('select-indent')\">{{process_output.indent?process_output.indent_detail.indent_number:'Select Indent'}}</button>\r\n                            <input type=\"number\" :name=\"'indirect_quantity'+process_output.id\" class=\"ui-autocomplete-input ops\" :value=\"process_output.indirect_quantity\" readonly v-if=\"process_output.indent\">\r\n                            <input type=\"hidden\" :name=\"'indent'+process_output.id\" :value=\"process_output.indent\">\r\n                            <input type=\"hidden\" :name=\"'packing_material_indent'+process_output.id\" :value=\"process_output.packing_material_indent\">\r\n                            <input type=\"hidden\" :name=\"'number_of_bags_indent'+process_output.id\" :value=\"process_output.number_of_bags_indent\">\r\n                          </div>\r\n                        </td>\r\n                        <td>\r\n                          <div class=\"o_thread_search_input\">\r\n                            <input placeholder=\"bags\" type=\"number\" class=\"ui-autocomplete-input ops\" autocomplete=\"off\" v-model=\"process_output.number_of_bags\" :name=\"'number_of_bags'+process_output.id\" required>\r\n                          </div>\r\n                        </td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n    <div>\r\n      <b-modal id=\"select-indent\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n          <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n            <slot name=\"header\">\r\n               <h5 class=\"mb-0\">Indent/<span class=\"text-success\">{{current_output.parameter_detail.name}}</span></h5>\r\n            </slot>\r\n            <div class=\"d-flex\">\r\n              <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"changePackingMaterial(current_output);$bvModal.hide('select-indent')\">Save</b-button>\r\n              <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"current_output.indent='';current_output.indent_detail={packing_materials_list:[]};changePackingMaterial(current_output);$bvModal.hide('select-indent')\">Remove</b-button>\r\n              <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('select-indent')\">Close</b-button>\r\n            </div>\r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n        </div>\r\n        <b-row class=\"main-scroll\">\r\n          <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n            <b-card header-bg-variant=\"transparent\" class=\"mt-2\">\r\n              <div class=\"user-info pt-2\">\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\">Indent : </span>\r\n                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"indent\" v-model=\"current_output.indent\" @change=\"selectIndent(current_output)\">\r\n                      <option v-for=\"indent in indents\" :value=\"indent.id\">{{indent.indent_number}}</option>\r\n                  </select>\r\n                </div>\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\">Packing Material : </span>\r\n                  <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"indent\" v-model=\"current_output.packing_material_indent\" @change=\"changePackingMaterial(current_output)\">\r\n                    <option v-for=\"packing_material in current_output.indent_detail.packing_materials_list\" :value=\"packing_material.id\">\r\n                      {{packing_material.material_detail.material_detail.name}}\r\n                      (<span v-for=\"specification in packing_material.material_detail.specifications\" v-if=\"specification.material_ids.includes(packing_material.material_detail.material)\">\r\n                        <b>{{specification.name}}</b>:{{specification.value}}/\r\n                      </span>)\r\n                    </option>\r\n                  </select>\r\n                </div>\r\n                <div class=\"o_thread_search_input mb-3\">\r\n                  <span style=\"width:120px;\"> Quantity: <br>(need :<b>{{current_output.indent_detail.quantity}}</b>)</span>\r\n                  <input placeholder=\"Quantity\" type=\"number\" v-model=\"current_output.indirect_quantity\" class=\"ui-autocomplete-input w-100 mx-1\" @input=\"changePackingMaterial(current_output)\" required>\r\n                </div>\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\"> No:Bags (indent based packing) </span>\r\n                  <input placeholder=\"bags\" type=\"number\" class=\"ui-autocomplete-input ops\" v-model=\"current_output.number_of_bags_indent\" readonly>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n          </b-col>\r\n        </b-row>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{\r\n        process_outputs:[],\r\n      },\r\n      id:null,\r\n      main_process_id:null,\r\n      packing_id:null,\r\n      action:'packing',\r\n      current_output:{parameter_detail:{},indent_detail:{packing_materials_list:[]}},\r\n      user_id:6,\r\n      indents:[],\r\n      packing_materials:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.main_process_id = this.$route.params.main_process_id\r\n    this.packing_id = this.$route.params.packing_id\r\n    this.checkPacking()\r\n    this.getIndents()\r\n    this.getPackingMaterials()\r\n  },\r\n  methods: {\r\n    checkPacking() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/get_process_outputs/?main_process='+this.main_process_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.form_data = jsonData\r\n        }else{\r\n          this.getProcessOutputs()\r\n        }\r\n      })\r\n    },\r\n    getProcessOutputs() {\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      formdata.append('main_process',this.main_process_id)\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/get_process_outputs/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        jsonData.forEach(function(val,key){\r\n            jsonData[key]['packing_material'] = ''\r\n            jsonData[key]['number_of_bags'] = 0\r\n            jsonData[key]['direct_quantity'] = 0\r\n            jsonData[key]['number_of_bags_indent'] = 0\r\n            jsonData[key]['indirect_quantity'] = 0\r\n            jsonData[key]['indent_detail'] = {packing_materials_list:[]}\r\n        })\r\n        this.form_data.process_outputs = jsonData\r\n      })\r\n    },\r\n    getIndents() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.indents = jsonData\r\n      })\r\n    },\r\n    getPackingMaterials(){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/packing_material_purchase_materials/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.packing_materials = jsonData\r\n      })\r\n    },\r\n    selectIndent(process_output){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/'+process_output.indent+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        process_output.indent_detail = jsonData\r\n      })\r\n    },\r\n    changePackingMaterial(process_output){\r\n      var self = this\r\n      var direct_packing = {max_quantity:0}\r\n      this.packing_materials.forEach(function(val,key){\r\n        if(val.id == process_output.packing_material){\r\n          direct_packing = val\r\n        }\r\n      })\r\n      if(process_output.packing_material){\r\n        process_output.direct_quantity = process_output.quantity\r\n      }else{\r\n        process_output.direct_quantity = 0\r\n      }\r\n      if(process_output.indent){\r\n        var indirect_packing = []\r\n        process_output.indent_detail.packing_materials_list.forEach(function(val,key){\r\n          if(val.id == process_output.packing_material_indent){\r\n            indirect_packing = val\r\n          }\r\n        })\r\n        if(process_output.packing_material){\r\n          process_output.direct_quantity = process_output.direct_quantity-process_output.indirect_quantity\r\n          process_output.number_of_bags = (process_output.direct_quantity/direct_packing.max_quantity)\r\n          process_output.number_of_bags = parseInt(process_output.number_of_bags)\r\n        }\r\n        process_output.number_of_bags_indent = (process_output.indirect_quantity/indirect_packing.material_detail.max_quantity)\r\n        process_output.number_of_bags_indent = parseInt(process_output.number_of_bags_indent)\r\n      }\r\n      if(process_output.packing_material){\r\n        process_output.number_of_bags = (process_output.direct_quantity/direct_packing.max_quantity)\r\n        process_output.number_of_bags = parseInt(process_output.number_of_bags)\r\n      }\r\n    },\r\n    calculation(parameter){\r\n      var self=this;\r\n      var total = 0;\r\n      var process_outputs = this.form_data.process_outputs\r\n      this.form_data.process_outputs = []\r\n      process_outputs.forEach(function(val,key){\r\n        if(val.quantity){\r\n          if(self.total_weight!=0){\r\n            var persentage = (val.quantity/self.total_weight)*100\r\n            persentage = persentage.toFixed(2)\r\n            process_outputs[key].quantity_persentage = persentage+'%'\r\n          }\r\n          if(val.parameter_type=='product'){\r\n            total = total+parseFloat(val.quantity)\r\n          }\r\n        }else{\r\n            process_outputs[key].quantity_persentage = '0%'\r\n        }\r\n      })\r\n      this.form_data.process_outputs = process_outputs\r\n      this.total_output = total\r\n    },\r\n    dataForm() {\r\n      this.createFormdata()\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('supervisor',this.user_id)\r\n      formdata.append('main_process',this.main_process_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/packing/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n@import '../../styles/style.css';\r\n#select-indent.modal.show .modal-dialog{\r\n  min-width: 400px!important;\r\n}\r\n</style>\r\n"]}]}