age:url('+my_page.background+');'\" >\r\n        <div class=\"creator-info theme-preview\":class=\"my_page.theme\">\r\n            <div class=\"d-flex flex-column filter container-fluid p-4\">\r\n                <div class=\"justify-content-center row mt-5\">\r\n                    <div class=\"col-lg-4\">\r\n                        <div v-for=\"content in contents\" v-if=\"content.eye_show || content.eye_show!=false\">\r\n                            <div class=\"mb-3 block-view\" v-if=\"content.type=='profile'\">\r\n                                <div class=\"text-center\">\r\n                                    <img :src=\"content.image\" style=\"width:100px;height:100px\" class=\"m-auto rounded-circle img-cover\">\r\n                                    <div class=\"mt-2\"> \r\n                                        <h4 class=\"font-regular text-reset\">\r\n                                            <span class=\"font-weight-bold text-white\" style=\"text-shadow:0px 1px 7px #000;\">{{content.name}}</span>\r\n                                            <!--<span class=\"font-weight-bold\">{{content.name}}</span>-->\r\n                                        </h4>\r\n                                        <div class=\"text-preserve mb-3\">{{content.bio}}</div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"mb-3 block-view block-view2\" v-else-if=\"content.type=='social_links'\">\r\n                                <div class=\"justify-content-center d-flex flex-wrap\">\r\n\r\n                                    <div class=\"trackable\" v-for=\"(scis,sky) in content.current_social_icons\">\r\n                                        <a class=\"text-reset\" target=\"_blank\" :href=\"validateLink(scis.url)\">\r\n                                            <i class=\"mr-2 ml-2\" v-for=\"(socialmedia,ihg) in socialmedias\" v-if=\"scis.social_type==