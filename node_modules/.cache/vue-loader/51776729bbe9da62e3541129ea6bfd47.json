                <a class=\"text-reset\" target=\"_blank\" :href=\"validateLink(content.link)\">\r\n                                            <i class=\"mr-2 ml-2 fab fa-twitter-square\" style=\"font-size:30px\"></i>\r\n                                        </a>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"mb-3 block-view\" v-else-if=\"content.type=='meeting'\">\r\n                                <div>\r\n                                    <div class=\"trackable\" >\r\n                                        <button type=\"button\" class=\"block-button  btn btn-white btn-block\">\r\n                                            <svg aria-hidden=\"true\" focusable=\"false\" data-prefix=\"fas\" data-icon=\"video\" class=\"svg-inline--fa fa-video fa-w-18 thumbnail\" role=\"img\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 576 512\"><path fill=\"currentColor\" d=\"M336.2 64H47.8C21.4 64 0 85.4 0 111.8v288.4C0 426.6 21.4 448 47.8 448h288.4c26.4 0 47.8-21.4 47.8-47.8V111.8c0-26.4-21.4-47.8-47.8-47.8zm189.4 37.7L416 177.3v157.4l109.6 75.5c21.2 14.6 50.4-.3 50.4-25.8V127.5c0-25.4-29.1-40.4-50.4-25.8z\"></path></svg>\r\n\r\n                                            <div class=\"flex-grow-1\">30 mins</div>\r\n                                        </button>\r\n                                    </div>\r\n                                </div>\r\n                                \r\n                            </div>\r\n                            <div class=\"mb-3 block-view\" v-else-if=\"content.type=='video'\">\r\n                                <div class=\"\">\r\n                                    <div class=\"trackable\" >\r\n                                        <iframe :src=\"getYoutubeUrl(validateLink(content.link))\" class=\"block-card\" style=\"width:100%;height:20%px;border:0\" v-if=\"content.video_type=='youtube'\"></iframe>\r\n                                        <iframe :src=\"validateLink(content.link)\" class=\"block-card\" style=\"width:100%;height:20%px;border:0\" v-else></iframe>\r\n                                    </div>\r\n                                </div>\r\n                                \r\n                            </div>\r\n                            <div class=\"mb-3 block-view\" v-else-if=\"content.type=='music'\">\r\n                                <div class=\"trackable text-center\" style=\"zoom:.6\">\r\n                                    <audio controls >\r\n                                        <source :src=\"validateLink(content.link)\" type=\"\">\r\n                                    </audio>\r\n                                </div>\r\n                                \r\n                            </div>\r\n                            <div class=\"mb-3 block-view\" v-else-if=\"content.type=='heading'\">\r\n                                <div style=\"margin:2rem 0\" class=\"text-center\">\r\n                                    <div class=\"mb-0 h2\">{{content.title}}</div>\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"mb-3 block-view\" v-else-if=\"content.type=='own_page'\">\r\n                                <div class=\"trackable\" id=\"6174717fc837f00792e3a428\">\r\n                                    <a target=\"_blank\"  class=\"block-button  btn btn-white btn-block\">Click here to get your own Firstpage\r\n                                        <div class=\"font-sm font-regular\" style=\"opacity:0.9\">\r\n                                        </div>\r\n                                    </a>\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"mb-3 block-view\" v-else-if=\"content.type=='text'\">\r\n                                <div class=\"block-card card block-button\">\r\n                                    <div class=\"card-body\">\r\n                                        <div class=\"font-lg font-semi-bold mb-2\">{{content.title}}</div>\r\n                                        <div class=\"text-preserve \">{{content.description}}</div>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"mb-3 block-view\" v-else-if=\"content.type=='link'\">\r\n                               \r\n                                <div>\r\n                                    <div class=\"trackable\" >\r\n                                        <a :href=\"validateLink(content.link)\" target=\"_blank\">\r\n                                        <button type=\"button\" class=\"block-button  btn btn-white btn-block\">\r\n                                            <div class=\"flex-grow-1\"><i :class=\"content.icon\"></i> {{content.title}}</div>\r\n                                        </button>\r\n                                        </a> \r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"mb-3 block-view\" v-else-if=\"content.type=='video-chat'\">\r\n                                <div class=\"trackable\" >\r\n                                    <button type=\"button\" class=\"block-button  btn btn-white btn-block\" @click=\"videoChat(content)\">\r\n                                        <div class=\"flex-grow-1\">\r\n                                            <i class=\"fas fa-video\"></i> <b>{{content.duration}}</b> {{content.title}}\r\n                                        </div>\r\n                                    </button>\r\n                                </div>\r\n                            </div>\r\n\r\n                            <div class=\"mb-3 block-view\" v-else>\r\n                               \r\n                                <div>\r\n                                    <div class=\"trackable\" >\r\n                                        <a target=\"_blank\">\r\n                                        <button type=\"button\" class=\"block-button  btn btn-white btn-block\">\r\n                                            <svg aria-hidden=\"true\" focusable=\"false\" data-prefix=\"fas\" data-icon=\"video\" class=\"svg-inline--fa fa-video fa-w-18 thumbnail\" role=\"img\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 576 512\">\r\n                                                <i :class=\"content.icon\"></i>\r\n                                            </svg>\r\n\r\n                                            <div class=\"flex-grow-1\">{{content.title}}</div>\r\n                                            <div class=\"text-preserve \">{{content.description}}</div>\r\n                                        </button>\r\n                                        </a> \r\n                                    </div>\r\n                                </div>\r\n\r\n                                \r\n                            </div>\r\n                             \r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <!--<div class=\"text-center mt-auto p-4\">\r\n                    <a class=\"filter-dark p-3  rounded-pill\">\r\n                        <img src=\"/img/logo2.png\" class=\"p-3\" width=\"100\">\r\n                    </a>\r\n                </div>-->\r\n                \r\n                    <div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"btn profile-preview-button \" v-if=\"$root.is_loggin\" style=\"width:100px\">\r\n            <img class=\"logged\" src=\"/img/profile.png\">\r\n        </div>\r\n        <div class=\"btn profile-preview-button \" v-b-modal.profile-preview-modal v-else>\r\n            <img class=\"logout\" src=\"/img/3dot.png\">\r\n        </div>\r\n        <b-modal id=\"profile-preview-modal\" hide-header  hide-footer>\r\n            <!-- login modal -->\r\n\r\n            <header class=\"modal-header customise-header\">\r\n              <div class=\"\">\r\n                <h5 id=\"headingModal___BV_modal_title_\" class=\"modal-title\">Login Page</h5>\r\n              </div>\r\n\r\n              <button type=\"button\" aria-label=\"Close\" class=\"close\" @click=\"$bvModal.hide('profile-preview-modal')\">×</button>\r\n            </header>\r\n            <div class=\"login preview\">\r\n              <div class=\"form\">\r\n\r\n                <div type=\"button\" class=\"btn-block bg-lightgray my-3 btn-lg btn  app-login-button\" @click=\"googleLogin()\">\r\n                    <img src=\"/img/google.png\" width=\"20\" class=\"pr-1\"> Login with Google\r\n                </div>\r\n              </div>\r\n            </div>\r\n        </b-modal>\r\n        <b-modal id=\"profile-video-chat-modal\" hide-header  hide-footer>\r\n            <!-- login modal -->\r\n            <header class=\"modal-header customise-header\">\r\n              <div class=\"\">\r\n                <h5 id=\"headingModal___BV_modal_title_\" class=\"modal-title\">{{current_content.title}}</h5>\r\n              </div>\r\n\r\n              <button type=\"button\" aria-label=\"Close\" class=\"close\" @click=\"$bvModal.hide('profile-video-chat-modal')\">×</button>\r\n            </header>\r\n            <div class=\"profile-modal text-left\">\r\n                <div class=\"container heading\">\r\n                    <div class=\"mb-4 border-bottom pb-3\">\r\n                        <div class=\"quill mt-3\">\r\n                            <div class=\"ql-container ql-snow ql-disabled\">\r\n                                <p>{{current_content.description}}</p>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <form class=\"\">\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"mr-2 mb-0 \">Duration: </label>\r\n                            <span>{{current_content.duration}} mins</span>\r\n                        </div>\r\n                        <div class=\"form-group\">\r\n                            <div class=\"mb-3\">\r\n                                <label class=\"m-0\">Head's up</label>\r\n                                <div class=\"sub-text\">Give {{current_content.username}} a short intro note about the meet</div>\r\n                            </div>\r\n                            <textarea name=\"description\" placeholder=\"I'd like to speak about…\" rows=\"4\" class=\"form-control\" aria-invalid=\"false\"></textarea>\r\n                            <div class=\"invalid-feedback\"></div>\r\n                        </div>\r\n                        <div class=\"mb-2 form-group\" v-if=\"current_content.pricing=='pay_what_you_want'\">\r\n                            <label for=\"amount\" class=\"\">Pay what you want</label>\r\n                            <div class=\"d-flex flex-wrap align-items-center\">\r\n                                <button type=\"button\" class=\"m-1 btn btn-sm btn-success\" v-for=\"price in prices\" @click=\"prices=[]\">₹{{price}}</button>\r\n                            </div>\r\n                            <div class=\"mb-0 mt-2 ml-2 form-group\">\r\n                                <b-input-group prepend=\"₹\" class=\"my-2\">\r\n                                    <b-form-input required name=\"price\" :value=\"current_content.price\" placeholder=\"Set your own\" class=\"w-50\"></b-form-input>\r\n                                </b-input-group>\r\n                            </div>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n                <hr>\r\n                <div class=\"mt-4 mx-5\">\r\n                    <button type=\"button\" @click=\"updateContent\" class=\"Pontmouse mb-1  btn-info btn-lg btn-block\"> Makepayment {{current_content.price}}</button>\r\n                </div>\r\n            </div>\r\n        </b-modal>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    \r\n  export default {\r\n    metaInfo: {\r\n      title: \"My Page\"\r\n    },\r\n    data() {\r\n      return {\r\n        contents:[],\r\n        my_page:[],\r\n        current_content:[],\r\n        page_url:'',\r\n        prices:[],\r\n      };\r\n    },\r\n    mounted () {\r\n        this.page_url = this.$route.params.url\r\n        this.getMypage()\r\n    },\r\n    watch: {\r\n        '$route': 'getMypage'\r\n    },\r\n    methods: {\r\n        async googleLogin() {\r\n          var googleUser = await this.$gAuth.signIn();\r\n\r\n          console.log(googleUser)\r\n          var token,id_token;\r\n          if(googleUser.$b){\r\n            token = googleUser.$b.access_token\r\n            id_token = googleUser.$b.id_token\r\n          }else if(googleUser.wc){\r\n            token = googleUser.wc.access_token\r\n            id_token = googleUser.wc.id_token\r\n          }else{\r\n            var access = googleUser[Object.keys(googleUser)[1]]\r\n            token = access.access_token\r\n            id_token = access.id_token\r\n          }\r\n          this.googleUserLogin(token,id_token)\r\n        },\r\n        googleUserLogin(access_token,id_token) {\r\n          this.loginUser({ access_token: access_token, id_token: id_token });\r\n        },\r\n        loginUser(data) {\r\n          var formdata = new FormData()\r\n          formdata.append('access_token',data.access_token)\r\n          formdata.append('id_token',data.id_token)\r\n          formdata.append('user_type',2)\r\n          if(this.login_step=='login'){\r\n            this.news_name = ''\r\n          }\r\n          formdata.append('news_name',this.news_name)\r\n          fetch(this.api_url+'/api/signinWithGoogle/', {\r\n              method : 'post',\r\n              body : formdata\r\n          })\r\n          .then((response) => {\r\n              return response.json()\r\n          })\r\n          .then((jsonData) => {\r\n            this.showAlert(jsonData.message)\r\n            if(jsonData.status){\r\n                localStorage.setItem(\"userInfo\", JSON.stringify(jsonData));\r\n                this.$root.is_loggin = true\r\n                var userdtls = JSON.parse(localStorage[\"userInfo\"]);\r\n                this.userId = userdtls.id\r\n                this.$root.user_type = userdtls.user_type\r\n                this.$root.userInfo = userdtls\r\n                \r\n            }else{\r\n                localStorage.removeItem(\"userInfo\");\r\n            }\r\n          })\r\n        },\r\n        videoChat(content){\r\n            this.current_content = content\r\n            var av_prices = [5,10,50,100,200,500,1000,2000,3000,5000,8000,10000,15000,20000,25000,30000,40000,50000,70000,80000]\r\n            var self = this\r\n            var price_limit = 0 \r\n            var sel_prices = []\r\n            sel_prices.push(self.current_content.price)\r\n            av_prices.forEach(function(price){\r\n                if(self.current_content.price<price && price_limit<=5){\r\n                    sel_prices.push(price)\r\n                    price_limit++\r\n                }\r\n            })\r\n            this.prices = sel_prices\r\n            this.$bvModal.show('profile-video-chat-modal')\r\n        },\r\n        getMypage() {\r\n            var formdata = new FormData()\r\n            formdata.append('page_url',this.page_url)\r\n            fetch(this.api_url+'/api/get_page/', {\r\n                method : 'post',\r\n                body : formdata\r\n            })\r\n            .then((response) => {\r\n                return response.json()\r\n            })\r\n            .then((jsonData) => {\r\n                if(jsonData.success){\r\n                    this.my_page = jsonData.page\r\n                    this.contents = JSON.parse