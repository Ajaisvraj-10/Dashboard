b-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">{{slugToWord('machine-issues')}}</span> / {{slugToWord(action)}}</h5>\r\n                    \r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Fixed\r\n                </b-button>\r\n                <router-link to=\"/app/maintenance/machine-issues/\">\r\n                  <b-button variant=\"danger\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n         <b-col lg=\"5\" xl=\"5\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"o_thread_search_input mt-3\">\r\n                  <span style=\"width:90px;\"> Break Type: </span>\r\n                  <input placeholder=\"Break\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.break_type\" name=\"break_type\" readonly>\r\n                </div>\r\n                <div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Machine: </span>\r\n                    <input type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.machine_detail.name\" readonly>\r\n                    <input type=\"hidden\" name=\"machine\" v-model=\"form_data.machine\"/>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:90px;\"> Motors: </span>\r\n                    <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"machine_mototrs\" v-model=\"form_data.machine_mototrs\" multiple>\r\n                      <option v-for=\"machine_mototr in machine_mototrs\" :value=\"machine_mototr.id\" v-if=\"form_data.machine_mototrs.includes(machine_mototr.id)\"> {{machine_mototr.name}}</option>\r\n                    </select>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:120px;\"> Break Start Date: </span>\r\n                    <input placeholder=\"00:01\" type=\"date\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.start_date\" name=\"start_date\" readonly>\r\n                  </div>\r\n                  <div class=\"o_thread_search_input\">\r\n                    <span style=\"width:120px;\"> Break Start Time: </span>\r\n                    <input placeholder=\"00:10\" type=\"time\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.start_time\" name=\"start_time\" readonly>\r\n                  </div>\r\n                </div>\r\n                <div class=\"o_thread_search_input mt-3\">\r\n                  <span style=\"width:90px;\"> Reason: </span>\r\n                  <textarea class=\"ui-autocomplete-input w-100 mx-1\" name=\"reason\" placeholder=\"Write here\" rows=\"4\" readonly>{{form_data.reason}}</textarea>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n         <b-col lg=\"7\" xl=\"7\" class=\"my-3\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n\r\n            <div class=\"row\">\r\n              <div class=\"col-md-12\">\r\n                <div class=\"o_thread_search_input mt-3\">\r\n                  <span style=\"width:120px;\"> Fixed Date: </span>\r\n                  <input placeholder=\"00:01\" type=\"date\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.end_date\" name=\"end_date\" required>\r\n                </div>\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\"> Fixed Time: </span>\r\n                  <input placeholder=\"00:10\" type=\"time\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.end_time\" name=\"end_time\" required>\r\n                </div>\r\n                <div class=\"o_thread_search_input mt-3\">\r\n                  <span style=\"width:120px;\"> What is the isssue?: </span>\r\n                  <textarea class=\"ui-autocomplete-input w-100 mx-1\" name=\"what_is_the_isssue\" placeholder=\"Write here\" rows=\"5\" v-model=\"form_data.what_is_the_isssue\"></textarea>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nexport default {\r\n  components:{\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{start_date:null,batch:null,process:null,break_type:'',machine_mototrs:[]},\r\n      id:null,\r\n      action:'create',\r\n      user_id:6,\r\n      machines:[],\r\n      machine_mototrs:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n    this.getMotors()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/machine_issues/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        if(!this.form_data.end_date){\r\n          this.form_data.end_date = this.currentDate()\r\n        }\r\n      })\r\n    },\r\n    getMotors() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/machine_mototrs/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.machine_mototrs = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('status', 'Fixed')\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/machine_issues/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          if(jsonData.break_down){\r\n            this.updateBreakDown(jsonData.break_down)\r\n          }else{\r\n            this.showAlert('Saved Successfully!')\r\n          }\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateBreakDown(break_id) {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('supervisor',this.form_data.break_detail.supervisor)\r\n      formdata.append('process',this.form_data.break_detail.process)\r\n      formdata.append('status', 'Fixed')\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/break_down/'+break_id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n</style>\r\n"]}]}