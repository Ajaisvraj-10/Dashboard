A;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"processing-list.vue","sourceRoot":"src/views/app/oodo/production/process","sourcesContent":["<template>\r\n  <div class=\"main-content create-main\">\r\n    <div>\r\n       <div class=\"d-flex mb-2 align-items-center justify-content-between\">\r\n          <slot name=\"header\">\r\n             <h5 class=\"mb-0\"><span class=\"text-success\">{{production_process.main_process_name}}</span> / {{production_process.name}}</h5>\r\n                \r\n          </slot>\r\n          <div class=\"d-flex\">\r\n            <b-button variant=\"primary\" type=\"button\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"newProcess()\">New Process\r\n            </b-button>\r\n            <router-link to=\"/app/production/process/processing\">\r\n              <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Discard\r\n              </b-button>\r\n            </router-link>\r\n          </div>\r\n           \r\n        </div>\r\n        <div class=\"my-2 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n   \r\n    <div class=\"d-flex main-scroll\" style=\"overflow-y:hidden;min-width:101%;\">\r\n\r\n      <div class=\" p-1\" v-for=\"process_detail in process_details\" :style=\"'max-width:'+max_width+';flex: 0 0 '+max_width\" v-if=\"process_detail.id!=default_packing_id\">\r\n    \r\n        <div class=\"card p-0 pb-2 my-1\">\r\n          <div class=\"card p-2 m-0 flex-row border\">\r\n            <div class=\"card col-md-3 align-self-center p-1\">\r\n              <img src=\"/img/website.png\" style=\"vertical-align:top\">\r\n            </div>\r\n            <div class=\" col-md-9 align-self-center\">\r\n              <b>{{process_detail.name}}  </b>\r\n              <div>\r\n                <b-button type=\"button\" variant=\"primary\" class=\" px-1 py-0\" style=\"font-size:12px\" @click=\"newProcess(process_detail)\">New</b-button>\r\n                <b-button type=\"button\"  variant=\"danger\" class=\"ml-1 px-1 py-0\" style=\"font-size:12px\" @click=\"breakDown(process_detail)\">Break</b-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"process_kanban\">\r\n            <div class=\"m-1\" v-for=\"(start_process,key) in process_detail.start_processes\" v-if=\"start_process.main_process_detail.current_process_detail==start_process.process_detail\">\r\n              <div class=\"px-2 card py-2\">\r\n                <div class=\"mx-1 mb-2\">\r\n                  <table class=\"w-100\">\r\n                    <tr class=\"\">\r\n                      <th class=\"text-primary\">#{{start_process.main_process}}/PROCESS</th>\r\n                      <th class=\"text-right\">\r\n                        LOT\r\n                        <span v-for=\"(process_product,key) in start_process.process_products\">\r\n                          {{key==0?'':', '}}{{process_product.lot}}\r\n                        </span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr v-for=\"(process_product,key) in start_process.process_products\">\r\n                      <th>{{key==0?'INPUT'+slugToWord(checkMult(start_process.process_products.length)):''}}</th>\r\n                      <th class=\"text-right\">\r\n                        {{key==0?'':', '}}{{process_product.parameter_detail.name}}({{process_product.quantity}}KG)\r\n                      </th>\r\n                    </tr>\r\n                  </table>\r\n                  <table class=\"w-100\">\r\n                    <tr>\r\n                      <th colspan=\"2\">STATUS</th>\r\n                      <th>\r\n                        <span class=\"text-success\">{{start_process.status}}</span>\r\n                      </th>\r\n                      <th class=\"text-right\">\r\n                        <span class=\"badge badge-danger p-1 cursor-pointer\" v-if=\"start_process.processes.length>0\" @click=\"startProcess('stop',process_detail,start_process)\" >Stop Process</span>\r\n                        <span class=\"badge badge-success p-1 cursor-pointer\" v-else @click=\"startProcess('input',process_detail,start_process)\" >Start Process</span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr v-if=\"process_detail.is_dryer && start_process.processes.length>0\">\r\n                      <th colspan=\"2\">\r\n                        {{start_process.dryer_details.dryer_end_time?'Dryer running time':start_process.dryer_details.dryer_start_time?'Dryer starting time':'Dryer'}}\r\n                      </th>\r\n                      <th :colspan=\"start_process.dryer_details.dryer_end_time?'2':''\">\r\n                        {{start_process.dryer_details.dryer_end_time?timeFormate(start_process.dryer_details.dryer_start_time)+' to '+timeFormate(start_process.dryer_details.dryer_end_time):start_process.dryer_details.dryer_start_time?timeFormate(start_process.dryer_details.dryer_start_time):'Not start'}}\r\n                      </th>\r\n                      <th class=\"text-right\" v-if=\"!start_process.dryer_details.dryer_end_time\">\r\n                        <span class=\"badge badge-danger cursor-pointer\" v-if=\"start_process.dryer_details.dryer_start_time\" @click=\"startProcess('dryer',process_detail,start_process)\" >End</span>\r\n                        <span class=\"badge badge-success cursor-pointer\" v-else @click=\"startProcess('dryer',process_detail,start_process)\" >Start</span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr v-if=\"process_detail.is_dryer && start_process.processes.length>0 && process_detail.consumable_items.length>0\">\r\n                      <th colspan=\"2\">\r\n                        {{start_process.firewood_details.firewood_end_time?'Furnace recommendation':start_process.firewood_details.firewood_start_time?'Furnace recommendation':'Furnace recommendation'}}\r\n                      </th>\r\n                      <th :colspan=\"start_process.firewood_details.firewood_end_time?'2':''\">\r\n                        {{start_process.firewood_details.firewood_end_time?timeFormate(start_process.firewood_details.firewood_start_time)+' to '+timeFormate(start_process.firewood_details.firewood_end_time):start_process.firewood_details.firewood_start_time?timeFormate(start_process.firewood_details.firewood_start_time):'Not start'}}\r\n                      </th>\r\n                      <th class=\"text-right\" v-if=\"!start_process.firewood_details.firewood_end_time\">\r\n                        <span class=\"badge badge-danger cursor-pointer\" v-if=\"start_process.firewood_details.firewood_start_time\" @click=\"startProcess('firewood',process_detail,start_process)\" >End</span>\r\n                        <span class=\"badge badge-success cursor-pointer\" v-else @click=\"startProcess('firewood',process_detail,start_process)\" >Start</span>\r\n                      </th>\r\n                    </tr>\r\n                  </table>\r\n                </div>\r\n                <div class=\"row\" v-if=\"start_process.processes.length>0\">\r\n                  <div class=\"col-md-6\">\r\n                    <b class=\"mx-1\">Shifts</b>\r\n                  </div>\r\n                  <div class=\"col-md-6 text-right\">\r\n                    <span class=\"badge badge-primary mx-1 p-1 cursor-pointer\" @click=\"startProcess('shift',process_detail,start_process)\">Add New Shift</span>\r\n                  </div>\r\n                </div>\r\n                <div class=\"card px-2 py-2 mt-1 \" v-for=\"process in start_process.processes\">\r\n                  <table class=\"\">\r\n                    <tr class=\"border border-rounded cursor-pointer\" @click=\"startProcess('shift',process_detail,start_process,process.id,true)\">\r\n                      <th class=\"text-info\">\r\n                        #{{process.supervisor_detail.name}}\r\n                      </th>\r\n                      <th>\r\n                        {{process.shift}}\r\n                      </th>\r\n                      <th>{{dateFormate(process.created_on)}}</th>\r\n                      <th class=\"text-right\">\r\n                        <span class=\"badge\" :class=\"process.status=='Completed'?'badge-success':process.status=='Draft'?'badge-secondary':process.status=='Starting'?'badge-success':process.status=='Stop'?'badge-danger':'badge-warning'\">{{process.status}}</span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr v-if=\"process_detail.is_machine\">\r\n                      <th colspan=\"2\">\r\n                        Machine Time\r\n                      </th>\r\n                      <th :colspan=\"process.end_time?'2':''\">\r\n                        {{process.end_time?timeFormate(process.start_time)+' to '+timeFormate(process.end_time):process.start_time?timeFormate(process.start_time):'Not start'}}\r\n                      </th>\r\n                      <th class=\"text-right\" v-if=\"!process.end_time\">\r\n                        <span class=\"badge badge-danger cursor-pointer\" v-if=\"process.start_time\" @click=\"startProcess('machine',process_detail,start_process,process.id)\" >End</span>\r\n                        <span class=\"badge badge-success cursor-pointer\" v-else @click=\"startProcess('machine',process_detail,start_process,process.id)\" >Start</span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr v-if=\"process_detail.have_workers\">\r\n                      <th colspan=\"2\">\r\n                        Workers\r\n                      </th>\r\n                      <th>\r\n                        {{process.workers.length>0?process.workers.length+' worker'+checkMult(process.workers.length):'No workers'}}\r\n                      </th>\r\n                      <th class=\"text-right\">\r\n                        <span class=\"badge badge-warning cursor-pointer\" @click=\"startProcess('workers',process_detail,start_process,process.id,true)\" v-if=\"process.workers.length\">View</span>\r\n                        <span class=\"badge badge-primary cursor-pointer\" @click=\"startProcess('workers',process_detail,start_process,process.id)\" v-else>Add</span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr v-if=\"process_detail.consumable_items.length>0\">\r\n                      <th colspan=\"2\">\r\n                        Consumable\r\n                      </th>\r\n                      <th>\r\n                        {{process.consumables_count>0?process.consumables_count+' consumable'+checkMult(process.consumables_count):'No consumables'}}\r\n                      </th>\r\n                      <th class=\"text-right\">\r\n                        <span class=\"badge badge-warning cursor-pointer\" @click=\"startProcess('consumables',process_detail,start_process,process.id,true)\" v-if=\"process.consumables_count\">View</span>\r\n                        <span class=\"badge badge-primary cursor-pointer\" @click=\"startProcess('consumables',process_detail,start_process,process.id)\" v-else>Add</span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr v-if=\"process_detail.outputs.length>0\">\r\n                      <th colspan=\"2\">\r\n                        Outputs\r\n                      </th>\r\n                      <th>\r\n                        {{process.process_outputs_count>0?process.process_outputs_count+' output'+checkMult(process.process_outputs_count):'No outputs'}}\r\n                      </th>\r\n                      <th class=\"text-right\">\r\n                        <span class=\"badge badge-warning cursor-pointer\" @click=\"startProcess('output',process_detail,start_process,process.id,true)\" v-if=\"process.process_outputs_count\">View</span>\r\n                        <span class=\"badge badge-primary cursor-pointer\" @click=\"startProcess('output',process_detail,start_process,process.id)\" v-else>Add</span>\r\n                      </th>\r\n                    </tr>\r\n                    <tr v-if=\"process_detail.is_unloading\">\r\n                      <th colspan=\"2\">\r\n                        Unloading\r\n                      </th>\r\n                      <th>\r\n                        {{process.unloading_workers>0?process.unloading_workers+' worker'+checkMult(process.unloading_workers):'No workers'}}\r\n                      </th>\r\n                      <th class=\"text-right\">\r\n                        <span class=\"badge badge-warning cursor-pointer\" @click=\"startProcess('unloading',process_detail,start_process,process.id,true)\" v-if=\"process.unloading_workers>0\">View</span>\r\n                        <span class=\"badge badge-primary cursor-pointer\" @click=\"startProcess('unloading',process_detail,start_process,process.id)\" v-else>Add</span>\r\n                      </th>\r\n                    </tr>\r\n                  </table>\r\n                </div>\r\n              </div>\r\n              <hr class=\"my-2\">\r\n            </div>\r\n            <div class=\"text-center\"> </div>\r\n          </div>\r\n        </div>\r\n    \r\n      </div>\r\n\r\n      <div class=\"p-1\" :style=\"'max-width:'+max_width+';flex: 0 0 '+max_width\" v-if=\"production_process.is_packing\">\r\n        <div class=\"card p-0 pb-2 my-1\">\r\n          <div class=\"card p-2 m-0 flex-row border\">\r\n            <div class=\"card col-md-3 align-self-center p-1\">\r\n              <img src=\"/img/website.png\" style=\"vertical-align:top\">\r\n            </div>\r\n            <div class=\"col-md-9 align-self-center\">\r\n              <b>Packing </b>\r\n              <div>\r\n               <!--  <router-link :to=\"'/app/production/process/processing/view/'+1\">\r\n                  <b-button  variant=\"primary\" class=\" px-1 py-0\" style=\"font-size:12px\">View</b-button>\r\n                </router-link> -->\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"process_kanban\">\r\n            <div class=\"m-1\" v-for=\"(start_process,key) in packing.start_processes\" v-if=\"start_process.main_process_detail.production_process==production_process.id\">\r\n              <div class=\"card px-3 py-2 cursor-pointer\" :class=\"key%2==0?'bg-lightgreen':'bg-lightred'\">\r\n                <table>\r\n                  <tr>\r\n                    <th class=\"text-primary\">#{{start_process.main_process}}/PROCESS</th>\r\n                    <th>\r\n                      LOT\r\n                      <span v-for=\"process_product in start_process.process_products\">\r\n                        {{process_product.lot}},\r\n                      </span>\r\n                    </th>\r\n                  </tr>\r\n                  <tr>\r\n                    <th>PRODUCT</th>\r\n                    <th v-for=\"process_product in start_process.process_products\">\r\n                      {{process_product.parameter_detail.name}},\r\n                    </th>\r\n                  </tr>\r\n                  <tr>\r\n                    <th>DATE</th>\r\n                    <th>{{dateFormate(start_process.created_on)}}</th>\r\n                  </tr>\r\n                  <tr>\r\n                    <th>STATUS</th>\r\n                    <th>\r\n                      <span class=\"text-success\">{{start_process.status}}</span>\r\n                    </th>\r\n                  </tr>\r\n                  <tr>\r\n                    <th colspan=\"2\" class=\"text-center\">\r\n                      <router-link :to=\"'/app/production/process/processing/view/'+packing.id+'/packing/'+start_process.main_process\">\r\n                        <button class=\"btn btn-warning px-2 py-1 btn-sm mt-2 w-100\">Pack Now</button>\r\n                      </router-link>\r\n                    </th>\r\n                  </tr>\r\n                </table>\r\n              </div>\r\n              <hr class=\"my-2\">\r\n            </div>\r\n            <div class=\"text-center\"> </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n    </div>\r\n    <div>\r\n      <div style=\"position:fixed;top:45%;right:1px;cursor:pointer\" @click=\"scrollRight()\">\r\n        <img src=\"/img/arrow-right.png\" style=\"width:15px;\">\r\n      </div>\r\n      <div style=\"position:fixed;top:45%;left:105px;cursor:pointer\" @click=\"scrollLeft()\">\r\n        <img src=\"/img/arrow-left.png\" style=\"width:15px;\">\r\n      </div>\r\n    </div>\r\n    <div>\r\n      <b-modal id=\"list-process\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n           <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">{{production_process.name}}</span> /{{current_process_detail.name}}</h5>\r\n              </slot>\r\n              <div class=\"d-flex\" v-if=\"list_process\">\r\n                <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"$bvModal.hide('list-process')\">Close</b-button>\r\n              </div>\r\n              <div class=\"d-flex\" v-else>\r\n                <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"createProcess()\">Start Process</b-button>\r\n                <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\" @click=\"list_process=true\">Back</b-button>\r\n              </div>\r\n            </div>\r\n            <div class=\" my-3 border-top\"></div>\r\n        </div>\r\n        <b-row class=\"main-scroll\">\r\n          <b-col lg=\"12\" xl=\"12\" v-if=\"list_process\">\r\n            <b-card class=\"mt-2\">\r\n              <div class=\"card px-2 py-1 mx-0 mt-2\" v-for=\"process_detail in process_details\">\r\n                <table class=\"\">\r\n                  <tr>\r\n                    <th>{{process_detail.name}}</th>\r\n                    <td class=\"text-right pr-2\">\r\n                      <button type=\"button\" class=\"btn btn-success px-2 py-1 btn-sm  my-2 \" @click=\"selectProcessDetail(process_detail)\">Start</button>\r\n                    </td>\r\n                  </tr>\r\n                </table>\r\n              </div>\r\n            </b-card>\r\n          </b-col>\r\n          <b-col lg=\"12\" xl=\"12\" class=\"\" v-else>\r\n            <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n              <b-card header-bg-variant=\"transparent\" class=\"mt-2 pt-0\">\r\n                <div class=\"\">\r\n                    <div class=\"o_thread_search_input\">\r\n                        <span>WareHouse  : </span>\r\n                        <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"warehouse\" v-model=\"warehouse_id\" @change=\"stockParameter()\">\r\n                          <option value=\"\" disabled>Select WareHouse</option>\r\n                          <option v-for=\"warehouse in warehouses\" :value=\"warehouse.id\">{{warehouse.name}}</option>\r\n                        </select>\r\n                  </div>\r\n                  <input type=\"hidden\" name=\"process_detail\" :value=\"current_process_detail.id\">\r\n                  <input type=\"hidden\" name=\"production_process\" :value=\"production_process.id\">\r\n                  <input type=\"hidden\" name=\"status\" value=\"Starting\">\r\n                  <table class=\"table mt-2 w-100\">\r\n                    <thead>\r\n                      <tr>\r\n                        <th>Input</th>\r\n                        <th>Lot</th>\r\n                        <th>Indent</th>\r\n                        <th>Available Qty(KG)</th>\r\n                        <th>Quantity(KG)</th>\r\n                        <th>Action</th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr v-for=\"(stock_parameter,key) in stock_parameters\">\r\n                        <td :style=\"stock_parameter.is_added?'opacity:1':'opacity:0.5'\">\r\n                          {{stock_parameter.parameter_detail.name}}\r\n                        </td>\r\n                        <td :style=\"stock_parameter.is_added?'opacity:1':'opacity:0.5'\">\r\n                          #{{stock_parameter.lot}}/Lot\r\n                          <input type=\"hidden\" :name=\"'lot'+stock_parameter.id\" :value=\"stock_parameter.lot\">\r\n                        </td>\r\n                        <td :style=\"stock_parameter.is_added?'opacity:1':'opacity:0.5'\">\r\n                          <select class=\"ui-autocomplete-input w-100 mx-1\" :name=\"'indent'+stock_parameter.id\" v-model=\"stock_parameter.indent\" :disabled=\"!stock_parameter.is_added\">\r\n                              <option v-for=\"indent in indents\" :value=\"indent.id\">{{indent.indent_number}}</option>\r\n                          </select>\r\n                        </td>\r\n                        <td :style=\"stock_parameter.is_added?'opacity:1':'opacity:0.5'\">\r\n                          {{stock_parameter.stock_quantity}}\r\n                        </td>\r\n                        <td :style=\"stock_parameter.is_added?'opacity:1':'opacity:0.5'\">\r\n                          <input type=\"number\" class=\"ui-autocomplete-input w-100\" :max=\"stock_parameter.stock_quantity\" v-model=\"stock_parameter.quantity\" :name=\"'quantity'+stock_parameter.id\" :disabled=\"!stock_parameter.is_added\">\r\n                          <input type=\"hidden\" :name=\"'is_added'+stock_parameter.id+'lot'\" :value=\"stock_parameter.is_added?'yes':'no'\">\r\n                        </td>\r\n                        <td>\r\n                          <button type=\"button\" class=\"btn btn-primary btn-sm px-3 py-1 ml-4\" @click=\"addProduct(stock_parameter)\" v-if=\"!stock_parameter.is_added\">Add</button>\r\n                          <button type=\"button\" class=\"btn btn-danger btn-sm px-3 py-1 ml-4\" @click=\"removeProduct(stock_parameter)\" v-else>Remove</button>\r\n                        </td>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </b-card>\r\n            </form>\r\n          </b-col>\r\n        </b-row>\r\n      </b-modal>\r\n\r\n      <b-modal id=\"start-process\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n          <ProcessingForm @closePopup=\"closePopup\" :action=\"action\" :process_detail_id=\"current_process_detail.id\" :start_process_id=\"start_process_id\" v-if=\"load_processing_form\" :processing_id=\"processing_id\" :is_view=\"is_view\"></ProcessingForm>\r\n        </div>\r\n      </b-modal>\r\n\r\n      <b-modal id=\"break-down\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <div>\r\n          <BreakDownForm @closeBreakPopup=\"closeBreakPopup\" :process_detail_id=\"current_process_detail.id\" v-if=\"load_break_form\" :processing_id=\"processing_id\"></BreakDownForm>\r\n        </div>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nimport ProcessingForm from '/src/views/app/oodo/production/process/processing-component'\r\nimport BreakDownForm from '/src/views/app/oodo/production/process/processing-break'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields,\r\n    ProcessingForm,\r\n    BreakDownForm,\r\n  },\r\n  data() {\r\n    return {\r\n      production_process:{id:null},\r\n      process_details:[],\r\n      current_process_detail:{id:null},\r\n      list_process:true,\r\n      warehouse_id:'',\r\n      stock_parameters:[],\r\n      start_process_id:null,\r\n      user_id:6,\r\n      indents:[],\r\n      packing:{},\r\n      print_requests:[],\r\n      packing_materials:[],\r\n      processing_id:null,\r\n      load_processing_form:false,\r\n      load_break_form:false,\r\n      is_view:false,\r\n      max_width:'350px',\r\n      action:'shift',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.production_process.id = this.$route.params.process_id\r\n    this.getProductionProcess()\r\n    this.getIndents()\r\n    this.getWarehouses()\r\n  },\r\n  methods: {\r\n    getProductionProcess() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/production_process/'+this.production_process.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n        return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.production_process = jsonData\r\n        this.getProcessDetails()\r\n      })\r\n    },\r\n    getProcessDetails() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/process_details/?production_process='+this.production_process.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n        return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.process_details = jsonData\r\n        this.setColSize()\r\n        this.getPacking()\r\n      })\r\n    },\r\n    getPacking() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/process_details/'+this.default_packing_id+'/?is_packing=true&production_process='+this.production_process.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.packing = jsonData\r\n      })\r\n    },\r\n    setColSize(){\r\n      var cols = this.process_details.length\r\n      this.max_width = '350px'\r\n      // if(cols>4){\r\n      //   this.max_width = (100/cols)+'%'\r\n      // }\r\n    },\r\n    newProcess(process_detail={}){\r\n      if(process_detail.id){\r\n        this.selectProcessDetail(process_detail)\r\n      }else{\r\n        this.current_process_detail = {}\r\n      }\r\n      this.$bvModal.show('list-process')\r\n    },\r\n    selectProcessDetail(process_detail){\r\n      this.current_process_detail = process_detail\r\n      this.list_process = false\r\n      this.warehouse_id = process_detail.warehouse\r\n      this.stockParameter()\r\n    },\r\n    getIndents() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/indent/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.indents = jsonData\r\n      })\r\n    },\r\n    stockParameter() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/parameter_stock/?inputs='+this.current_process_detail.inputs+'&warehouse='+this.warehouse_id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.stock_parameters = jsonData\r\n      })\r\n    },\r\n    addProduct(stock_parameter){\r\n      stock_parameter.is_added = true\r\n      stock_parameter.quantity = stock_parameter.stock_quantity\r\n    },\r\n    removeProduct(stock_parameter){\r\n      stock_parameter.is_added = false\r\n      stock_parameter.quantity = ''\r\n    },\r\n    createProcess() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('inputs',this.current_process_detail.inputs)\r\n      formdata.append('supervisor',this.user_id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/main_process/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.getProcessDetails()\r\n          this.list_process = true\r\n          this.$bvModal.hide('list-process')\r\n          this.startProcess('shift',this.current_process_detail,jsonData)\r\n          this.showAlert('Saved Successfully!')\r\n\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    startProcess(action,process_detail,start_process,processing_id=null,is_view=false){\r\n      this.action = action\r\n      this.current_process_detail = process_detail\r\n      this.start_process_id = start_process.id\r\n      var false_message = ''\r\n      if(!processing_id && (action=='shift' || action=='dryer' || action=='firewood' || action=='unloading' || action=='stop')){\r\n        start_process.processes.forEach(function(process_){\r\n          if(action=='shift' && process_.process_outputs_count==0 && !is_view && !process_detail.is_restacking){\r\n            false_message = 'Please complete this shift!'\r\n          }else{\r\n            processing_id = process_.id\r\n          }\r\n          if(action=='stop' && process_.process_outputs_count==0 && !process_detail.is_restacking){\r\n            false_message = 'Please complete this shift!'\r\n          }\r\n          if(process_detail.is_restacking && process_.workers.length<=0){\r\n            false_message = 'Please complete this shift!'\r\n          }\r\n        })\r\n      }\r\n      if(action=='shift' && start_process.total_weight<=0 && !is_view){\r\n        false_message = 'The input product is over!'\r\n      }\r\n      if(false_message){\r\n        this.showAlert(false_message)\r\n        return false\r\n      }\r\n      if(action=='shift' && !is_view){\r\n        processing_id = null\r\n      }\r\n      this.processing_id = processing_id\r\n      this.is_view = is_view\r\n      this.load_processing_form = true\r\n      this.$bvModal.show('start-process')\r\n    },\r\n    closePopup(bool){\r\n      if(bool){\r\n        this.load_processing_form = false\r\n        this.$bvModal.hide('start-process')\r\n        this.getProcessDetails()\r\n      }\r\n    },\r\n    breakDown(process_detail){\r\n      this.current_process_detail = process_detail\r\n      var self = this\r\n      var processing_id = null\r\n      process_detail.start_processes.forEach(function(start_process){\r\n        if(start_process.main_process_detail.current_process_detail==start_process.process_detail){\r\n          start_process.processes.forEach(function(process_){\r\n            processing_id = process_.id\r\n          })\r\n        }\r\n      })\r\n      if(processing_id){\r\n        this.processing_id = processing_id\r\n        this.load_break_form = true\r\n        this.$bvModal.show('break-down')\r\n      }else{\r\n        this.showAlert('Please create a shift!')\r\n      }\r\n    },\r\n    closeBreakPopup(bool){\r\n      if(bool){\r\n        this.load_break_form = false\r\n        this.$bvModal.hide('break-down')\r\n      }\r\n    },\r\n    scrollLeft() {\r\n      let content = document.querySelector(\".main-scroll\");\r\n      // content.scrollLeft -= 150;\r\n      content.scrollTo({left: content.scrollLeft -= 160,behavior: 'smooth',})\r\n    },\r\n    scrollRight() {\r\n      let content = document.querySelector(\".main-scroll\");\r\n      // content.scrollLeft += 150;\r\n      content.scrollTo({left: content.scrollLeft += 160,behavior: 'smooth',})\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '/src/views/app/oodo/styles/style.css';\r\n  #list-process.modal.show .modal-dialog{\r\n    min-width: 650px!important;\r\n  }\r\n  #start-process.modal.show .modal-dialog{\r\n    min-width: 600px!important;\r\n  }\r\n</style>\r\n"]}]}