/slot>\r\n            <div class=\"d-flex\">\r\n              <span style=\"width:50px;\">Date : </span>\r\n              <input type=\"date\" v-model=\"current_date\" class=\"o_thread_search_input\" @change=\"getRows()\">\r\n            </div>\r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <div class=\"row\">\r\n        <div class=\"col-md-12\">\r\n          <div class=\"card\">\r\n            <b-tabs pills card vertical>\r\n              <b-tab v-for=\"attendance in workers_attendances\" :title=\"attendance.shift+' SHIFT'\" :active=\"tab==attendance.shift\" @click=\"tab=attendance.shift\">\r\n                <div v-if=\"tab==attendance.shift\">\r\n                  <div class=\"row\">\r\n                    <div class=\"col-md-10\">\r\n                      <h5>Workers (shift {{attendance.shift}})</h5>\r\n                    </div>\r\n                    <div class=\"col-md-2 text-right pb-2\">\r\n                      <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                         @click=\"form_data={shift:attendance.shift, attendance:'FULL TIME',workers:attendance.workers, attendances:attendance.attendances};$bvModal.show('worker-modal')\" v-if=\"attendance.workers.length>0 && current_date==today\">+ Start Attends\r\n                      </b-button>\r\n                    </div>\r\n                  </div>\r\n                  <table class=\"table table-striped\"> \r\n                    <thead class=\"\">\r\n                      <tr >\r\n                        <th>\r\n                           <b-form-checkbox v-model=\"attendance.select_all\" @change=\"selectAll(attendance)\">\r\n                            <b>#Selct All</b>\r\n                          </b-form-checkbox></th>\r\n                        <th>Gender</th>\r\n                        <th>{{current_date}}</th>\r\n                      </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                      <tr v-for=\"attendance2 in attendance.attendances\" v-if=\"attendance2.shift==attendance.shift\">\r\n                        <th>\r\n                           <b-form-checkbox name=\"workers\" v-model=\"attendance.workers\" :value=\"attendance2.id\">\r\n                            <b>#{{attendance2.worker_detail.id}}/{{attendance2.worker_detail.name}}</b>\r\n                          </b-form-checkbox>\r\n                        </th>\r\n                        <th>{{attendance2.worker_detail.gender}}</th>\r\n                        <th>\r\n                          <select class=\"\" name=\"attendance\" v-model=\"attendance2.attendance\" @change=\"addAttedence(attendance2)\" :disabled=\"current_date!=today\">\r\n                            <option v-for=\"attendance3 in ['FULL TIME','HALF TIME','ABSENT']\" :value=\"attendance3\"> {{attendance3}}</option>\r\n                          </select>\r\n                        </th>\r\n                      </tr>\r\n                    </tbody>\r\n                  </table>\r\n                </div>\r\n              </b-tab>\r\n            </b-tabs>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </b-card>\r\n\r\n    <div>\r\n      <b-modal id=\"worker-modal\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <form ref=\"dataForm\" @submit.prevent=\"dataForm()\">\r\n          <div>\r\n             <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n                <slot name=\"header\">\r\n                   <h5 class=\"mb-0\"><span class=\"text-success\">Start Attends</span>/{{form_data.shift}}</h5>\r\n                </slot>\r\n                <div class=\"d-flex\">\r\n                  <b-button type=\"submit\" variant=\"success\" class=\" px-3 py-1 d-sm-block mx-1\">Submit</b-button>\r\n                  <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1 d-sm-block mx-1\" @click=\"$bvModal.hide('worker-modal')\">Close</b-button>\r\n                </div>\r\n              </div>\r\n              <div class=\" my-3 border-top\"></div>\r\n          </div>\r\n          <b-row class=\"main-scroll\">\r\n            <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n              <b-card header-bg-variant=\"transparent\" class=\"mt-2 pt-0\">\r\n                <div class=\"o_thread_search_input\">\r\n                  <span style=\"width:120px;\">Attendance : </span>\r\n                  <select class=\"\" name=\"attendance\" v-model=\"form_data.attendance\">\r\n                    <option v-for=\"attendance in ['FULL TIME','HALF TIME','ABSENT']\" :value=\"attendance\"> {{attendance}}</option>\r\n                  </select>\r\n                  <input type=\"hidden\" name=\"shift\" :value=\"form_data.shift\">\r\n                </div>\r\n                <div class=\"o_thread_search_input mx-1 mt-2\">\r\n                  <span style=\"width:120px;\">Workers ({{form_data.workers.length}}): </span>\r\n                  <select v-model=\"form_data.workers\" multiple name=\"workers\" v-show=\"false\">\r\n                    <option v-for=\"w in form_data.workers\" :value=\"w\">{{w}}</option>\r\n                  </select>\r\n                  <table>\r\n                    <tr v-for=\"attendance2 in form_data.attendances\" v-if=\"attendance2.shift==form_data.shift || !attendance2.shift\">\r\n                      <th>\r\n                        <b>#{{attendance2.worker_detail.id}}/{{attendance2.worker_detail.name}}</b>\r\n                      </th>\r\n                      <th>({{attendance2.worker_detail.gender}})</th>\r\n                    </tr>\r\n                  </table>\r\n                </div>\r\n                <hr class=\"my-2\">\r\n              </b-card>\r\n            </b-col>\r\n          </b-row>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Fields from '@/data/Fields'\r\nimport contacts from '@/data/list_grid'\r\nimport TableHeader from '/src/views/app/oodo/components/table_header'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TableHeader\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{date:'', shift:'DAY', attendance:'FULL TIME',workers:[]},\r\n      workers_attendances:[],\r\n      select_all:false,\r\n      tab:'DAY',\r\n      current_date:this.currentDate(),\r\n      today:this.currentDate(),\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'production'\r\n    this.getRows()\r\n  },\r\n  methods: {\r\n    getRows() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers_attendance/?date='+this.current_date, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.workers_attendances = jsonData\r\n      })\r\n    },\r\n    selectAll(attendance){\r\n        attendance.workers = []\r\n        if(!attendance.select_all){\r\n          attendance.attendances.forEach(function(attendance2){\r\n            attendance.workers.push(attendance2.id)\r\n          })\r\n        }\r\n    },\r\n    dataForm() {\r\n      if(!this.form_data.attendance || !this.form_data.shift){\r\n        return false\r\n      }\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers_attendance_mult/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.$bvModal.hide('worker-modal')\r\n          this.getRows()\r\n          this.showAlert('Done!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    addAttedence(attendance) {\r\n      if(!attendance.attendance || !attendance.shift){\r\n        return false\r\n      }\r\n      var formdata = new FormData()\r\n      formdata.append('worker', attendance.worker)\r\n      formdata.append('attendance', attendance.attendance)\r\n      formdata.append('shift', attendance.shift)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/workers_attendance/'+attendance.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Done!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}