slot>\r\n              <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Save\r\n                </b-button>\r\n                <router-link to=\"/app/procurement/sample-procurement\">\r\n                  <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Discard\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\"mt-3\">\r\n          <h5></h5>\r\n        </b-col>\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Status: </span>\r\n                              <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"status\">\r\n                                  <option v-for=\"stat in status\" :value=\"stat\" :selected=\"stat==form_data.status\">{{stat}}</option>\r\n                              </select>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\">Purchase Type  : </span>\r\n                              <input placeholder=\"Purchase Type\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"purchase_types[0].name\" readonly>\r\n                            </div>\r\n\r\n                            <ProductFields :product_id=\"product_id\" :readonly=\"action=='edit'?true:false\" v-if=\"load_products\" @productId=\"productId\"></ProductFields>\r\n\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Date: </span>\r\n                              <input placeholder=\"Date\" type=\"date\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.date\" name=\"date\" required readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Organic Status: </span>\r\n                              <input placeholder=\"\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" :value=\"form_data.organic_status\" name=\"organic_status\" readonly>\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <b-col lg=\"6\" xl=\"6\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\"o_thread_composer  \">\r\n                     <div class=\"o_composer_container my-2 row mx-0\">\r\n                        <div class=\"col-md-1 px-2\">\r\n\r\n                        </div>\r\n                        <div class=\" col-md-10\">\r\n\r\n                          <div class=\"user-info pt-2\">\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Farmer Code: </span>\r\n                              <input type=\"text\" placeholder=\"Search (Farmer code)\" class=\"\" autocomplete=\"off\" v-model=\"form_data.farmer_detail.farmer_code\" @input=\"searchFarmers()\" required>\r\n                                  <div class=\"farmer_search_drpdwn\" v-show=\"farmers.length>0\">\r\n                                    <div class=\"farmer_heading\">\r\n                                      Farmers Result <span @click=\"farmers=[]\"><i class=\"fa fa-times-circle\" aria-hidden=\"true\"></i></span>\r\n                                    </div>\r\n                                    <div class=\"farmer_results\">\r\n                                      <p v-for=\"farmer in farmers\" @click=\"selectFarmer(farmer)\">{{farmer.farmer_code}}-{{farmer.name}}</p>\r\n                                    </div>\r\n                                  </div>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Farmer name: </span>\r\n                              <input type=\"text\" placeholder=\"Farmer name\" class=\"\" autocomplete=\"off\" :value=\"form_data.farmer_detail.name\" readonly required>\r\n                              <input type=\"hidden\" name=\"farmer\" :value=\"form_data.farmer\">\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Estimate QTY (Kg): </span>\r\n                              <input placeholder=\"Qty\" type=\"text\" :value=\"form_data.estimate_qty\" class=\"\" autocomplete=\"off\" name=\"estimate_qty\" readonly>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Farmer Estimate QTY (Kg): </span>\r\n                              <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" name=\"farmer_estimate\" required v-model=\"form_data.farmer_estimate\">\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> Available QTY (KG): </span>\r\n                              <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" name=\"available_qty\" v-model=\"form_data.available_qty\" required>\r\n                            </div>\r\n                            <div class=\"o_thread_search_input\">\r\n                              <span style=\"width:120px;\"> QC Status: </span>\r\n                              <input placeholder=\" \" type=\"text\" class=\"\" autocomplete=\"off\" :value=\"form_data.qc_status\" name=\"qc_status\" readonly></td>\r\n                            </div>\r\n                            \r\n                          </div>\r\n                        </div>\r\n\r\n\r\n                       \r\n                     </div>\r\n                     \r\n                    \r\n                  </div>\r\n\r\n\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{purchase_type:'Farmer Purchase',date:new Date(),'estimate_qty':0,'farmer_estimate':0,'farmer_detail':{'name':'', 'farmer_code':''},'farmer':null, 'available_qty':0,'qc_status':'Waiting'},\r\n      id:null,\r\n      load_products:false,\r\n      product_id:null,\r\n      action:'create',\r\n      farmers:[],\r\n      total_keys:0,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.form_data.date = this.currentDate()\r\n    this.$root.current_section = 'procurement'\r\n    this.action = this.$route.params.action\r\n    this.getPurchaseTypes()\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.load_products = true\r\n    }\r\n\r\n  },\r\n  watch: {\r\n    '$route': 'changeURL'\r\n  },\r\n  methods: {\r\n    changeURL(){\r\n      this.form_data.date = this.currentDate()\r\n      this.action = this.$route.params.action\r\n      this.getPurchaseTypes()\r\n      if(this.action == 'edit'){\r\n        this.id = this.$route.params.id\r\n        this.getForm()\r\n      }else{\r\n        this.load_products = true\r\n      }\r\n    },\r\n    searchFarmers(line=''){\r\n      var farmer_code = this.form_data.farmer_detail.farmer_code\r\n      if(farmer_code.length<4){\r\n        return false\r\n      }\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      formdata.append('farmer_code',farmer_code)\r\n      formdata.append('search_type','sample_procurement')\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/search_farmers/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmers = jsonData\r\n      })\r\n    },\r\n    selectFarmer(farmer=null){\r\n      if(farmer){\r\n        this.form_data.farmer_detail.farmer_code = farmer.farmer_code\r\n        this.form_data.farmer_detail.name = farmer.name\r\n        this.form_data.farmer = farmer.id\r\n      }\r\n      this.farmers = []\r\n      var headers = new Headers();\r\n      var formdata = new FormData();\r\n      formdata.append('farmer', this.form_data.farmer)\r\n      formdata.append('product', this.form_data.product)\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmer_full_details/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data.estimate_qty = jsonData.estimate_qty\r\n      })\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_procurement/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.product_id = jsonData.product\r\n        this.form_data = jsonData\r\n        this.load_products = true\r\n      })\r\n    },\r\n    productId(id){\r\n      this.product_id = id\r\n      this.form_data.product = id\r\n      this.selectFarmer()\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('aeo',2)\r\n      formdata.append('current_status','sample_procurement')\r\n      formdata.append('purchase_type',this.purchase_types[0].id)\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_procurement/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/qc/sample_procurement/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n@import '../styles/style.css';\r\n</style>\r\n"]}]}