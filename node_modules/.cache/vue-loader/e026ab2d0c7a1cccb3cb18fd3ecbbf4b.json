  <div class=\"d-flex\">\r\n                 <b-button variant=\"success\" type=\"button\" @click=\"updateStatus('Approved')\" class=\" px-3 py-1  d-sm-block mx-1\" v-if=\"form_data.status=='Pending'\"\r\n                  >Approved\r\n                </b-button>\r\n                <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  @click=\"updateStatus('Declined')\" v-if=\"form_data.status=='Pending'\">Declined\r\n                </b-button>\r\n                <router-link to=\"/app/fpo/aeo-updations/farmer-files\">\r\n                  <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                    >Back\r\n                  </b-button>\r\n                </router-link>\r\n                <b-dropdown id=\"dropdown-right\" right text=\"Action\" variant=\"default\" class=\"action-button mx-1\">\r\n                  <b-dropdown-header id=\"dropdown-header-label\">\r\n                      <h6 class=\"cursor-pointer my-2\">Delete</h6>\r\n                      <h6 class=\"cursor-pointer my-2\">Duplicate</h6>\r\n\r\n\r\n                      \r\n                     \r\n                  </b-dropdown-header>\r\n                </b-dropdown>\r\n              </div>\r\n             \r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <!-- <div class=\"wrapper\"> -->\r\n      <b-row class=\"main-scroll pt-2\">\r\n        <b-col lg=\"12\" xl=\"12\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <h1>{{form_data.file_type}}</h1>\r\n            <div class=\"p-5 text-center\">\r\n              <a :href=\"form_data.file\" target=\"_blank\">\r\n                <button class=\"btn btn-success\" type=\"button\">View File</button>\r\n              </a>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"FPO - ICS\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{status:'Pending'},\r\n      id:null,\r\n      action:'edit',\r\n      total_fields:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'fpo'\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/farmer_files/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n      })\r\n    },\r\n    updateStatus(status) {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('status',status)\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/update_farmer_file/'+this.id+'/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.success){\r\n          this.showAlert(status)\r\n          this.$router.push('/app/fpo/aeo-updations/farmer-files')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}