      <slot name=\"header\">\r\n               <h5 class=\"mb-0\">{{slugToWord('workers-book')}}</h5>\r\n            </slot>\r\n            <div class=\"d-flex\">\r\n              <b-button type=\"button\" variant=\"success\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                 @click=\"$bvModal.show('attendance')\">+ Add Shift\r\n              </b-button>\r\n            </div>\r\n           \r\n        </div>\r\n        <div class=\"my-3 border-top\"></div>\r\n    </div>\r\n    <!-- <div class=\"wrapper\"> -->\r\n    <b-card class=\"main-scroll\">\r\n      <vue-good-table\r\n        :columns=\"columns\"\r\n        :line-numbers=\"false\"\r\n        :search-options=\"{\r\n          enabled: true,\r\n          placeholder: 'Search'\r\n        }\"\r\n        :pagination-options=\"{\r\n          enabled: false,\r\n          mode: 'records'\r\n        }\"\r\n        styleClass=\"tableOne vgt-table\"\r\n        :rows=\"rows.results\"\r\n      >\r\n        <template slot=\"table-row\" slot-scope=\"props\">\r\n          <span v-if=\"props.column.field == 'id'\">\r\n            <router-link :to=\"'/app/security-gate/workers-book-workers/'+props.row.id\">\r\n              <b-button  variant=\"primary\" class=\" px-3 py-1  d-sm-block mx-1\" >View</b-button>\r\n            </router-link>\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'workers'\">\r\n            {{props.row.workers.length}}\r\n          </span>\r\n          <span v-else-if=\"props.column.field == 'created_on'\">\r\n            {{dateFormate(props.row.created_on)}}\r\n          </span>\r\n        </template>\r\n\r\n        \r\n      </vue-good-table>\r\n      <TablePagination :page=\"page\" :rows=\"rows\" :load_first=\"load_first\" @changePage=\"changePage\" v-if=\"rows.count>0\"></TablePagination>\r\n    </b-card>\r\n\r\n    <div>\r\n      <b-modal id=\"attendance\" hide-header  hide-footer :no-close-on-backdrop=\"true\">\r\n        <form ref=\"createShift\" @submit.prevent=\"createShift()\">\r\n          <div>\r\n            <div class=\"d-flex mb-2 align-items-center  justify-content-between\">\r\n              <slot name=\"header\">\r\n                 <h5 class=\"mb-0\"><span class=\"text-success\">Add Shift</span></h5>\r\n              </slot>\r\n              <div class=\"d-flex\">\r\n                <b-button type=\"submit\" variant=\"success\" class=\" px-3 py-1 d-sm-block mx-1\">Save</b-button>\r\n                <b-button type=\"button\" variant=\"danger\" class=\" px-3 py-1 d-sm-block mx-1\" @click=\"$bvModal.hide('attendance')\">Close</b-button>\r\n              </div>\r\n            </div>\r\n            <div class=\" my-3 border-top\"></div>\r\n            <b-row class=\"main-scroll\">\r\n              <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n                <b-card header-bg-variant=\"transparent\" class=\"mt-2 pt-0\">\r\n                  <div  class=\"o_thread_search_input col-12\">\r\n                    <span  style=\"width: 120px;color:black;\">Date : </span>\r\n                    <input placeholder=\"Date\" type=\"date\" v-model=\"form_data.date\" autocomplete=\"off\" class=\"form-control w-100 mt-1\" style=\"background-color:#f5fafb;\" readonly>\r\n                  </div>\r\n                  <div  class=\"o_thread_search_input col-12\">\r\n                    <span  style=\"width: 120px;color:black;\">Shift : </span>\r\n                    <select class=\"form-control w-100 mt-1\" name=\"shift\" v-model=\"form_data.shift\" style=\"background-color:#f5fafb;\">\r\n                      <option v-for=\"shift in shifts\" :value=\"shift\">{{shift}}</option>\r\n                    </select>\r\n                  </div>\r\n                  <hr class=\"my-2\">\r\n                </b-card>\r\n              </b-col>\r\n            </b-row>\r\n          </div>\r\n        </form>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TablePagination from '/src/views/app/oodo/components/table_pagination'\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"States\"\r\n  },\r\n  components:{\r\n    TablePagination\r\n  },\r\n  data() {\r\n    return {\r\n      columns: [\r\n        {\r\n          label: \"Shift\",\r\n          field: \"shift\"\r\n        },\r\n        {\r\n          label: \"Workers\",\r\n          field: \"workers\"\r\n        },\r\n        {\r\n          label: \"Date\",\r\n          field: \"created_on\"\r\n        },\r\n        {\r\n          label: \"Action\",\r\n          field: \"id\"\r\n        },\r\n       \r\n      ],\r\n      rows: {results:[],count:0},\r\n      page:0,\r\n      load_first:true,\r\n      form_data:{date:this.currentDate(), shift:'DAY'},\r\n    };\r\n  },\r\n  mounted(){\r\n    this.$root.current_section = 'security-gate'\r\n    this.changePage(1)\r\n  },\r\n  methods: {\r\n    changePage(count){\r\n      this.page = count\r\n      this.load_first = true\r\n      this.getRows()\r\n    },\r\n    getRows() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token)\r\n      fetch(this.api_url+'/security/workers_shift/?page='+this.page, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.detail=='Invalid page.'){\r\n          this.rows = {results:[],count:0}\r\n        }else{\r\n          this.load_first = false\r\n          this.rows = jsonData\r\n        }\r\n      })\r\n    },\r\n    createShift() {\r\n      var headers = new Headers();\r\n      var formdata = new FormData()\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      formdata.append('shift', this.form_data.shift)\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token)\r\n      fetch(this.api_url+'/security/workers_shift/', {\r\n          method : 'POST',\r\n          body:formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.$bvModal.hide('attendance')\r\n          this.changePage(1)\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n  @import '../styles/style.css';\r\n\r\n  \r\n</style>\r\n"]}]}