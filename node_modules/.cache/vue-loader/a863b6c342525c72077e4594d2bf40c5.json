                 \r\n            </slot>\r\n            <div class=\"d-flex\">\r\n               <b-button variant=\"success\" type=\"submit\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                >Save\r\n              </b-button>\r\n              <router-link to=\"/app/employee/internal-inspector\">\r\n                <b-button variant=\"danger\" class=\" px-3 py-1  d-sm-block mx-1\"\r\n                  >Discard\r\n                </b-button>\r\n              </router-link>\r\n            </div>\r\n          </div>\r\n          <div class=\" my-3 border-top\"></div>\r\n      </div>\r\n      <b-row class=\"main-scroll pt-2\">\r\n        <b-col lg=\"12\" xl=\"12\" class=\"\">\r\n          <b-card  header-bg-variant=\"transparent\" class=\"card-scroll\">\r\n            <div>\r\n              <div class=\"ul-contact-detail__timeline\">\r\n                <div class=\"row\">\r\n                  <div class=\" col-md-6\">\r\n                    <div class=\"user-info pt-2\">\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\">Name: </span>\r\n                        <input placeholder=\"Name\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.name\" name=\"name\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\">Phone: </span>\r\n                        <input placeholder=\"Phone\" type=\"text\" class=\"ui-autocomplete-input\" autocomplete=\"off\" v-model=\"form_data.phone\" name=\"phone\" required>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\"> AEO: </span>\r\n                        <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"aeo\" v-model=\"form_data.aeo\" @change=\"form_data.farmers=[];getVfcs()\">\r\n                            <option v-for=\"aeo in aeos\" :value=\"aeo.id\">{{aeo.name}}</option>\r\n                        </select>\r\n                      </div>\r\n                      <div class=\"o_thread_search_input\">\r\n                        <span style=\"width:120px;\"> VFC: </span>\r\n                        <select class=\"ui-autocomplete-input w-100 mx-1\" name=\"vfcs\" v-model=\"form_data.vfcs\" multiple @change=\"getFarmers()\" style=\"height:50vh\">\r\n                            <option v-for=\"vfc in vfcs\" :value=\"vfc.id\">{{vfc.name}}</option>\r\n                        </select>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-md-6 border-left\" style=\"height:75vh;overflow-y:auto\">\r\n                    <div class=\"p-1\">\r\n                      <b>Farmers ({{form_data.farmers.length}}/{{farmers.length}})</b>\r\n                      <b-form-checkbox v-model=\"select_all\" @change=\"selectAll()\">\r\n                        <b>#Selct All</b>\r\n                      </b-form-checkbox></th>\r\n                      <label class=\"w-100 border cursor-pointer p-1\" v-for=\"farmer in farmers\">\r\n                        <b-form-checkbox v-model=\"form_data.farmers\" name=\"farmers\" :value=\"farmer.id\">\r\n                          <span class=\"cursor-pointer\"><b>{{farmer.name}}</b> <span class=\"text-info\">{{farmer.farmer_code}}</span></span>\r\n                        </b-form-checkbox>\r\n                      </label>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport LocationFields from '/src/views/app/oodo/components/location_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Employee - Internal Inspector\"\r\n  },\r\n  components:{\r\n    LocationFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{aeo:null, vfcs:[], farmers:[]},\r\n      aeos:[],\r\n      vfcs:[],\r\n      farmers:[],\r\n      select_all:false,\r\n      id:null,\r\n      action:'create',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'employee'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }else{\r\n      this.getAeos()\r\n    }\r\n  },\r\n  methods: {\r\n    selectAll(){\r\n        this.form_data.farmers = []\r\n        var self = this\r\n        if(!this.select_all){\r\n          this.farmers.forEach(function(farm){\r\n            self.form_data.farmers.push(farm.id)\r\n          })\r\n        }\r\n    },\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/internal_inspector/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        this.getAeos()\r\n      })\r\n    },\r\n    getAeos() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/employee/aeo/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.aeos = jsonData\r\n        if(this.form_data.aeo){\r\n          this.getVfcs()\r\n        }\r\n      })\r\n    },\r\n    getVfcs() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token)\r\n      fetch(this.api_url+'/fpo/vfc/?aeo='+this.form_data.aeo, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmers = []\r\n        this.vfcs = jsonData\r\n        this.not_start = false\r\n        if(this.form_data.vfcs.length>0){\r\n          this.getFarmers()\r\n        }\r\n      })\r\n    },\r\n    getFarmers() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token)\r\n      fetch(this.api_url+'/fpo/get_farmer_by_vfc/?data_for_farmers=yes&vfcs='+this.form_data.vfcs+'&farmer_for_inspector='+this.id, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.farmers = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.action == 'create'){\r\n        this.createFormdata()\r\n      }else{\r\n        this.updateFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/internal_inspector/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/fpo/internal_inspector/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n  @import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}