warehouses\" v-if=\"form_data.warehouse==warehouse.id\">\r\n                      <h5>{{warehouse.name}}</h5>\r\n                      <div v-for=\"department in departments\" v-if=\"department.id==form_data.department\">\r\n                        <b>{{department.name}} Department </b>\r\n                        <table class=\"w-100 ml-2\">\r\n                          <tr v-for=\"main_production_process in department.production_processes\">\r\n                            <th>\r\n                              <b-form-checkbox :name=\"'main_production_process'\" v-model=\"form_data.main_production_process\" :id=\"'main_production_process'+main_production_process.id\" :value=\"main_production_process.id\" @change=\"selectAllProcess(main_production_process)\">\r\n                                <b>{{main_production_process.name}}</b>\r\n                              </b-form-checkbox>\r\n                              <div>\r\n                                <div v-for=\"production_process in main_production_process.production_processes\" v-if=\"form_data.main_production_process.includes(main_production_process.id)\">\r\n                                  <td class=\"pl-3 mt-3\">\r\n                                    <b-form-checkbox name=\"production_process\" v-model=\"form_data.production_process\" :id=\"'production_process'+production_process.id\" :value=\"production_process.id\">\r\n                                      <b>{{production_process.name}}</b>\r\n                                    </b-form-checkbox>\r\n                                  </td>\r\n                                </div>\r\n                              </div>\r\n                            </th>\r\n                          </tr>\r\n                          <tr>\r\n                            <td class=\"pt-2\">\r\n                              <b-form-checkbox name=\"is_attendance\" v-model=\"form_data.is_attendance\" :id=\"'is_attendance'+department.id\">\r\n                                <b>Attendance</b>\r\n                              </b-form-checkbox>\r\n                            </td>\r\n                          </tr>\r\n                          <tr>\r\n                            <td class=\"\">\r\n                              <b-form-checkbox name=\"is_printing\" v-model=\"form_data.is_printing\" :id=\"'is_printing'+department.id\">\r\n                                <b>Printing</b>\r\n                              </b-form-checkbox>\r\n                            </td>\r\n                          </tr>\r\n                          <tr>\r\n                            <td class=\"\">\r\n                              <b-form-checkbox name=\"is_clean_machine\" v-model=\"form_data.is_clean_machine\" :id=\"'is_clean_machine'+department.id\">\r\n                                <b>Clean Machine</b>\r\n                              </b-form-checkbox>\r\n                            </td>\r\n                          </tr>\r\n                        </table>\r\n                      </div>\r\n                    </div>\r\n                    <div v-if=\"form_data.warehouse==-1\">\r\n                      <div v-for=\"yard in yards\" v-if=\"form_data.yards.includes(yard.id)\">\r\n                        <b>{{yard.name}}</b>\r\n                        <table class=\"w-100 ml-2\">\r\n                          <tr>\r\n                            <td class=\"pb-2\">\r\n                              <b-form-checkbox name=\"is_attendance\" v-model=\"form_data.is_attendance\" :id=\"'is_attendance'+yard.id\">\r\n                                <b>Attendance</b>\r\n                              </b-form-checkbox>\r\n                            </td>\r\n                          </tr>\r\n                        </table>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <hr>\r\n              </div>\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n      </b-row>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport ProductFields from '/src/views/app/oodo/components/product_fields'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"PRODUCTION - PLANNING\"\r\n  },\r\n  components:{\r\n    ProductFields\r\n  },\r\n  data() {\r\n    return {\r\n      form_data:{warehouse:null, department:null, main_production_process:[],production_process:[],date:null, yard:null},\r\n      id:null,\r\n      action:'create',\r\n      departments:[],\r\n      yards:[],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$root.current_section = 'production'\r\n    this.action = this.$route.params.action\r\n    if(this.action == 'edit'){\r\n      this.id = this.$route.params.id\r\n      this.getForm()\r\n    }\r\n    this.getWarehouses()\r\n    this.getDepartments()\r\n    this.getYards()\r\n  },\r\n  methods: {\r\n    getForm() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/planning/'+this.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.form_data = jsonData\r\n        if(this.yards.length>0){\r\n          this.form_data.warehouse = -1\r\n        }\r\n      })\r\n    },\r\n    getDepartments() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/departments/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.departments = jsonData\r\n      })\r\n    },\r\n    selectAllProcess(main_production_process){\r\n      var self = this\r\n      main_production_process.production_processes.forEach(function(val){\r\n        if(!self.form_data.production_process.includes(val.id)){\r\n          self.form_data.production_process.push(val.id)\r\n        }\r\n      })\r\n    },\r\n    getYards() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/product/yards/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.yards = jsonData\r\n      })\r\n    },\r\n    dataForm() {\r\n      if(this.id){\r\n        this.updateFormdata()\r\n      }else{\r\n        this.createFormdata()\r\n      }\r\n    },\r\n    createFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('manager', this.form_data.manager)\r\n      formdata.append('date', this.form_data.date)\r\n      formdata.append('supervisor', this.form_data.supervisor)\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/planning/', {\r\n          method : 'POST',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.showAlert('Saved Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n    updateFormdata() {\r\n      var formdata = new FormData(this.$refs['dataForm'])\r\n      formdata.append('manager', this.form_data.manager)\r\n      formdata.append('date', this.form_data.date)\r\n      formdata.append('supervisor', this.form_data.supervisor)\r\n      if(this.lock_btn){\r\n        return false\r\n      }\r\n      this.lock_btn = true\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Token \"+this.$root.token);\r\n      fetch(this.api_url+'/production/planning/'+this.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lock_btn = false\r\n        if(jsonData.id>0){\r\n          this.showAlert('Updated Successfully!')\r\n        }else{\r\n          this.showAlert('Something Wrong!')\r\n        }\r\n      })\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style >\r\n\r\n@import '../styles/style.css';\r\n\r\n\r\n</style>\r\n"]}]}